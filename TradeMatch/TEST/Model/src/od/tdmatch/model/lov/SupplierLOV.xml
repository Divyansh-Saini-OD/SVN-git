<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SupplierLOV"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.lov.SupplierLOVRowImpl"
  ComponentClass="od.tdmatch.model.lov.SupplierLOVImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.lov.SupplierLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT distinct sup.segment1 supplier_no, sup.vendor_name supplier    
  FROM ap_supplier_sites_all site,    
       ap_suppliers sup    
 WHERE EXISTS (SELECT  /*+ LEADING(h) */   
                                                                                'x'    
                 FROM  ap_invoices_all ai,    
                       (SELECT distinct invoice_id    
                          FROM ap_holds_all     
                         WHERE creation_date > '01-JAN-11'     
                           AND NVL(status_flag,'S')= 'S'    
                       ) h    
                WHERE ai.invoice_id=h.invoice_id    
                  AND ai.invoice_num not like '%ODDBUIA'  
                  AND ai.creation_date>'01-JAN-11'    
                  AND ai.cancelled_date IS NULL    
                  AND ai.vendor_id=sup.vendor_id    
                  AND ai.vendor_site_id=site.vendor_site_id    
                  AND EXISTS (SELECT 'x'    
                                FROM  xx_fin_translatevalues tv    
                                     ,xx_fin_translatedefinition td    
                               WHERE td.TRANSLATION_NAME = 'XX_AP_TR_MATCH_INVOICES'    
                                 AND tv.TRANSLATE_ID  = td.TRANSLATE_ID    
                                 AND tv.enabled_flag='Y'    
                                 AND SYSDATE BETWEEN tv.start_date_active and NVL(tv.end_date_active,sysdate)    
                                 AND tv.target_value1||''=ai.source    
                             )    
                   AND EXISTS (SELECT  'x'                                                   
                                 FROM  po_lines_all pl                                                   
                               WHERE pl.po_header_id = NVL(ai.po_header_id,ai.quick_po_header_id)                                                   
                             )                                                                                                                                   
                 )                                 
   AND site.vendor_id=sup.vendor_id                      
   AND exists (SELECT 'x'  
         FROM xx_fin_translatevalues tv,                                                                   
              xx_fin_translatedefinition td                   
        WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                    
          AND tv.translate_id  = td.translate_id                                                                   
          AND tv.enabled_flag = 'Y'                                                                   
          AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)  
          AND tv.target_value1 = site.attribute8)]]></SQLQuery>
  <ViewAttribute
    Name="SupplierNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUPPLIER_NO"
    Expression="SUPPLIER_NO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.lov.SupplierLOV.SupplierNo_LABEL"/>
        <DISPLAYWIDTH
          Value="10"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Supplier"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUPPLIER"
    Expression="SUPPLIER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.lov.SupplierLOV.Supplier_LABEL"/>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="SupplierNo"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="od.tdmatch.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

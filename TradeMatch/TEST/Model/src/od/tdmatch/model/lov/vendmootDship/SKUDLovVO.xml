<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SKUDLovVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.lov.vendmootDship.SKUDLovVORowImpl"
  ComponentClass="od.tdmatch.model.lov.vendmootDship.SKUDLovVOImpl"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.lov.vendmootDship.SKUDLovVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT msi.segment1 SKU_Number       
  FROM mtl_system_items_b msi      
 WHERE msi.organization_id=441             
   AND EXISTS (SELECT /*+ LEADING(h) */   
					  'x'      
                 FROM po_lines_all pl,      
                       ap_invoices_all ai,      
					   (SELECT distinct invoice_id      
						  FROM ap_holds_all       
                         WHERE creation_date > '01-JAN-11'       
                           AND NVL(status_flag,'S')= 'S'      
                       ) h      
                WHERE ai.invoice_id=h.invoice_id      
                  AND ai.invoice_num not like '%ODDBUIA'  
				  AND ai.creation_date>'01-JAN-11'      
                  AND ai.cancelled_date IS NULL      
				  AND ai.source='US_OD_DROPSHIP'				     
				  AND pl.po_header_id=NVL(ai.po_header_id,ai.quick_po_header_id)      
                  AND pl.item_id=msi.inventory_item_id      
				  AND EXISTS (SELECT 'x'      
                                                FROM ap_supplier_sites_all site      
                                               WHERE vendor_site_id=ai.vendor_site_id      
                                                 AND exists (SELECT 'x'  
                                                             FROM xx_fin_translatevalues tv,                                                                   
                                                                  xx_fin_translatedefinition td                   
                                                            WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                    
                                                              AND tv.translate_id  = td.translate_id                                                                   
                                                              AND tv.enabled_flag = 'Y'                                                                   
                                                              AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)  
                                                              AND tv.target_value1 = site.attribute8)      
                                              )      
              )]]></SQLQuery>
  <ViewAttribute
    Name="SkuNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SKU_NUMBER"
    Expression="SKU_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.lov.vendmootDship.SKUDLovVO.SkuNumber_LABEL"/>
        <DISPLAYWIDTH
          Value="15"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="SkuNumber"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="od.tdmatch.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

  <SQLQuery><![CDATA[SELECT /*+ (LEADING h) */            
        ai.invoice_id,            
       (SELECT tv.target_value2             
         FROM xx_fin_translatevalues tv,                                                               
              xx_fin_translatedefinition td               
        WHERE td.translation_name = 'XX_AP_VENDOR_ASSISTANTS'                                                                
          AND tv.translate_id  = td.translate_id                                                               
          AND tv.target_value1 = aps.attribute6||''                                                               
          AND tv.enabled_flag = 'Y'                                                               
          AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)) vendor_assistant,            
       ai.org_id,                                   
       ai.source,                                   
       ai.invoice_num,                                   
       ai.invoice_date,                                          
       ai.voucher_num,                                                   
       ai.invoice_amount,                                                               
       ai.attribute5 PO_Type,                                            
       NVL(py.discount_date,py.due_date) due_date,                                          
       ph.segment1 po_number,                                   
       asup.vendor_name supplier_name,                                                               
       asup.segment1 supplier_no,                                                               
       aps.vendor_site_code supplier_site,                                        
       DECODE(SIGN(instr(e.name,'/')),0,NULL,1,'D') discount,                                                               
       ltrim(SUBSTR(hrl.location_code,1,6),'0') location,                                                               
       0 Qty_Hold_Amt,                                      
       0 Price_Hold_Amt,         
                
'N' Frt_Hold,                                     
           'N' po_hold_chr,                     
           DECODE(SUBSTR(ai.attribute5,1,1),'F','Y','N') front_doorC,                
		   (CASE WHEN SUBSTR(ai.attribute5,1,1) = 'N' OR SUBSTR(ai.attribute5,10,1) = 'N' then 'Y' else 'N' end) non_codec,                                                 
           DECODE(SUBSTR(ai.attribute5,1,1),'D','Y','N') drop_shipc ,           
           ph.creation_date podate ,        
           1 sku,       
           'Y' pricingExcp,       
           'Y' quantityExcp,       
           XX_AP_TRADE_MATCH_UTL_PKG.xx_ap_freight_invoice(ai.invoice_id) freightExcp      
  FROM ap_terms e,                                                               
       hr_locations_all hrl,             
       po_headers_all ph,            
       ap_payment_schedules_all py,            
       ap_invoices_all ai,            
       ap_suppliers asup,            
       ap_supplier_sites_all aps,            
       (SELECT distinct invoice_id            
          FROM ap_holds_all             
         WHERE creation_date > '01-JAN-11'             
           AND NVL(status_flag,'S')= 'S'  
           AND hold_lookup_code IN ('MAX QTY ORD','QTY ORD','QTY REC','AMT ORD','MAX AMT REC','MAX AMT ORD','AMT REC','OD MAX PRICE','OD Favorable','PRICE','LINE VARIANCE','DIST VARIANCE')  
        ) h            
WHERE             
  ai.invoice_id=h.invoice_id    
  AND py.invoice_id=ai.invoice_id     
  AND aps.vendor_id=ai.vendor_id     
  and aps.vendor_site_id=ai.vendor_site_id     
  AND asup.vendor_id = aps.vendor_id          
  AND ph.po_header_id = NVL(ai.po_header_id,ai.quick_po_header_id)             
  AND hrl.location_id = ph.ship_to_location_id            
  AND exists (SELECT 'x'         
         FROM xx_fin_translatevalues tv,                                                                          
              xx_fin_translatedefinition td                          
        WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                           
          AND tv.translate_id  = td.translate_id                                                                          
          AND tv.enabled_flag = 'Y'                                                                          
          AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)         
          AND tv.target_value1 = aps.attribute8)              
  AND e.term_id=ai.terms_id                                                                 
  AND EXISTS (SELECT  'x'                                                           
                FROM mtl_system_items_b msi,                                                           
                     po_lines_all pl                                                           
               WHERE pl.po_header_id = NVL(ai.po_header_id,ai.quick_po_header_id)                                                           
                 AND msi.inventory_item_id = pl.item_id                                                               
                 AND to_char(msi.organization_id) = '441'            
                 and msi.segment1=NVL(:skunum,msi.segment1)              
              )            
   AND EXISTS (SELECT 'x'            
                 FROM  xx_fin_translatevalues tv            
                      ,xx_fin_translatedefinition td            
                WHERE td.TRANSLATION_NAME = 'XX_AP_TR_MATCH_INVOICES'            
                  AND tv.TRANSLATE_ID  = td.TRANSLATE_ID            
                  AND tv.enabled_flag='Y'            
                  AND SYSDATE BETWEEN tv.start_date_active and NVL(tv.end_date_active,sysdate)            
                  AND tv.target_value1=ai.source            
                 )]]></SQLQuery>

<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="vend-moot-detail1-btf">
    <default-activity>init</default-activity>
    <transaction>
      <requires-existing-transaction/>
    </transaction>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__6">
      <name>pSupplierName</name>
      <value>#{pageFlowScope.pSupplierName}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <managed-bean id="__5">
      <managed-bean-name>InvWkBenchCommonBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.managedBeans.InvWkBenchCommonBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="VendorMootDetail">
      <page>/VendorMootDetailsC.jsff</page>
    </view>
    <task-flow-return id="Return">
      <outcome>
        <name>Return</name>
      </outcome>
    </task-flow-return>
    <method-call id="init">
      <method>#{pageFlowScope.InvWkBenchCommonBean.executeVendorMootDtVO}</method>
      <outcome>
        <fixed-outcome>step1</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>init</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>step1</from-outcome>
        <to-activity-id>VendorMootDetail</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>VendorMootDetail</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>Home</from-outcome>
        <to-activity-id>Return</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

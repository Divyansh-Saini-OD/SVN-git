<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="emp-vend-summary-btf">
    <default-activity>MootSummary</default-activity>
    <transaction>
      <new-transaction/>
    </transaction>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <task-flow-reentry>
      <reentry-allowed/>
    </task-flow-reentry>
    <input-parameter-definition id="__6">
      <name>pEmployeeId</name>
      <value>#{pageFlowScope.pEmployeeId}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <input-parameter-definition id="__2">
      <name>mSuppName</name>
      <value>#{pageFlowScope.mSuppName}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <input-parameter-definition id="__19">
      <name>invoiceNo</name>
      <value>#{pageFlowScope.invoiceNo}</value>
      <class>java.lang.Number</class>
    </input-parameter-definition>
    <input-parameter-definition id="__15">
      <name>pVAName</name>
      <value>#{pageFlowScope.pVAName}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <input-parameter-definition id="__17">
      <name>pMoot</name>
      <value>#{pageFlowScope.pMoot}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <input-parameter-definition id="__18">
      <name>pNrf</name>
      <value>#{pageFlowScope.pNrf}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <input-parameter-definition id="__26">
      <name>mSuppSite</name>
      <value>#{pageFlowScope.mSuppSite}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <managed-bean id="__3">
      <managed-bean-name>InvWkBenchCommonBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.managedBeans.InvWkBenchCommonBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__20">
      <managed-bean-name>InvoiceDetailsBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.managedBeans.InvoiceDetailsBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__16">
      <managed-bean-name>MyDate</managed-bean-name>
      <managed-bean-class>java.util.Date</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__28">
      <managed-bean-name>vendorMootBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.managedBeans.vendorMootBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="EmpInvSummary">
      <page>/EmpVendorSUmmary.jsff</page>
    </view>
    <method-call id="methodCall1">
      <method>#{pageFlowScope.InvWkBenchCommonBean.executeVAEmpVendorSummaryVO}</method>
      <outcome>
        <fixed-outcome>view</fixed-outcome>
      </outcome>
    </method-call>
    <view id="VendMoot">
      <page>/VendorMootDetailsC.jsff</page>
    </view>
    <method-call id="BeforeCallingVendMoot">
      <method>#{pageFlowScope.InvWkBenchCommonBean.executeVendorMootDtVO}</method>
      <outcome>
        <fixed-outcome>Step2</fixed-outcome>
      </outcome>
    </method-call>
    <view id="MootSummary">
      <page>/MootSummary.jsff</page>
    </view>
    <method-call id="initInvDetail">
      <method>#{pageFlowScope.InvoiceDetailsBean.invoiceDetails}</method>
      <outcome>
        <to-string/>
      </outcome>
    </method-call>
    <view id="InvDetail">
      <page>/InvoiceDetails.jsff</page>
    </view>
    <control-flow-rule id="__4">
      <from-activity-id>EmpInvSummary</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>return</from-outcome>
        <to-activity-id>MootSummary</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__1">
        <from-outcome>VendorMoot</from-outcome>
        <to-activity-id>BeforeCallingVendMoot</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__9">
      <from-activity-id>methodCall1</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>view</from-outcome>
        <to-activity-id>EmpInvSummary</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__8">
      <from-activity-id>BeforeCallingVendMoot</from-activity-id>
      <control-flow-case id="__11">
        <from-outcome>Step2</from-outcome>
        <to-activity-id>VendMoot</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>VendMoot</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome>return2</from-outcome>
        <to-activity-id>methodCall1</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__23">
        <from-outcome>Step3</from-outcome>
        <to-activity-id>initInvDetail</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__21">
      <from-activity-id>MootSummary</from-activity-id>
      <control-flow-case id="__22">
        <from-outcome>EmpVendSummary</from-outcome>
        <to-activity-id>methodCall1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__24">
      <from-activity-id>InvDetail</from-activity-id>
      <control-flow-case id="__25">
        <from-outcome>return3</from-outcome>
        <to-activity-id>BeforeCallingVendMoot</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>initInvDetail</from-activity-id>
      <control-flow-case id="__14">
        <from-outcome>Step4</from-outcome>
        <to-activity-id>InvDetail</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__27">
        <from-outcome>Step5</from-outcome>
        <to-activity-id>VendMoot</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="invoice-not-validated-btf">
    <default-activity>initInvoiceNotValidated</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>InvoicesNotValidateBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.managedBeans.InvoicesNotValidateBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__3">
      <managed-bean-name>MyDate</managed-bean-name>
      <managed-bean-class>java.util.Date</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="InvoicesNotValidated">
      <page>/InvoicesNotValidated.jsff</page>
    </view>
    <method-call id="initInvoiceNotValidated">
      <method>#{pageFlowScope.InvoicesNotValidateBean.initInvoiceValidated}</method>
      <outcome>
        <fixed-outcome>search</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__2">
      <from-activity-id>initInvoiceNotValidated</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>search</from-outcome>
        <to-activity-id>InvoicesNotValidated</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:f="http://java.sun.com/jsf/core" xmlns:h="http://java.sun.com/jsf/html">
  <af:panelGridLayout id="pgl1">
    <af:gridRow marginTop="5px" id="gr1" marginBottom="5px">
      <af:gridCell marginStart="5px" width="100%" marginEnd="5px" id="gc1">
        <af:outputFormatted value="Match Error Employee's Workable Summary : " id="of1" inlineStyle="font-size:large;"/>
        <af:outputFormatted value="#{MyDate}" id="of2" inlineStyle="font-size:large;">
          <af:convertDateTime pattern="MM/dd/YYYY"/>
        </af:outputFormatted>
      </af:gridCell>
    </af:gridRow>
    <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="gr2">
      <af:gridCell marginStart="5px" width="100%" marginEnd="5px" id="gc2" halign="stretch">
        <af:panelCollection id="pc1" defaultToolbarItemDisplay="icon" inlineStyle="width:980px;">
          <f:facet name="menus">
            <af:menu text="File" id="m1">
              <af:commandMenuItem text="Export to Excel" id="cmi1">
                <af:exportCollectionActionListener type="excelHTML" exportedId="t1" filename="MootSummaryData.xls"
                                                   title="MootSummaryData"/>
              </af:commandMenuItem>
            </af:menu>
          </f:facet>
          <f:facet name="toolbar"/>
          <f:facet name="statusbar"/>
          <af:table value="#{bindings.VAInvoiceSummaryVO2.collectionModel}" var="row"
                    rows="#{bindings.VAInvoiceSummaryVO2.rangeSize}"
                    emptyText="#{bindings.VAInvoiceSummaryVO2.viewable ? 'No data to display.' : 'Access Denied.'}"
                    rowBandingInterval="0" selectedRowKeys="#{bindings.VAInvoiceSummaryVO2.collectionModel.selectedRow}"
                    selectionListener="#{bindings.VAInvoiceSummaryVO2.collectionModel.makeCurrent}"
                    rowSelection="single" fetchSize="#{bindings.VAInvoiceSummaryVO2.rangeSize}" id="t1"
                    styleClass="AFStretchWidth">
            <af:column headerText="#{bindings.VAInvoiceSummaryVO2.hints.VendorAssistant.label}" id="c10" width="130"
                       headerNoWrap="true">
              <af:link text="#{row.VendorAssistant}" id="l1" action="EmpVendSummary" selected="true">
                <af:setPropertyListener from="#{row.EmployeeId}" to="#{pageFlowScope.pEmployeeId}" type="action"/>
                <af:setPropertyListener from="#{row.VendorAssistant}" to="#{pageFlowScope.pVAName}" type="action"/>
              </af:link>
              <f:facet name="footer">
                <af:outputText value="Total" id="ot17"/>
              </f:facet>
            </af:column>
            <af:column headerText="#{bindings.VAInvoiceSummaryVO2.hints.TotalInvCount.label}" id="c1" align="right"
                       width="70">
              <af:outputText value="#{row.TotalInvCount}"
                             shortDesc="#{bindings.VAInvoiceSummaryVO2.hints.TotalInvCount.tooltip}" id="ot18">
                <af:convertNumber groupingUsed="false"
                                  pattern="#{bindings.VAInvoiceSummaryVO2.hints.TotalInvCount.format}"/>
              </af:outputText>
              <f:facet name="footer">
                <af:outputText value="#{bindings.SumTotInvCount}" id="ot1"/>
              </f:facet>
            </af:column>
            <af:column headerText="#{bindings.VAInvoiceSummaryVO2.hints.TotalInvAmount.label}" id="c3" align="right"
                       width="85">
              <af:outputText value="#{row.TotalInvAmount}"
                             shortDesc="#{bindings.VAInvoiceSummaryVO2.hints.TotalInvAmount.tooltip}" id="ot3"
                             inlineStyle="text-align:right;" converter="javax.faces.BigDecimal">
                <af:convertNumber groupingUsed="false"
                                  pattern="#{bindings.VAInvoiceSummaryVO2.hints.TotalInvAmount.format}"/>
              </af:outputText>
              <f:facet name="footer">
                <af:outputText value="#{bindings.SumTotInvAmount}" id="ot10"/>
              </f:facet>
            </af:column>
            <af:column headerText="#{bindings.VAInvoiceSummaryVO2.hints.TotalLineAmount.label}" id="c4" align="right"
                       width="85">
              <af:outputText value="#{row.TotalLineAmount}"
                             shortDesc="#{bindings.VAInvoiceSummaryVO2.hints.TotalLineAmount.tooltip}" id="ot4"
                             inlineStyle="text-align:right;">
                <af:convertNumber groupingUsed="false"
                                  pattern="#{bindings.VAInvoiceSummaryVO2.hints.TotalLineAmount.format}"/>
              </af:outputText>
              <f:facet name="footer">
                <af:outputText value="#{bindings.SumTotLineAmount}" id="ot11"/>
              </f:facet>
            </af:column>
            <af:column headerText="#{bindings.VAInvoiceSummaryVO2.hints.TotalMootCount.label}" id="c5" align="right">
              <af:outputText value="#{row.TotalMootCount}"
                             shortDesc="#{bindings.VAInvoiceSummaryVO2.hints.TotalMootCount.tooltip}" id="ot5"
                             inlineStyle="text-align:right;">
                <af:convertNumber groupingUsed="false"
                                  pattern="#{bindings.VAInvoiceSummaryVO2.hints.TotalMootCount.format}"/>
              </af:outputText>
              <f:facet name="footer">
                <af:outputText value="#{bindings.SumTotMootCount}" id="ot12"/>
              </f:facet>
            </af:column>
            <af:column headerText="#{bindings.VAInvoiceSummaryVO2.hints.TotalMootInvAmount.label}" id="c6" align="right"
                       width="85">
              <af:outputText value="#{row.TotalMootInvAmount}"
                             shortDesc="#{bindings.VAInvoiceSummaryVO2.hints.TotalMootInvAmount.tooltip}" id="ot6"
                             inlineStyle="text-align:right;">
                <af:convertNumber groupingUsed="false"
                                  pattern="#{bindings.VAInvoiceSummaryVO2.hints.TotalMootInvAmount.format}"/>
              </af:outputText>
              <f:facet name="footer">
                <af:outputText value="#{bindings.SumTotMootInvAmount}" id="ot13"/>
              </f:facet>
            </af:column>
            <af:column headerText="#{bindings.VAInvoiceSummaryVO2.hints.TotalMootLineAmount.label}" id="c7"
                       align="right" width="85">
              <af:outputText value="#{row.TotalMootLineAmount}"
                             shortDesc="#{bindings.VAInvoiceSummaryVO2.hints.TotalMootLineAmount.tooltip}" id="ot7"
                             inlineStyle="text-align:right;">
                <af:convertNumber groupingUsed="false"
                                  pattern="#{bindings.VAInvoiceSummaryVO2.hints.TotalMootLineAmount.format}"/>
              </af:outputText>
              <f:facet name="footer">
                <af:outputText value="#{bindings.SumTotMootLineAmount}" id="ot14"/>
              </f:facet>
            </af:column>
            <af:column headerText="#{bindings.VAInvoiceSummaryVO2.hints.TotalNrfCount.label}" id="c8" align="right">
              <af:outputText value="#{row.TotalNrfCount}"
                             shortDesc="#{bindings.VAInvoiceSummaryVO2.hints.TotalNrfCount.tooltip}" id="ot8"
                             inlineStyle="text-align:right;">
                <af:convertNumber groupingUsed="false"
                                  pattern="#{bindings.VAInvoiceSummaryVO2.hints.TotalNrfCount.format}"/>
              </af:outputText>
              <f:facet name="footer">
                <af:outputText value="#{bindings.SumTotNrfCount}" id="ot15"/>
              </f:facet>
            </af:column>
            <af:column headerText="#{bindings.VAInvoiceSummaryVO2.hints.TotalNrfAmount.label}" id="c9" align="right"
                       width="70">
              <af:outputText value="#{row.TotalNrfAmount}"
                             shortDesc="#{bindings.VAInvoiceSummaryVO2.hints.TotalNrfAmount.tooltip}" id="ot9">
                <af:convertNumber groupingUsed="false"
                                  pattern="#{bindings.VAInvoiceSummaryVO2.hints.TotalNrfAmount.format}"/>
              </af:outputText>
              <f:facet name="footer">
                <af:outputText value="#{bindings.SumTotNrfAmount}" id="ot16"/>
              </f:facet>
            </af:column>
          </af:table>
        </af:panelCollection>
      </af:gridCell>
    </af:gridRow>
  </af:panelGridLayout>
  <af:spacer width="10" height="10" id="s2"/>
    <af:panelGroupLayout id="pgl4" layout="horizontal" halign="left">
        <af:outputText value="Total # of Records" id="ot26"/>
        <af:spacer width="10" height="10" id="s1"/>
        <af:outputText value="#{bindings.VAInvoiceSummaryVO2Iterator.estimatedRowCount}" id="ot31"/>
    </af:panelGroupLayout>
</jsp:root>

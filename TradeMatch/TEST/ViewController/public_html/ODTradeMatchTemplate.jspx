<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:f="http://java.sun.com/jsf/core">
    <jsp:directive.page contentType="text/html;charset=UTF-8"/>
    <af:pageTemplateDef var="attrs" definition="private">
        <af:xmlContent>
            <component xmlns="http://xmlns.oracle.com/adf/faces/rich/component">
                <description/>
                <display-name>ODTradeMatchTemplate</display-name>
                <facet>
                    <facet-name>
                        LeftFacet
                    </facet-name>
                </facet>
                <facet>
                    <facet-name>
                        RightFacet
                    </facet-name>
                </facet>
                <attribute>
                    <attribute-name>
                        HeaderTitle
                    </attribute-name>
                    <attribute-class>
                        java.lang.String
                    </attribute-class>
                    <required>
                        true
                    </required>
                </attribute>
                <attribute>
                    <attribute-name>
                        TabLabel
                    </attribute-name>
                    <attribute-class>
                        java.lang.String
                    </attribute-class>
                    <required>
                        true
                    </required>
                </attribute>
            </component>
        </af:xmlContent>
        <af:decorativeBox id="pt_db1">
            <f:facet name="top">
                <af:panelGridLayout id="pt_pgl2">
                    <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="pt_gr1">
                        <af:gridCell marginStart="5px" width="25%" id="pt_gc1"/>
                        <af:gridCell marginStart="5px" width="50%" id="pt_gc2">
                            <af:outputFormatted value="#{attrs.HeaderTitle}" id="pt_of1"
                                                inlineStyle="font-size:large; font-variant:titling-caps; font-weight:bold;"/>
                        </af:gridCell>
                        <af:gridCell marginStart="5px" width="25%" marginEnd="5px" id="pt_gc3"/>
                    </af:gridRow>
                </af:panelGridLayout>
            </f:facet>
            <f:facet name="center">
                <af:panelSplitter id="pt_ps1">
                    <f:facet name="first">
                        <af:panelGroupLayout id="pt_pgl1" inlineStyle="background-color:inherit;">
                            <af:facetRef facetName="LeftFacet"/>
                        </af:panelGroupLayout>
                    </f:facet>
                    <f:facet name="second">
                        <af:facetRef facetName="RightFacet"/>
                    </f:facet>
                </af:panelSplitter>
            </f:facet>
        </af:decorativeBox>
    </af:pageTemplateDef>
</jsp:root>

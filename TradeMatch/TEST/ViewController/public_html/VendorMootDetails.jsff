<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:f="http://java.sun.com/jsf/core">
  <af:panelGridLayout id="pgl1" inlineStyle="column-count:2;">
    <af:gridRow marginTop="5px" height="10px" id="gr1">
      <af:gridCell marginStart="5px" width="auto" marginEnd="5px" id="gc1">
        <af:outputFormatted value="Office Depot - AP Trade - Match Exception - Vendor's MOOT Details : " id="of1"
                            inlineStyle="font-size:large;"/>
        <af:outputFormatted value="#{MyDate}" id="of3" inlineStyle="font-size:large;">
          <af:convertDateTime pattern="MM/dd/YYYY"/>
        </af:outputFormatted>
      </af:gridCell>
    </af:gridRow>
    <af:gridRow marginTop="5px" height="10px" id="gr2">
      <af:gridCell marginStart="5px" width="auto" marginEnd="5px" id="gc2"/>
    </af:gridRow>
    <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="gr3">
      <af:gridCell marginStart="5px" width="auto" marginEnd="5px" id="gc3">
        <af:panelGroupLayout layout="vertical" id="pgl2">
          <af:query id="qryId1" headerText="Search" disclosed="true"
                    value="#{bindings.VendMootDtVOCriteriaQuery.queryDescriptor}"
                    model="#{bindings.VendMootDtVOCriteriaQuery.queryModel}"
                    queryListener="#{customQueryBean.customizeQueryMethod}"
                    queryOperationListener="#{bindings.VendMootDtVOCriteriaQuery.processQueryOperation}"
                    resultComponentId="::pc1:resId1" rows="6" maxColumns="3" saveQueryMode="hidden"
                    displayMode="compact" searchButtonText="Go" resetButtonText="Clear" modeChangeVisible="false"
                    modeButtonPosition="footer"/>
                    <f:attribute name="queryExpression" value="#{bindings.VendMootDtVOCriteriaQuery.processQuery}"/>
          <af:panelCollection id="pc1" inlineStyle="width:900px;">
            <f:facet name="menus">
              <af:menu text="File" id="m1">
                <af:commandMenuItem text="Export to Excel" id="cmi1">
                  <af:exportCollectionActionListener type="excelHTML" exportedId="resId1"
                                                     filename="VendorMOOTDetails.xls" title="VendorMOOTDetails"
                                                     exportedRows="all"/>
                </af:commandMenuItem>
              </af:menu>
            </f:facet>
            <f:facet name="toolbar"/>
            <f:facet name="statusbar"/>
            <af:table value="#{bindings.VendMootDtVO1.collectionModel}" var="row"
                      rows="#{bindings.VendMootDtVO1.rangeSize}"
                      emptyText="#{bindings.VendMootDtVO1.viewable ? 'No data to display.' : 'Access Denied.'}"
                      rowBandingInterval="0" selectedRowKeys="#{bindings.VendMootDtVO1.collectionModel.selectedRow}"
                      selectionListener="#{bindings.VendMootDtVO1.collectionModel.makeCurrent}" rowSelection="single"
                      fetchSize="#{bindings.VendMootDtVO1.rangeSize}" id="resId1" styleClass="AFStretchWidth"
                      width="1000">
              <af:column headerText="#{bindings.VendMootDtVO1.hints.VendorAssistant.label}" id="resId1c11"
                         headerNoWrap="true" align="left" width="130">
                <af:outputText value="#{row.VendorAssistant}"
                               shortDesc="#{bindings.VendMootDtVO1.hints.VendorAssistant.tooltip}" id="ot10"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.SupplierName.label}" id="resId1c7"
                         headerNoWrap="true" width="150">
                <af:outputText value="#{row.SupplierName}"
                               shortDesc="#{bindings.VendMootDtVO1.hints.SupplierName.tooltip}" id="ot6"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.SupplierNo.label}" id="resId1c8" align="center"
                         width="70">
                <af:outputText value="#{row.SupplierNo}" shortDesc="#{bindings.VendMootDtVO1.hints.SupplierNo.tooltip}"
                               id="ot7"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.SupplierSite.label}" id="resId1c9" align="center"
                         width="100">
                <af:outputText value="#{row.SupplierSite}"
                               shortDesc="#{bindings.VendMootDtVO1.hints.SupplierSite.tooltip}" id="ot8"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.Discount.label}" id="resId1c13" width="30"
                         align="center">
                <af:outputText value="#{row.Discount}" shortDesc="#{bindings.VendMootDtVO1.hints.Discount.tooltip}"
                               id="ot12"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.InvoiceNum.label}" id="resId1c3" width="100">
                <af:outputText value="#{row.InvoiceNum}" shortDesc="#{bindings.VendMootDtVO1.hints.InvoiceNum.tooltip}"
                               id="ot2"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.VoucherNum.label}" id="resId1c14" width="60">
                <af:outputText value="#{row.VoucherNum}" shortDesc="#{bindings.VendMootDtVO1.hints.VoucherNum.tooltip}"
                               id="ot13"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.InvoiceDate.label}" id="resId1c5" minimumWidth="25"
                         width="75" align="center">
                <af:outputText value="#{row.InvoiceDate}"
                               shortDesc="#{bindings.VendMootDtVO1.hints.InvoiceDate.tooltip}" id="ot4">
                  <af:convertDateTime pattern="#{bindings.VendMootDtVO1.hints.InvoiceDate.format}"/>
                </af:outputText>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.PoNumber.label}" id="resId1c4" minimumWidth="25"
                         width="75">
                <af:outputText value="#{row.PoNumber}" shortDesc="#{bindings.VendMootDtVO1.hints.PoNumber.tooltip}"
                               id="ot3"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.Location.label}" id="resId1c15" width="75">
                <af:outputText value="#{row.Location}" shortDesc="#{bindings.VendMootDtVO1.hints.Location.tooltip}"
                               id="ot14"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.DueDate.label}" id="resId1c6" width="75"
                         align="center">
                <af:outputText value="#{row.DueDate}" shortDesc="#{bindings.VendMootDtVO1.hints.DueDate.tooltip}"
                               id="ot5">
                  <af:convertDateTime pattern="#{bindings.VendMootDtVO1.hints.DueDate.format}"/>
                </af:outputText>
                <f:facet name="footer">
                  <af:outputFormatted value="Total" id="of2"/>
                </f:facet>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.InvoiceAmount.label}" id="resId1c16" width="75"
                         align="right">
                <af:outputText value="#{row.InvoiceAmount}"
                               shortDesc="#{bindings.VendMootDtVO1.hints.InvoiceAmount.tooltip}" id="ot15">
                  <af:convertNumber groupingUsed="false"
                                    pattern="#{bindings.VendMootDtVO1.hints.InvoiceAmount.format}"/>
                </af:outputText>
                <f:facet name="footer">
                  <af:outputText value="#{bindings.SumInvAmount.inputValue}"
                                 shortDesc="#{bindings.SumInvAmount.hints.tooltip}" id="ot9"/>
                </f:facet>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.QtyHoldAmt.label}" id="resId1c24" width="60"
                         align="right">
                <af:outputText value="#{row.QtyHoldAmt}" shortDesc="#{bindings.VendMootDtVO1.hints.QtyHoldAmt.tooltip}"
                               id="ot23">
                  <af:convertNumber groupingUsed="false" pattern="#{bindings.VendMootDtVO1.hints.QtyHoldAmt.format}"/>
                </af:outputText>
                <f:facet name="footer">
                  <af:outputText value="#{bindings.SumQtyHoldAmt.inputValue}"
                                 shortDesc="#{bindings.SumQtyHoldAmount.hints.tooltip}" id="ot11"/>
                </f:facet>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.PriceHoldAmt.label}" id="resId1c25" width="60"
                         align="right">
                <af:outputText value="#{row.PriceHoldAmt}"
                               shortDesc="#{bindings.VendMootDtVO1.hints.PriceHoldAmt.tooltip}" id="ot24">
                  <af:convertNumber groupingUsed="false" pattern="#{bindings.VendMootDtVO1.hints.PriceHoldAmt.format}"/>
                </af:outputText>
                <f:facet name="footer">
                  <af:outputText value="#{bindings.SumPriceHoldAmt.inputValue}"
                                 shortDesc="#{bindings.SumPriceHoldAmt.hints.tooltip}" id="ot16"/>
                </f:facet>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.FrontDoorc.label}" id="c3" width="20">
                <af:outputText value="#{row.FrontDoorc}" shortDesc="#{bindings.VendMootDtVO1.hints.FrontDoorc.tooltip}"
                               id="ot18"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.NonCodec.label}" id="c4" width="20">
                <af:outputText value="#{row.NonCodec}" shortDesc="#{bindings.VendMootDtVO1.hints.NonCodec.tooltip}"
                               id="ot19"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.DropShipc.label}" id="c5" width="20">
                <af:outputText value="#{row.DropShipc}" shortDesc="#{bindings.VendMootDtVO1.hints.DropShipc.tooltip}"
                               id="ot20"/>
              </af:column>
              <af:column headerText="#{bindings.VendMootDtVO1.hints.FrtHold.label}" id="c2" width="50">
                <af:outputText value="#{row.FrtHold}" shortDesc="#{bindings.VendMootDtVO1.hints.FrtHold.tooltip}"
                               id="ot17"/>
              </af:column>
            </af:table>
          </af:panelCollection>
        </af:panelGroupLayout>
      </af:gridCell>
    </af:gridRow>
  </af:panelGridLayout>
</jsp:root>

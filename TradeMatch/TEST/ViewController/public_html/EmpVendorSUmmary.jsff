<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:f="http://java.sun.com/jsf/core">
  <af:panelGridLayout id="pgl2" inlineStyle="width: 1020px;">
    <af:gridRow marginTop="5px" height="auto" id="gr1">
      <af:gridCell marginStart="5px" width="1020px" marginEnd="5px" id="gc1" inlineStyle="border-color:Black;">
        <af:outputFormatted value="Office Depot-AP Trade-Match Exception-Employee's Vendor Summary : " id="of1"
                            inlineStyle="font-size:large; text-align:center;"/>
        <af:outputFormatted value="#{MyDate}" id="of2" inlineStyle="font-size:large;">
          <af:convertDateTime pattern="MM/dd/YYYY"/>
        </af:outputFormatted>
      </af:gridCell>
    </af:gridRow>
    <af:gridRow id="gr3" rendered="#{pageFlowScope.pEmployeeId eq null?false: true}">
      <af:gridCell id="gc4" halign="end">
        <af:button text="Back" id="b1" action="return"/>
      </af:gridCell>
    </af:gridRow>
    <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="gr2">
      <af:gridCell marginStart="5px" width="1020px" marginEnd="5px" id="gc2" halign="end">
        <af:panelGroupLayout id="pgl3" layout="horizontal">
          <af:outputText value="#{bindings.OrgId.label}" id="ot26"/>
          <af:selectOneChoice value="#{bindings.OrgId.inputValue}"
                              required="#{bindings.OrgId.hints.mandatory}" shortDesc="#{bindings.OrgId.hints.tooltip}"
                              id="soc2">
            <f:selectItems value="#{bindings.OrgId.items}" id="si2"/>
            <f:validator binding="#{bindings.OrgId.validator}"/>
          </af:selectOneChoice>
        </af:panelGroupLayout>
      </af:gridCell>
    </af:gridRow>
  </af:panelGridLayout>
  <af:panelGroupLayout id="pgl1">
    <af:panelGridLayout id="pgl4">
      <af:gridRow marginTop="5px" height="auto" id="gr4">
        <af:gridCell marginStart="5px" width="120px" id="gc3">
          <af:outputText value="#{bindings.SupplierName.hints.label}" id="ot22"/>
        </af:gridCell>
        <af:gridCell marginStart="5px" width="200px" id="gc5">
          <af:inputListOfValues id="supplierNameId" popupTitle="Search and Select: #{bindings.SupplierName.hints.label}"
                                value="#{bindings.SupplierName.inputValue}"
                                model="#{bindings.SupplierName.listOfValuesModel}"
                                required="#{bindings.SupplierName.hints.mandatory}"
                                columns="#{bindings.SupplierName.hints.displayWidth}"
                                shortDesc="#{bindings.SupplierName.hints.tooltip}">
            <f:validator binding="#{bindings.SupplierName.validator}"/>
          </af:inputListOfValues>
        </af:gridCell>
        <af:gridCell marginStart="5px" width="120px" id="gc6">
          <af:outputText value="#{bindings.VendorAssistant.label}" id="ot23"/>
        </af:gridCell>
        <af:gridCell marginStart="5px" width="200px" marginEnd="5px" id="gc7">
          <af:selectOneChoice value="#{bindings.VendorAssistant.inputValue}"
                              required="#{bindings.VendorAssistant.hints.mandatory}"
                              shortDesc="#{bindings.VendorAssistant.hints.tooltip}" id="soc1">
            <f:selectItems value="#{bindings.VendorAssistant.items}" id="si1"/>
            <f:validator binding="#{bindings.VendorAssistant.validator}"/>
          </af:selectOneChoice>
        </af:gridCell>
      </af:gridRow>
      <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="gr5">
        <af:gridCell marginStart="5px" width="25%" id="gc8">
          <af:outputText value="#{bindings.SupplierNo.hints.label}" id="ot24"/>
        </af:gridCell>
        <af:gridCell marginStart="5px" width="25%" id="gc9">
          <af:inputListOfValues id="supplierNoId" popupTitle="Search and Select: #{bindings.SupplierNo.hints.label}"
                                value="#{bindings.SupplierNo.inputValue}"
                                model="#{bindings.SupplierNo.listOfValuesModel}"
                                required="#{bindings.SupplierNo.hints.mandatory}"
                                columns="#{bindings.SupplierNo.hints.displayWidth}"
                                shortDesc="#{bindings.SupplierNo.hints.tooltip}">
            <f:validator binding="#{bindings.SupplierNo.validator}"/>
          </af:inputListOfValues>
        </af:gridCell>
        <af:gridCell marginStart="5px" width="25%" id="gc10"/>
        <af:gridCell marginStart="5px" width="25%" marginEnd="5px" id="gc11"/>
      </af:gridRow>
      <af:gridRow id="gr6">
        <af:gridCell id="gc12">
          <af:outputText value="#{bindings.SupplierSite.hints.label}" id="ot25"/>
        </af:gridCell>
        <af:gridCell id="gc13">
          <af:inputListOfValues id="supplierSiteId" popupTitle="Search and Select: #{bindings.SupplierSite.hints.label}"
                                value="#{bindings.SupplierSite.inputValue}"
                                model="#{bindings.SupplierSite.listOfValuesModel}"
                                required="#{bindings.SupplierSite.hints.mandatory}"
                                columns="#{bindings.SupplierSite.hints.displayWidth}"
                                shortDesc="#{bindings.SupplierSite.hints.tooltip}">
            <f:validator binding="#{bindings.SupplierSite.validator}"/>
          </af:inputListOfValues>
        </af:gridCell>
        <af:gridCell id="gc14"/>
        <af:gridCell id="gc15"/>
      </af:gridRow>
    </af:panelGridLayout>
    <af:panelGroupLayout id="pgl5" inlineStyle="width: 1020px" halign="end" layout="horizontal">
      <af:button text="Go" id="b2" action="#{pageFlowScope.InvWkBenchCommonBean.EmpVendSearchAction}"/>
      <af:spacer width="10" height="10" id="s3"/>
      <af:button text="Clear" id="b3" actionListener="#{pageFlowScope.InvWkBenchCommonBean.clearEmpVendSearch}"/>
    </af:panelGroupLayout>
    <af:panelCollection id="pc1" inlineStyle="width: 1020px;">
      <f:facet name="menus">
        <af:menu text="File" id="m1">
          <af:commandMenuItem text="Export to Excel" id="cmi1">
            <af:exportCollectionActionListener type="excelHTML" exportedId="resId1"
                                               filename="EmployeeVendorSummaryData.xls"
                                               title="EmployeeVendorSummaryData"/>
          </af:commandMenuItem>
        </af:menu>
      </f:facet>
      <f:facet name="toolbar"/>
      <f:facet name="statusbar"/>
      <af:table value="#{bindings.VAEmpVendorSummaryVO1.collectionModel}" var="row"
                rows="#{bindings.VAEmpVendorSummaryVO1.rangeSize}"
                emptyText="#{bindings.VAEmpVendorSummaryVO1.viewable ? 'No data to display.' : 'Access Denied.'}"
                rowBandingInterval="0" selectedRowKeys="#{bindings.VAEmpVendorSummaryVO1.collectionModel.selectedRow}"
                selectionListener="#{bindings.VAEmpVendorSummaryVO1.collectionModel.makeCurrent}" rowSelection="single"
                fetchSize="#{bindings.VAEmpVendorSummaryVO1.rangeSize}" id="resId1" styleClass="AFStretchWidth"
                partialTriggers="::b2">
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.VendorAssistant.label}" id="resId1c1"
                   inlineStyle="text-wrap:none;" noWrap="true" headerNoWrap="true" width="130">
          <af:outputText value="#{row.VendorAssistant}"
                         shortDesc="#{bindings.VAEmpVendorSummaryVO1.hints.VendorAssistant.tooltip}" id="ot1"/>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.VendorName.label}" id="resId1c2">
          <af:link text="#{row.VendorName}" id="l1" action="VendorMoot">
            <af:setPropertyListener from="#{row.VendorName}" to="#{pageFlowScope.mSuppName}" type="action"/>
            <af:setPropertyListener from="#{null}" to="#{pageFlowScope.pMoot}" type="action"/>
            <af:setPropertyListener from="#{null}" to="#{pageFlowScope.pNrf}" type="action"/>
            <af:setPropertyListener from="#{row.VendorSiteCode}" to="#{pageFlowScope.mSuppSite}" type="action"/>
          </af:link>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.Supplier.label}" id="resId1c3">
          <af:outputText value="#{row.Supplier}"
                         shortDesc="#{bindings.VAEmpVendorSummaryVO1.hints.VendorName.tooltip}" id="ot2"/>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.VendorSiteCode.label}" id="resId1c4">
          <af:outputText value="#{row.VendorSiteCode}"
                         shortDesc="#{bindings.VAEmpVendorSummaryVO1.hints.VendorSiteCode.tooltip}" id="ot4"/>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.Disc.label}" id="resId1c5" width="25">
          <af:outputText value="#{row.Disc}" shortDesc="#{bindings.VAEmpVendorSummaryVO1.hints.Disc.tooltip}" id="ot5"/>
          <f:facet name="footer"/>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.TotalInvCount.label}" id="resId1c6" align="right"
                   width="75">
          <af:link text="#{row.TotalInvCount}" id="l2" action="VendorMoot">
            <af:setPropertyListener from="#{row.VendorName}" to="#{pageFlowScope.mSuppName}" type="action"/>
            <af:setPropertyListener from="#{null}" to="#{pageFlowScope.pMoot}" type="action"/>
            <af:setPropertyListener from="#{null}" to="#{pageFlowScope.pNrf}" type="action"/>
            <af:setPropertyListener from="#{row.VendorSiteCode}" to="#{pageFlowScope.mSuppSite}" type="action"/>
          </af:link>
          <f:facet name="footer">
            <af:outputText value="#{bindings.SumInvCount.inputValue}"
                           shortDesc="#{bindings.SumTotInvCount.hints.tooltip}" id="ot15"/>
          </f:facet>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.TotalInvAmount.label}" id="resId1c7" align="right"
                   width="75">
          <af:outputText value="#{row.TotalInvAmount}"
                         shortDesc="#{bindings.VAEmpVendorSummaryVO1.hints.TotalInvAmount.tooltip}" id="ot7">
            <af:convertNumber groupingUsed="false"
                              pattern="#{bindings.VAEmpVendorSummaryVO1.hints.TotalInvAmount.format}"/>
          </af:outputText>
          <f:facet name="footer">
            <af:outputText value="#{bindings.SumInvAmount.inputValue}"
                           shortDesc="#{bindings.SumTotInvAmt.hints.tooltip}" id="ot16"/>
          </f:facet>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.TotalLineAmount.label}" id="resId1c8"
                   align="right" width="75">
          <af:outputText value="#{row.TotalLineAmount}"
                         shortDesc="#{bindings.VAEmpVendorSummaryVO1.hints.TotalLineAmount.tooltip}" id="ot8">
            <af:convertNumber groupingUsed="false"
                              pattern="#{bindings.VAEmpVendorSummaryVO1.hints.TotalLineAmount.format}"/>
          </af:outputText>
          <f:facet name="footer">
            <af:outputText value="#{bindings.SumLineAmount.inputValue}"
                           shortDesc="#{bindings.SumTotLineAmt.hints.tooltip}" id="ot17"/>
          </f:facet>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.TotalMootCount.label}" id="resId1c9" align="right"
                   width="75">
          <af:link text="#{row.TotalMootCount}" id="l3" action="VendorMoot">
            <af:setPropertyListener from="#{row.VendorName}" to="#{pageFlowScope.mSuppName}" type="action"/>
            <af:setPropertyListener from="#{row.TotalMootCount}" to="#{pageFlowScope.pMoot}" type="action"/>
            <af:setPropertyListener from="#{null}" to="#{pageFlowScope.pNrf}" type="action"/>
            <af:setPropertyListener from="#{row.VendorSiteCode}" to="#{pageFlowScope.mSuppSite}" type="action"/>
          </af:link>
          <f:facet name="footer">
            <af:outputText value="#{bindings.SumMootCount1.inputValue}"
                           shortDesc="#{bindings.SumMootCount.hints.tooltip}" id="ot18"/>
          </f:facet>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.TotalMootInvAmount.label}" id="resId1c10"
                   align="right" width="75">
          <af:outputText value="#{row.TotalMootInvAmount}"
                         shortDesc="#{bindings.VAEmpVendorSummaryVO1.hints.TotalMootInvAmount.tooltip}" id="ot10">
            <af:convertNumber groupingUsed="false"
                              pattern="#{bindings.VAEmpVendorSummaryVO1.hints.TotalMootInvAmount.format}"/>
          </af:outputText>
          <f:facet name="footer">
            <af:outputText value="#{bindings.SumMootInvAmount.inputValue}"
                           shortDesc="#{bindings.SumTotMootInvAmt.hints.tooltip}" id="ot19"/>
          </f:facet>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.TotalMootLineAmount.label}" id="resId1c11"
                   align="right" width="75">
          <af:outputText value="#{row.TotalMootLineAmount}"
                         shortDesc="#{bindings.VAEmpVendorSummaryVO1.hints.TotalMootLineAmount.tooltip}" id="ot11">
            <af:convertNumber groupingUsed="false"
                              pattern="#{bindings.VAEmpVendorSummaryVO1.hints.TotalMootLineAmount.format}"/>
          </af:outputText>
          <f:facet name="footer">
            <af:outputText value="#{bindings.SumMootLineAmount.inputValue}"
                           shortDesc="#{bindings.SumTotMootLineAmt.hints.tooltip}" id="ot20"/>
          </f:facet>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.TotalNrfCount.label}" id="resId1c12" align="right"
                   width="75">
          <af:link text="#{row.TotalNrfCount}" id="l4" action="VendorMoot">
            <af:setPropertyListener from="#{row.VendorName}" to="#{pageFlowScope.mSuppName}" type="action"/>
            <af:setPropertyListener from="#{row.TotalNrfCount}" to="#{pageFlowScope.pNrf}" type="action"/>
            <af:setPropertyListener from="#{null}" to="#{pageFlowScope.pMoot}" type="action"/>
            <af:setPropertyListener from="#{row.VendorSiteCode}" to="#{pageFlowScope.mSuppSite}" type="action"/>
          </af:link>
          <f:facet name="footer">
            <af:outputText value="#{bindings.SumNrfCount.inputValue}"
                           shortDesc="#{bindings.SumTotNrfCount.hints.tooltip}" id="ot21"/>
          </f:facet>
        </af:column>
        <af:column headerText="#{bindings.VAEmpVendorSummaryVO1.hints.TotalNrfAmount.label}" id="resId1c13"
                   align="right" width="75">
          <af:outputText value="#{row.TotalNrfAmount}"
                         shortDesc="#{bindings.VAEmpVendorSummaryVO1.hints.TotalNrfAmount.tooltip}" id="ot13">
            <af:convertNumber groupingUsed="false"
                              pattern="#{bindings.VAEmpVendorSummaryVO1.hints.TotalNrfAmount.format}"/>
          </af:outputText>
          <f:facet name="footer">
            <af:outputText value="#{bindings.SumNrfAmount.inputValue}"
                           shortDesc="#{bindings.SumTotNrfAmt.hints.tooltip}" id="ot14"/>
          </f:facet>
        </af:column>
      </af:table>
    </af:panelCollection>
  </af:panelGroupLayout>
  <af:spacer width="10" height="10" id="s2"/>
  <af:panelGroupLayout id="pgl6" layout="horizontal" halign="left">
        <af:outputText value="Total # of Records" id="ot27"/>
        <af:spacer width="10" height="10" id="s1"/>
        <af:outputText value="#{bindings.VAEmpVendorSummaryVO1Iterator.estimatedRowCount}" id="ot31"/>
    </af:panelGroupLayout>
</jsp:root>

<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="XXInvoiceReasonVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.XXInvoiceReasonVORowImpl"
  ComponentClass="od.tdmatch.model.XXInvoiceReasonVOImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.XXInvoiceReasonVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_inv_id"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <SQLQuery><![CDATA[select sum(amount1) amount from(     
select sum(unmatched_qty*unit_price) amount1         
from xx_ap_chbk_action_holds         
where reason_code is not null        
AND invoice_id=:p_inv_id     
union     
select sum(org_invoice_qty*unit_price) amount1     
from xx_ap_chbk_action_holds a     
where invoice_id=:p_inv_id     
and hold_lookup_code='PRICE'  
AND 1=(select count(*) from xx_ap_chbk_action_holds where invoice_id=:p_inv_id and line_number=a.line_number)  
)]]></SQLQuery>
  <ViewAttribute
    Name="Amount"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="AMOUNT"
    Expression="AMOUNT"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="Amount"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="od.tdmatch.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

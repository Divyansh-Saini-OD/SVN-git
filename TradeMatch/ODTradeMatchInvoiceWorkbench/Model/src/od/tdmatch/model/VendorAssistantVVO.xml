  <SQLQuery><![CDATA[SELECT a.segment1 supplier_no, 
  a.vendor_name supplier_name, 
  b.vendor_site_code supplier_site_no, 
  c.name terms, 
  b.terms_date_basis, 
  b.payment_method_lookup_code Pay_Method, 
  b.pay_group_lookup_code Pay_Group, 
  b.hold_all_payments_flag Site_hold, 
  b.hold_reason site_payment_Hold_reason, 
  'status', 
  p.full_name vendor_assistant, 
  b.attribute8 site_category, 
  h.name 
FROM hr_operating_units h, 
  per_all_people_f p, 
  ap_terms c, 
  ap_supplier_sites_all b, 
  ap_suppliers a 
WHERE 1              =1 
AND b.vendor_id      =a.vendor_id 
AND b.org_id         =h.organization_id 
AND c.term_id        =b.terms_id 
AND p.employee_number=b.attribute6 
AND sysdate BETWEEN NVL(p.effective_start_date,sysdate) AND NVL(p.effective_end_date,sysdate)]]></SQLQuery>

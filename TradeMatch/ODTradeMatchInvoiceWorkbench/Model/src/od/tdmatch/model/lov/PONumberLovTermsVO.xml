  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.lov.PONumberLovTermsVORowImpl"
  ComponentClass="od.tdmatch.model.lov.PONumberLovTermsVOImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.lov.PONumberLovTermsVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT ph.segment1,ph.po_header_id           
  FROM po_headers_all ph           
 WHERE         
                   EXISTS (SELECT 1        
                                FROM ap_supplier_sites_all site        
                               WHERE site.vendor_site_id=ph.vendor_site_id        
                                 AND exists (SELECT 'x'                 
                                               FROM xx_fin_translatevalues tv,                                                                                  
                                                    xx_fin_translatedefinition td                                  
                                              WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                                   
                                                AND tv.translate_id  = td.translate_id                                                                                  
                                                AND tv.enabled_flag = 'Y'                                                                                  
                                                AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)                 
                                                AND tv.target_value1 = site.attribute8        
                                             )        
                                       
                             )]]></SQLQuery>
  <ViewAttribute
    Name="Segment1"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SEGMENT1"
    Expression="SEGMENT1"

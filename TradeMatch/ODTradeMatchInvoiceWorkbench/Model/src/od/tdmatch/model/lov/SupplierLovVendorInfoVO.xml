<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SupplierLovVendorInfoVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.lov.SupplierLovVendorInfoVORowImpl"
  ComponentClass="od.tdmatch.model.lov.SupplierLovVendorInfoVOImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.lov.SupplierLovVendorInfoVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT distinct sup.segment1 supplier_no, sup.vendor_name supplier,sup.vendor_id        
  FROM ap_supplier_sites_all site,            
       ap_suppliers sup            
 WHERE                      
    site.vendor_id=sup.vendor_id                              
   AND exists (SELECT 'x'             
         FROM xx_fin_translatevalues tv,                                                                              
              xx_fin_translatedefinition td                              
        WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                               
          AND tv.translate_id  = td.translate_id                                                                              
          AND tv.enabled_flag = 'Y'                                                                              
          AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)             
          AND tv.target_value1 = site.attribute8) 
          and sup.end_date_active is null 
          and site.inactive_date is null]]></SQLQuery>
  <ViewAttribute
    Name="SupplierNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUPPLIER_NO"
    Expression="SUPPLIER_NO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.lov.SupplierLovVendorInfoVO.SupplierNo_LABEL"/>
        <DISPLAYWIDTH
          Value="25"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Supplier"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUPPLIER"
    Expression="SUPPLIER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.lov.SupplierLovVendorInfoVO.Supplier_LABEL"/>
        <DISPLAYWIDTH
          Value="25"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="VendorId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="VENDOR_ID"
    Expression="VENDOR_ID"
    SQLType="NUMERIC">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="SupplierNo"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="od.tdmatch.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

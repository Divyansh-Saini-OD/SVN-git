<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="InvoiceNumDLovVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.lov.vendmootDship.InvoiceNumDLovVORowImpl"
  ComponentClass="od.tdmatch.model.lov.vendmootDship.InvoiceNumDLovVOImpl"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.lov.vendmootDship.InvoiceNumDLovVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT /*+ HEADING(h) */        
       ai.invoice_num          
  FROM ap_invoices_all ai,          
         (SELECT /*+ INDEX(aph XX_AP_HOLDS_N1) */     
        distinct aph.invoice_id          
          FROM ap_holds_all aph          
         WHERE  
         --aph.creation_date > '01-JAN-11'           
           --AND  
           NVL(aph.status_flag,'S')= 'S'  
           AND release_lookup_code is null  
        ) h          
 WHERE ai.invoice_id=h.invoice_id          
   AND ai.invoice_num not like '%ODDBUIA'      
   --AND ai.creation_date>'01-JAN-11'          
   AND ai.cancelled_date IS NULL          
   AND EXISTS (SELECT 'x'          
                 FROM ap_supplier_sites_all aps         
                WHERE vendor_site_id=ai.vendor_site_id          
                AND exists (SELECT 'x'      
                            FROM xx_fin_translatevalues tv,                                                                       
                                xx_fin_translatedefinition td                       
                             WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                        
                            AND tv.translate_id  = td.translate_id                                                                       
                            AND tv.enabled_flag = 'Y'                                                                       
                            AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)      
                            AND tv.target_value1 = aps.attribute8)          
                            )          
   AND ai.attribute5 like 'DropShip%'       
   AND EXISTS (SELECT  'x'                                                       
		     FROM po_lines_all pl                                                       
		    WHERE pl.po_header_id = NVL(ai.po_header_id,ai.quick_po_header_id)                                                       
              )]]></SQLQuery>
  <ViewAttribute
    Name="InvoiceNum"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="INVOICE_NUM"
    Expression="INVOICE_NUM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.lov.vendmootDship.InvoiceNumDLovVO.InvoiceNum_LABEL"/>
        <DISPLAYWIDTH
          Value="25"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="InvoiceNum"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="od.tdmatch.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

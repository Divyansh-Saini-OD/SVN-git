  <SQLQuery><![CDATA[SELECT /*+ (LEADING h) */                               
        ai.invoice_id,                               
       (SELECT tv.target_value2                                
         FROM xx_fin_translatevalues tv,                                                                                  
              xx_fin_translatedefinition td                                  
        WHERE td.translation_name = 'XX_AP_VENDOR_ASSISTANTS'                                                                                   
          AND tv.translate_id  = td.translate_id                                                                                  
          AND tv.target_value1 = nvl(aps.attribute6,'Open')||''                                                                                  
          AND tv.enabled_flag = 'Y'                                                                                  
          AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)) vendor_assistant,                               
       ai.org_id,                                                      
       ai.source,                                                      
       ai.invoice_num,                                                      
       ai.invoice_date,                                                             
       nvl(ai.voucher_num,doc_sequence_value) voucher_num,                                                                      
       ai.invoice_amount,                                                                                  
       ai.attribute5 PO_Type,                                                               
       NVL(py.discount_date,py.due_date) due_date,                                                             
       ph.segment1 po_number,                                                      
       asup.vendor_name supplier_name,                                                                                  
       asup.segment1 supplier_no,                                                                                  
       aps.vendor_site_code supplier_site,                                                           
       DECODE(SIGN(instr(e.name,'/')),0,NULL,1,'D') discount,                                                                                  
       ltrim(SUBSTR(hrl.location_code,1,6),'0') location,                                                                                  
       XX_AP_TRADE_MATCH_UTL_PKG.xxod_tdmatch_quantity(ai.invoice_id) Qty_Hold_Amt,                                                                                             
           XX_AP_TRADE_MATCH_UTL_PKG.xxod_tdmatch_price(ai.invoice_id) Price_Hold_Amt,                                                                                             
           XX_AP_TRADE_MATCH_UTL_PKG.xx_ap_freight_invoice(ai.invoice_id) Frt_Hold,                                                                                        
           'N' po_hold_chr,                                                                        
                                              
           DECODE(SUBSTR(ai.attribute5,1,1),'F','Y','N') front_doorC,                                   
		   (CASE WHEN SUBSTR(ai.attribute5,1,1) = 'N' OR SUBSTR(ai.attribute5,10,1) = 'N' then 'Y' else 'N' end) non_codec,                                                                    
           DECODE(SUBSTR(ai.attribute5,1,1),'D','Y','N') drop_shipc ,                              
           ph.creation_date podate ,                           
           1 sku,                          
           'Y' pricingExcp,                          
           'Y' quantityExcp,                          
           XX_AP_TRADE_MATCH_UTL_PKG.xx_ap_freight_invoice(ai.invoice_id) freightExcp,      
           ai.gl_date      
  FROM ap_terms e,                                                                                  
       hr_locations_all hrl,                                
       po_headers_all ph,                               
       ap_payment_schedules_all py,                               
       ap_invoices_all ai,                               
       ap_suppliers asup,                               
       ap_supplier_sites_all aps,                               
       (SELECT /*+ INDEX(aph XX_AP_HOLDS_N1) */                    
          distinct aph.invoice_id                               
          FROM ap_holds_all aph                               
         WHERE aph.creation_date > '01-JAN-11'                                
           AND NVL(aph.status_flag,'S')= 'S'                     
           AND aph.hold_lookup_code IN ('MAX QTY ORD','QTY ORD','QTY REC','AMT ORD','MAX AMT REC','MAX AMT ORD','AMT REC','OD MAX PRICE','OD Favorable','PRICE','LINE VARIANCE','DIST VARIANCE')                     
           AND aph.release_lookup_code is null                 
        ) h                               
WHERE                                
  ai.invoice_id=h.invoice_id                      
  AND py.invoice_id=ai.invoice_id                        
  AND aps.vendor_id=ai.vendor_id  
  AND NVL(ai.validation_request_id,1)<>-9999999999  
  AND aps.vendor_id=nvl(:p_vendor_id,aps.vendor_id)               
  AND aps.vendor_site_id=nvl(:p_vendor_site_id,aps.vendor_site_id)                
  and NVL(ai.po_header_id,ai.quick_po_header_id)=nvl(:p_po_header_id,NVL(ai.po_header_id,ai.quick_po_header_id))              
  and nvl(aps.attribute6,'Open')=nvl(:p_employee_id,nvl(aps.attribute6,'Open'))             
  AND ai.gl_date BETWEEN NVL(:p_fr_gldate,ai.gl_date) AND NVL(:p_to_gldate,ai.gl_date)                                                      
     
  and aps.vendor_site_id=ai.vendor_site_id                        
  AND asup.vendor_id = aps.vendor_id                             
  AND ph.po_header_id = NVL(ai.po_header_id,ai.quick_po_header_id)                                
  AND hrl.location_id = ph.ship_to_location_id                               
  AND exists (SELECT 'x'                            
         FROM xx_fin_translatevalues tv,                                                                                             
              xx_fin_translatedefinition td                                             
        WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                                              
          AND tv.translate_id  = td.translate_id                                                                                             
          AND tv.enabled_flag = 'Y'                                                                                             
          AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)                            
          AND tv.target_value1 = aps.attribute8)                                 
  AND e.term_id=ai.terms_id                                                                                    
  AND EXISTS (SELECT  'x'                                                                              
                FROM ap_invoice_lines_all al                                                                             
               WHERE  al.invoice_id=ai.invoice_id            
               AND al.inventory_item_id=nvl(:skunum,al.inventory_item_id)            
                                                                                                 
                                              
                                                  
              )                               
   AND EXISTS (SELECT 'x'                               
                 FROM  xx_fin_translatevalues tv                               
                      ,xx_fin_translatedefinition td                               
                WHERE td.TRANSLATION_NAME = 'XX_AP_TR_MATCH_INVOICES'                               
                  AND tv.TRANSLATE_ID  = td.TRANSLATE_ID                               
                  AND tv.enabled_flag='Y'                               
                  AND SYSDATE BETWEEN tv.start_date_active and NVL(tv.end_date_active,sysdate)                               
                  AND tv.target_value1=ai.source                               
                 )          
                 AND ((EXISTS (SELECT 'x' from ap_holds_all aia                                                                   
                           where aia.invoice_id=ai.invoice_id                                                                   
                           AND aia.hold_lookup_code like 'OD Max Freight%'           
                           AND aia.release_lookup_code IS NULL                                                                   
                        ) AND (:p_frt_expt = 'Y' ))                                                                   
        OR				                                                                  
	(:p_frt_expt is null or :p_frt_expt = 'N'))                                             
  AND ((:p_qty_expt is null or :p_qty_expt = 'N')                                             
		OR                                             
		(:p_qty_expt = 'Y' and exists (SELECT 'x'--distinct invoice_id                                               
             FROM ap_holds_all                                                
            WHERE creation_date > '01-JAN-11'                                                
              AND NVL(status_flag,'S')= 'S'                                               
              AND invoice_id = ai.invoice_id                                             
              AND hold_lookup_code like 'QTY%'                                    
              AND release_lookup_code||''  is null                                    
          )))	                                             
                                                      
 AND ((:p_price_expt is null or :p_price_expt = 'N')                                             
		OR                                             
		(:p_price_expt = 'Y' and exists (SELECT 'x'--distinct invoice_id                                               
             FROM ap_holds_all                                                
            WHERE creation_date > '01-JAN-11'                                                
              AND NVL(status_flag,'S')= 'S'                                               
              AND invoice_id = ai.invoice_id                                             
              AND hold_lookup_code = 'PRICE'                                    
              AND release_lookup_code||''  is null                                    
          )))	                                             
        AND (((SUBSTR(ai.attribute5,1,1) ='F' AND :p_front_door = 'Y' )		                                             
  OR (ai.attribute5= 'Non-Code'  AND :p_non_code = 'Y' )          
  OR (ai.attribute5='DropShip VW' and :p_drop_ship='Y')         
  )                                                             
  OR  ((:p_front_door is null or :p_front_door = 'N')                                                            
  AND (:p_non_code is null or :p_non_code = 'N')         
  AND (:p_drop_ship is null or :p_drop_ship='N')         
  ))]]></SQLQuery>

  <SQLQuery><![CDATA[SELECT a.segment1 supplier_no,                            
  a.vendor_name supplier_name,                            
  b.vendor_site_code supplier_site_no,                            
  c.name terms,                            
  b.terms_date_basis,                            
  b.payment_method_lookup_code Pay_Method,                            
  b.pay_group_lookup_code Pay_Group,                            
  b.hold_all_payments_flag Site_hold,                            
  b.hold_reason site_payment_Hold_reason,                            
  decode (sign(sysdate-nvl(b.inactive_date,sysdate+1)),-1,'Active',0,'Active','Inactive') status,                            
  ast.target_value2 vendor_assistant,                            
  b.attribute8 site_category,                            
  h.name operating_unit_name,                            
  h.organization_id,                            
  nvl(b.attribute6,'Open') employee_number,                      
  --(select iby.remit_advice_email from IBY_EXTERNAL_PAYEES_ALL iby where iby.party_site_id=b.party_site_id) remit_advice_email,            
  (select iby.remit_advice_email from IBY_EXTERNAL_PAYEES_ALL iby where iby.payee_party_id = a.party_id and iby.party_site_id=b.party_site_id and rownum =1) remit_advice_email,   
  b.vendor_site_id            
FROM  (SELECT  b.target_value2 ,b.target_value1                 
  FROM  xx_fin_translatevalues b                 
       ,xx_fin_translatedefinition a                    
 WHERE  a.TRANSLATION_NAME = 'XX_AP_VENDOR_ASSISTANTS'                  
   AND b.TRANSLATE_ID  = a.TRANSLATE_ID                 
   AND b.enabled_flag='Y'                 
   AND SYSDATE BETWEEN b.start_date_active and NVL(b.end_date_active,sysdate)) ast,               
   hr_operating_units h,                            
                              
  ap_terms c,                            
  ap_supplier_sites_all b,                            
  ap_suppliers a                        
                         
WHERE 1              =1          
AND (:p_vendor_id IS NULL or a.vendor_id=:p_vendor_id)          
AND (:p_vendor_site_id IS NULL or b.vendor_site_id=:p_vendor_site_id)            
AND (:p_employee_id IS NULL or nvl(b.attribute6,'Open')=:p_employee_id)          
AND (:p_org_id IS NULL or b.org_id=:p_org_id)       
AND (:p_site_category IS NULL or b.attribute8=:p_site_category)       
AND b.vendor_id      =a.vendor_id                             
AND b.org_id         =h.organization_id                            
AND c.term_id        =b.terms_id                            
AND nvl(b.attribute6,'Open')=ast.target_value1            
AND nvl(b.inactive_date,sysdate+1)>sysdate            
AND exists (SELECT 'x'                         
         FROM xx_fin_translatevalues tv,                                                                                          
              xx_fin_translatedefinition td                                          
        WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                                           
          AND tv.translate_id  = td.translate_id                                                                                          
          AND tv.enabled_flag = 'Y'                                                                                          
          AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)                         
          AND tv.target_value1 = b.attribute8)]]></SQLQuery>

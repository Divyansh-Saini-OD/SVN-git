  <SQLQuery><![CDATA[SELECT /*+ LEADING (ai) INDEX(b AP_SUPPLIER_SITES_UI) */ a.vendor_name Vendor_name,                                                      
       a.segment1 vendor_no,                                                      
       b.vendor_site_code site,                                                      
       DECODE(SIGN(instr(e.name,'/')),0,NULL,1,'D') discount,                                                             
       ai.invoice_num,                                                      
       ai.invoice_date,                                                             
       ph.segment1 po_number,                                                      
       NVL(py.discount_date,py.due_date) due_date,                                                      
       ai.invoice_amount,                                                      
       e.term_id Payment_terms,                                                      
       ai.terms_date,                                                      
       ai.payment_method_code,                                                      
       ai.pay_group_lookup_code,                                                     
       ph.creation_date po_date,                                                    
       ai.org_id org_id,                                                   
       ai.invoice_id invoice_id,    
       e.name term_name    
  FROM  ap_terms e                                                      
       ,po_headers_all ph                                                      
       ,ap_payment_schedules_all py                                                      
       ,ap_supplier_sites_all b                                                      
       ,ap_suppliers a                                                      
       ,ap_invoices_all ai                                                      
 WHERE  ai.payment_status_flag IN ('N','P')                                                      
   and ( ai.vendor_id=:p_vendor_id)                        
   AND ( b.vendor_site_id=:p_vendor_site_id)                        
   AND (ai.po_header_id is not null or ai.quick_po_header_id is not null)                         
   and ai.terms_id=nvl(:p_term_id,ai.terms_id)                                           
   and NVL(ai.po_header_id,ai.quick_po_header_id)=nvl(:p_po_header_id,NVL(ai.po_header_id,ai.quick_po_header_id))                         
   AND ph.po_header_id=NVL(ai.po_header_id,ai.quick_po_header_id)                                                           
   AND a.vendor_id=ai.vendor_id                                                      
   AND b.vendor_id=a.vendor_id                                                      
   AND b.vendor_site_id=ai.vendor_site_id                                                      
   AND py.invoice_id=ai.invoice_id                         
   AND py.checkrun_id IS NULL                            
   AND e.term_id=ai.terms_id                                                      
   AND (NVL(py.amount_remaining,0) * NVL(ai.exchange_rate,1)) != 0                                                         
   AND   'APPROVED'=AP_INVOICES_PKG.GET_APPROVAL_STATUS(ai.invoice_id,                                                       
                                           ai.invoice_amount,                                                      
                                           ai.payment_status_flag,                                                      
                                           ai.invoice_type_lookup_code                                                      
                                          )                         
   AND exists (SELECT 'x'                                                
         FROM xx_fin_translatevalues tv,                                                                                                                 
              xx_fin_translatedefinition td                                                                 
        WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                                                                  
          AND tv.translate_id  = td.translate_id                                                                                                                 
          AND tv.enabled_flag = 'Y'                                                                                                                 
          AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)                                                
          AND tv.target_value1 = b.attribute8||'')                                      
   AND EXISTS (SELECT 'x'                                                 
                   FROM xx_fin_translatevalues tv                                                 
                       ,xx_fin_translatedefinition td                                                 
                  WHERE td.TRANSLATION_NAME = 'XX_AP_TR_MATCH_INVOICES'                                                 
                    AND tv.TRANSLATE_ID  = td.TRANSLATE_ID                                                 
                    AND tv.enabled_flag='Y'                                                 
                    AND SYSDATE BETWEEN tv.start_date_active and NVL(tv.end_date_active,sysdate)                                                 
                    AND tv.target_value1=ai.source                                                 
                )                                  
   AND ai.invoice_date BETWEEN NVL(to_date(to_char(NVL(:p_invoice_date_from,ai.invoice_date)) || ' 00:00:00','DD-MON-RR HH24:MI:SS'),ai.invoice_date)                          
                           AND nvl(to_date(to_char(NVL(:p_invoice_date_to,ai.invoice_date))|| ' 23:59:59','DD-MON-RR HH24:MI:SS'),ai.invoice_date)                             
   AND NVL(py.discount_date,py.due_date) BETWEEN NVL(to_date(to_char(NVL(:p_due_date_from,NVL(py.discount_date,py.due_date))) || ' 00:00:00','DD-MON-RR HH24:MI:SS'),NVL(py.discount_date,py.due_date))                          
                                             AND nvl(to_date(to_char(NVL(:p_due_date_to,NVL(py.discount_date,py.due_date)))|| ' 23:59:59','DD-MON-RR HH24:MI:SS'),NVL(py.discount_date,py.due_date))                         
  AND ph.creation_date BETWEEN NVL(to_date(to_char(NVL(:p_po_date_from,ph.creation_date)) || ' 00:00:00','DD-MON-RR HH24:MI:SS'),ph.creation_date)                          
                                             AND nvl(to_date(to_char(NVL(:p_po_date_to,ph.creation_date))|| ' 23:59:59','DD-MON-RR HH24:MI:SS'),ph.creation_date)                         
UNION                          
SELECT /*+ LEADING (ai) INDEX(b AP_SUPPLIER_SITES_UI) */ a.vendor_name Vendor_name,                                                      
       a.segment1 vendor_no,                                                      
       b.vendor_site_code site,                                                      
       DECODE(SIGN(instr(e.name,'/')),0,NULL,1,'D') discount,                                                             
       ai.invoice_num,                                                      
       ai.invoice_date,                                                             
       null po_number,                                                      
       NVL(py.discount_date,py.due_date) due_date,                                                      
       ai.invoice_amount,                                                      
       e.term_id Payment_terms,                                                      
       ai.terms_date,                                                      
       ai.payment_method_code,                                                      
       ai.pay_group_lookup_code,                                                     
       null po_date,                                                    
       ai.org_id org_id,                                                   
       ai.invoice_id invoice_id,    
       e.name term_name    
  FROM  ap_terms e                          
       ,ap_payment_schedules_all py                                                      
       ,ap_supplier_sites_all b                                                      
       ,ap_suppliers a                                                      
       ,ap_invoices_all ai                                                      
 WHERE  ai.payment_status_flag IN ('N','P')                                                      
   and ( ai.vendor_id=:p_vendor_id  )                      
   AND ( b.vendor_site_id=:p_vendor_site_id)                        
   and ai.terms_id=nvl(:p_term_id,ai.terms_id)                     
   AND (:p_po_header_id IS NULL )     
   AND (ai.po_header_id is null AND ai.quick_po_header_id is null)                         
   AND a.vendor_id=ai.vendor_id                                                      
   AND b.vendor_id=a.vendor_id                                                      
   AND b.vendor_site_id=ai.vendor_site_id                                                      
   AND py.invoice_id=ai.invoice_id                         
   AND py.checkrun_id IS NULL                            
   AND e.term_id=ai.terms_id                                                                                     
   AND (NVL(py.amount_remaining,0) * NVL(ai.exchange_rate,1)) != 0                                                         
   AND   'APPROVED'=AP_INVOICES_PKG.GET_APPROVAL_STATUS(ai.invoice_id,                                                       
                                           ai.invoice_amount,                                                      
                                           ai.payment_status_flag,                                                      
                                           ai.invoice_type_lookup_code                                                      
                                          )                                     
   AND exists (SELECT 'x'                                                
         FROM xx_fin_translatevalues tv,                                                                                                                 
              xx_fin_translatedefinition td                                                                 
        WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                                                                  
          AND tv.translate_id  = td.translate_id                                                                                                                 
          AND tv.enabled_flag = 'Y'                                                                                                                 
          AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)                                                
          AND tv.target_value1 = b.attribute8||'')                                      
   AND EXISTS (SELECT 'x'                                                 
                   FROM xx_fin_translatevalues tv                                                 
                       ,xx_fin_translatedefinition td                                                 
                  WHERE td.TRANSLATION_NAME = 'XX_AP_TR_MATCH_INVOICES'                                                 
                    AND tv.TRANSLATE_ID  = td.TRANSLATE_ID                                                 
                    AND tv.enabled_flag='Y'                                                 
                    AND SYSDATE BETWEEN tv.start_date_active and NVL(tv.end_date_active,sysdate)                                                 
                    AND tv.target_value1=ai.source                                                 
                )                                  
   AND ai.invoice_date BETWEEN NVL(to_date(to_char(NVL(:p_invoice_date_from,ai.invoice_date)) || ' 00:00:00','DD-MON-RR HH24:MI:SS'),ai.invoice_date)                          
                           AND nvl(to_date(to_char(NVL(:p_invoice_date_to,ai.invoice_date))|| ' 23:59:59','DD-MON-RR HH24:MI:SS'),ai.invoice_date)                             
   AND NVL(py.discount_date,py.due_date) BETWEEN NVL(to_date(to_char(NVL(:p_due_date_from,NVL(py.discount_date,py.due_date))) || ' 00:00:00','DD-MON-RR HH24:MI:SS'),NVL(py.discount_date,py.due_date))                          
                                             AND nvl(to_date(to_char(NVL(:p_due_date_to,NVL(py.discount_date,py.due_date)))|| ' 23:59:59','DD-MON-RR HH24:MI:SS'),NVL(py.discount_date,py.due_date))]]></SQLQuery>

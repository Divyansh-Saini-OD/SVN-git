<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="RDetailReceiptLOV"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.reports.vo.lov.RDetailReceiptLOVRowImpl"
  ComponentClass="od.tdmatch.model.reports.vo.lov.RDetailReceiptLOVImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.reports.vo.lov.RDetailReceiptLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT RSH.RECEIPT_NUM , 
  SIT.VENDOR_SITE_CODE , 
  rsh.shipment_header_id --RETURN VALUE 
FROM APPS.RCV_SHIPMENT_HEADERS RSH , 
  APPS.AP_SUPPLIER_SITES_ALL SIT 
WHERE 1                =1 
AND RSH.RECEIPT_NUM   <> '0' 
AND RSH.VENDOR_SITE_ID = SIT.VENDOR_SITE_ID 
AND EXISTS 
  (SELECT 1 
  FROM APPS.XX_FIN_TRANSLATEVALUES TV, 
    APPS.xx_fin_translatedefinition td 
  WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES' 
  AND tv.translate_id       = td.translate_id 
  AND tv.enabled_flag       = 'Y' 
  AND sysdate BETWEEN tv.start_date_active AND NVL(tv.end_date_active,sysdate) 
  AND Tv.Target_Value1 = Sit.Attribute8 
    ||'' 
  )]]></SQLQuery>
  <ViewAttribute
    Name="ReceiptNum"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="RECEIPT_NUM"
    Expression="RECEIPT_NUM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.reports.vo.lov.RDetailReceiptLOV.ReceiptNum_LABEL"/>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="VendorSiteCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="15"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VENDOR_SITE_CODE"
    Expression="VENDOR_SITE_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="15"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.reports.vo.lov.RDetailReceiptLOV.VendorSiteCode_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="ShipmentHeaderId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="SHIPMENT_HEADER_ID"
    Expression="SHIPMENT_HEADER_ID"
    SQLType="NUMERIC"
    IsQueriable="false"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="od.tdmatch.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

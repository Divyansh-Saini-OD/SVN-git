  <SQLQuery><![CDATA[select    
SUM(a.Receipt_line_amt) TotalRECEIPT_LINE_AMT,        
SUM(a.Uninv_Amt) TotalUNINV_AMT    
from table (XX_AP_TRADE_RCT_INQ_PKG.XX_AP_TRADE_RCT_INQUIRY   
( P_DATE_FROM => to_date(:P_date_from,'DD-MON-YY') , P_DATE_TO => to_date(:P_date_to,'DD-MON-YY'), P_PERIOD_FROM => :P_Period_From, P_PERIOD_TO => :P_Period_To, P_INVOICE_NUM=>:P_invoice_num, P_RECEIPT_NUM=>:P_Receipt_Num, P_PO_NUM=>:p_PO,P_USER_ID=>:bindUserId)) a         
where  a.SUPPLIER_NAME= NVL( :P_Supplier_Name,a.SUPPLIER_NAME)     
and  a.SUPPLIER_SITE = NVL(:P_Supplier_Site, a.SUPPLIER_SITE)     
and a.SKU =NVL(:p_SKU,a.SKU )     
order by a.RECEIPT_NUM asc,a.REC_LINE_NUM]]></SQLQuery>

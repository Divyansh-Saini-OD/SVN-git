<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="trade-ap-inv-match-btf">
    <default-activity>ApTradeMatchSummCreate</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__3">
      <managed-bean-name>TrMaApInvoiceSumSearchBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.bean.TrMaApInvoiceSumSearchBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__4">
      <managed-bean-name>MyDate</managed-bean-name>
      <managed-bean-class>java.util.Date</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="ApTradeMatchSummary">
      <page>/reports/ApTradeMatchSummary.jsff</page>
    </view>
    <method-call id="ApTradeMatchSummCreate">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>ApTradeMatchSummCreate</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>ApTradeMatchSummary</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

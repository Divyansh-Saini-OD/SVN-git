<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="po-inquiry-btf">
    <default-activity>PoInquirySearch</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>MyDate</managed-bean-name>
      <managed-bean-class>java.util.Date</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__5">
      <managed-bean-name>PoInquirySearchBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.bean.PoInquirySearchBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <method-call id="PoInquirySearch">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <view id="PoInquiry1">
      <page>/reports/PoInquiry.jsff</page>
    </view>
    <control-flow-rule id="__2">
      <from-activity-id>PoInquirySearch</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>PoInquiry1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

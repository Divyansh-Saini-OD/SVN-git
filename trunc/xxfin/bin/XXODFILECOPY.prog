sleep 10

#appspw=`echo $var3|awk -F= '{print $2}'`


#var9=`echo $1 | awk '{print $9}' | tr -d '"'`
#var3=`echo $1 | awk '{print $3}' | cut -c11- |tr -d '"'`
appspw=`echo $var3|awk -F= '{print $2}'`

#Added for defect 2482
#var9=$5
var9=$5
#var10=$6
#var3=$1


echo " 1 : " $1
echo " 2 : " $2
echo " 3 : " $3
echo " 4 : " $4
echo " 5 : " $5

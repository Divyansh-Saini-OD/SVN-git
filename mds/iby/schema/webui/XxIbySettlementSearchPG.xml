<?xml version = '1.0' encoding = 'UTF-8'?>
<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="10.1.3_1312" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <!--*[Enter Javadoc like developer documentation, delete this line once you fill the included template.] [Put a one line description of the region here.] [Put a brief description of the usage of the region here.] @param [Parameter_name] [Put parameter use here.] @param [Parameter_name] [Put parameter use here.] @see [Put any additional links apart from controller class and BC4J objects directly referenced by this region]-->
      <oa:pageLayout id="PageLayoutRN" amDefName="od.oracle.apps.iby.settlement.server.SettlementAM" windowTitle="Credit Card Settlement" title="Credit Card Settlement Report" controllerClass="od.oracle.apps.iby.schema.webui.XxIbySettlementSearchCO">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:productBranding>
            <oa:image id="ProdBrand" source="FNDTAPPBRAND.gif"/>
         </ui:productBranding>
         <ui:pageStatus>
            <oa:flowLayout id="PageStatusRN">
               <ui:contents>
                  <oa:staticStyledText id="PAgeHelp" styleClass="OraInstructionText" messageAppShortName="AK" messageName="FWK_TBX_T_PAGE_GENERAL"/>
               </ui:contents>
            </oa:flowLayout>
         </ui:pageStatus>
         <ui:contents>
            <oa:messageComponentLayout id="RecSearchRN">
               <ui:contents>
                  <oa:messageTextInput id="LowDate" dataType="DATE" prompt="LowDate" queryable="true" selectiveSearchCriteria="true" required="yes"/>
                  <oa:messageTextInput id="HighDate" dataType="DATE" prompt="HighDate" queryable="true" selectiveSearchCriteria="true" required="yes"/>
                  <oa:messageChoice id="OrdType1" prompt="Order Types" queryable="true" readOnly="false" selectiveSearchCriteria="true" styleClass="OraFieldText" required="no" pickListViewDef="od.oracle.apps.iby.settlement.lov.XxIbyOrderTypeLovVO" pickListDispAttr="OrderTypes" pickListValAttr="OrderTypes" defaultValue="POS"/>
                  <oa:messageChoice id="TrxType1" queryable="true" selectiveSearchCriteria="true" prompt="Trx Type" pickListViewDef="od.oracle.apps.iby.settlement.lov.XxIbyTransTypeLovVO" pickListDispAttr="Transtype" pickListValAttr="Transtype" defaultValue=""/>
                  <oa:messageTextInput id="StoreNum1" dataType="VARCHAR2" prompt="Location/Store Number"/>
                  <oa:messageTextInput id="RegNum1" dataType="VARCHAR2" prompt="Reg Number"/>
                  <oa:messageTextInput id="TrxNum1" dataType="VARCHAR2" prompt="Trx Num"/>
                  <oa:messageTextInput id="DollarAmt1" dataType="NUMBER" prompt="Dollar Amt"/>
                  <oa:messageTextInput id="BatchNum1" dataType="VARCHAR2" prompt="Batch Name"/>
                  <oa:messageTextInput id="RecptNum1" dataType="VARCHAR2" prompt="Reciept Number"/>
               </ui:contents>
            </oa:messageComponentLayout>
            <oa:rowLayout id="region1" hAlign="center">
               <ui:contents>
                  <oa:submitButton id="Search" text="Search" prompt="Search"/>
                  <oa:submitButton id="Clear" text="Clear" prompt="Clear"/>
               </ui:contents>
            </oa:rowLayout>
            <oa:table id="ResultsTable">
               <ui:contents>
                  <oa:messageTextInput id="DateVal" dataType="DATE" prompt="Date" viewName="XxIbyBatchTrxnsHistoryVO1" viewAttr="DateVal" queryable="false" selectiveSearchCriteria="false" readOnly="true"/>
                  <oa:messageTextInput id="StoreNum" dataType="VARCHAR2" prompt="Location/Store Number" viewName="XxIbyBatchTrxnsHistoryVO1" viewAttr="StoreNum" maximumLength="240" queryable="true" selectiveSearchCriteria="true" readOnly="true" translatableDefaultValue=""/>
                  <oa:messageTextInput id="OrdType" prompt="Order Types" queryable="false" readOnly="true" selectiveSearchCriteria="false" styleClass="OraFieldText" viewName="XxIbyBatchTrxnsHistoryVO1" viewAttr="OrdType" required="no" maximumLength="14"/>
                  <oa:messageTextInput id="RegNum" dataType="VARCHAR2" prompt="Register Number" viewName="XxIbyBatchTrxnsHistoryVO1" viewAttr="RegNum" maximumLength="240" queryable="true" sortState="ascending" selectiveSearchCriteria="true" readOnly="true"/>
                  <oa:messageTextInput id="TrxNum" dataType="VARCHAR2" prompt="Tansaction Number" viewName="XxIbyBatchTrxnsHistoryVO1" viewAttr="TrxNum" maximumLength="240" queryable="true" selectiveSearchCriteria="true" readOnly="true"/>
                  <oa:messageTextInput id="DollarAmt" dataType="NUMBER" prompt="DollarAmt" viewName="XxIbyBatchTrxnsHistoryVO1" viewAttr="DollarAmt" queryable="true" selectiveSearchCriteria="true" readOnly="true"/>
                  <oa:messageTextInput id="BatchNum" dataType="VARCHAR2" prompt="BatchNum" viewName="XxIbyBatchTrxnsHistoryVO1" viewAttr="BatchNum" maximumLength="240" queryable="true" selectiveSearchCriteria="true" readOnly="true"/>
                  <oa:messageTextInput id="TrxType" viewName="XxIbyBatchTrxnsHistoryVO1" viewAttr="TrxType" queryable="true" selectiveSearchCriteria="true" prompt="Trx Type" readOnly="true" maximumLength="240"/>
                  <oa:messageTextInput id="ReceiptNum" dataType="VARCHAR2" prompt="ReceiptNum" viewName="XxIbyBatchTrxnsHistoryVO1" viewAttr="ReceiptNum" maximumLength="240" queryable="false" readOnly="true" rendered="false" selectiveSearchCriteria="false"/>
                  <oa:messageTextInput id="RecptNum" dataType="VARCHAR2" prompt="Reciept Number" viewName="XxIbyBatchTrxnsHistoryVO1" viewAttr="RecptNum" maximumLength="100" selectiveSearchCriteria="true" queryable="true" sortState="ascending" readOnly="true" rendered="true"/>
                  <oa:link id="DetailsLink" destination="OA.jsp?page=/od/oracle/apps/iby/schema/webui/XxIbySettlementDetailsPG&amp;RecptNumber={@ReceiptNum}&amp;retainAM=Y" text="More Info"/>
                  <oa:messageTextInput id="CashRecId" dataType="VARCHAR2" prompt="CashRecId" viewName="XxIbyBatchTrxnsHistoryVO1" viewAttr="CashRecId" maximumLength="255" rendered="false"/>
               </ui:contents>
            </oa:table>
         </ui:contents>
      </oa:pageLayout>
   </content>
</page>

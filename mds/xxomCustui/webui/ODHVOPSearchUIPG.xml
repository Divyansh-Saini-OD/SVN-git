<?xml version = '1.0' encoding = 'UTF-8'?>
<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="10.1.3_1312" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <oa:pageLayout id="SearchPG" windowTitle="Order Search" title="Order Search" controllerClass="od.oracle.apps.xxom.xxomCustui.webui.ODHVOPSearchUICO" amDefName="od.oracle.apps.xxom.xxomCustui.server.xxomCustuiAM">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:contents>
            <oa:defaultSingleColumn id="xxomOrderSerchVOSearchRN" text="Simple Search" comment="Note that the search is case insensitive">
               <ui:contents>
                  <oa:messageTextInput id="OrigSysDocumentRef" dataType="VARCHAR2" prompt="Orig Sys Document Ref" maximumLength="50" tipMessageName="XX_OM_SAS_TRANSACTON_TIP" tipMessageAppShortName="xxom" tipType="shortTip"/>
                  <oa:messageTextInput id="OrderNumber" dataType="NUMBER" prompt="Order Number"/>
               </ui:contents>
            </oa:defaultSingleColumn>
            <oa:table id="xxomOrderSerchResultsRN" text="Search Results">
               <ui:contents>
                  <oa:messageStyledText id="OrigSysDocumentRef1" dataType="VARCHAR2" prompt="Orig Sys Document Ref" viewName="xxomOrderSerchVO1" viewAttr="OrigSysDocumentRef" maximumLength="50"/>
                  <oa:messageStyledText id="OrderNumber1" dataType="NUMBER" prompt="Order Number" viewName="xxomOrderSerchVO1" viewAttr="OrderNumber"/>
                  <oa:messageStyledText id="OrderSourceName" dataType="VARCHAR2" prompt="Order Source Name" viewName="xxomOrderSerchVO1" viewAttr="OrderSource"/>
                  <oa:link id="HeaderAttLink" text="Header Attributes" destination="OA.jsp?page=/od/oracle/apps/xxom/xxomCustui/webui/xxomOrderHeaderAttPG&amp;HeaderId={@HeaderId}&amp;retainAM=Y&amp;addBreadCrumb=Y"/>
                  <oa:link id="PaymentsLink" destination="OA.jsp?page=/od/oracle/apps/xxom/xxomCustui/webui/xxomOrderPaymentsPG&amp;HeaderId={@HeaderId}&amp;retainAM=Y&amp;addBreadCrumb=Y" text="Payments"/>
                  <oa:link id="LineLink" destination="OA.jsp?page=/od/oracle/apps/xxom/xxomCustui/webui/xxomOrderLinesPG&amp;HeaderId={@HeaderId}&amp;retainAM=Y&amp;addBreadCrumb=Y" text="Order Lines"/>
               </ui:contents>
            </oa:table>
            <oa:pageButtonBar id="region1">
               <ui:contents>
                  <oa:submitButton id="Go" text="Go" prompt="Go"/>
                  <oa:submitButton id="Clear" text="Clear" prompt="Clear"/>
               </ui:contents>
            </oa:pageButtonBar>
         </ui:contents>
      </oa:pageLayout>
   </content>
</page>

      <oa:pageLayout amDefName="oracle.apps.asn.lead.server.LeadCrteAM" id="PageLayoutRN" windowTitle="Oracle Sales" title="Create Lead" controllerClass="od.oracle.apps.xxcrm.asn.lead.webui.ODLeadCrteCO" shortDesc="Create Lead" helpTarget="ASN_LeadCrtePG" function="ASN_LEADCRTEPG" regionName="Create Lead">

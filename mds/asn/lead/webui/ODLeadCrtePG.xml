               <ui:contents>
                  <oa:messageTextInput id="ASNLeadCrteNm" required="yes" viewName="LeadCreateVO" viewAttr="Description" shortDesc="Lead Name" maximumLength="240" use="/oracle/apps/asn/lead/attributesets/Lead/Description_LeadName" columns="20"/>
                  <oa:messageLayout id="ASNLeadCrteCustLyt" prompt="Customer" required="yes">
                     <ui:contents>
                        <oa:rowLayout id="ASNLeadCrteCustLytRow" regionName="Customer">

<jrad:page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="9.0.3.8.13_1550" xml:lang="en-US" file-version="$Header: CustSearchPG.xml 115.16 2005/01/04 02:42:14 rramacha ship $" xmlns:user="http://xmlns.oracle.com/jrad/user">
   <jrad:content>
      <oa:pageLayout amDefName="od.oracle.apps.xxcrm.asn.custsrch.common.customer.server.ODCustSearchAM" id="PageLayoutRN" controllerClass="od.oracle.apps.xxcrm.asn.custsrch.common.customer.webui.ODCustSearchCO" title="Customer" helpTarget="ASN_CustSearchPG" function="ASN_CUSTSEARCHPG">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>

<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="10.1.3_1312" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <oa:pageLayout id="TotalAssetsRptRN" amDefName="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSTotalAssetsRptAM" windowTitle="MPS Total Assets Report" title="MPS Total Assets Report" controllerClass="od.oracle.apps.xxcrm.mps.reports.webui.XXCSMPSTotalAssetsRptCO">

                           <ui:contents>
                              <oa:table name="region1" id="MPSPartyName">
                                 <ui:contents>
                                    <oa:messageStyledText id="PartyName" prompt="PartyName" viewName="XXCSMPSPartyNameVO1" viewAttr="PartyName" queryable="true"/>
                                    <oa:messageStyledText id="AopsCustNumber" viewName="XXCSMPSPartyNameVO1" viewAttr="AopsCustNumber" prompt="AopsCustNumber"/>
                                 </ui:contents>
                              </oa:table>
                           </ui:contents>
                        </oa:listOfValues>
                     </lovContents>
                     <lovMappings>
                        <lovMap id="lovMap1" lovItem="PartyName" resultTo="txtPName" criteriaFrom="txtPName"/>
                     </lovMappings>
                  </oa:messageLovInput>
                  <oa:messageChoice id="ProgramTypeParam" pickListViewDef="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSProgramTypeVO" pickListViewName="XXCSMPSProgramTypeVO1" pickListDispAttr="ProgramType" pickListValAttr="ProgramType" prompt="Program Type"/>

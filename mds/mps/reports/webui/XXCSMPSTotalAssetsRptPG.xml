                     <lovContents>
                        <oa:listOfValues id="ModelLovRN">
                           <ui:contents>
                              <oa:table name="ModelLovRN" id="MPSModel">
                                 <ui:contents>
                                    <oa:messageStyledText id="Model" viewName="XXCSMPSModelVO1" viewAttr="Model" prompt="Model" queryable="true"/>
                                    <oa:messageStyledText id="ModelId" viewName="XXCSMPSModelVO1" viewAttr="ModelId" prompt="ModelId"/>
                                 </ui:contents>
                              </oa:table>
                           </ui:contents>
                        </oa:listOfValues>
                     </lovContents>
                     <lovMappings>
                        <lovMap id="lovMap2" lovItem="Model" resultTo="ModelLov" criteriaFrom="ModelLov"/>
                     </lovMappings>
                  </oa:messageLovInput>
                  <oa:messageTextInput id="ManagedStatusInput" prompt="Managed Status"/>
                  <oa:messageTextInput id="ActiveStatusInput" prompt="Active Status"/>
               </ui:contents>
            </oa:messageComponentLayout>

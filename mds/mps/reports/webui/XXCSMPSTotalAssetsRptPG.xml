                  <oa:column id="ActiveStatusCol" rendered="false">
                     <ui:columnHeader>
                        <oa:sortableHeader id="sortableActiveStatus" prompt="ActiveStatus" sortByViewAttr="ActiveStatus" sortState="yes"/>
                     </ui:columnHeader>

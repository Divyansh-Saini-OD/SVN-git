         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:contents>
            <oa:rowLayout id="Spacer">
               <ui:contents>
                  <oa:spacer id="SpacerItem" height="15"/>
               </ui:contents>
            </oa:rowLayout>
            <oa:header id="CustContactUpdateHDR" text="Customer Contact Update"/>
            <oa:messageComponentLayout id="CustomerContactRN" rows="4" columns="2">
               <ui:contents>
                  <oa:messageTextInput id="Contact" prompt="Contact" viewAttr="SiteContact" viewName="OD_MPSCustContactUpdateVO" maximumLength="250"/>
                  <oa:messageTextInput id="Phone" prompt="Phone" viewAttr="SiteContactPhone" viewName="OD_MPSCustContactUpdateVO" maximumLength="25"/>
                  <oa:messageTextInput id="Address" prompt="Address" viewAttr="SiteAddress1" viewName="OD_MPSCustContactUpdateVO" maximumLength="250"/>
                  <oa:messageTextInput id="Address1" prompt="Address 2" viewAttr="SiteAddress2" viewName="OD_MPSCustContactUpdateVO" maximumLength="250"/>
                  <oa:messageTextInput id="City" prompt="City" viewAttr="SiteCity" viewName="OD_MPSCustContactUpdateVO" maximumLength="100"/>
                  <oa:messageTextInput id="State" prompt="State" viewAttr="SiteState" viewName="OD_MPSCustContactUpdateVO" maximumLength="100"/>
                  <oa:messageTextInput id="Zip" prompt="Zip" viewAttr="SiteZipCode" viewName="OD_MPSCustContactUpdateVO" maximumLength="50"/>
                  <oa:messageTextInput id="CostCenter" prompt="Cost Center" viewAttr="DeviceCostCenter" viewName="OD_MPSCustContactUpdateVO" maximumLength="150"/>
                  <oa:messageTextInput id="Location" viewAttr="DeviceLocation" prompt="Location" viewName="OD_MPSCustContactUpdateVO" maximumLength="150" readOnly="false"/>
                  <oa:messageTextInput id="PONumber" viewAttr="PoNumber" prompt="PO Number" viewName="OD_MPSCustContactUpdateVO" maximumLength="50" readOnly="false"/>
                  <oa:messageTextInput id="ShipToSeq" viewAttr="Attribute5" prompt="Ship Seq" viewName="OD_MPSCustContactUpdateVO" maximumLength="5" readOnly="false"/>
                  <oa:messageTextInput id="SeqUpdateDate" viewName="OD_MPSCustContactUpdateVO" viewAttr="SeqUpdateDate" prompt="Ship Seq Update Date" disabled="true"/>
               </ui:contents>
            </oa:messageComponentLayout>
            <oa:tableLayout id="ButtonRN" hAlign="center">
               <ui:contents>
                  <oa:rowLayout id="region2" hAlign="center">
                     <ui:contents>
                        <oa:cellFormat id="region3" hAlign="center">
                           <ui:contents>
                              <oa:submitButton id="Save" text="Save" prompt="Save"/>
                           </ui:contents>
                        </oa:cellFormat>
                        <oa:cellFormat id="region32" hAlign="center">
                           <ui:contents>
                              <oa:spacer id="SpacerBtn" width="25"/>
                           </ui:contents>
                        </oa:cellFormat>
                        <oa:cellFormat id="region31" hAlign="center">
                           <ui:contents>
                              <oa:submitButton id="Back" text="Back" prompt="Back" warnAboutChanges="true">
                                 <ui:primaryClientAction>
                                    <ui:fireAction event="Back">
                                       <ui:parameters>
                                          <ui:parameter key="partyName" value="${oa.OD_MPSCustContactUpdateVO.PartyName}"/>
                                          <ui:parameter key="partyId" value="${oa.OD_MPSCustContactUpdateVO.PartyId}"/>
                                          <ui:parameter key="customerNumber" value="${oa.OD_MPSCustContactUpdateVO.AopsCustNumber}"/>
                                          <ui:parameter key="serialNo" value="${oa.OD_MPSCustContactUpdateVO.SerialNo}"/>

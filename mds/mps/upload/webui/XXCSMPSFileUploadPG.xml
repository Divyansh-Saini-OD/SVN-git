                  </oa:messageComponentLayout>
                  <oa:pageButtonBar id="pageButtons" rendered="true">
                     <ui:contents>
                        <oa:submitButton id="Apply" text="Upload" prompt="Upload"/>
                        <oa:resetButton id="Cancel" text="Cancel"/>
                     </ui:contents>
                  </oa:pageButtonBar>
               </ui:contents>
            </oa:header>
            <oa:rowLayout id="region1" hAlign="right">

         </ui:corporateBranding>
         <ui:contents>
            <oa:header id="UploadRegion">
               <ui:contents>
                  <oa:messageComponentLayout id="mainRN">
                     <ui:contents>
                        <oa:messageChoice id="ExcelTemplateChoice" pickListViewDef="od.oracle.apps.xxcrm.mps.upload.server.XXCSMPSFlexFieldVO" pickListViewName="XXCSMPSFlexFieldVO" pickListDispAttr="Type" pickListValAttr="Value" prompt="Choose Template" required="yes"/>

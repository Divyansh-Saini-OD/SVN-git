<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="9.0.3.8.10_1080" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header: XXODCDHCustStatusUpload.xml 115.0 2009/01/12 08:40:02 ssmohan noship $">
   <content>
      <oa:pageLayout id="XXODCDHCustStatusUpload" title="Custom Customer Profiles Upload Tool" amDefName="od.oracle.apps.xxcrm.cdh.uploads.custstatus.server.XXODCDHUploadCustStatusAM" windowTitle="Custom Customer Status Upload Tool" controllerClass="od.oracle.apps.xxcrm.cdh.uploads.custstatus.webui.XXODCDHCustStatusUploadCO">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:productBranding>
            <oa:image id="XXODCDHProductBranding" prompt="Custom TCA Applications"/>
         </ui:productBranding>
         <ui:copyright>
            <oa:messageStyledText id="item1"/>
         </ui:copyright>
         <ui:privacy>
            <oa:link id="item2"/>
         </ui:privacy>
         <ui:contents>
            <oa:spacer id="spacer0" height="10"/>
            <oa:staticStyledText id="msgLabel1" text="Please upload a csv file for uploading Customer Status." prompt="Please upload a csv file for uploading Customer Status." styleClass="ORAHEADER"/>
            <oa:spacer id="spacer1" height="10"/>
            <oa:tip id="item4" text="The File should contain the following columns in the following order -"/>
            <oa:spacer id="spacer2" height="10"/>
            <oa:messageStyledText id="msgLabel2" prompt="- Batch ID"/>
            <oa:messageStyledText id="msgLabel4" prompt="- Account Orig System Reference"/>
            <oa:messageStyledText id="msgLabel5" prompt="- Account Site Orig System Reference" />

            <oa:spacer id="spacer3" height="10"/>
            <oa:link id="item8" destination="javascript:void window.open(&quot;template.csv&quot;)" text="For a tempate, click here"/>
            <oa:spacer id="spacer4" height="10"/>
            <oa:link id="item89" destination="javascript:void window.open(&quot;CustStatus_Upload.html&quot;)" text="For a document to help, click here"/>
            <oa:spacer id="spacer5" height="10"/>

			<oa:spacer id="spacer6" height="30"/>
            <oa:messageFileUpload id="fileSelector" prompt="Upload CSV File" required="yes" dataType="CLOB"/>
            <oa:spacer id="spacer7" height="10"/>
            <oa:messageTextInput id="txtFilePath" rendered="false"/>

            <oa:submitButton id="btnSubmit" prompt="Upload Customer Status"/>
			<oa:submitButton id="btnViewRequests" prompt="ViewRequests"/>

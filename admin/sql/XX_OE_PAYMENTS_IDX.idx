SET SHOW         OFF
SET VERIFY       OFF
SET ECHO         OFF
SET TAB          OFF
SET FEEDBACK     ON
WHENEVER SQLERROR CONTINUE

PROMPT
PROMPT Creation of Index on custom table XX_OE_PAYMENTS.....
PROMPT

PROMPT
PROMPT Creating non-unique index XX_OE_PAYMENTS_N1 .....
PROMPT

CREATE INDEX XX_OE_PAYMENTS_N1 ON XX_OE_PAYMENTS(PAYMENT_TRX_ID, PAYMENT_TYPE_CODE);

PROMPT
PROMPT Creating unique index XX_OE_PAYMENTS_U1 .....
PROMPT

CREATE UNIQUE INDEX XX_OE_PAYMENTS_U1 ON XX_OE_PAYMENTS(HEADER_ID, LINE_ID, PAYMENT_NUMBER);

PROMPT
PROMPT Creating non-unique index XX_OE_PAYMENTS_N2 .....
PROMPT

CREATE INDEX XX_OE_PAYMENTS_N2 ON XX_OE_PAYMENTS(LINE_ID);

WHENEVER SQLERROR EXIT 1

PROMPT
PROMPT Exiting....
PROMPT

EXIT;
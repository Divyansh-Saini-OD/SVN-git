SET SHOW         OFF
SET VERIFY       OFF
SET ECHO         OFF
SET TAB          OFF
SET FEEDBACK     ON
WHENEVER SQLERROR CONTINUE

PROMPT
PROMPT Creation of Index on custom table XXOM_IMPORT_INT.....
PROMPT

PROMPT
PROMPT Creating non-unique index XXOM_IMPORT_INT_N1 .....
PROMPT

CREATE INDEX XXOM_IMPORT_INT_N1 ON XXOM_IMPORT_INT(SEQUENCE_NUM);

PROMPT
PROMPT Creating non-unique index XXOM_IMPORT_INT_N2 .....
PROMPT

CREATE INDEX XXOM_IMPORT_INT_N2 on XXOM_IMPORT_INT (order_number || sub_order_number);

WHENEVER SQLERROR EXIT 1

PROMPT
PROMPT Exiting....
PROMPT

EXIT;
SET VERIFY   OFF
SET TERM     ON
SET FEEDBACK OFF
SET SHOW     OFF
SET ECHO     OFF
SET TAB      OFF

WHENEVER SQLERROR EXIT 1

PROMPT
PROMPT Creating Custom Sequence XX_INV_ITEM_PURGE_S ...
PROMPT

CREATE SEQUENCE XX_INV_ITEM_PURGE_S INCREMENT BY 1 START WITH 1 NOCACHE NOORDER;

PROMPT
PROMPT Exiting....
PROMPT

SET FEEDBACK ON

EXIT;


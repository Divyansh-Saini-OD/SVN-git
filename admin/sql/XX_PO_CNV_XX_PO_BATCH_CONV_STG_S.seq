SET SHOW         OFF
SET VERIFY       OFF
SET ECHO         OFF
SET TAB          OFF
SET FEEDBACK     ON
WHENEVER SQLERROR CONTINUE

BEGIN
  EXECUTE IMMEDIATE 'DROP SEQUENCE XXFIN.XX_PO_POCONV_BATCHID_S';
EXCEPTION
  WHEN OTHERS THEN NULL;
END;
/
CREATE SEQUENCE XXFIN.XX_PO_POCONV_BATCHID_S
START WITH 1
NOCACHE
NOORDER;
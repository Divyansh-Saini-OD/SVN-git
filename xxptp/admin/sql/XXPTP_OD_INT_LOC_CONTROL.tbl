CREATE TABLE OD_INT_LOC_CONTROL
(
  PROCESS_ID           NUMBER,
  PROCESS_NAME         VARCHAR2(100 BYTE),
  RUNNING_FLAG         VARCHAR2(1 BYTE),
  STOP_RUNNING_FLAG    VARCHAR2(1 BYTE),
  SLEEP_TIME           NUMBER(6),
  UPDATE_DATETIME      TIMESTAMP(6)             DEFAULT systimestamp,
  EMAIL_TO             VARCHAR2(500 BYTE),
  PROCESS_LOCKED_FLAG  VARCHAR2(1 BYTE)         DEFAULT 'N'
);

GRANT ALL ON OD_INT_LOC_CONTROL TO APPS,apps_rw;

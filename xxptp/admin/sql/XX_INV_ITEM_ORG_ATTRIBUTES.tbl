                                                OD_WHSE_ITEM_CD            VARCHAR2(6),
                                                OD_FUTURE_IND              VARCHAR2(6),
                                                OD_ABC_CLASS               VARCHAR2(6),
                                                LOCAL_ITEM_DESC            VARCHAR2(100),

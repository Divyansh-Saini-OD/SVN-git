SET VERIFY      OFF
SET TERM        ON
SET FEEDBACK    OFF
SET SHOW        OFF
SET ECHO        OFF
SET TAB         OFF

WHENEVER SQLERROR CONTINUE;

PROMPT
PROMPT Creating the Custom Table ......
PROMPT

PROMPT
PROMPT Creating the Table XX_INV_ERROR_LOG......
PROMPT

CREATE TABLE XX_INV_ERROR_LOG
(
  CONTROL_ID       NUMBER         NOT NULL,
  RMS_PROCESS_ID   NUMBER         NOT NULL,
  PROCESS_NAME     VARCHAR2(100)  NOT NULL,
  KEY_VALUE_1      VARCHAR2(100)          ,
  KEY_VALUE_2      VARCHAR2(100)          ,
  KEY_VALUE_3      VARCHAR2(100)          ,
  KEY_VALUE_4      VARCHAR2(100)          ,
  KEY_VALUE_5      VARCHAR2(100)          ,
  ERROR_MESSAGE    VARCHAR2(2000)         ,
  PROCESS_FLAG     VARCHAR2(1)            ,               
  CREATION_DATE    DATE           NOT NULL,
  CREATED_BY       NUMBER         NOT NULL,
  LAST_UPDATE_DATE DATE           NOT NULL,
  LAST_UPDATED_BY  NUMBER         NOT NULL
);


WHENEVER SQLERROR CONTINUE;

PROMPT
PROMPT Exiting....
PROMPT

SET FEEDBACK ON

EXIT;


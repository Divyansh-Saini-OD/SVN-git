<?xml version = '1.0' encoding = 'UTF-8'?>
<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="9.0.3.8.13_1550" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <!--*-->
      <oa:pageLayout windowTitle="Validate and Upload Data" id="FileUploadPage" akRegionCode="AK_ATTACH_UPLOAD_PAGE" regionName="AK Attach Upload Page" amDefName="od.oracle.apps.xxptp.wac.fileupload.server.ODWacFileUploadAM" controllerClass="od.oracle.apps.xxptp.wac.fileupload.webui.ODWacFileUploadCO" helpTarget="" adminCustomizable="true" title="Validate and Upload Data" rendered="true" amStateRequired="false" warnAboutChanges="true" securityMode="standard" scope="" postDefault="true">
         <ui:corporateBranding>
            <oa:image id="corporateBranding" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:contents>
            <oa:defaultSingleColumn id="MainRegion" regionName="AK Attach Upload" adminCustomizable="false">
               <ui:contents>
                  <oa:defaultSingleColumn id="TemplateDownloadRN" text="Template Download" rendered="true" adminCustomizable="false">
                     <ui:contents>
                        <oa:messageComponentLayout id="DescriptionRN1">
                           <ui:contents>
                              <oa:messageDownload id="TemplateDownload1" prompt="Click link to download template"/>
                           </ui:contents>
                        </oa:messageComponentLayout>
                     </ui:contents>
                  </oa:defaultSingleColumn>
                  <oa:defaultSingleColumn id="FileUploadRN" text="File Upload" rendered="true" adminCustomizable="false">
                     <ui:contents>
                        <oa:messageComponentLayout id="DescriptionRN" width="40%" adminCustomizable="false" searchRegion="true">
                           <ui:contents>
                              <oa:messageFileUpload id="FileInput" columns="50" adminCustomizable="false" rendered="true" use="/oracle/apps/fnd/attributesets/AttachAttributeSets/FILE_UPLOAD" required="yes"/>
                              <oa:messageTextInput id="ValidationStatus" prompt="Validation Status" columns="50" readOnly="false" disabled="false"/>
                              <oa:messageTextInput id="WFItemKeyNumber" prompt="WF Item Key Number" columns="50" readOnly="false"/>
                              <oa:messageTextInput use="/oracle/apps/fnd/attributesets/AttachAttributeSets/DESCRIPTION" readOnly="false" required="no" columns="100" rows="6" secret="false" shortDesc="Error details" id="ErrorDetails" adminCustomizable="false" maximumLength="10000" prompt="Errors"/>
                           </ui:contents>
                        </oa:messageComponentLayout>
                     </ui:contents>
                  </oa:defaultSingleColumn>
               </ui:contents>
            </oa:defaultSingleColumn>
            <oa:pageButtonBar id="PageButtonBarRN" adminCustomizable="false">
               <ui:contents>
                  <oa:button use="/oracle/apps/fnd/attributesets/Buttons/Cancel" destination="OA.jsp?OAFunc=OAHOMEPAGE" id="Cancel" userCustomizable="false" adminCustomizable="false"/>
                  <oa:submitButton use="/oracle/apps/fnd/attributesets/Buttons/Apply" textTranslationMaxLength="5" promptTranslationMaxLength="5" text="Apply" id="Okay" textTranslationExpansion="none" adminCustomizable="false" prompt="Apply"/>
                  <oa:button id="Clear" text="Clear" use="/oracle/apps/fnd/attributesets/Buttons/ClearAll" destination="OA.jsp?OAFunc=XX_WAC_FILE_UPLOAD"/>
               </ui:contents>
            </oa:pageButtonBar>
         </ui:contents>
      </oa:pageLayout>
   </content>
</page>

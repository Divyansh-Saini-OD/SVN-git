<?xml version='1.0' encoding='UTF-8'?>
<oa:tableLayout version="9.0.3.8.9_1057" xml:lang="en-US" file-version="$Header: HzPuiDQMMultiContactLOV.xml 115.13 2005/03/25 04:07:46 jhuang noship $" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui"

<ViewObject
   Name="AdvSrchResultsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.cs.csz.oa.server.CszViewRowImpl"
   ComponentClass="oracle.apps.cs.csz.search.server.AdvSrchResultsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select
    (select serial_number from csi_item_instances where instance_id= sr.Customer_Product_Id) InstSerialNumber, 
    (select external_reference from csi_item_instances where instance_id= sr.Customer_Product_Id) InstTag, 
    (select name from csi_systems_vl where sr.system_id = system_id) SysNumber,
    sr.incident_id IncidentId,
    sr.incident_number IncidentNumber,
    (select  resource_name from jtf_rs_resource_extns_vl where sr.incident_owner_id = resource_id) OwnerName,
    (select summary from cs_incidents_all_tl where incident_id=sr.incident_id and language=userenv('lang')) Summary,
    (select party_name from hz_parties where party_id = sr.customer_id) CustomerName,
    (select known_as from hz_parties where party_id = sr.customer_id) Alias,
    (select party_number from hz_parties where party_id = sr.customer_id) CustomerNumber,
apps.CSZ_SERVICEREQUEST_UTIL_PVT.get_contact_name(sr.incident_id, sr.customer_id) ContactName,
    (select name from cs_incident_statuses_tl where incident_status_id = sr.incident_status_id and language = userenv('lang')) IncidentStatus,
    (select name from cs_incident_types_tl where incident_type_id = sr.incident_type_id and language=userenv('LANG')) IncidentType,
    (select name from cs_incident_urgencies_tl where incident_urgency_id = sr.incident_urgency_id and language=userenv('LANG')) IncidentUrgency,
    (select name from cs_incident_severities_tl where incident_severity_id = sr.incident_severity_id and language=userenv('LANG')) IncidentSeverity,
    sr.contract_number ContractNumber,
    channel.meaning CreationChannel,
    problem.meaning ProblemMeaning,
    resolution.meaning ResolutionMeaning,
/*apps.CSZ_SERVICEREQUEST_UTIL_PVT_A.get_sr_escalated(sr.incident_id)*/ 'X' EscalatedFlag,
    /*decode(sr.status_flag, 'O', 'Y', 'N')*/ 'X' ActiveFlag,
    /*decode(sr.unassigned_indicator, 1,'N',3,'N','Y') */ 'X' OwnerNotExistFlag,
/*apps.CSZ_SERVICEREQUEST_UTIL_PVT.get_sr_jeopardy(sr.Incident_Id,sr.Obligation_Date,sr.Expected_Resolution_Date,sr.Inc_Responded_By_Date,sr.Actual_Resolution_Date)*/ 'N' JeopardyFlag,
    /*decode(
     decode( sign(sr.actual_resolution_date - sr.expected_resolution_date), 1, 1, 0) +
     decode( sign(sr.inc_responded_by_date - sr.obligation_date), 1, 1, 0), 2, 'Y', 1, 'Y', 'N')*/ 'X'  MissDeadline,
   (select concatenated_segments from mtl_system_items_vl
      where sr.platform_id = inventory_item_id and organization_id =sr.inv_platform_org_id) PlatformName,
   (select concatenated_segments from mtl_system_items_vl
      where sr.inventory_item_id = inventory_item_id and organization_id =sr.inv_organization_id) ProductName,
    (select account_number from hz_cust_accounts where sr.account_id = cust_account_id) AccountNumber,
  sr.account_id AccountId,
  sr.incident_owner_id OwnerId,
  sr.customer_id CustomerId,
  sr.incident_type_id IncidentTypeId,
  sr.incident_urgency_id IncidentUrgencyId,
  sr.incident_severity_id IncidentSeverityId,
  sr.sr_creation_channel CreationChannelCode,
  sr.inventory_item_id ProductId,
  sr.inv_organization_id ProductOrgId,
  sr.customer_product_id CustomerProductId,
  sr.platform_id PlatformId,
  sr.inv_platform_org_id PlatformOrgId,
  sr.problem_code ProblemCode,
  sr.resolution_code ResolutionCode,
  sr.incident_date IncidentDate,
  sr.incident_occurred_date OccurredDate,
  sr.incident_resolved_date ResolvedDate,
  sr.close_date CloseDate,
  sr.inc_responded_by_date RespondedByDate,
  sr.expected_resolution_date ExpResolutionDate,
  sr.incident_last_modified_date LastModifiedDate,
 /*sr.status_flag StatusFlag,*/
  'O' StatusFlag,
  sr.resource_type srownertypecode,
  sr.group_type srgrouptypecode,
  sr.owner_group_id SrGroupId,
  sr.system_id SystemId,
  sr.actual_resolution_date ActualResolutionDate,
  sr.Obligation_Date ObligationDate,
 (SELECT decode(status.sort_order,
		NULL,
		'9999'||status.name,
		substr('0000',1,(4 -length(to_char(status.sort_order))))
        	  || to_char(status.sort_order) 
   		      ||status.name) 
	FROM cs_incident_statuses_vl status
	WHERE status.incident_status_id = sr.incident_status_id)statusordattr,
(SELECT sev.importance_level
	FROM cs_incident_severities_b sev
	WHERE sev.incident_severity_id = sr.incident_severity_id) SevImportanceLevel,
(SELECT urg.importance_level
	FROM cs_incident_urgencies_b urg
	WHERE urg.incident_urgency_id = sr.incident_urgency_id)UrgImportanceLevel,
sr.unassigned_indicator UnassignedIndicator,
  sr.incident_status_id IncidentStatusId,
(SELECT 
grp.group_name 
from 
jtf_rs_groups_vl grp  
where 
grp.group_id = sr.owner_group_id) AssignedGroup

from
  cs_incidents_b_sec sr, 
  cs_lookups channel,
  cs_lookups problem,    
  cs_lookups resolution
where   
  sr.sr_creation_channel = channel.lookup_code(+)   and
  channel.lookup_type(+) = 'CS_SR_CREATION_CHANNEL' and 
  sr.problem_code = problem.lookup_code(+)   and       
  problem.lookup_type(+) = 'REQUEST_PROBLEM_CODE' and 
  sr.resolution_code =  resolution.lookup_code(+) and        
  resolution.lookup_type(+) = 'REQUEST_RESOLUTION_CODE'
   ]]></SQLQuery>

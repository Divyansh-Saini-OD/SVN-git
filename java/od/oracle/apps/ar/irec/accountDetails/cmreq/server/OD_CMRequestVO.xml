   Name="OD_CMRequestVO"
   Extends="oracle.apps.ar.irec.accountDetails.cmreq.server.CMRequestVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.ar.irec.accountDetails.cmreq.server.OD_CMRequestVORowImpl"
   ComponentClass="od.oracle.apps.ar.irec.accountDetails.cmreq.server.OD_CMRequestVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT
  u.user_name user_name,
  u.email_address email_address,
  '' phone_number,
  req.request_id request_id,
  req.creation_date request_date,
  lk_stat.meaning request_status,
  req.customer_trx_id previous_customer_trx_id,
  lk_reason.meaning reason,
  req.comments comments,
  req.line_amount req_line_amount,
  req.tax_amount req_tax_amount,
  req.freight_amount req_freight_amount,
  l_type.meaning    class_name,
  types.type                              class,
  types.type    trxtype,
  nvl(tl.sequence_num,1)                  term_sequence_number,
  trx.customer_trx_id                     customer_trx_id,
  trx.trx_number                          trx_number,
  trx.ship_to_customer_id                 ship_to_customer_id,
  trx.ship_to_contact_id                  ship_to_contact_id,
  trx.invoice_currency_code               invoice_currency_code,
  trx.ship_via                            ship_via,
  nvl(tax_original, to_number(null))      trx_tax_amount,
  trx.ship_to_site_use_id                 ship_to_site_use_id,
  types.description,
  ps.customer_id,
  ps.customer_site_use_id,
  b_hp.party_name b_customer_name,
  b_hl.address1 b_address1,
  b_hl.address2 b_address2,
  b_hl.address3 b_address3,
  b_hl.address4 b_address4,
  b_hl.city b_city,
  b_hl.state b_state,
  b_hl.province b_province,
  b_hl.postal_code b_postal_code,
  b_hl.country b_country,
/* Bug # 2836559 */
decode(b_hl.country, null, null,
       nvl(b_sp.default_country,'xxxxxx'),
       decode( b_sp.print_home_country_flag,'Y',b_dft.territory_short_name,null),
       NVL(b_ft.territory_short_name, b_hl.country))
                                        b_country_name,
/* Bug # 2836559 */
/* Bug # 2863594 - hikumar */
decode(b_hl.city,null,null,b_hl.city||', ') ||
decode(b_hl.state,null,null,b_hl.state||' ') ||
decode(b_hl.province,null,null,b_hl.province||' ') ||
decode(b_hl.postal_code,null,null,b_hl.postal_code||' ') ||
decode(b_hl.country, null, null,
       nvl(b_sp.default_country,'xxxxxx'),
       decode( b_sp.print_home_country_flag,'Y',b_dft.territory_short_name,null),
       NVL(b_ft.territory_short_name, b_hl.country))
                              b_Combined_Address,
/* Bug # 2863594  - hikumar */

s_hp.party_name s_customer_name,
s_hl.address1 s_address1,
s_hl.address2 s_address2,
s_hl.address3 s_address3,
s_hl.address4 s_address4,
s_hl.city s_city,
s_hl.state s_state,
s_hl.province s_province,
s_hl.postal_code s_postal_code,
s_hl.country s_country,
/* Bug # 2836559 */
decode(s_hl.country, null, null,
       nvl(s_sp.default_country,'xxxxxx'),
       decode( s_sp.print_home_country_flag,'Y',s_dft.territory_short_name,null),
       NVL(s_ft.territory_short_name, s_hl.country))
                                        s_country_name,
/* Bug # 2836559 */
/* Bug # 2863594 - hikumar */
decode(s_hl.city,null,null,s_hl.city||', ') ||
decode(s_hl.state,null,null,s_hl.state||' ') ||
decode(s_hl.province,null,null,s_hl.province||' ') ||
decode(s_hl.postal_code,null,null,s_hl.postal_code||' ') ||
decode(s_hl.country, null, null,
       nvl(s_sp.default_country,'xxxxxx'),
       decode( s_sp.print_home_country_flag,'Y',s_dft.territory_short_name,null),
       NVL(s_ft.territory_short_name, s_hl.country))
                             s_Combined_Address,
/* Bug # 2863594  - hikumar */

    b_party.person_first_name ||' '|| b_party.person_last_name b_attn,
    s_party.person_first_name ||' '|| s_party.person_last_name s_attn,

	req.orig_trx_number  orig_trx_number,
	req.tax_ex_cert_num  tax_ex_cert_num
--,trx.Attribute12
,(SELECT rCT.ATTRIBUTE12
    FROM ra_customer_trx rct
    WHERE req.cm_customer_trx_id = rct.customer_trx_id
    ) AS ATTRIBUTE12
FROM 
     fnd_user u,
     ar_lookups lk_stat,
     ar_lookups lk_reason,
     ra_cm_requests req,
	 ar_lookups                              o_type,
     ar_lookups                              l_type,
     ra_batch_sources                        bs,
     ar_invoice_count_terms_v                t_count,
     ar_adjustments                          com_adj,
     ar_payment_schedules                    ps,
     ra_terms_lines                          tl,
     ra_terms                                t,
     ra_salesreps                            sales,
     org_freight                             org,
     ra_cust_trx_types                       types,
     ra_customer_trx                         trx,
 
     /* Bug 3699255 */
	 hz_locations  b_hl,
	 hz_parties    b_hp,
	 hz_cust_accounts b_hca,
	 hz_party_sites b_hps,
  	 HZ_CUST_SITE_USES    b_hcsu,
	 HZ_CUST_ACCT_SITES   b_hcas,
	 
	 ar_system_parameters b_sp,
	 fnd_territories_vl b_ft,
	 fnd_territories_vl b_dft,

	 hz_locations  s_hl,
	 hz_parties    s_hp,
	 hz_cust_accounts s_hca,
	 hz_party_sites s_hps,
	 HZ_CUST_SITE_USES    s_hcsu,
	 HZ_CUST_ACCT_SITES   s_hcas,
	 
	 ar_system_parameters s_sp,
	 fnd_territories_vl s_ft,
     fnd_territories_vl s_dft,

     hz_cust_account_roles b_acct_role,
	 hz_relationships      b_rel,
	 hz_parties            b_party,

	 hz_cust_account_roles s_acct_role,
	 hz_relationships      s_rel,
	 hz_parties            s_party

WHERE req.created_by = u.user_id(+)
AND req.request_id = :1
AND lk_stat.lookup_type = 'CREDIT_MEMO_REQUEST_STATUS'
AND lk_stat.lookup_code = req.status
AND lk_stat.externally_visible_flag = 'Y'
AND lk_reason.lookup_type = 'CREDIT_MEMO_REASON'
AND lk_reason.lookup_code = req.cm_reason_code

        AND trx.cust_trx_type_id                = types.cust_trx_type_id
        AND trx.ship_via                        = org.freight_code(+)
        AND trx.org_id                          = org.organization_id(+)
        AND trx.term_id                         = tl.term_id(+)
        AND trx.term_id                         = t.term_id(+)
        AND trx.customer_trx_id+0               = ps.customer_trx_id(+)
        AND trx.primary_salesrep_id             = sales.salesrep_id(+)
        AND trx.batch_source_id                 = bs.batch_source_id(+)
        AND ps.payment_schedule_id +
            DECODE( ps.class,'INV',0,'')        = com_adj.payment_schedule_id(+)
        AND com_adj.subsequent_trx_id           is NULL
        AND 'C'                                 = com_adj.adjustment_type(+)
	/* Bug  2571868: */
        /* AND types.default_printing_option       = 'PRI' */
        AND l_type.lookup_type                  = 'INV/CM/ADJ'
        AND l_type.lookup_code                  = DECODE(types.type,'DEP','INV',types.type)
        AND l_type.lookup_code                  <> 'ADJ'
        AND NVL(ps.terms_sequence_number,
            NVL(tl.sequence_num,0))             = NVL(tl.sequence_num,
                                                  NVL(ps.terms_sequence_number,0))
        AND o_type.lookup_type                  = 'YES/NO'
        AND o_type.lookup_code                  = decode(nvl(ps.amount_due_remaining,0),0,'N','Y')
        AND nvl(trx.term_id, -1)                = t_count.term_id
        AND trx.customer_trx_id(+)                 = req.customer_trx_id
        AND ps.terms_sequence_number           = :2 
	AND ps.customer_id                     = nvl( :3 ,ps.customer_id)
        AND ps.customer_site_use_id            = nvl( :4 ,ps.customer_site_use_id )

	AND b_hca.cust_account_id(+)    = trx.bill_to_customer_id
	AND b_hca.party_id = b_hp.party_id
AND b_hcsu.site_use_id(+) = trx.bill_to_site_use_id 
    AND b_hcas.cust_acct_site_id(+) = b_hcsu.cust_acct_site_id 
    AND b_hps.party_site_id(+) = b_hcas.party_site_id 
    AND b_hl.location_id (+)    = b_hps.location_id
	AND nvl( null ,b_hl.country)  = b_ft.territory_code(+)
	AND nvl(b_sp.default_country,'US')        = b_dft.territory_code

	AND s_hca.cust_account_id(+)    = trx.ship_to_customer_id
    AND s_hca.party_id = s_hp.party_id(+)
    AND s_hcsu.site_use_id(+) = trx.ship_to_site_use_id
    AND s_hcas.cust_acct_site_id(+) = s_hcsu.cust_acct_site_id 
    AND s_hps.party_site_id(+) = s_hcas.party_site_id 
    AND s_hl.location_id (+)    = s_hps.location_id
    AND nvl( null , s_hl.country)  = s_ft.territory_code(+)
    AND nvl(s_sp.default_country,'US')        = s_dft.territory_code

	AND b_acct_role.cust_account_role_id(+) = trx.bill_to_contact_id
	AND nvl(b_acct_role.ROLE_TYPE, 'CONTACT')  = 'CONTACT'
	AND b_acct_role.party_id            = b_rel.party_id(+)
	AND b_rel.subject_id                = b_party.party_id(+)
	AND nvl(b_rel.SUBJECT_TABLE_NAME, 'HZ_PARTIES')  = 'HZ_PARTIES'
	AND nvl(b_rel.OBJECT_TABLE_NAME, 'HZ_PARTIES')   = 'HZ_PARTIES'
	AND nvl(b_rel.DIRECTIONAL_FLAG, 'F')	= 'F'
	
	AND s_acct_role.cust_account_role_id(+) = trx.ship_to_contact_id
	AND nvl(s_acct_role.ROLE_TYPE, 'CONTACT')  = 'CONTACT'
	AND s_acct_role.party_id            = s_rel.party_id(+)
	AND s_rel.subject_id                = s_party.party_id(+)
	AND nvl(s_rel.SUBJECT_TABLE_NAME, 'HZ_PARTIES')  = 'HZ_PARTIES'
	AND nvl(s_rel.OBJECT_TABLE_NAME, 'HZ_PARTIES')   = 'HZ_PARTIES'
	AND nvl(s_rel.DIRECTIONAL_FLAG, 'F')	= 'F'
   ]]></SQLQuery>

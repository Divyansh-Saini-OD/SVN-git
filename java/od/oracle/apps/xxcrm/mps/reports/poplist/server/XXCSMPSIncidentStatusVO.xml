<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="XXCSMPSIncidentStatusVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.poplist.server.XXCSMPSIncidentStatusVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT CV.INCIDENT_STATUS_ID, CV.NAME 
FROM CS_SR_STATUS_GROUPS_TL CT,
     CS_SR_ALLOWED_STATUSES CA,
     CS_INCIDENT_STATUSES_VL CV
WHERE CT.GROUP_NAME = 'MPS Support'
AND  CT.STATUS_GROUP_ID = CA.STATUS_GROUP_ID
AND CA.INCIDENT_STATUS_ID = CV.INCIDENT_STATUS_ID
   ]]></SQLQuery>

<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="XXCSMPSCustomersListVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSCustomersListVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT DISTINCT HC.ACCOUNT_NUMBER,  
                MB.AOPS_CUST_NUMBER, 
                MB.PARTY_NAME
               ,DECODE(nvl(MB.ESSENTIALS_ATR_FLAG,'N'),'Y','ATR','MPS') PROGRAM_TYPE 
               ,MB.ACTIVE_STATUS
               ,MB.MANAGED_STATUS
from hz_parties hp,
     hz_cust_accounts hc,
     xx_cs_mps_device_b mb
WHERE MB.PARTY_ID = HP.PARTY_ID
  AND HP.PARTY_ID = HC.PARTY_ID
  AND MB.PARTY_ID IS NOT NULL
order by mb.party_name
   ]]></SQLQuery>

<ViewObject
   Name="XXCSMPSIncidentsOpenVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSIncidentsOpenVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT mb.account_name as partyName
    ,substrb(mb.orig_system_reference,1,8)  as aopsCustNumber
    ,inc_b.incident_number as incidentNumber
    ,inc_b.creation_date as creationDate
    ,stat.name as status
    ,inc_b.summary
    ,jtfg.group_name as owner
    ,typ.name  srType 
    ,urg.name urgency
    ,sev.name severity
    ,inc_b.Problem_Code as ProblemCode
    ,inc_b.resolution_Code as resolutionCode
    ,inc_b.expected_resolution_date as expectedResolutionDate 
    ,inc_b.obligation_date as expectedResponseDate
    ,inc_b.original_order_number as originalOrderNumber
    ,inc_b.INC_RESPONDED_BY_DATE as incRespondedByDate
    ,fn.description as createdBy  
    ,inc_b.resolution_summary as resolutionSummary
    ,typ.attribute7
    ,stat.name as incidentStatus
    ,typ.name as incidentStatusType
    ,inc_b.incident_type_id as incidentTypeId
    ,stat.incident_status_id as incidentStatusId
    ,inc_b.incident_attribute_12 as deviceLocation
    ,inc_b.incident_attribute_4 as deviceCostCenter
    ,inc_b.incident_attribute_5 as model
    ,inc_b.incident_attribute_6  as ipAddress
    ,inc_b.incident_attribute_7  as deviceContact
    ,inc_b.incident_attribute_8  as devicePhone
    ,inc_b.incident_attribute_13 as programType
    ,inc_b.incident_attribute_3  as serialNo
    ,inc_b.incident_attribute_10 as JIT
    ,inc_b.incident_attribute_1  as orderNumber
    ,inc_b.incident_attribute_2  as orderDate
    ,fn.user_name as createdByEmpNumber  
    ,inc_b.incident_owner_id as incidentOwnerId
  FROM hz_cust_accounts_all mb,
    cs_incidents inc_b,
    cs_incident_statuses_tl stat,
    CS_INCIDENT_URGENCIES_TL urg,
    CS_INCIDENT_SEVERITIES_TL SEV,
    cs_incident_types_vl typ,
    jtf_rs_groups_tl jtfg,
    fnd_user fn
  WHERE fn.user_id = inc_b.created_by
  AND mb.party_id = inc_b.customer_id
  and inc_b.incident_urgency_id = urg.incident_urgency_id
  and inc_b.incident_severity_id = sev.incident_severity_id
  AND inc_b.incident_status_id    = stat.incident_status_id
  AND inc_b.incident_type_id      = typ.incident_type_id
  AND jtfg.group_id               = inc_b.owner_group_id
  AND typ.name like 'MPS%'
  AND typ.attribute7 = 'CaseMgmt'
  AND mb.ACCOUNT_NAME                                             = NVL(:0,mb.ACCOUNT_NAME)
  AND inc_b.creation_date >= nvl(:1, to_date('01-JAN-1900','DD-MON-YYYY'))
  AND inc_b.creation_date <= nvl(:2, sysdate+1)  
  AND INCIDENT_NUMBER = nvl(:3,INCIDENT_NUMBER)
  AND inc_b.incident_type_id = nvl(:4,inc_b.incident_type_id)
  AND stat.incident_status_id = nvl(:5,stat.incident_status_id)
  AND substrb(mb.orig_system_reference,1,8) = nvl(:6,substrb(mb.orig_system_reference,1,8))
  AND nvl(trunc(inc_b.obligation_date), trunc(sysdate)) <= nvl(:7,nvl(trunc(inc_b.obligation_date), trunc(sysdate)))
  AND nvl(trunc(inc_b.expected_resolution_date), trunc(sysdate)) <= nvl(:8,nvl(trunc(inc_b.expected_resolution_date), trunc(sysdate)))
  AND nvl(trim(incident_attribute_13),'PROG_TYPE') = nvl(:9,nvl(trim(incident_attribute_13),'PROG_TYPE')) --program_type
  AND nvl(trim(incident_attribute_3),'SER_NO') = nvl(:10,nvl(trim(incident_attribute_3),'SER_NO')) --serial_no
  AND nvl(inc_b.summary, 'SUMM')= nvl(nvl(:11, inc_b.summary),'SUMM')
  order by mb.account_name
   ]]></SQLQuery>

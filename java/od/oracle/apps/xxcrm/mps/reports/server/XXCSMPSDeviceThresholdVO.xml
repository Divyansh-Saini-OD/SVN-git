<ViewObject
   Name="XXCSMPSDeviceThresholdVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSDeviceThresholdVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT REGEXP_REPLACE(db.serial_no, '[[:cntrl:]]', '') Serial_Number,
  db.party_name Customer_Name,
  db.level_limit Device_Level,
  db.aops_cust_number AOPS_Number,
  db.program_type Program_Type,
  db.active_Status Active_Status,
  (SELECT attribute2
  FROM xx_cs_mps_device_details
  WHERE db.serial_no=serial_no
  AND supplies_label='TONERLEVEL_BLACK'
  ) Black_Threshold,
  (SELECT attribute2
  FROM xx_cs_mps_device_details
  WHERE db.serial_no=serial_no
  AND supplies_label='TONERLEVEL_MAGENTA'
  ) Magenta_Threshold,
  (SELECT attribute2
  FROM xx_cs_mps_device_details
  WHERE db.serial_no=serial_no
  AND supplies_label='TONERLEVEL_CYAN'
  ) Cyan_Threshold,
  (SELECT attribute2
  FROM xx_cs_mps_device_details
  WHERE db.serial_no=serial_no
  AND supplies_label='TONERLEVEL_YELLOW'
  ) Yellow_Threshold,
  db.managed_status Managed_Status
FROM xx_cs_mps_device_b  db
WHERE db.party_name = NVL(:1, db.party_name)
  AND db.active_Status = NVL(:2, db.active_Status)
   ]]></SQLQuery>

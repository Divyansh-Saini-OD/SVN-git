   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.ar.hz.components.contact.server.ODHzPuiContRelTableVORowImpl"
   ComponentClass="oracle.apps.ar.hz.components.contact.server.HzPuiContRelTableVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select HzPuiRelationshipsEO.relationship_id,
       HzPuiRelationshipsEO.directional_flag,
       decode(to_char(HzPuiRelationshipsEO.END_DATE,'DD-MM-YYYY') , 
              '31-12-4712', null, 
              HzPuiRelationshipsEO.END_DATE) END_DATE,
       HzPuiRelationshipsEO.party_id,
       HZ_PARTIES.party_name, 
       HZ_PARTIES.person_first_name, 
       HZ_PARTIES.person_last_name,
       HZ_PARTIES.known_as, 
       HZ_PARTIES.party_number,
       HZ_RELATIONSHIP_GROUPINGS_V.role_meaning_singular,
       HZ_ORG_CONTACTS.job_title, 
       fnd1.meaning job_title_code, 
       HZ_ORG_CONTACTS.department,
       fnd2.meaning department_code, 
       RelationshipParty.email_address email,
       HzPuiRelationshipsEO.start_date,
       REL_GROUP_CODE, 
       ROLE, 
       SUBJECT_ID, 
       OBJECT_ID,
       HZ_LOCATIONS.address1 address1, 
       HZ_LOCATIONS.city city, 
       HZ_LOCATIONS.country country, 
       HZ_PARTIES.PARTY_TYPE, 
       HzPuiRelationshipsEO.SUBJECT_TYPE, 
       HzPuiRelationshipsEO.OBJECT_TYPE, 
       HzPuiRelationshipsEO.STATUS,
       decode(RelationshipParty.primary_phone_contact_pt_id,
              null, 'NoRestriction',
              decode(hz_utility_v2pub.is_restriction_exist(
                       'HZ_CONTACT_POINTS',
                       RelationshipParty.primary_phone_contact_pt_id,
                       'DO_NOT'),
                     'Y', 'HasRestriction',
                     'N', 'NoRestriction')) AS CONTACT_RESTRICTION,
       HZ_ORG_CONTACTS.org_contact_id,
       RelationshipParty.primary_phone_country_code,
       RelationshipParty.primary_phone_area_code,
       RelationshipParty.primary_phone_number,
       RelationshipParty.primary_phone_extension,
       RelationshipParty.primary_phone_line_type,
       HZ_PARTY_SITES.location_id,
       HzPuiRelationshipsEO.actual_content_source,
	   HzPartySitesExtEO.extension_id
from   HZ_RELATIONSHIPS      HzPuiRelationshipsEO, 
       HZ_RELATIONSHIP_GROUPINGS_V, 
       HZ_PARTIES, 
       HZ_PARTY_SITES, 
       HZ_ORG_CONTACTS, 
       HZ_LOCATIONS, 
       HZ_PARTIES            RelationshipParty,
       FND_LOOKUP_VALUES     Fnd1, 
       FND_LOOKUP_VALUES     Fnd2,
       HZ_PARTY_SITES_EXT_VL HzPartySitesExtEO,
       EGO_ATTR_GROUPS_V     Egoag 
where  HzPuiRelationshipsEO.relationship_id = HzPartySitesExtEO.n_ext_attr1
AND    HzPartySitesExtEO.attr_group_id = Egoag.attr_group_id  
AND    HzPartySitesExtEO.party_site_id = HZ_PARTY_SITES.party_site_id  -- Organization Sites
and    HzPuiRelationshipsEO.subject_id = HZ_PARTY_SITES.party_id -- Organization
AND    HzPartySitesExtEO.language = userenv('LANG') 
AND    Egoag.attr_group_type = 'HZ_PARTY_SITES_GROUP'
AND    Egoag.attr_group_name = 'SITE_CONTACTS'
AND    HZ_PARTY_SITES.location_id = HZ_LOCATIONS.location_id (+)
and    HzPuiRelationshipsEO.relationship_type = HZ_RELATIONSHIP_GROUPINGS_V.relationship_type
and    HzPuiRelationshipsEO.relationship_code = HZ_RELATIONSHIP_GROUPINGS_V.backward_rel_code
and    HZ_PARTIES.party_id = HzPuiRelationshipsEO.object_id
and    HzPuiRelationshipsEO.relationship_id = hz_org_contacts.party_relationship_id(+)
and    HzPuiRelationshipsEO.subject_id <> HzPuiRelationshipsEO.object_id
and    HzPuiRelationshipsEO.object_type  = HZ_RELATIONSHIP_GROUPINGS_V.subject_type
and    HzPuiRelationshipsEO.subject_type = HZ_RELATIONSHIP_GROUPINGS_V.object_type
and    HzPuiRelationshipsEO.party_id = RelationshipParty.party_id
and    fnd1.view_application_id(+) = 222 
and    fnd1.language(+) = userenv('LANG') 
and    fnd1.lookup_type(+) = 'RESPONSIBILITY' 
and    fnd1.lookup_code(+) = HZ_ORG_CONTACTS.job_title_code 
and    fnd2.view_application_id(+) = 222 
and    fnd2.lookup_type(+) = 'DEPARTMENT_TYPE' 
and    fnd2.language(+) = userenv('LANG') 
and    fnd2.lookup_code(+) = HZ_ORG_CONTACTS.department_code
and  TRUNC(SYSDATE) BETWEEN nvl(TRUNC(HzPartySitesExtEO.d_ext_attr1),  TRUNC(sysdate-1))
AND nvl(TRUNC(HzPartySitesExtEO.d_ext_attr2), TRUNC(sysdate+1)) AND
  HzPartySitesExtEO.c_ext_attr1 = 'A'
   ]]></SQLQuery>

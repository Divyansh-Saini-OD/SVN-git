<?xml version='1.0' encoding='windows-1252' ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="XXCSQAPanelsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.cs.csz.incident.server.XXCSQAPanelsVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select distinct ip.panel_name,
       qd.transaction_id
from   ies_question_data qd,
       ies_questions qp,
       ies_panels ip
where  ip.panel_id = qp.panel_id
and    qp.question_id = qd.question_id
and    ip.panel_name <> 'Device'
and    qd.transaction_id = :1
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.41.57" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess" />
   </DesignTime>
   <ViewAttribute
      Name="PanelName"
      IsPersistent="false"
      IsNotNull="true"
      Precision="256"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="PANEL_NAME"
      Expression="PANEL_NAME"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="256" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="TransactionId"
      IsPersistent="false"
      IsNotNull="true"
      Type="oracle.jbo.domain.Number"
      ColumnType="NUMBER"
      AliasName="TRANSACTION_ID"
      Expression="TRANSACTION_ID"
      SQLType="NUMERIC" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="22" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="DetailFlag"
      IsSelected="false"
      IsPersistent="false"
      Type="java.lang.Boolean"
      ColumnType="NUMBER"
      AliasName="VIEW_ATTR"
      SQLType="BIT" >
   </ViewAttribute>
   <AttrArray Name="KeyAttributes">
      <Item Value="PanelName" />
      <Item Value="TransactionId" />
   </AttrArray>
   <ViewLinkAccessor
      Name="XXCSQuestionAnswersVO"
      ViewLink="od.oracle.apps.xxcrm.cs.csz.incident.server.XXCSPanelsToQAVL"
      Type="oracle.jbo.RowIterator"
      IsUpdateable="false" >
   </ViewLinkAccessor>
</ViewObject>

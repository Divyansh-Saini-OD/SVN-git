<ViewObject
   Name="OD_ServiceRequestRcvQtyVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.cs.csz.servicerequest.server.OD_ServiceRequestRcvQtyVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.cs.csz.servicerequest.server.OD_ServiceRequestRcvQtyVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
Select Xp.Received_Quantity, Pl.Quantity , Pll.Quantity_Received, Ph.Segment1, Msi.Attribute2
from po_headers_all ph,
     po_lines_all pl,
     Po_Line_Locations_All Pll,
     Xx_Cs_Tds_Parts Xp,
     mtl_system_items_b msi
where pll.po_line_id = pl.po_line_id
And  Xp.Request_Number = Ph.Segment1
And  Xp.Item_Number = Msi.Attribute2
And  Xp.Store_Id = Msi.Organization_Id
and ph.po_header_id = pl.po_header_id
And Pl.Item_Id = Xp.Inventory_Item_Id
And Msi.Inventory_Item_Id = Pl.Item_Id
   ]]></SQLQuery>

<ViewObject
   Name="ODRepStoreSrchVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.stl.admin.server.ODRepStoreSrchVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT ODRepStoreMapEO.REP_STORE_ID, 
       ODRepStoreMapEO.STORE_ID, 
       ODRepStoreMapEO.START_DATE_ACTIVE, 
       ODRepStoreMapEO.END_DATE_ACTIVE, 
       ODRepStoreMapEO.RESOURCE_ID, 
       ODRepStoreMapEO.ROLE_ID, 
       ODRepStoreMapEO.GROUP_ID,
       r.resource_name,
       g.group_name,
       rol.role_name,
       ODRepStoreMapEO.LEGACY_REP_ID,
    ( case when nvl(ODRepStoreMapEO.END_DATE_ACTIVE,sysdate+1)>sysdate then 'Y' else 'N'   end ) ACTIVE,
r.source_email
FROM XXCRM_REP_STORE_MAP ODRepStoreMapEO,
     jtf_rs_resource_extns_vl r,
     jtf_rs_groups_vl g,
     jtf_rs_roles_vl rol
where ODRepStoreMapEO.RESOURCE_ID =r.RESOURCE_ID(+)
and  ODRepStoreMapEO.ROLE_ID = rol.role_id(+)
and ODRepStoreMapEO.GROUP_ID = g.group_id(+)
   ]]></SQLQuery>

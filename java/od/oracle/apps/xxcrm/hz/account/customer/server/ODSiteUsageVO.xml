<ViewObject
   Name="ODSiteUsageVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.hz.account.customer.server.ODSiteUsageVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.hz.account.customer.server.ODSiteUsageVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select  asu.SITE_USE_CODE  ,    
use_lkp.meaning as site_use,     
primary_flag
from hz_cust_site_uses_all asu,
ar_lookups use_lkp 
where   asu.CUST_ACCT_SITE_ID = :1
and use_lkp.lookup_type = 'SITE_USE_CODE' 
and use_lkp.lookup_code = asu.site_use_code
   ]]></SQLQuery>

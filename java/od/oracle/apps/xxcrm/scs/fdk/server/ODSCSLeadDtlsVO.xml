<ViewObject
   Name="ODSCSLeadDtlsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.scs.fdk.server.ODSCSLeadDtlsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_ALL"
   FetchSize="20"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT leadeo.sales_lead_id,
  leadeo.primary_contact_party_id,
  leadeo.status_code,
  leadeo.close_reason,
  leadeo.lead_rank_id,
  leadeo.source_promotion_id,
  leadeo.customer_id,
  leadeo.currency_code,
  substrb(leadeo.description,   1,   240) AS description,
    decode(leadeo.status_code,   'CONVERTED_TO_OPPORTUNITY',   'ASNLeadCnvtdYes',   'ASNLeadCnvtdNo') AS
conv_to_oppty_flag,
   leadeo.object_version_number,
  leadeo.lead_number,
  leadeo.budget_amount,
  leadeo.vehicle_response_code
    ,hp.party_name
     ,     hp.party_type
         ,hz_format_pub.format_address(hl.location_id, null, null, ', ', null, null,null, null)   as address   
,hps.orig_system_reference
,SUBSTR(hps.orig_system_reference, 1, INSTR(hps.orig_system_reference,'-', 1, 1)-1) customer
,SUBSTR(hps.orig_system_reference, INSTR(hps.orig_system_reference,'-', 1, 1)+1,(INSTR(hps.orig_system_reference,'-', 1, 2))-(INSTR(hps.orig_system_reference,'-', 1, 1)+1)
) sequence
,cav.resource_id
,cav.role_id
,cav.group_id
,cav.legcy_id
,leadeo.address_id
FROM as_sales_leads leadeo,
          hz_parties hp,
          hz_party_sites hps,
          hz_locations hl,
          XX_TM_CURR_ASSIGN_LEGCY_RPID_V cav
WHERE
  leadeo.customer_id = hp.party_id
AND    leadeo.address_id = hps.party_site_id(+)
AND    leadeo.customer_id = hps.party_id (+)
AND    hps.location_id = hl.location_id (+)
and leadeo.sales_lead_id=cav.entity_id(+)
and cav.entity_type(+)='LEAD'
AND      leadeo.sales_lead_id = :1
   ]]></SQLQuery>

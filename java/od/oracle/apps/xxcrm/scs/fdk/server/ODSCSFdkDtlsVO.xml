<ViewObject
   Name="ODSCSFdkDtlsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.scs.fdk.server.ODSCSFdkDtlsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select 
q.FDK_CODE_DESC code , COALESCE( r.FDK_VALUE_DESC, to_char(d.FDK_TXT),to_char(d.FDK_DATE)) value
 from
  XXCRM.XXSCS_FDBK_HDR h
,  XXCRM.XXSCS_FDBK_LINE_DTL d
,  XXCRM.XXSCS_FDBK_QSTN q
, XXCRM.XXSCS_FDBK_RESP r
where 
d.FDBK_ID=h.FDBK_ID
and d.FDK_CODE =q.FDK_CODE
and r.FDK_VALUE(+)=d.FDK_VALUE
and d.FDK_CODE <> 'CONTACT_ID'
and h.FDBK_ID=:1
   ]]></SQLQuery>

   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select 
       HCAS.PARTY_SITE_ID,
       HPS.party_site_number,
       HCAS.status,
       (HL.address1||' '||HL.address2||' '||HL.address3||' '||HL.address4||' '||HL.CITY||' '||HL.STATE||HL.POSTAL_CODE) Address,
       HCAS.cust_account_id ,
       HCAS.CUST_ACCT_SITE_ID,
       HP.party_id,
       HP.party_number,
       HP.party_name
from 
       hz_cust_acct_sites_all HCAS, 
       hz_party_sites HPS,
       hz_locations HL,
       hz_parties HP
where  
       HPS.PARTY_SITE_ID = HCAS.PARTY_SITE_ID
       AND HPS.LOCATION_ID = HL.LOCATION_ID
       AND HPS.party_id = HP.party_id
   ]]></SQLQuery>

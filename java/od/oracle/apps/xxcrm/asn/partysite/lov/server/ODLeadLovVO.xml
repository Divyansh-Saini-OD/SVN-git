<ViewObject
   Name="ODLeadLovVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.asn.partysite.lov.server.ODLeadLovVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.partysite.lov.server.ODLeadLovVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT LeadEO.sales_lead_id 
, LeadEO.lead_number as lead_Number ,  
hp.party_name as Party,LeadEO.TOTAL_AMOUNT as Amount, 
SUBSTRB(LeadEO.description, 1,240) as Lead_Name, 
ast.meaning as Status,
fnt.name as Currency, 
LeadEO.currency_code as Currency_Code
from as_sales_leads  LeadEO  , hz_parties hp ,
as_statuses_tl ast,fnd_currencies_tl fnt 
WHERE   LeadEO.customer_id = hp.party_id    
AND  ast.language = USERENV('LANG')
and LeadEO.currency_code = fnt.currency_code 
and fnt.language = USERENV('LANG')
and ast.status_code = LeadEO.status_code
   ]]></SQLQuery>

<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODHZBasicPtySiteInfoVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.partysite.server.ODHZBasicPtySiteInfoVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 

  (   case nvl(ps.party_site_name,'x') 
       when 'x' then 
            (select address_lines_phonetic from hz_locations where location_id = ps.location_id ) 
        end  ) party_site_name ,

  ps.party_site_number ,

  ps.orig_system_reference ,

  DEMOGRAPHICS.n_ext_attr1 DUNS_NUMBER ,

  ps.status ,

  DEMOGRAPHICS.n_ext_attr6 CERRENT_FY_REVENUE ,

  DEMOGRAPHICS.n_ext_attr7 NEXT_FY_REVENUE ,

  DEMOGRAPHICS.n_ext_attr8 OD_WHITE_COLLAR_WORKER ,

  DEMOGRAPHICS.n_ext_attr5 WHITE_COLLAR_WORKER ,

  SPEND.n_ext_attr1 POTENTIAL_SPEND ,

  hz_format_phone_v2pub.get_formatted_phone(cpp.contact_point_id) PRIMARY_PHONE ,

  DEMOGRAPHICS.c_ext_attr2 CONTACT_FREQUENCY

FROM 

  hz_party_sites PS ,

  hz_contact_points CPP,

  (SELECT party_site_id,

        HZPSE1.n_ext_attr1   FROM ego_attr_groups_v EGOAG1 ,

        hz_party_sites_ext_b HZPSE1

        WHERE 

        EGOAG1.attr_group_id = HZPSE1.attr_group_id   AND

        EGOAG1.attr_group_type = 'HZ_PARTY_SITES_GROUP'   AND

        EGOAG1.attr_group_name = 'SITE_SPEND' ) SPEND,  

  (SELECT party_site_id, HZPSE2.n_ext_attr1, HZPSE2.c_ext_attr2,   

        hzpse2.n_ext_attr6, hzpse2.n_ext_attr7, hzpse2.n_ext_attr8,  

        hzpse2.n_ext_attr5  FROM ego_attr_groups_v EGOAG2 ,hz_party_sites_ext_b HZPSE2   

        WHERE EGOAG2.attr_group_id = HZPSE2.attr_group_id   AND

        EGOAG2.attr_group_type = 'HZ_PARTY_SITES_GROUP'   AND

        EGOAG2.attr_group_name = 'SITE_DEMOGRAPHICS' ) DEMOGRAPHICS     

WHERE PS.party_site_id = :1 and

  CPP.owner_table_id(+) = PS.party_site_id   and

  cpp.owner_table_name(+) = 'HZ_PARTY_SITES' and

  cpp.contact_point_type(+) = 'PHONE'   and

  cpp.primary_flag(+) = 'Y' and

  cpp.status(+) = 'A'   and

  SPEND.party_site_id(+) = PS.party_site_id   and

  DEMOGRAPHICS.party_site_id(+) = PS.party_site_id

   ]]></SQLQuery>

   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
  ODTasksEO.TASK_ID,
  ODTasksEO.TASK_TYPE_ID,
  ODTasksEO.TASK_STATUS_ID,
  ODTasksEO.TASK_NAME,
  ODTasksEO.TASK_NUMBER,  
  ODTasksEO.CALENDAR_START_DATE,
  ODTasksEO.CALENDAR_END_DATE,
  ODTasksEO.OBJECT_VERSION_NUMBER,
  ODTasksEO.SOURCE_OBJECT_TYPE_CODE,
  ODTasksEO.SOURCE_OBJECT_ID,
  ODTasksEO.PLANNED_START_DATE,
  ODTasksEO.SCHEDULED_START_DATE,
  ODTasksEO.ACTUAL_START_DATE,
  ODTasksEO.PLANNED_END_DATE,
  ODTasksEO.SCHEDULED_END_DATE,
  ODTasksEO.ACTUAL_END_DATE,
  ODTasksEO.DATE_SELECTED,
  ODTasksEO.CUSTOMER_ID,
  ODTasksEO.address_id,
  ODTasksEO.CREATED_BY,
  ODTasksEO.CREATION_DATE,
  ODTasksEO.TASK_PRIORITY_ID,
  ODTasksEO.OWNER_ID,
  ODTasksEO.OWNER_TYPE_CODE,
  ODTasksEO.private_flag,
  ODTasksEO.publish_flag,
  ODTasksEO.deleted_flag,
  ODTasksEO.ENTITY,
  Rscreator.SOURCE_NAME AS CREATOR_NAME,
  DECODE(private_flag,'Y','PRIVATE',
  DECODE(publish_flag,
  'Y',
  'PUBLISH',
  'INTERNAL')
  ) AS TASK_VISIBILITY,
  ts.NAME AS TASK_STATUS_NAME,
  tt.NAME AS TASK_TYPE_NAME,
  JTF_TASK_UTL.get_owner(ODTasksEO.OWNER_TYPE_CODE,
  ODTasksEO.OWNER_ID) as OwnerName,
  tp.NAME AS TASK_PRIORITY_NAME,
'VE' ViewVEVD,
'UE' UpdateUEUD,
'DE' DeleteDEDD
FROM 
  jtf_tasks_vl ODTasksEO,
  jtf_task_statuses_tl ts,
  jtf_task_types_tl tt,
  JTF_RS_RESOURCE_EXTNS Rscreator,
  jtf_task_priorities_tl tp
WHERE 
  ODTasksEO.created_by = Rscreator.user_id(+) AND
  ODTasksEO.entity = 'TASK' AND
  ODTasksEO.source_object_type_code in (select object_code from jtf_objects_b where enter_from_task = 'Y') AND
  NVL(ODTasksEO.deleted_flag,'N') = 'N' AND
  ODTasksEO.task_status_id = ts.task_status_id AND
  ts.language = userenv('LANG') AND
  ODTasksEO.task_type_id = tt.task_type_id AND
  tt.language = userenv('LANG') AND
  ODTasksEO.task_priority_id = tp.task_priority_id AND
  tp.language = userenv('LANG') AND 
  ODTasksEO.customer_id = :1 AND
  ODTasksEO.address_id= :2
   ]]></SQLQuery>

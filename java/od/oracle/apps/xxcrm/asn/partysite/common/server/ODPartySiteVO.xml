   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT party_id,
       party_site_id,
       party_site_name,       DECODE(location_id,null,null,hz_format_pub.format_address(location_id,null,null,',')) party_site_address,
       party_site_number registry_id,       hz_utility_v2pub.get_lookupmeaning('AR_LOOKUPS', 'REGISTRY_STATUS', status) status,
identifying_address_flag
FROM hz_party_sites
WHERE party_id = :1
AND DECODE(:2, -9999, identifying_address_flag, party_site_id) = DECODE(:2, -9999,'Y', :2)
   ]]></SQLQuery>

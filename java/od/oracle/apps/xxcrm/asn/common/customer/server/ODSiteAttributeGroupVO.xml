<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODSiteAttributeGroupVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.common.customer.server.ODSiteAttributeGroupVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT flv.lookup_type,
flv.lookup_code,
flv.meaning,
flv.enabled_flag,
flv.start_date_active,
flv.end_date_active,
egb.page_id page_id
FROM fnd_lookup_values flv ,
ego_pages_v egv,
ego_pages_b egb
WHERE 
egv.object_id = egb.object_id
AND egv.object_name = 'HZ_PARTY_SITES'
AND egb.internal_name = egv.internal_name
AND egb.internal_name = flv.lookup_code
AND lookup_type = 'XXOD_ASN_SITE_ATTRIBUTES' 
AND enabled_flag = 'Y'
AND trunc(SYSDATE) BETWEEN nvl(trunc(start_date_active),SYSDATE) AND nvl(trunc(end_date_active),SYSDATE+1)
   ]]></SQLQuery>

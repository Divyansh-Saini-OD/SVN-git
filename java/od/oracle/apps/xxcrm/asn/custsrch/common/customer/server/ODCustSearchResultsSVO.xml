<ViewObject
   Name="ODCustSearchResultsSVO"
   BindingStyle="Oracle"
   CustomQuery="true"   RowClass="od.oracle.apps.xxcrm.asn.custsrch.common.customer.server.ODCustSearchResultsSVORowImpl" ComponentClass="od.oracle.apps.xxcrm.asn.custsrch.common.customer.server.ODCustSearchResultsSVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT      /*+ USE_NL(hp,ps,terr,fnd2,sitedemov) */
                 ps.party_site_id, hp.party_id, hp.party_name,
                    (CASE (SELECT count(1)
                             FROM hz_cust_accounts hca
                            WHERE hca.party_id = hp.party_id
                              AND ROWNUM = 1
                              AND status = 'A')
                        WHEN 0
                           THEN 'P' || hp.party_number
                        ELSE (SELECT orig_system_reference
                                FROM hz_cust_accounts
                               WHERE party_id = hp.party_id
								 AND ROWNUM = 1
                                 AND status = 'A')
                     END
                    ) prospect,
                    ps.party_site_number,
                    hz_utility_v2pub.get_all_purposes(ps.party_site_id) site_use_type,
                    ' ' address_key,
                     (SELECT (                
                     CASE NVL(address_style,'-9X9Y9Z') WHEN 'AS_DEFAULT' THEN                
                            address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address4
                          || '.'
                          || state
                          || '.'
                          || county
                          || '.'
                          || city
                          || '.'
                          || postal_code                                       
                     WHEN '-9X9Y9Z' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address4
                          || '.'
                          || state
                          || '.'
                          || county
                          || '.'
                          || city
                          || '.'
                          || postal_code
                     WHEN 'JP' THEN
                             postal_code
                          || '.'
                          || state
                          || '.'
                          || city
                          || '.'
                          || address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address_lines_phonetic
                     WHEN 'NE' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || state
                          || '.'
                          || postal_code
                          || '.'
                          || city
                     WHEN 'POSTAL_ADDR_DEF' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address4
                          || '.'
                          || city
                          || '.'
                          || county
                          || '.'
                          || state
                          || '.'
                          || province
                          || '.'
                          || postal_code
                     WHEN 'POSTAL_ADDR_US' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address4
                          || '.'
                          || city
                          || '.'
                          || county
                          || '.'
                          || state
                          || '.'
                          || postal_code
                     WHEN 'SA' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || city
                          || '.'
                          || province
                          || '.'
                          || state
                          || '.'
                          || county
                          || '.'
                          || postal_code
                     WHEN 'SE' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || postal_code
                          || '.'
                          || city
                          || '.'
                          || state
                     WHEN 'UAA' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || city
                          || '.'
                          || state
                          || '.'
                          || postal_code
                     WHEN 'AS_DEFAULT_CA' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || city
                          || '.'
                          || province
                          || '.'
                          || postal_code
				     ELSE
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address4
                          || '.'
                          || state
                          || '.'
                          || county
                          || '.'
                          || city
                          || '.'
                          || postal_code
                     END )
                     FROM hz_locations
                     WHERE location_id = ps.location_id) address,
                    (CASE (select count(1) 
                           from hz_cust_accounts hca where hca.party_id = hp.party_id and rownum = 1 and status = 'A') 
                    WHEN 0 THEN  ps.party_site_number  
                    ELSE (SUBSTR(ps.orig_system_reference,instr(ps.orig_system_reference,'-')+1,(instr(ps.orig_system_reference,'-',1,2)-instr(ps.orig_system_reference,'-'))-1))       
                    END) site_seq_num,
                    (SELECT email_address
                       FROM hz_contact_points hzcp
                      WHERE ps.party_site_id = hzcp.owner_table_id
                        AND hzcp.primary_flag = 'Y'
                        AND hzcp.owner_table_name = 'HZ_PARTY_SITES'
                        AND ((email_address IS NOT NULL) OR (email_address <> '')
                            )
                        AND ROWNUM = 1) email,
                    (SELECT (   phone_country_code
                             || phone_area_code
                             || phone_number
                             || phone_extension
                            ) pphone
                       FROM hz_contact_points hzcp
                      WHERE ps.party_site_id = hzcp.owner_table_id
                        AND hzcp.primary_flag = 'Y'
                        AND hzcp.owner_table_name = 'HZ_PARTY_SITES'
                        AND phone_line_type = 'GEN'
                        AND ((phone_number IS NOT NULL) OR (phone_number <> ''))
                        AND ROWNUM = 1) pphone,
                      (CASE
                        SIGN((SELECT COUNT(1)
                                   FROM
                                        jtf_rs_resource_extns resource_det,
                                        xx_tm_nam_terr_curr_assign_v terr_vw
                                  WHERE  terr_vw.resource_id = resource_det.resource_id
                                  AND terr_vw.entity_id = ps.party_site_id
                                  AND terr_vw.entity_type = 'PARTY_SITE'
                                  AND EXISTS (SELECT 1
                                                FROM jtf_rs_roles_b        role_det
                                                    ,jtf_rs_role_relations rassign
                                                WHERE role_det.role_id = rassign.role_id
                                                AND   role_det.attribute15 = 'BSD'
                                                AND   rassign.role_resource_type = 'RS_INDIVIDUAL'
                                                AND   rassign.role_resource_id = resource_det.resource_id 
                                               )                              
                                  AND  rownum = 1)
                                )
                        WHEN 1  
                        THEN (SELECT resource_det.source_name s_name
                                   FROM
                                        jtf_rs_resource_extns resource_det,
                                        xx_tm_nam_terr_curr_assign_v terr_vw
                                  WHERE  terr_vw.resource_id = resource_det.resource_id
                                  AND terr_vw.entity_id = ps.party_site_id
                                  AND terr_vw.entity_type = 'PARTY_SITE'
                                  AND EXISTS (SELECT 1
                                                FROM jtf_rs_roles_b        role_det
                                                    ,jtf_rs_role_relations rassign
                                                WHERE role_det.role_id = rassign.role_id
                                                AND   role_det.attribute15 = 'BSD'
                                                AND   rassign.role_resource_type = 'RS_INDIVIDUAL'
                                                AND   rassign.role_resource_id = resource_det.resource_id 
                                               )                              
                                  AND  rownum = 1)
                     ELSE
                     (SELECT resource_det.source_name s_name
                                   FROM
                                        jtf_rs_resource_extns resource_det,
                                        xx_tm_nam_terr_curr_assign_v terr_vw
                                  WHERE  terr_vw.resource_id = resource_det.resource_id
                                  AND terr_vw.entity_id = ps.party_site_id
                                  AND terr_vw.entity_type = 'PARTY_SITE'
                                  AND EXISTS (SELECT 1
                                                FROM jtf_rs_roles_b        role_det
                                                    ,jtf_rs_role_relations rassign
                                                WHERE role_det.role_id = rassign.role_id
                                                AND   role_det.attribute14 = 'BDM'
                                                AND   rassign.role_resource_type = 'RS_INDIVIDUAL'
                                                AND   rassign.role_resource_id = resource_det.resource_id 
                                               )                              
                                  AND  rownum = 1)      
                     END
                    ) primary_sales_person,
                    (CASE NVL(sitedemov.sitedemo_cancross,'-9X9Y9Z')
                        WHEN '-9X9Y9Z'
                           THEN '*' || hp.attribute1
                        ELSE sitedemov.sitedemo_cancross
                     END
                    ) can_cross_sell,
                    (CASE NVL(sitedemov.sitedemo_contactfreq,'-9X9Y9Z')
                        WHEN '-9X9Y9Z'
                           THEN '*' || hp.attribute2
                        ELSE sitedemov.sitedemo_contactfreq
                     END
                    ) conct_freq,
                    (CASE NVL(to_char(sitedemov.SITEDEMO_WCW),'-9X9Y9Z')
                        WHEN '-9X9Y9Z'
                           THEN '*' || hp.attribute10
                        ELSE TO_CHAR (sitedemov.sitedemo_wcw)
                     END
                    ) white_collar_wrk,
                    hp.party_type, hp.party_number, hp.tax_reference,
                    hp.jgzz_fiscal_code taxpayer_id, hp.duns_number_c duns_number,
                    hp.known_as known_as, hp.known_as2 known_as2,
                    hp.known_as3 known_as3, hp.known_as4 known_as4,
                    hp.known_as5 known_as5, hp.organization_name_phonetic,
                    hp.person_first_name_phonetic, hp.person_last_name_phonetic,
                    ps.location_id,
                    terr.territory_short_name country,
                    hp.url primary_url,
                    fnd2.meaning certification_level_meaning,
                    100 score, ('100%') percentage, 'N' select_flag,
                    'N' disable_hgrid_flag, 'Y' enable_hgrid_flag,
                    ' ' source_system             
           FROM  hz_parties                   hp
                ,hz_party_sites               ps
                ,fnd_territories_vl           terr
                ,fnd_lookup_values            fnd2
                ,xx_cdh_s_ext_sitedemo_v      sitedemov               
           WHERE hp.party_type IN ('PERSON', 'ORGANIZATION')
           AND   hp.status           = 'A'              
           AND   hp.party_id         = (SELECT party_id
                                        FROM hz_parties
                                        WHERE party_id = hp.party_id
                                        AND   rownum = 1)
           AND   ps.party_id               = hp.party_id
           AND   ps.status                 = 'A'
           AND hp.country = terr.territory_code
           AND   fnd2.lookup_type(+) = 'HZ_PARTY_CERT_LEVEL'
           AND   fnd2.LANGUAGE(+)    = USERENV ('LANG')
           AND   fnd2.lookup_code(+) = hp.certification_level
           AND   sitedemov.party_site_id(+) = ps.party_site_id
           AND :1 IN ('S','T') 
           AND EXISTS(
           SELECT 1
           FROM
                 xx_tm_nam_terr_entity_dtls   xtnted
                ,xx_tm_nam_terr_rsc_dtls      xtntrd
                ,xx_tm_nam_terr_defn          xtntd
           WHERE xtnted.entity_id          = ps.party_site_id
           AND   xtntrd.named_acct_terr_id = xtnted.named_acct_terr_id
           AND   xtntd.named_acct_terr_id  = xtntrd.named_acct_terr_id
           AND   xtnted.named_acct_terr_id = xtntd.named_acct_terr_id
           AND   xtntrd.resource_id        = :2
           AND   xtnted.entity_type        = 'PARTY_SITE'          
           AND   sysdate between nvl(xtntrd.start_date_active,sysdate-1) and nvl(xtntrd.end_date_active,sysdate +1)
           AND   sysdate between nvl(xtnted.start_date_active,sysdate-1) and nvl(xtnted.end_date_active, sysdate+1)
           AND   sysdate between nvl(xtntd.start_date_active,sysdate-1) and nvl(xtntd.end_date_active,sysdate+1)
           AND   xtnted.status             = 'A'
           AND   xtntrd.status             = 'A'
           AND   xtntd.status              = 'A'   
           )
		   AND rownum <= :3
   ]]></SQLQuery>

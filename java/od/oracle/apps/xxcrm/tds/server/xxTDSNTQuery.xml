<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="xxTDSNTQuery"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.tds.server.xxTDSNTQueryImpl"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT cb.incident_number, cb.creation_date,cb.incident_id
FROM CS_INCIDENTS_ALL_B CB
 WHERE 1=1
--and incident_status_id = 6100
and incident_id in (
select incident_id from cs_incidents_all_b
where creation_date  > to_date(:1,'YYYY/MM/DD HH24:MI:SS') 
and problem_code = 'TDS-SERVICES'
and not exists (select'X' from jtf_tasks_b
where source_object_type_code = 'SR'
and source_object_id = cs_incidents_all_b.incident_id))
   ]]></SQLQuery>
   <DesignTime>

<?xml version="1.0" encoding='windows-1252'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODCDHACHIDSearchVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.cdh.ach.server.ODCDHACHIDSearchVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT
  /*+ index(ext, XX_CDH_CUST_ACCT_EXT_B_N3) index(acct, HZ_CUST_ACCOUNTS_U1)*/
  acct.account_name,
  acct.account_number,
  ext.cust_account_id,
  SUBSTR(acct.orig_system_reference,1,8) aops_customer_id,
  ext.c_ext_attr1,
  ext.c_ext_attr2 comments
FROM xx_cdh_cust_acct_ext_b ext, 
     hz_cust_accounts acct
WHERE ext.cust_account_id = acct.cust_account_id
AND ext.C_EXT_ATTR1       IS NOT NULL
AND ext.attr_group_id      =
  (SELECT attr_group_id
  FROM ego_attr_groups_v v
  WHERE attr_group_type = 'XX_CDH_CUST_ACCOUNT'
  AND attr_group_name   = 'ACH_ID'
  )
   ]]></SQLQuery>

   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT distinct xfu.clob_code
                ,xtfu.template_name
                ,xcb.responsibility_id
FROM xxtps_template_file_uploads xtfu
     ,xxtps_file_uploads xfu
     , xx_crm_bulk_resp_setup_v xcb
WHERE xfu.CLOB_CODE = xcb.template_code
AND xfu.clob_code = xtfu.template_code
   ]]></SQLQuery>

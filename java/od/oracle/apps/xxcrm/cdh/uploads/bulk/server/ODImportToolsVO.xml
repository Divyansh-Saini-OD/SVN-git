<ViewObject
   Name="ODImportToolsVO"
   OrderBy="CREATION_DATE desc"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.cdh.uploads.bulk.server.ODImportToolsVORowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT ODImportToolsEO.FILE_UPLOAD_ID,
  ODImportToolsEO.CLOB_TYPE,
  ODImportToolsEO.CLOB_CODE,
  ODImportToolsEO.FILE_NAME,
  ODImportToolsEO.FILE_STATUS,
  ODImportToolsEO.FILE_CONTENT_TYPE,
  ODImportToolsEO.FILE_DATA,
  ODImportToolsEO.PROGRAM,
  ODImportToolsEO.CREATED_BY,
  ODImportToolsEO.CREATION_DATE,
  ODImportToolsEO.LAST_UPDATED_BY,
  ODImportToolsEO.REQUEST_ID,
  ODImportToolsEO.ERROR_FILE_DATA,
  ODImportToolsEO.OUTPUT_FILE_DATA,
  ODImportToolsEO.TOTAL_PROCESSED_RECORDS,
  ODImportToolsEO.NO_OF_SUCCESS_RECORDS,
  ODImportToolsEO.NO_OF_ERROR_RECORDS,
  DECODE(File_Status,'C',1,'E',1,'S',1,'X',1,'R',1,0) UploadAction,
DECODE(NO_OF_ERROR_RECORDS,0,'N','Y')
Errswitcher,
DECODE(NO_OF_SUCCESS_RECORDS,0,'No','Yes')
Outswitcher,
  (SELECT FLV.meaning
  FROM fnd_lookup_values FLV
  WHERE lookup_type    = 'XX_CRM_BULK_IMPORT_STATUS'
  AND FLV.enabled_flag = 'Y'
  AND sysdate BETWEEN NVL(start_date_active,sysdate) AND NVL(end_date_active,sysdate)
  AND FLV.lookup_code = ODImportToolsEO.File_Status
  ) status
,nvl((SELECT source_name FROM jtf_rs_resource_extns where user_id = ODImportToolsEO.created_by),(SELECT user_name FROM fnd_user where user_id = ODImportToolsEO.created_by)) UserName
FROM XXTPS_FILE_UPLOADS ODImportToolsEO
WHERE CLOB_CODE IN (
SELECT xcb.template_code
FROM  xx_crm_bulk_resp_setup_v xcb
WHERE  xcb.responsibility_id =  FND_GLOBAL.resp_id)
   ]]></SQLQuery>

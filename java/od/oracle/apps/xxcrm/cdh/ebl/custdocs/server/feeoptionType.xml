   RowClass="od.oracle.apps.xxcrm.cdh.ebl.custdocs.server.feeoptionTypeRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.cdh.ebl.custdocs.server.feeoptionTypeImpl"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select xftv.TARGET_VALUE1 meaning,xftv.SOURCE_VALUE1 lookup_code,SOURCE_VALUE2,SOURCE_VALUE3
FROM xx_fin_translatedefinition xft,
     xx_fin_translatevalues xftv
WHERE xft.translate_id = xftv.translate_id
AND xftv.ENABLED_FLAG = 'Y'
AND sysdate between xftv.START_DATE_ACTIVE and  NVL(xftv.END_DATE_ACTIVE,sysdate+1)
AND xft.translation_name = 'OD_IREC_BILL_DOC_DEFAULTS'
   ]]></SQLQuery>

--DECODE(C_EXT_ATTR1,'Consolidated Bill', 'Case1' ,'Case2') DeliveryMethod,-- Commented By Rafi on 31-Aug-2018 for SKU Level ePDF Defect #NAIT-58403
DECODE(N_EXT_ATTR1,fnd_profile.value('XXOD_EBL_SKU_LEVEL_CONS_EPDF'),'Case3',DECODE(N_EXT_ATTR1,fnd_profile.value('XXOD_EBL_SKU_LEVEL_INV_EPDF'),'Case3',
DECODE(N_EXT_ATTR1,fnd_profile.value('XXOD_EBL_CENTRAL_OPSTECH'),'Case4',DECODE(C_EXT_ATTR1,'Consolidated Bill', 'Case1','Case2')))) DeliveryMethod, -- Added  By Rafi on 31-Aug-2018 for SKU Level ePDF Defect #NAIT-58403 and Added by Reddy Sekhar K on 11-Sept-2018 for the eBill Central Requirement NAIT-56624
--DECODE(C_EXT_ATTR3,'EDI','N','PRINT','N','ELEC','N', 'eTXT','T', DECODE(N_EXT_ATTR15,NULL,'Y','N')) EBillDet,--Commented by Reddy Sekhar K
DECODE(C_EXT_ATTR3,'EDI','N','PRINT','N','ELEC','N','OPSTECH','N','eTXT','T', DECODE(N_EXT_ATTR15,NULL,'Y','N')) EBillDet,--Added by Reddy Sekhar K on 11-Sept-2018 for the eBill Central Requirement NAIT-56624
DECODE(fnd_profile.value('APPS_DATABASE_ID'),'GSIPRDGB', 1, 
        DECODE(fnd_profile.value('XXOD_EBL_SUPER_USER'),'Y',0,1)
        ) SupUsr,
decode(C_EXT_ATTR16,'COMPLETE',1,0) ReadOnlyFlag,
--decode(C_EXT_ATTR2,'Y',1,0) PayDocInd,--Commented by Reddy Sekhar K for the eBill Central Requirement NAIT-56624
decode(N_EXT_ATTR1,fnd_profile.value('XXOD_EBL_CENTRAL_OPSTECH'),0,decode(C_EXT_ATTR2,'Y',1,0)) PayDocInd,--Added by Reddy Sekhar K on 11-Sept-2018 for the eBill Central Requirement NAIT-56624
'QUERY' IntRecStatus,
(SELECT ATTRIBUTE1 FROM RA_TERMS RT where RT.NAME=ODEbillCustDocEO.C_EXT_ATTR14) FREQUENCY,
(SELECT ATTRIBUTE2 FROM RA_TERMS RT where RT.NAME=ODEbillCustDocEO.C_EXT_ATTR14) REPORT_DAY,
(SELECT ATTRIBUTE5 FROM RA_TERMS RT where RT.NAME=ODEbillCustDocEO.C_EXT_ATTR14) PAY_TERM,
DECODE(fnd_profile.value('APPS_DATABASE_ID'),'GSIPRDGB',
      DECODE(fnd_profile.value('XXOD_EBL_SUPER_USER'),'Y',DECODE(d_ext_attr2,NULL,1,0),1),0) SupUsrEnd,
CASE WHEN C_EXT_ATTR1 = 'Consolidated Bill' THEN 0
                               ELSE 1 END  BillLevelRequiredFlag,
fnd_profile.value('APPS_DATABASE_ID') INSTANCE_NAME,
decode(N_EXT_ATTR1,fnd_profile.value('XXOD_EBL_CENTRAL_OPSTECH'),1,0) mailAttentionMsg,--Added by Reddy Sekhar K on 11-Sept-2018 for the eBill Central Requirement NAIT-56624
decode(C_EXT_ATTR16,'COMPLETE',1,0) paytermLov, --Added by Reddy Sekhar K on 11-Sept-2018 for the eBill Central Requirement NAIT-56624
ODEbillCustDocEO.BC_POD_FLAG,--Added by Reddy Sekhar K on 13-NOv-2018 for the Bill Complete Requirement NAIT-61952
DECODE(BC_POD_FLAG,'Y','case1','B','case2','P','case3','N','case4','case5') bcpodcase----Added by Reddy Sekhar K on 13-NOv-2018 for the Bill Complete Requirement NAIT-61952 and POD NAIT-66520
,fee_option
FROM XX_CDH_CUST_ACCT_EXT_B ODEbillCustDocEO
WHERE attr_group_id = (SELECT attr_group_id 
                       FROM   ego_attr_groups_v 

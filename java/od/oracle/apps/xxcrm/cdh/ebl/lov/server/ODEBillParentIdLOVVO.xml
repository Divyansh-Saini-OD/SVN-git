<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODEBillParentIdLOVVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.cdh.ebl.lov.server.ODEBillParentIdLOVVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT HCA.account_number
      ,HCA.account_name
      ,substr(HCA.orig_system_reference,1,8) BILLING_ID
      ,XCAE.n_ext_attr2 CUST_DOC_ID
      ,HCA.cust_account_id
FROM  hz_cust_accounts HCA
     ,XX_CDH_CUST_ACCT_EXT_B XCAE 
WHERE HCA.cust_account_id = XCAE.cust_account_id 
AND   XCAE.N_EXT_ATTR17 = 1
AND   C_EXT_ATTR16='COMPLETE'
AND   attr_group_id=(SELECT attr_group_id
                   FROM   ego_attr_groups_v
                   WHERE  attr_group_type = 'XX_CDH_CUST_ACCOUNT'
                   AND    attr_group_name = 'BILLDOCS' )
   ]]></SQLQuery>

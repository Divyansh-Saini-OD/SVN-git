      Name="Code"
      IsPersistent="false"
      Precision="80"
      Type="java.lang.String"
      AliasName="CODE"
      ColumnType="VARCHAR2"
      Expression="CODE"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="22" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="Meaning"
      IsPersistent="false"
      Precision="23"
      Type="java.lang.String"
      AliasName="MEANING"
      ColumnType="VARCHAR2"
      Expression="MEANING"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="23" />
      </DesignTime>

<ViewObject
   Name="ODEBillStdFieldsSumVO"
   OrderBy="field_id"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.cdh.ebl.eblmain.server.ODEBillStdFieldsSumVORowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   FetchMode="FETCH_AS_NEEDED"
   PageIterMode="Partial"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT EF.field_id
     , EF.field_name
     , EF.include_in_core
     , EF.include_in_detail
     , EF.data_format
     , EF.default_seq
  FROM xx_cdh_ebilling_fields_v  EF
 WHERE EF.include_in_standard = 'Y'
 AND EF.field_id is not null
 AND EF.field_name is not null
 AND EF.target_value25 = 'Y'
 --Added By Rafi Mohammed on 25Jul2018 to restrict Sales --Person for defect 52049
 AND EF.field_name not in (SELECT val.source_value2 
                         FROM xx_fin_translatedefinition def,xx_fin_translatevalues val
                         WHERE def.translate_id = val.translate_id AND def.translation_name = 'XX_CDH_EBL_EXL_SP_DETAILS')
   ]]></SQLQuery>

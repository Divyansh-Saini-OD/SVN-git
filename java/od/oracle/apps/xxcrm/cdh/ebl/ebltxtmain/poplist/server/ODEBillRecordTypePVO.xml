<?xml version="1.0" encoding='windows-1252'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODEBillRecordTypePVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
	  v.source_value1 Code, v.source_value2 Meaning
 FROM XX_FIN_TRANSLATEDEFINITION D
JOIN XX_FIN_TRANSLATEVALUES V
ON D.translate_id=V.translate_id
WHERE D.translation_name='XXOD_EBL_TXT_RECORD_TYPE'
AND NVL(V.enabled_flag, 'N')='Y'
AND TRUNC(SYSDATE) BETWEEN V.start_date_active AND NVL(V.end_date_active,SYSDATE)
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_version" Value="9.0.3.12.53" />
      <Attr Name="_CodeGenFlagNew" Value="4" />
   </DesignTime>
   <ViewAttribute
      Name="Code"
      IsPersistent="false"
      IsNotNull="true"
      Precision="30"
      Type="java.lang.String"
      AliasName="CODE"
      ColumnType="VARCHAR2"
      Expression="CODE"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="30" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="Meaning"
      IsPersistent="false"
      IsNotNull="true"
      Precision="80"
      Type="java.lang.String"
      AliasName="MEANING"
      ColumnType="VARCHAR2"
      Expression="MEANING"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="80" />
      </DesignTime>
   </ViewAttribute>
</ViewObject>

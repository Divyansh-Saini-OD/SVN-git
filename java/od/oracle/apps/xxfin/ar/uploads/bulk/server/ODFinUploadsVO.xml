   Name="ODFinUploadsVO"
   OrderBy="CREATION_DATE DESC"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxfin.ar.uploads.bulk.server.ODFinUploadsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT ODFinUploadsEO.FILE_UPLOAD_ID, 
       ODFinUploadsEO.CLOB_TYPE, 
       ODFinUploadsEO.CLOB_CODE, 
       ODFinUploadsEO.FILE_NAME, 
       ODFinUploadsEO.FILE_STATUS, 
       ODFinUploadsEO.FILE_CONTENT_TYPE, 
       ODFinUploadsEO.FILE_DATA, 
       ODFinUploadsEO.PROGRAM, 
       ODFinUploadsEO.CREATED_BY, 
       ODFinUploadsEO.CREATION_DATE, 
       ODFinUploadsEO.LAST_UPDATED_BY, 
       ODFinUploadsEO.LAST_UPDATE_DATE, 
       ODFinUploadsEO.REQUEST_ID, 
       ODFinUploadsEO.ERROR_FILE_DATA, 
       ODFinUploadsEO.LAST_UPDATE_LOGIN, 
       ODFinUploadsEO.OUTPUT_FILE_DATA, 
       ODFinUploadsEO.TOTAL_PROCESSED_RECORDS, 
       ODFinUploadsEO.NO_OF_SUCCESS_RECORDS, 
       ODFinUploadsEO.NO_OF_ERROR_RECORDS,       DECODE(File_Status,'C',1,'E',1,'S',1,'X',1,'R',1,0) UploadAction,
DECODE(NO_OF_ERROR_RECORDS,0,'N','Y')
Errswitcher,
DECODE(NO_OF_SUCCESS_RECORDS,0,'No','Yes')
Outswitcher,
  (SELECT FLV.meaning
  FROM fnd_lookup_values FLV
  WHERE lookup_type    = 'XX_CRM_BULK_IMPORT_STATUS'
  AND FLV.enabled_flag = 'Y'
  AND sysdate BETWEEN NVL(start_date_active,sysdate) AND NVL(end_date_active,sysdate)
  AND FLV.lookup_code = ODFinUploadsEO.File_Status
  ) status
,nvl((SELECT source_name FROM jtf_rs_resource_extns where user_id = ODFinUploadsEO.created_by),(SELECT user_name FROM fnd_user where user_id = ODFinUploadsEO.created_by)) UserName
FROM XXCRM_FILE_UPLOADS ODFinUploadsEO
WHERE ODFinUploadsEO.CLOB_CODE IN ( SELECT xcb.template_code FROM  xxfin_bulk_resp_setup_v xcb WHERE  xcb.responsibility_id =  FND_GLOBAL.resp_id)
   ]]></SQLQuery>

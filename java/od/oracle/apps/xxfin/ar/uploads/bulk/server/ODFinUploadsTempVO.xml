FROM xxcrm_template_file_uploads xtfu
WHERE xtfu.template_file_upload_id  IN
(SELECT xcb.template_id
FROM  XXFIN_BULK_RESP_SETUP_V xcb
WHERE  xcb.responsibility_id =  FND_GLOBAL.resp_id
)

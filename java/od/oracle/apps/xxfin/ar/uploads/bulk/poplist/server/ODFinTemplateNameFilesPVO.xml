   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT distinct xfu.clob_code
                ,xtfu.template_name
                ,xcb.responsibility_id
FROM xxcrm_template_file_uploads xtfu
     ,xxcrm_file_uploads xfu
     , XXFIN_BULK_RESP_SETUP_V xcb
WHERE xfu.CLOB_CODE = xcb.template_code
AND xfu.clob_code = xtfu.template_code
   ]]></SQLQuery>

<!-- dbdrv: exec java oracle/jrad/tools/xml/importer XMLImporter.class java &phase=dat+24 checkfile:~PROD:~PATH:~FILE &fullpath:~PROD:~PATH:~FILE -username &un_apps -password &pw_apps -dbconnection &jdbc_db_addr  -userId "1" -rootPackage /oracle/apps/~PROD  -rootdir &fullpath:~PROD:mds:directory  -->
<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="9.0.3.8.13_1566" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <oa:pageLayout id="PageLayoutRN" windowTitle="eBill Retrieval" title="eBill Retrieval" amDefName="od.oracle.apps.xxfin.ar.irec.statements.server.StatementsAM">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:contents>
            <oa:stackLayout id="mainRN" controllerClass="od.oracle.apps.xxfin.ar.irec.statements.webui.StatementsCO">
               <ui:contents>
                  <oa:query id="QueryRN" mode="resultsBasedSearch" dispSimple="false" dispCustomized="false" dispAdvanced="true" defaultPanel="advanced" advSearchPanelButtonLabel="">
                     <ui:contents>
                        <oa:table id="ResultsTable" shortDesc="Statements Table">
                           <ui:contents>
                              <oa:messageStyledText id="file_id" dataType="NUMBER" prompt="file_id" viewName="StatementsVO1" viewAttr="file_id" rendered="false"/>
                              <oa:messageStyledText id="billing_dt" dataType="DATE" prompt="Bill Period End Date" viewName="StatementsVO1" viewAttr="billing_dt" queryable="true" sortState="yes" initSortSeq="none"/>
                              <oa:messageStyledText id="file_name" viewName="StatementsVO1" viewAttr="file_name" destination="/OA_HTML/OA.jsp?page=/od/oracle/apps/xxfin/ar/irec/statements/webui/StatementPG&amp;file_id={!file_id}" queryable="true" sortState="yes" prompt="File Name" targetFrame="_BLANK"/>
                           </ui:contents>
                           <ui:tableSelection>
                              <oa:multipleSelection id="multipleSelection1" viewName="StatementsVO1" viewAttr="SelectedFlag"/>
                           </ui:tableSelection>
                        </oa:table>
                     </ui:contents>
                  </oa:query>
                  <oa:messageComponentLayout id="buttonRN">
                     <ui:contents>
                        <oa:messageLayout id="buttonLayout">
                           <ui:contents>
                              <oa:submitButton id="btnZipAndDownload" text="Zip and Download" prompt="Zip and Download" hAlign="left"/>
                           </ui:contents>
                        </oa:messageLayout>
                     </ui:contents>
                  </oa:messageComponentLayout>
                  <oa:header id="eMailRN" text="To eMail your above checked selections:">
                     <ui:contents>
                        <oa:stackLayout id="eMailOptionStackRN">
                           <ui:contents>
                              <oa:messageTextInput id="inputEmailAddress" prompt="Enter (comma separated) eMail Address(es):" columns="45"/>
                              <oa:messageCheckBox id="cbHideZipExtension" checkedValue="Y" unCheckedValue="N" defaultValue="N" text="Click here to have any .zip file extensions renamed to _zip if you cannot receive Zip attachments." prompt="Click here to have any .zip file extensions renamed to _zip if you cannot receive Zip attachments." tipType="none"/>
                              <oa:spacer id="spacerEmailBtnTable" height="10"/>
                              <oa:tableLayout id="eMailSendBtnTBL" hAlign="left">
                                 <ui:contents>
                                    <oa:rowLayout id="rowHeader">
                                       <ui:contents>
                                          <oa:cellFormat id="cellDeliveryOptions" hAlign="center"/>
                                          <oa:cellFormat id="cellOneEmail" styleClass="OraTableColumnHeader OraTableBorder1111">
                                             <ui:contents>
                                                <oa:staticStyledText id="txtOneEmail" text="One eMail containing all selections" prompt="One eMail containing all selections" cellNoWrapFormat="true"/>
                                             </ui:contents>
                                          </oa:cellFormat>
                                          <oa:cellFormat id="cellMultipleEmails" styleClass="OraTableColumnHeader OraTableBorder1111" rendered="false">
                                             <ui:contents>
                                                <oa:staticStyledText id="txtMultipleEmails" text="Multiple eMails with one attachment each" prompt="Multiple eMails with one attachment each" cellNoWrapFormat="true"/>
                                             </ui:contents>
                                          </oa:cellFormat>
                                       </ui:contents>
                                    </oa:rowLayout>
                                    <oa:rowLayout id="rowNoCompression">
                                       <ui:contents>
                                          <oa:cellFormat id="cellNoCompression" styleClass="OraTableRowHeader OraTableBorder1111">
                                             <ui:contents>
                                                <oa:staticStyledText id="txtNoCompression" text="No compression" prompt="No compression" cellNoWrapFormat="true"/>
                                             </ui:contents>
                                          </oa:cellFormat>
                                          <oa:cellFormat id="cellNoCompOneEmail" styleClass="OraTableBorder0110" hAlign="center">
                                             <ui:contents>
                                                <oa:submitButton id="btnNoCompOneEmail" text="Send" prompt="Send"/>
                                             </ui:contents>
                                          </oa:cellFormat>
                                          <oa:cellFormat id="cellNoCompMultEmails" styleClass="OraTableBorder0110" hAlign="center" rendered="false">
                                             <ui:contents>
                                                <oa:submitButton id="btnNoCompMultEmails" text="Send" prompt="Send"/>
                                             </ui:contents>
                                          </oa:cellFormat>
                                       </ui:contents>
                                    </oa:rowLayout>
                                    <oa:rowLayout id="rowZipIndividually" rendered="false">
                                       <ui:contents>
                                          <oa:cellFormat id="cellZipIndividually" styleClass="OraTableRowHeader OraTableBorder1111">
                                             <ui:contents>
                                                <oa:staticStyledText id="txtZipIndividually" text="Zip selections individually" prompt="Zip selections individually" cellNoWrapFormat="true"/>
                                             </ui:contents>
                                          </oa:cellFormat>
                                          <oa:cellFormat id="cellZipIndOneEmail" styleClass="OraTableBorder0110" hAlign="center">
                                             <ui:contents>
                                                <oa:submitButton id="btnZipIndOneEmail" text="Send" prompt="Send"/>
                                             </ui:contents>
                                          </oa:cellFormat>
                                          <oa:cellFormat id="cellZipIndMultEmails" styleClass="OraTableBorder0110" hAlign="center" rendered="false">
                                             <ui:contents>
                                                <oa:submitButton id="btnZipIndMultEmails" text="Send" prompt="Send"/>
                                             </ui:contents>
                                          </oa:cellFormat>
                                       </ui:contents>
                                    </oa:rowLayout>
                                    <oa:rowLayout id="rowZipTogether">
                                       <ui:contents>
                                          <oa:cellFormat id="cellZipTogether" styleClass="OraTableRowHeader OraTableBorder1111" hAlign="center">
                                             <ui:contents>
                                                <oa:staticStyledText id="txtZipTogether" text="Zip together into one file" prompt="Zip together into one file" cellNoWrapFormat="true"/>
                                             </ui:contents>
                                          </oa:cellFormat>
                                          <oa:cellFormat id="cellZipTogOneEmail" styleClass="OraTableBorder0110" hAlign="center">
                                             <ui:contents>
                                                <oa:submitButton id="btnZipTogOneEmail" prompt="Send"/>
                                             </ui:contents>
                                          </oa:cellFormat>
                                       </ui:contents>
                                    </oa:rowLayout>
                                 </ui:contents>
                              </oa:tableLayout>
                           </ui:contents>
                        </oa:stackLayout>
                     </ui:contents>
                  </oa:header>
                  <oa:spacer id="spacerBottom" height="30"/>
               </ui:contents>
            </oa:stackLayout>
         </ui:contents>
         <ui:pageStatus>
            <oa:flowLayout id="PageStatusRN">
               <ui:contents>
                  <oa:staticStyledText id="PageHelp" styleClass="OraInstructionText" messageAppShortName="XXFIN" messageName="AR_EBL_RESEND_HELP"/>
               </ui:contents>
            </oa:flowLayout>
         </ui:pageStatus>
      </oa:pageLayout>
   </content>
</page>

<ViewObject
   Name="XxIbyBatchTrxnsHistoryVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxfin.iby.settlement.server.XxIbyBatchTrxnsHistoryVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT   ixreceiptnumber as receipt_num 
        ,TO_DATE(ixsettlementdate)  as date_val
        ,ixstorenumber     store_num 
        ,DECODE(ixregisternumber,'54','Manual Refunds','55','iRec','99','AOPS','POS') as ord_type
        ,ixregisternumber as reg_num 
        ,DECODE(IXREGISTERNUMBER,'54', NVL(IXINVOICE,' '),'55', NVL(IXINVOICE,' ') ,'56', ' ','99', IXINVOICE ,NVL(IXTRANSNUMBER,'')) as TRX_NUM 
        ,NVL(IXIPAYMENTBATCHNUMBER,' ') as BATCH_NUM
        ,ixamount/100    as   dollar_amt 
        ,ixtransactiontype as trx_type 
        ,ixrecptnumber as recpt_num 
        ,attribute7 as cash_rec_id 
   FROM  XX_IBY_BATCH_TRXNS_HISTORY 
  WHERE IXRECORDTYPE = 101 
  and TO_DATE(ixsettlementdate) >= :1
  AND TO_DATE(ixsettlementdate) <= :2 
  AND ixrecptnumber = :3
  AND ixstorenumber = :4
  AND ixregisternumber = nvl(:5,ixregisternumber)
  AND IXTRANSNUMBER = nvl(:6,IXTRANSNUMBER) 
  AND IXIPAYMENTBATCHNUMBER = NVL(:7,IXIPAYMENTBATCHNUMBER) 
  AND ixamount/100 = nvl(:8,ixamount/100) 
  AND IXTRANSACTIONTYPE = NVL(:9,IXTRANSACTIONTYPE) 
  AND DECODE(ixregisternumber,'54','Manual Refunds','55','iRec','99','AOPS','POS') = nvl(:10,DECODE(ixregisternumber,'54','Manual Refunds','55','iRec','99','AOPS','POS'))
   ]]></SQLQuery>

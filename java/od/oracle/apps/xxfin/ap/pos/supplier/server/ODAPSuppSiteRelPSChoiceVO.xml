   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxfin.ap.pos.supplier.server.ODAPSuppSiteRelPSChoiceVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select (POV.VENDOR_NAME ||' / '|| POVS.VENDOR_SITE_CODE) as Supplier_Name_Site_code,
       POVS.VENDOR_SITE_ID,
       POV.VENDOR_ID,
       POVS.VENDOR_SITE_CODE
  FROM ap_supplier_sites_all POVS, ap_suppliers POV
WHERE POVS.VENDOR_ID = POV.VENDOR_ID
AND   POVS.PAY_SITE_FLAG = 'Y'
   ]]></SQLQuery>

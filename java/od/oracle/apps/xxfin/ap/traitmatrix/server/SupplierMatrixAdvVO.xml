<ViewObject
   Name="SupplierMatrixAdvVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxfin.ap.idms.traitmatrix.server.SupplierMatrixAdvVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select assa.vendor_site_code sitename,
ap.vendor_name supplier_name,
assa.vendor_site_id Supplier,
assa.vendor_site_code alternate_supplier,
xstm.sup_trait suptrait,
xstm.attribute1 attribute1,
xst.description description,
assa.inactive_date,
Case 
When assa.inactive_date Is Null  
THEN 'Active'
When assa.inactive_date >= Sysdate
Then 'Active'
Else 'Inactive'
end as site_status,
xst.master_sup_ind Master_Indicator,
xstm.enable_flag Enable_Flag
from ap_supplier_sites_all assa,
ap_suppliers ap,
XX_AP_SUP_TRAITS xst,
XX_AP_SUP_TRAITS_MATRIX xstm
where 
ap.vendor_id=assa.vendor_id 
AND assa.vendor_site_id= xstm.supplier
and xstm.sup_trait = xst.sup_trait
   ]]></SQLQuery>

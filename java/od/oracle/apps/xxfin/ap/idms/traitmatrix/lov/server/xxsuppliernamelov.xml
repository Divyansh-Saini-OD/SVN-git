<ViewObject
   Name="xxsuppliernamelov"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxfin.ap.idms.traitmatrix.lov.server.xxsuppliernamelovImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select ap.Vendor_name supplier_name,
NVL(TO_NUMBER(LTRIM(aps.vendor_site_code_alt,'0')), aps.vendor_site_id) supplier_number,
aps.vendor_site_code alternate_supplier,
aps.pay_site_flag
from 
ap_suppliers ap,
ap_supplier_SITES_ALL aps
where ap.vendor_id=aps.vendor_id
AND NVL(aps.inactive_date,sysdate+1) > sysdate
   ]]></SQLQuery>

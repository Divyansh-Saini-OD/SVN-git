<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="StoresVO"
   OrderBy="stores"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.poplist.server.StoresVORowImpl"
   ComponentClass="od.oracle.apps.xxmer.poplist.server.StoresVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select inv.location_number_sw as stores, hr.attribute1, inv.ORGANIZATION_ID
from XX_INV_ORG_LOC_RMS_ATTRIBUTE inv,
	 HR_ALL_ORGANIZATION_UNITS HR
	 WHERE hr.attribute1 = inv.DEFAULT_WH_SW
         and inv.ORGANIZATION_ID is not null
union
select inv.location_number_sw as stores, hr.attribute1, inv.ORGANIZATION_ID
from XX_INV_ORG_LOC_RMS_ATTRIBUTE inv,
	 HR_ALL_ORGANIZATION_UNITS HR
	 WHERE hr.attribute1 = inv.od_default_wh_csc_s
         and inv.ORGANIZATION_ID is not null
   ]]></SQLQuery>

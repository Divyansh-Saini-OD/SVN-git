   Name="ODLeadTimeOCVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.odleadtimeoc.server.ODLeadTimeOCVORowImpl"
   ComponentClass="od.oracle.apps.xxmer.odleadtimeoc.server.ODLeadTimeOCVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT source_id
      ,destination_id 
      ,item_id
      ,overall_lt - ship_lt proclt
      ,ship_lt
      ,decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'SUN'),0,' ',OVERALL_LT) SUN1
      ,decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'MON'),0,' ',OVERALL_LT) MON1
      ,decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'TUE'),0,' ',OVERALL_LT) TUE1
      ,decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'WED'),0,' ',OVERALL_LT) WED1
      ,decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'THU'),0,' ',OVERALL_LT) THU1
      ,decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'FRI'),0,' ',OVERALL_LT) FRI1
      ,decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'SAT'),0,' ',OVERALL_LT) SAT1
      ,CASE WHEN ORDERCYCLE_FREQUENCY = 2 THEN
       decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'SUN'),0,' ',OVERALL_LT) END SUN2
      ,CASE WHEN ORDERCYCLE_FREQUENCY = 2 THEN
       decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'MON'),0,' ',OVERALL_LT) END MON2
      ,CASE WHEN ORDERCYCLE_FREQUENCY = 2 THEN
       decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'TUE'),0,' ',OVERALL_LT) END TUE2
      ,CASE WHEN ORDERCYCLE_FREQUENCY = 2 THEN
       decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'WED'),0,' ',OVERALL_LT) END WED2
      ,CASE WHEN ORDERCYCLE_FREQUENCY = 2 THEN
       decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'THU'),0,' ',OVERALL_LT) END THU2
      ,CASE WHEN ORDERCYCLE_FREQUENCY = 2 THEN
       decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'FRI'),0,' ',OVERALL_LT) END FRI2
      ,CASE WHEN ORDERCYCLE_FREQUENCY = 2 THEN
       decode(instr(NVL(ORDER_CYCLE_DAYS,' '),'SAT'),0,' ',OVERALL_LT) END SAT2
      ,decode(ORDERCYCLE_FREQUENCY,'1','Weekly','Biweekly') ORDERCYCLE_FREQUENCY
      ,vs.vendor_site_code
      ,vs.vendor_site_id
      ,vs.attribute9 source_Legacy_id
      ,vd.attribute1 dest_Legacy_id
      ,vd.organization_id dest_site_id
      ,ms.organization_code
      ,segment1 item_legacy_id
     ,it.description
     ,LeadTimeOrderCycleEO.last_update_date
     ,(SELECT user_name from fnd_user WHERE user_id = LeadTimeOrderCycleEO.last_updated_by) last_updated_by
     ,LeadTimeOrderCycleEO.creation_date
     ,(SELECT user_name from fnd_user WHERE user_id = LeadTimeOrderCycleEO.created_by) created_by
  FROM xx_po_lead_time_order_cycle   LeadTimeOrderCycleEO
       ,po_vendor_sites_all           vs
       ,hr_all_organization_units     vd
       ,org_organization_definitions  ms
       ,(SELECT inventory_item_id,
                         segment1
                         ,description
           FROM mtl_system_items_b ib
          WHERE organization_id = (select unique master_organization_id
                                     from mtl_parameters))  it
 WHERE source_id                            = vs.vendor_site_id
   AND destination_id                       = vd.organization_id(+)
   AND destination_id                       = ms.organization_id(+)
   AND item_id                              = inventory_item_id(+)
   AND fnd_profile.value_specific('ORG_ID') = vs.org_id
   ]]></SQLQuery>

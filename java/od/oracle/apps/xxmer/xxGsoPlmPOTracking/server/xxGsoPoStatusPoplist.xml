<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="xxGsoPoStatusPoplist"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxmer.xxGsoPlmPOTracking.server.xxGsoPoStatusPoplistImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select a.flex_value
  from apps.fnd_flex_values a,
       apps.fnd_flex_value_sets b
 where b.flex_value_set_name='XX_GSO_PO_STATUS'
   and b.flex_value_set_id=a.flex_value_set_id
and sysdate between nvl(start_date_active,sysdate) and nvl(end_date_active,sysdate)
UNION
SELECT '--SELECT FROM LIST--' FROM DUAL
   ]]></SQLQuery>

      AliasName="PO_STATUS_CD"
      ColumnType="VARCHAR2"
      Expression="PO_STATUS_CD"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="25" />
      </DesignTime>
   </ViewAttribute>
</ViewObject>

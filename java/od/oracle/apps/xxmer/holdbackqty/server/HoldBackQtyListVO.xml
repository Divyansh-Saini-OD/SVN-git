   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT  
   OdHoldBackQtyEO.ITEM,   
   msit.description AS Description,  
   mtcc.segment3  AS Dept,     
   mtcc.segment4  AS Classs,   
   mtcc.segment5  AS Subclass, 
   moq.organization_id  AS Source_Location,   
   OdHoldBackQtyEO.HOLD_QUANTITY,    
   moq.transaction_quantity AS Qty_on_Hand,  
   OdHoldBackQtyEO.START_DATE,
   OdHoldBackQtyEO.END_DATE,  
   OdHoldBackQtyEO.LAST_UPDATE_DATE,  
   OdHoldBackQtyEO.LAST_UPDATED_BY, 
   OdHoldBackQtyEO.CREATION_DATE, 
   OdHoldBackQtyEO.CREATED_BY, 
   OdHoldBackQtyEO.LAST_UPDATE_LOGIN, 
   OdHoldBackQtyEO.WAREHOUSE_LOCATION AS WAREHOUSE_LOCATION1  
FROM mtl_item_categories_v        mtcc,    
     inv.mtl_parameters               mtp,
     inv.mtl_system_items_b           msib,
     mtl_onhand_quantities        moq,
     inv.mtl_system_items_tl          msit,
     xxmer.od_hold_back_quantity      OdHoldBackQtyEO
WHERE mtcc.category_set_name = 'Inventory'
AND   mtcc.ORGANIZATION_ID   = mtp.ORGANIZATION_ID
AND   mtp.ORGANIZATION_ID    = mtp.MASTER_ORGANIZATION_ID
AND   mtcc.inventory_item_id = msib.inventory_item_id
AND   msib.organization_id   = moq.organization_id
AND   msib.inventory_item_id = moq.inventory_item_id
AND   msit.inventory_item_id = msib.inventory_item_id
AND   msit.organization_id   = msib.organization_id
AND   msib.segment1          = OdHoldBackQtyEO.item 
AND   moq.organization_id    = OdHoldBackQtyEO.warehouse_location
   ]]></SQLQuery>

<ViewObject
   Name="LeafPlannerVO"
   OrderBy="LAST_NAME"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.lov.wfh.server.LeafPlannerVORowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT PH.PLANNER_ID AS PLANNER_ID
     , EMPL.FIRST_NAME AS FIRST_NAME
     , EMPL.LAST_NAME AS LAST_NAME
FROM XX_WFH_PLANNER_HIERARCHY_ALL PH INNER JOIN XX_TEMP_WFH_EMPLOYEE EMPL
       ON PH.PLANNER_ID = EMPL.EMPLOYEE_ID
WHERE PH.PLANNER_ID NOT IN
  (SELECT DISTINCT MANAGER_ID
   FROM XX_WFH_PLANNER_HIERARCHY_ALL
   WHERE MANAGER_ID IS NOT NULL)
   ]]></SQLQuery>

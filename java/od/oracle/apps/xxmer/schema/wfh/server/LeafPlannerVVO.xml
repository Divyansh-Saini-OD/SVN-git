<ViewObject
   Name="LeafPlannerVVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.schema.wfh.server.LeafPlannerVVORowImpl"
   ComponentClass="od.oracle.apps.xxmer.schema.wfh.server.LeafPlannerVVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT PH.PLANNER_ID AS LEAF_PLANNER_ID
FROM XX_WFH_PLANNER_HIERARCHY_ALL PH
WHERE PH.PLANNER_ID = :1
  AND PH.PLANNER_ID NOT IN
  (SELECT DISTINCT MANAGER_ID
   FROM XX_WFH_PLANNER_HIERARCHY_ALL
   WHERE MANAGER_ID IS NOT NULL)
   ]]></SQLQuery>

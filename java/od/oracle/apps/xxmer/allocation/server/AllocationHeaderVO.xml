<ViewObject
   Name="AllocationHeaderVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.allocation.server.AllocationHeaderVORowImpl"
   ComponentClass="od.oracle.apps.xxmer.allocation.server.AllocationHeaderVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select  ah.batch_no
		,ah.batch_desc	
		,i.segment1 as item_id
		,i.description
		,i.PRIMARY_UNIT_OF_MEASURE
		,v.vendor_name	
		,people.full_name	
		,ah.CREATION_DATE		 
		,ah.LAST_UPDATE_DATE
		,ah.ALLOCATION_HEADER_ID
		from xx_po_allocation_header ah			
			 ,mtl_system_items_b i
			,MTL_PARAMETERS p
			 ,fnd_user u
			 ,po_vendors v
			,PER_ALL_PEOPLE_F people
	    where p.ORGANIZATION_ID = p.MASTER_ORGANIZATION_ID 
          and i.ORGANIZATION_ID = p.MASTER_ORGANIZATION_ID 
		  and i.INVENTORY_ITEM_ID = ah.ITEM_ID
		  
		  and u.user_id = ah.AGENT_ID 	
		 and u.employee_id = people.person_id
		  and v.vendor_id = ah.VENDOR_ID
   ]]></SQLQuery>

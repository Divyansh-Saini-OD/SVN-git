<ViewObject
   Name="ODSCAuditHistoryVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.scdashboard.server.ODSCAuditHistoryVORowImpl"
   ComponentClass="od.oracle.apps.xxmer.scdashboard.server.ODSCAuditHistoryVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT DISTINCT au.character4  od_sc_inspection_id,
                au.character3  od_sc_inspection_no,
                au.character5  od_sc_inspection_type,
                au.character12 od_sc_inspect_date, 
                au.character43 od_sc_grade,
                au.character28 od_sc_factory_id, 
	        au.character22 od_sc_od_vendor_id,
                vm.character1  od_sc_vendor_number, 
                vm.character2  od_sc_vendor_name,
                vm.character16 od_sc_factory_number,
	        vm.character17 od_sc_factory_name,
                DECODE (au.character43,
                        'Satisfactory', 'Green',
                        'Minor Progress Needed', 'Yellow',
                        'Needs Improvement', 'Orange',
                        'Zero Tolerance', 'Red',
                        'Denied Entry', 'Red'
                       ) color
           FROM qa_results vm,
                qa_results au,
                qa_plans mp,
                qa_plans ap
          where ap.name='OD_SC_VENDOR_AUDIT'
            and mp.name='OD_PB_SC_VENDOR_MASTER'
            and vm.plan_id=mp.plan_id 
	    and au.plan_id=ap.plan_id
	    and (   (   vm.character1 = au.character22
                     AND vm.character2 = au.character23
                    )
                 OR (   vm.character16 = au.character28
                     AND vm.character17 = au.character29
                    )
                )
   ]]></SQLQuery>

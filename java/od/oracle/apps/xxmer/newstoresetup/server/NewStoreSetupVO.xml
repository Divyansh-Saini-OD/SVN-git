<ViewObject
   Name="NewStoreSetupVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.newstoresetup.server.NewStoreSetupVORowImpl"
   ComponentClass="od.oracle.apps.xxmer.newstoresetup.server.NewStoreSetupVOImpl"
   MsgBundleClass="od.oracle.apps.xxmer.newstoresetup.server.common.NewStoreSetupVORowImplMsgBundle"
   ComponentInterface="od.oracle.apps.xxmer.newstoresetup.NewStoreSetupVO"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT rpad(ou.organization_id,10)       loc_id
      ,nvl(ou.name,'Not found')          loc_name
      ,ns.store_dir_rcv_date             store_dir_rcv_date
      ,to_number(ou.attribute1)          legacy_id
      ,to_date(decode(ou.attribute3,NULL,
              ns.store_dir_rcv_date,
              ou.attribute3))            open_date
      ,ms.organization_code              organization_code
  FROM xxmer_new_store_reloc_param   ns
      ,hr_all_organization_units     ou
      ,org_organization_definitions  ms
 WHERE ns.loc_id                            = ou.organization_id
   AND ou.organization_id                   = ms.organization_id
   AND ns.rec_status                      =  'A'
   AND fnd_profile.value_specific('ORG_ID') = ms.operating_unit
ORDER BY process_date
   ]]></SQLQuery>

   Name="OD_PA_NOTES_VO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.plmpjrdashboard.server.OD_PA_NOTES_VORowImpl"
   ComponentClass="od.oracle.apps.xxmer.plmpjrdashboard.server.OD_PA_NOTES_VOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT PPA.NAME PROJ_NAME, PPA.SEGMENT1 PROJ_NUMBER,PT.TASK_NAME,PT.TASK_NUMBER,PPS.PROJECT_STATUS_NAME TASK_STATUS
,Ppvs.Scheduled_start_Date,Ppvs.Scheduled_Finish_Date,Pt.Parent_Task_Id,
PPEB.C_EXT_ATTR1 DEPARTMENT,PPEB.C_EXT_ATTR2 CLASS,PPEB.C_EXT_ATTR5 DIVISION,PT.TASK_MANAGER_PERSON_ID
,PT.ATTRIBUTE10 TASK_GROUP
,odpn.NOTE_ID, 
       odpn.NOTE_NUMBER, 
       odpn.PROJECT_ID, 
       odpn.TASK_ID, 
       odpn.NOTE_DESC, 
       odpn.CREATION_DATE, 
       odpn.CREATED_BY, 
       odpn.LAST_UPDATE_DATE, 
       odpn.LAST_UPDATED_BY, 
       ODPN.LAST_UPDATE_LOGIN, 
 (SELECT FULL_NAME FROM PER_ALL_PEOPLE_F PAPF-- where upper(first_name) like upper('Gaurav')
                      WHERE PAPF.PERSON_ID IN(
                            SELECT EMPLOYEE_ID FROM FND_USER FU
                            where fu.user_id = odpn.created_by
                            )
                            AND SYSDATE BETWEEN papf.EFFECTIVE_START_DATE AND papf.EFFECTIVE_END_DATE
                            ) AS SENT_FROM
From 
Pa_Projects_All Ppa,
Pa_Tasks Pt,
Pa_Proj_Elements Ppe,
Pa_Project_Statuses Pps,
Pa_Proj_Elem_Ver_Schedule Ppvs,
Pa_Projects_Erp_Ext_B Ppeb,
EGO_ATTR_GROUPS_V EAG,
FND_APPLICATION FA,
OD_PA_NOTES odpn
Where 
Ppa.Project_Id = Pt.Project_Id
And Pt.Task_Id = Ppe.Proj_Element_Id
And Ppe.Status_Code = Pps.Project_Status_Code
and pps.status_type = 'TASK' 
-- Comment not showing records after comp/uncompAND pps.project_status_name = 'Not Started'
and ppe.object_type='PA_TASKS'
and ppa.attribute_category = 'Private Brand'
AND nvl(ppa.attribute10,'No') = 'Yes'
AND ppa.enabled_flag = 'Y'
and ppa.template_flag = 'N'
And Pt.Task_Id = Ppvs.Proj_Element_Id
and pt.project_id = ppvs.project_id
and ppeb.attr_group_id = eag.attr_group_id 
and eag.attr_group_name = 'PB_GEN_INFO'
and eag.application_id = fa.application_id 
AND FA.APPLICATION_SHORT_NAME = 'PA' 
AND PPEB.PROJECT_ID = PPA.PROJECT_ID
AND ODPN.PROJECT_ID = PT.PROJECT_ID
AND ODPN.TASK_ID = PT.TASK_ID
AND ODPN.PROJECT_ID = PPA.PROJECT_ID
   ]]></SQLQuery>

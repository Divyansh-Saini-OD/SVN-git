   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT distinct project_status_name, pps.project_status_code
  FROM pa_proj_elements ppe
     , pa_project_statuses pps
 Where Ppe.Status_Code = Pps.Project_Status_Code 
   AND status_type = 'TASK'
   ]]></SQLQuery>

<?xml version="1.0" encoding='windows-1252'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="xxConcProgDetailVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcomn.common.fileUpload.server.xxConcProgDetailVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select reqId, program_name from (SELECT level level_val,
  a.request_id reqId,
  lpad(a.request_id, (LENGTH(request_id)+ level*5), ' ' ) request_id,
  (SELECT b.user_concurrent_program_name
  FROM fnd_concurrent_programs_vl b
  WHERE b.concurrent_program_id = a.concurrent_program_id
  ) program_name,
  a.actual_start_date,
  a.actual_completion_date,
  a.status_code,
  a.phase_code,
  argument_text
FROM fnd_concurrent_requests a
WHERE 1                         =1
  START WITH a.request_id       = :1
  CONNECT BY prior a.request_id = a.parent_request_id
) where level_val= 2
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_version" Value="9.0.3.14.00" />
      <Attr Name="_CodeGenFlagNew" Value="36" />
   </DesignTime>
   <ViewAttribute
      Name="Reqid"
      IsPersistent="false"
      IsNotNull="true"
      Precision="15"
      Scale="0"
      Type="oracle.jbo.domain.Number"
      ColumnType="NUMBER"
      AliasName="REQID"
      Expression="REQID"
      SQLType="NUMERIC" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="22" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="ProgramName"
      IsPersistent="false"
      Precision="240"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="PROGRAM_NAME"
      Expression="PROGRAM_NAME"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="240" />
      </DesignTime>
   </ViewAttribute>
</ViewObject>

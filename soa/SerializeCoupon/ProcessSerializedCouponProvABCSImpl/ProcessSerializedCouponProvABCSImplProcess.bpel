<process name="ProcessSerializedCouponProvABCSImplProcess"
               targetNamespace="http://xmlns.oracle.com/CouponSerialization/ProcessSerializedCouponProvABCSImpl/ProcessSerializedCouponProvABCSImplProcess"
               xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
               xmlns:client="http://xmlns.oracle.com/CouponSerialization/ProcessSerializedCouponProvABCSImpl/ProcessSerializedCouponProvABCSImplProcess"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://xmlns.officedepot.com/ABCSImpl/POS/Custom/SerializedCouponEBO/V1"
         xmlns:ns2="http://xmlns.officedepot.com/EnterpriseMessage/POS/Custom/EBM/SerializedCouponRequestEBM/V1"
         xmlns:ns5="http://xmlns.officedepot.com/EnterpriseObjects/POS/Custom/EBO/SerializedCouponResponseEBO/V1"
         xmlns:ns3="http://xmlns.officedepot.com/EnterpriseObjects/POS/Custom/Common/SerializedCouponCommon/V1"
         xmlns:ns4="http://xmlns.officedepot.com/EnterpriseMessage/POS/Custom/EBM/SerializedCouponResponseEBM/V1"
         xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns6="http://xmlns.oracle.com/pcbpel/adapter/mq/CouponSerialization/ProcessSerializedCouponProvABCSImpl/testMQConnect"
         xmlns:ns7="http://xmlns.officedepot.com/ProcessSerializedCouponProvABCSImpl/MQRequest"
         xmlns:ns8="http://xmlns.officedepot.com/ProcessSerializedCouponProvABCSImpl/MQResponse"
         xmlns:ns9="http://xmlns.oracle.com/pcbpel/adapter/mq/CouponSerialization/ProcessSerializedCouponProvABCSImpl/testMQConnect1"
         xmlns:ns10="http://xmlns.oracle.com/pcbpel/adapter/mq/CouponSerialization/ProcessSerializedCouponProvABCSImpl/testMQAdapter2"
         xmlns:ns11="http://xmlns.oracle.com/pcbpel/adapter/mq/CouponSerialization/ProcessSerializedCouponProvABCSImpl/testMQ"
         xmlns:ns12="http://xmlns.oracle.com/pcbpel/adapter/mq/CouponSerialization/ProcessSerializedCouponProvABCSImpl/testMQEnqueue"
         xmlns:ns13="http://xmlns.oracle.com/singleString"
         xmlns:ns14="http://TargetNamespace.com/testMQ">

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="ProcessSerializedCouponProvABCSImplService"
                 partnerLinkType="ns1:ProcessSerializedCouponProvABCSImpl" myRole="ProcessSerializedCouponProvABCSImplProcessProvider"/>
    <partnerLink name="testMQ" partnerLinkType="ns11:EnqueueDequeue_plt"
                 partnerRole="EnqueueDequeue_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable"
              messageType="ns1:ProcessSerializedCouponReqMsg"/>
    <variable name="outputVariable"
              messageType="ns1:ProcessSerializedCouponRespMsg"/>
    <variable name="Invoke1_EnqueueDequeue_InputVariable_1"
              messageType="ns11:EnqueueDequeue_msg_out"/>
    <variable name="Invoke1_EnqueueDequeue_OutputVariable_1"
              messageType="ns11:EnqueueDequeue_msg_in"/>
  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in ProcessSerializedCouponProvABCSImplProcess.wsdl) -->

    <!-- Generate reply to synchronous request -->
    <receive name="Receive" createInstance="yes" variable="inputVariable"
             partnerLink="ProcessSerializedCouponProvABCSImplService"
             portType="ns1:ProcessSerializedCouponProvABCSImplService"
             operation="processSerializedCoupon"/>
    <assign name="Assign1">
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:SerializedCouponRequestEBM/SerializedCouponHeader/ns3:SourceID"/>
        <to variable="outputVariable" part="payload"
            query="/ns4:SerializedCouponResponseEBM/ns4:SerializedCouponHeader/ns3:SourceID"/>
      </copy>
      <copy>
        <from expression='ora:getCurrentDateTime("MMddyyyy")'/>
        <to variable="outputVariable" part="payload"
            query="/ns4:SerializedCouponResponseEBM/ns4:SerializedCouponData/ns5:ConsumptionActivity/ns3:UsedDate"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:SerializedCouponRequestEBM/SerializedCouponHeader/ns3:SourceID"/>
        <to variable="outputVariable" part="payload"
            query="/ns4:SerializedCouponResponseEBM/ns4:SerializedCouponData/ns5:ConsumptionActivity/ns3:SourceID"/>
      </copy>
      <copy>
        <from expression="string('WEB')"/>
        <to variable="outputVariable" part="payload"
            query="/ns4:SerializedCouponResponseEBM/ns4:SerializedCouponData/ns5:ConsumptionActivity/ns3:Channel"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:SerializedCouponRequestEBM/SerializedCouponData/Identification/ns3:CouponCode"/>
        <to variable="outputVariable" part="payload"
            query="/ns4:SerializedCouponResponseEBM/ns4:SerializedCouponData/ns5:Identification/ns3:CouponCode"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:SerializedCouponRequestEBM/SerializedCouponData/Identification/ns3:AgentID"/>
        <to variable="outputVariable" part="payload"
            query="/ns4:SerializedCouponResponseEBM/ns4:SerializedCouponData/ns5:Identification/ns3:AgentID"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:SerializedCouponRequestEBM/SerializedCouponData/MarkAsUsedIndicator"/>
        <to variable="outputVariable" part="payload"
            query="/ns4:SerializedCouponResponseEBM/ns4:SerializedCouponData/ns5:UsedCouponIndicator"/>
      </copy>
      <copy>
        <from expression="string('1')"/>
        <to variable="outputVariable" part="payload"
            query="/ns4:SerializedCouponResponseEBM/ns4:SerializedCouponData/ns5:UsageAccrual/ns3:NumberOfUses"/>
      </copy>
      <copy>
        <from expression="string('1')"/>
        <to variable="outputVariable" part="payload"
            query="/ns4:SerializedCouponResponseEBM/ns4:SerializedCouponData/ns5:UsageAccrual/ns3:NumberOfUsesLeft"/>
      </copy>
    </assign>
    <assign name="Assign4">
      <copy>
        <from expression="12345678"/>
        <to variable="Invoke1_EnqueueDequeue_InputVariable_1" part="request"
            query="/ns14:request/ns14:C1"/>
      </copy>
      <copy>
        <from expression="9876543210"/>
        <to variable="Invoke1_EnqueueDequeue_InputVariable_1" part="request"
            query="/ns14:request/ns14:C2"/>
      </copy>
      <copy>
        <from expression="05970308122223333"/>
        <to variable="Invoke1_EnqueueDequeue_InputVariable_1" part="request"
            query="/ns14:request/ns14:C3"/>
      </copy>
      <copy>
        <from expression="'R'"/>
        <to variable="Invoke1_EnqueueDequeue_InputVariable_1" part="request"
            query="/ns14:request/ns14:C4"/>
      </copy>
    </assign>
    <invoke name="Invoke1"
            partnerLink="testMQ" portType="ns11:EnqueueDequeue_ptt"
            operation="EnqueueDequeue"
            inputVariable="Invoke1_EnqueueDequeue_InputVariable_1"
            outputVariable="Invoke1_EnqueueDequeue_OutputVariable_1"
            bpelx:invokeAsDetail="no"/>
    <reply name="Reply" variable="outputVariable"
           partnerLink="ProcessSerializedCouponProvABCSImplService"
           portType="ns1:ProcessSerializedCouponProvABCSImplService"
           operation="processSerializedCoupon"/>
  </sequence>
</process>
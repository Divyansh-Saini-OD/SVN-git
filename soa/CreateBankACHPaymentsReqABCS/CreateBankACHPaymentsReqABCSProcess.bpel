<process name="CreateBankACHPaymentsReqABCSProcess"
               targetNamespace="http://xmlns.oracle.com/CreateBankACHPayments/CreateBankACHPaymentsReqABCS/CreateBankACHPaymentsReqABCSProcess"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/CreateBankACHPayments/CreateBankACHPaymentsReqABCS/CreateBankACHPaymentsReqABCSProcess"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/http/CreateBankACHPayments/CreateBankACHPaymentsReqABCS/CreateBankPaymentHTTPAdapter"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns3="http://www.princetonecom.com/connect/bankpaymentresponse1"
         xmlns:ns2="http://www.princetonecom.com/connect/bankpaymentrequest1"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns4="http://xmlns.oracle.com/HttpRequestResponseData"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns5="http://xmlns.oracle.com/CreateBankACHPaymentReqABCS/CreateBankACHPaymentFaultMessage/V1"
         xmlns:ns6="http://xmlns.oracle.com/pcbpel/adapter/jms/CreateBankACHPayments/CreateBankACHPaymentsReqABCS/InsertErrorLoggerJMSProducer"
         xmlns:ns7="http://www.officedepot.com/officedepot/ODComnErrorStructure/1.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns8="http://xmlns.oracle.com/pcbpel/adapter/http/CreateBankACHPayments/CreateBankACHPaymentsReqABCS/CreateBankPaymentsHTTPAdapter">

    <import namespace="http://xmlns.oracle.com/CreateBankACHPayments/CreateBankACHPaymentsReqABCS/CreateBankACHPaymentsReqABCSProcess" location="CreateBankACHPaymentsReqABCSProcess.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="createbankachpaymentsreqabcsprocess_client" partnerLinkType="client:CreateBankACHPaymentsReqABCSProcess" myRole="CreateBankACHPaymentsReqABCSProcessProvider"/>
    <partnerLink name="InsertErrorLoggerJMSProducer"
                 partnerLinkType="ns6:Produce_Message_plt"
                 partnerRole="Produce_Message_role"/>
    <partnerLink name="CreateBankPaymentsHTTPAdapter"
                 partnerLinkType="ns8:Request_Response_plt"
                 partnerRole="Request-Response_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:CreateBankACHPaymentsReqABCSProcessRequestMessage"/>

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="client:CreateBankACHPaymentsReqABCSProcessResponseMessage"/>
    <variable name="ApiInputString" type="xsd:string"/>
    <variable name="ParseString"
              element="ns3:bank-payment-response"/>
    <variable name="OutputString" type="xsd:string"/>
    <variable name="FaultMessage"
              messageType="client:CreateBankACHPaymentsReqABCSProcessResponseMessage"/>
    <variable name="SetCompositeTitle" type="xsd:string"/>
    <variable name="InvokeInsertErrorLogger_InputVariable"
              messageType="ns6:Produce_Message_msg"/>
    <variable name="InvokeEndPointURL_InputVariable"
              messageType="ns8:HttpClientBPELProcessProcessRequest_msg_in"/>
    <variable name="InvokeEndPointURL_OutputVariable"
              messageType="ns8:HttpClientBPELProcessProcessResponse_msg_out"/>
    <variable name="InvokeInsertTimeOutErrorLogger_InputVariable"
              messageType="ns6:Produce_Message_msg"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="Sequence2">
        <assign name="AssignCatchAll">
          <copy>
            <from>ora:setCompositeInstanceTitle('Catch all Error')</from>
            <to>$SetCompositeTitle</to>
          </copy>
          <copy>
            <from>ora:getFaultAsString()</from>
            <to>$FaultMessage.payload/ns3:message/ns3:message-text</to>
          </copy>
          <copy>
            <from>100</from>
            <to>$FaultMessage.payload/ns3:message/ns3:message-code</to>
          </copy>
        </assign>
        <reply name="ReplyCatchAll" faultName="client:executefault"
               variable="FaultMessage"
               partnerLink="createbankachpaymentsreqabcsprocess_client"
               portType="client:CreateBankACHPaymentsReqABCSProcess"
               operation="process"/>
        <assign name="AssignInsertErrorLogger">
          <copy>
            <from>ora:getCompositeName()</from>
            <to>$InvokeInsertErrorLogger_InputVariable.body/ns7:ODComnErrorStructureList/ns7:ProcessInfo/ns7:ProcessName</to>
          </copy>
          <copy>
            <from>xp20:current-dateTime()</from>
            <to>$InvokeInsertErrorLogger_InputVariable.body/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorDateTime</to>
          </copy>
          <copy>
            <from>ora:getFaultAsString()</from>
            <to>$InvokeInsertErrorLogger_InputVariable.body/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorText</to>
          </copy>
        </assign>
        <invoke name="InvokeInsertErrorLogger" bpelx:invokeAsDetail="no"
                partnerLink="InsertErrorLoggerJMSProducer"
                portType="ns6:Produce_Message_ptt" operation="Produce_Message"
                inputVariable="InvokeInsertErrorLogger_InputVariable"/>
        <exit name="TerminateCatchAll"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in CreateBankACHPaymentsReqABCSProcess.wsdl) -->
    <receive name="ReceivePaymentInput" partnerLink="createbankachpaymentsreqabcsprocess_client" portType="client:CreateBankACHPaymentsReqABCSProcess" operation="process" variable="inputVariable" createInstance="yes"/>

    <!-- Generate reply to synchronous request -->
    <assign name="AssignDataToHttpBindingInput">
      <copy>
        <from>dvm:lookupValue("oramds:/apps/ODCommon/dvm/MDS_CreateBankACHPaymentsReqABCS_Credentials.dvm","SystemName","ECheckPayment", "BusinessID",'')</from>
        <to>$inputVariable.payload/ns2:identity/ns2:business-id</to>
      </copy>
      <copy>
        <from>dvm:lookupValue("oramds:/apps/ODCommon/dvm/MDS_CreateBankACHPaymentsReqABCS_Credentials.dvm","SystemName","ECheckPayment", "LogIn",'')</from>
        <to>$inputVariable.payload/ns2:identity/ns2:login</to>
      </copy>
      <copy>
        <from>dvm:lookupValue("oramds:/apps/ODCommon/dvm/MDS_CreateBankACHPaymentsReqABCS_Credentials.dvm","SystemName","ECheckPayment", "Password",'')</from>
        <to>$inputVariable.payload/ns2:identity/ns2:password</to>
      </copy>
      <copy>
        <from>ora:getContentAsString($inputVariable.payload)</from>
        <to>$ApiInputString</to>
      </copy>
      <copy>
        <from>concat('&lt;?xml version="2.0" encoding="UTF-8" ?&gt;',$ApiInputString)</from>
        <to>$InvokeEndPointURL_InputVariable.HttpClientBPELProcessProcessRequest/ns4:xml</to>
      </copy>
    </assign>
    <invoke name="InvokeEndPointURL"
            partnerLink="CreateBankPaymentsHTTPAdapter"
            portType="ns8:Request_Response_ptt" operation="Request-Response"
            inputVariable="InvokeEndPointURL_InputVariable"
            outputVariable="InvokeEndPointURL_OutputVariable"
            bpelx:invokeAsDetail="no"/>
    <assign name="AssignHttpReponseToSOAPResponse">
      <copy>
        <from>ora:getContentAsString($InvokeEndPointURL_OutputVariable.HttpClientBPELProcessProcessResponse)</from>
        <to>$OutputString</to>
      </copy>
      <copy>
        <from>ora:parseEscapedXML(concat('&lt;?xml version="1.0" encoding="UTF-8" ?&gt;',$OutputString))</from>
        <to>$ParseString</to>
      </copy>
      <copy>
        <from>$ParseString</from>
        <to>$outputVariable.payload</to>
      </copy>
    </assign>
    <reply name="ReplyPaymentOutput" partnerLink="createbankachpaymentsreqabcsprocess_client" portType="client:CreateBankACHPaymentsReqABCSProcess" operation="process" variable="outputVariable"/>
  </sequence>
</process>
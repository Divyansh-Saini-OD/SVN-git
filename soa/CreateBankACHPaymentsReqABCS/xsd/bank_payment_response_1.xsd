<?xml version= '1.0' encoding= 'UTF-8' ?>
<xsd:schema targetNamespace="http://www.princetonecom.com/connect/bankpaymentresponse1" xmlns="http://www.princetonecom.com/connect/bankpaymentresponse1"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified"
     version="1">

 <xsd:simpleType name="Alpha">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z]*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Numeric">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaNumeric">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z0-9]*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AllChars">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[^|~&quot;]*"/>
    </xsd:restriction>
  </xsd:simpleType>


<xsd:simpleType name="BusinessId">
		<xsd:restriction base="xsd:long">
			<xsd:minExclusive value="0"/>
			<xsd:maxInclusive value="9999999999"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Login">
		<xsd:restriction base="AlphaNumeric">
			<xsd:minLength value="8"/>
			<xsd:maxLength value="32"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Password">
		<xsd:restriction base="AlphaNumeric">
			<xsd:minLength value="8"/>
			<xsd:maxLength value="32"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NachaStandardEntryClass">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="ACK"/>
			<xsd:enumeration value="ADV"/>
			<xsd:enumeration value="ATX"/>
			<xsd:enumeration value="CBR"/>
			<xsd:enumeration value="CCD"/>
			<xsd:enumeration value="CIE"/>
			<xsd:enumeration value="COR"/>
			<xsd:enumeration value="CTX"/>
			<xsd:enumeration value="DNE"/>
			<xsd:enumeration value="ENR"/>
			<xsd:enumeration value="MTE"/>
			<xsd:enumeration value="PBR"/>
			<xsd:enumeration value="POP"/>
			<xsd:enumeration value="POS"/>
			<xsd:enumeration value="PPD"/>
			<xsd:enumeration value="RCK"/>
			<xsd:enumeration value="RET"/>
			<xsd:enumeration value="SHR"/>
			<xsd:enumeration value="TEL"/>
			<xsd:enumeration value="TRC"/>
			<xsd:enumeration value="TRX"/>
			<xsd:enumeration value="WEB"/>
			<xsd:enumeration value="XCK"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="IndividualIdentifier">
		<xsd:restriction base="AlphaNumeric">
			<xsd:maxLength value="22"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BillingAccountNumber">
		<xsd:restriction base="AllChars">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="60"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BankAccountType">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="PERSONAL CHECKING"/>
			<xsd:enumeration value="PERSONAL SAVINGS"/>
			<xsd:enumeration value="BUSINESS CHECKING"/>
			<xsd:enumeration value="BUSINESS SAVINGS"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RoutingNumber">
		<xsd:restriction base="xsd:token">
			<xsd:pattern value="[0-3][0-9]{8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BankAccountNumber">
		<xsd:restriction base="xsd:token">
			<xsd:pattern value="[a-zA-Z0-9\-]{4,17}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="InternationalBankInformation">
		<xsd:restriction base="xsd:token">
			<xsd:pattern value="[a-zA-Z0-9\-]{1,9}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="InternationalBankAccountNumber">
		<xsd:restriction base="xsd:token">
			<xsd:pattern value="[a-zA-Z0-9\-]{1,32}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="InstitutionName">
		<xsd:restriction base="AllChars">
			<xsd:maxLength value="45"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AccountHolderName">
		<xsd:restriction base="AllChars">
			<xsd:maxLength value="45"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ExpirationMonth">
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="[1-9]|0[1-9]|1[0-2]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ExpirationYear">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="2000"/>
			<xsd:maxInclusive value="9999"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CreditCardNumber">
		<xsd:restriction base="Numeric">
			<xsd:minLength value="14"/>
			<xsd:maxLength value="19"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CreditCardType">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="MC"/>
			<xsd:enumeration value="VISA"/>
			<xsd:enumeration value="DISC"/>
			<xsd:enumeration value="AMEX"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SecurityCode">
		<xsd:restriction base="Numeric">
			<xsd:minLength value="3"/>
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EcommerceIndicator">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ECOMMERCE"/>
			<xsd:enumeration value="TELEPHONE"/>
			<xsd:enumeration value="RECURRING"/>
			<xsd:enumeration value="VOICE RESPONSE UNIT"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ResponseCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AuthorizationCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AddressValidationCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SecurityResultCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TraceCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="12"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DebitCardNumber">
		<xsd:restriction base="Numeric">
			<xsd:minLength value="12"/>
			<xsd:maxLength value="19"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DollarAmount">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="999999999999.99"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EmailAddress">
		<xsd:annotation>
			<xsd:documentation>
         Allows any character in the username (before the @) and checks
         that the domain name does not t with a ".", that the 
         top level domain contains only alphas and is at least 2 characters.
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:pattern value=".+@[^\.].*\.[a-zA-Z]{2,}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CreditDebitIndicator">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="DEBIT"/>
			<xsd:enumeration value="CREDIT"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ConfirmationNumber">
		<xsd:restriction base="xsd:long">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="99999999999999"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TransactionCode">
		<xsd:restriction base="AllChars">
			<xsd:maxLength value="45"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FeeWaiverReason">
		<xsd:restriction base="AllChars">
			<xsd:maxLength value="45"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RemitFieldValue">
		<xsd:restriction base="AllChars">
			<xsd:maxLength value="64"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BillerName">
		<xsd:restriction base="AllChars">
			<xsd:maxLength value="45"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BillingAccountHolderName">
		<xsd:restriction base="AllChars">
			<xsd:maxLength value="45"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CardRoute">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="STAR"/>

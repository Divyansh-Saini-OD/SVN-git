	<xsd:simpleType name="ModifiedBankAccountNumber">
		<xsd:restriction base="AllChars">
			<xsd:minLength value="2"/>
			<xsd:maxLength value="17"/>
		</xsd:restriction>
	</xsd:simpleType>


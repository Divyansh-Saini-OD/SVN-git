		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Pending"/>
			<xsd:enumeration value="Completed"/>
			<xsd:enumeration value="Submitted"/>
			<xsd:enumeration value="Cancelled"/>
			<xsd:enumeration value="Returned"/>
			<xsd:enumeration value="Reimbursed"/>
			<xsd:enumeration value="Failed"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FundingAccountType">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="BANK"/>
			<xsd:enumeration value="CREDIT CARD"/>
			<xsd:enumeration value="DEBIT CARD"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DateTimeType">
		<xsd:annotation>
			<xsd:documentation>
        The OFX element "DTTM" is of type "DateTimeType"
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:whiteSpace value="collapse"/>
			<xsd:minLength value="8"/>
			<xsd:pattern value="[0-9]{4}((0[1-9])|(1[0-2]))((0[1-9])|([1-2][0-9])|(3[0-1]))|[0-9]{4}((0[1-9])|(1[0-2]))((0[1-9])|([1-2][0-9])|(3[0-1]))(([0-1][0-9])|(2[0-3]))[0-5][0-9](([0-5][0-9])|(60))|[0-9]{4}((0[1-9])|(1[0-2]))((0[1-9])|([1-2][0-9])|(3[0-1]))(([0-1][0-9])|(2[0-3]))[0-5][0-9](([0-5][0-9])|(60))\.[0-9]{3}|[0-9]{4}((0[1-9])|(1[0-2]))((0[1-9])|([1-2][0-9])|(3[0-1]))(([0-1][0-9])|(2[0-3]))[0-5][0-9](([0-5][0-9])|(60))\.[0-9]{3}(\[[\+\-]?.+(:.+)?\])?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CompanyName">
		<xsd:restriction base="AllChars">
			<xsd:maxLength value="45"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProductType">
		<xsd:restriction base="xsd:token">

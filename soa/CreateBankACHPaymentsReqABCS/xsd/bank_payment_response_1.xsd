    <xsd:complexType name="BankPaymentResponse">
        <xsd:annotation>
            <xsd:documentation>Bank payment response.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ConnectResponse">
                <xsd:sequence>
                    <xsd:group ref="BankPaymentRequestGroup" minOccurs="0"/>
                    <xsd:group ref="BankPaymentResponseGroup" minOccurs="0"/>
                    <xsd:group ref="PaymentRequestGroup" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="bank-payment-response" type="BankPaymentResponse"/>
</xsd:schema>

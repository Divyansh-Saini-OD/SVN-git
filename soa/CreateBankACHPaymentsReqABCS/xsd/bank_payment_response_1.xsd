    </xsd:restriction>
  </xsd:simpleType>



  <xsd:complexType name="PaymentRemitField">
    <xsd:annotation>
      <xsd:documentation>
        Optional remit data.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="value" type="RemitFieldValue"/>
    </xsd:sequence>
  </xsd:complexType>

	<xsd:complexType name="BillingAccount">
		<xsd:annotation>
			<xsd:documentation>
        A billing account.
      </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="billing-account-number" type="BillingAccountNumber"/>
			<xsd:element name="billing-account-holder-name" type="BillingAccountHolderName" minOccurs="0"/>
			<xsd:group ref="BillingDivisionGroup" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:group name="BillingDivisionGroup">
		<xsd:choice>
			<xsd:element name="billing-division-id" type="BusinessId"/>
			<xsd:element name="division-code" type="BusinessCode"/>
		</xsd:choice>
	</xsd:group>




	<xsd:complexType name="Remittance">
		<xsd:annotation>
			<xsd:documentation>
        The billing account and remittance amount, along with the optional remit data.
        </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="billing-account" type="BillingAccount"/>
			<xsd:element name="remit-amount" type="DollarAmount"/>
			<xsd:element name="remit-fee" type="DollarAmount" minOccurs="0"/>
			<xsd:element name="fee-waiver-reason" type="FeeWaiverReason" minOccurs="0"/>
			<xsd:element name="payment-remit-field" type="PaymentRemitField" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>





	<xsd:complexType name="FundingAccount">

		<xsd:sequence>
			<xsd:element name="funding-account-id" type="UniqueId" minOccurs="0"/>
			<xsd:element name="account-nickname" type="AccountNickName" minOccurs="0"/>
			<xsd:element name="create-date" type="xsd:date" minOccurs="0"/>
			<xsd:element name="last-update-date" type="xsd:date" minOccurs="0"/>
			<xsd:element name="last-used-date" type="xsd:date" minOccurs="0"/>
			<xsd:element name="account-status" type="AccountStatus" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:group name="FundingAccountAddressGroup">
		<xsd:sequence>
			<xsd:element name="account-holder-name" type="AccountHolderName"/>
			<xsd:element name="account-address-1" type="AddressLine"/>
			<xsd:element name="account-address-2" type="AddressLine" minOccurs="0"/>
			<xsd:element name="account-city" type="City"/>
			<xsd:element name="account-state" type="State"/>
			<xsd:element name="account-postal-code" type="PostalCode"/>
			<xsd:element name="account-country-code" type="CountryCode"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:element name="funding-account" type="FundingAccount" abstract="true"/>




	<xsd:complexType name="BankAccount">
		<xsd:annotation>
			<xsd:documentation>
        A bank account.
      </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FundingAccount">
				<xsd:sequence>
					<xsd:element name="bank-account-type" type="BankAccountType"/>
					<xsd:element name="routing-number" type="RoutingNumber"/>
					<xsd:element name="bank-account-number" type="BankAccountNumber"/>
					<xsd:element name="institution-name" type="InstitutionName" minOccurs="0"/>
					<xsd:element name="nsf-count" type="NSFCount" minOccurs="0"/>
					<xsd:group ref="FundingAccountAddressGroup" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

  <xsd:group name="BankPaymentRequestGroup">
    <xsd:annotation>
      <xsd:documentation>
        A bank account plus the bank account-specific payment information.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="bank-account" type="BankAccount"/>
      <xsd:element name="nacha-standard-entry-class" type="NachaStandardEntryClass"/>
      <xsd:element name="individual-identifier" type="IndividualIdentifier" minOccurs="0"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="BankPaymentResponseGroup">
    <xsd:annotation>
      <xsd:documentation>
        A bank account plus the payment-specific bank account information.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="modified-routing-number" type="RoutingNumber"/>
      <xsd:element name="modified-bank-account-number" type="ModifiedBankAccountNumber"/>
      <xsd:group ref="PaymentResponseGroup"/>
    </xsd:sequence>
  </xsd:group>

   	<xsd:group name="PaymentRequestGroup">
		<xsd:annotation>
			<xsd:documentation>
        Common payment request elements.
      </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="credit-debit-indicator" type="CreditDebitIndicator"/>
			<xsd:element name="requested-payment-date" type="xsd:date"/>
			<xsd:element name="remittance" type="Remittance" maxOccurs="unbounded"/>
			<xsd:element name="product" type="ProductType" minOccurs="0"/>
			<xsd:element name="transaction-code" type="TransactionCode" minOccurs="0"/>
			<xsd:element name="email-address" type="EmailAddress" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group> 





    
    
    

	<xsd:group name="ModifyPaymentRequestGroup">
		<xsd:annotation>
			<xsd:documentation>
        Common modify payment request elements.
      </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requested-payment-date" type="xsd:date"/>
			<xsd:element name="remittance" type="Remittance"/>
			<xsd:element name="transaction-code" type="TransactionCode" minOccurs="0"/>
			<xsd:element name="email-address" type="EmailAddress" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="PaymentResponseGroup">
		<xsd:annotation>
			<xsd:documentation>
        Common payment response elements.
        </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="confirmation-number" type="ConfirmationNumber"/>
			<xsd:element name="payment-amount" type="DollarAmount"/>
			<xsd:element name="payment-fee" type="DollarAmount"/>
			<xsd:element name="payment-date" type="xsd:date"/>
		</xsd:sequence>
	</xsd:group>







   
   
    <xsd:simpleType name="MessageCode">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="99999"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MessageText">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="2048"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="Message">
    <xsd:annotation>
      <xsd:documentation>
        Connect response messages.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="message-code" type="MessageCode"/>
      <xsd:element name="message-text" type="MessageText"/>
    </xsd:sequence>
  </xsd:complexType>
  
  
    
      <xsd:complexType name="ConnectResponse" abstract="true">
    <xsd:annotation>
      <xsd:documentation>
        Abstract request common to all Connect requests.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="message" type="Message" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
    
    
    

    
    
    
    
    
    
    
    
    

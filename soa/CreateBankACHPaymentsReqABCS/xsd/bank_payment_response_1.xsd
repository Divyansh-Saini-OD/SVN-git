		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BusinessCode">
		<xsd:restriction base="AlphaNumeric">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="8"/>
		</xsd:restriction>
	</xsd:simpleType>

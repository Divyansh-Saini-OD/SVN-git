-->
<process name="NASupplier_rms_inb"
         targetNamespace="http://xmlns.officedepot.com/Merchant/NASupplier_rms_inb/NASupplier_rms_inb"
         xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:client="http://xmlns.officedepot.com/Merchant/NASupplier_rms_inb/NASupplier_rms_inb"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://www.openapplications.org/officedepot/finance/foundation/1"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/PeopleSoft/RMSDBAdapter/UpdateSupplierRMS_db"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:aia="http://www.oracle.com/XSL/Transform/java/oracle.apps.aia.core.xpath.AIAFunctions"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ns4="http://www.openapplications.org/oagis/9/officedepot/1"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/db/RMS10/OD_EBS_SUPPLIER_SQL/VALIDATE_OPERATION/"
         xmlns:ns3="http://www.openapplications.org/oagis/9"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns6="http://xmlns.oracle.com/pcbpel/adapter/jms/SyncSupplier/NASupplier_rms_inb/CommonErrorLoggingService"
         xmlns:ns7="http://www.officedepot.com/officedepot/ODComnErrorStructure/1.0"
         xmlns:ns8="http://xmlns.oracle.com/pcbpel/adapter/jms/10GTo11G/OTMCOSOrderReleaseABCS/InsertErrorLoggerJMSPublisher"
         xmlns:ns9="ODError">

    <import namespace="http://xmlns.officedepot.com/Merchant/NASupplier_rms_inb/NASupplier_rms_inb" location="NASupplier_rms_inb.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
    <partnerLinks>
        <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
        <partnerLink name="nasupplier_rms_inb_client" partnerLinkType="client:NASupplier_rms_inb" myRole="NASupplier_rms_inbProvider" partnerRole="NASupplier_rms_inbRequester"/>
        <partnerLink name="UpdateSupplierRMS_db"
                     partnerLinkType="ns2:UpdateSupplierRMS_db_plt"
                     partnerRole="UpdateSupplierRMS_db_role"/>
        <partnerLink name="InsertErrorLoggerJMSPublisher"
                     partnerLinkType="ns8:Produce_Message_plt"
                     partnerRole="Produce_Message_role"/>
    </partnerLinks>

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <variables>
        <!-- Reference to the message passed as input during initiation -->
        <variable name="inputVariable" messageType="client:NASupplier_rms_inbRequestMessage"/>

        <!-- Reference to the message that will be sent back to the requester during callback -->
        <variable name="outputVariable" messageType="client:NASupplier_rms_inbResponseMessage"/>
        <variable name="Invoke1_UpdateSupplierRMS_db_InputVariable"
                  messageType="ns2:args_in_msg"/>
        <variable name="Invoke1_UpdateSupplierRMS_db_OutputVariable"
                  messageType="ns2:args_out_msg"/>
        <variable name="supplierPartyCount" type="xsd:integer"/>
        <variable name="supplierPartyCounter" type="xsd:integer"/>
        <variable name="currentSupplierPartyMaster"
                  element="ns3:SupplierPartyMaster"/>
            <variable name="messagePayload" type="xsd:string"/>
        <variable name="errorCount" type="xsd:integer"/>
        <variable name="globalReturnMessage" element="ns1:CommonResponse"/>
        <variable name="returnCode" type="xsd:integer"/>
        <variable name="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                  messageType="ns8:Produce_Message_msg"/>
    </variables>
    <faultHandlers>
        <catchAll>
            <sequence name="SequenceErrorHandler">
                <assign name="AssignInsertErrorLogger">
                    <copy>
                        <from>ora:getCompositeInstanceId()</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ProcessInfo/ns9:BusinessProcessId</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('SOURCE_SYSTEM_NAME')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ProcessInfo/ns9:TradingPartnerDetails/ns9:TPFrom</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('TP_TO')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ProcessInfo/ns9:TradingPartnerDetails/ns9:TPTo</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('ENTITY_ID')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:Entity/ns9:EntityID</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('MESSAGE_OPERATION')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:MessageDetails/ns9:MessageOperation</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('SOA_PROCESS_NAME')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ProcessInfo/ns9:BusinessProcessName</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('CATCH_ALL_PROCESS_STEP')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ProcessInfo/ns9:BusinessProcessStep</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('DOMAIN')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ProcessInfo/ns9:BusinessProcessDomain</to>
                    </copy>
                    <copy>
                        <from>ora:getCompositeURL()</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ProcessInfo/ns9:SystemName</to>
                    </copy>
                    <copy>
                        <from>ora:getFaultName()</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ErrorDetails/ns9:ErrorCode</to>
                    </copy>
                    <copy>
                        <from>concat(ora:getPreference('SOA_PROCESS_NAME'),ora:getPreference('SOA_ERROR_SUBJECT'))</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ErrorDetails/ns9:ErrorDescription</to>
                    </copy>
                    <copy>
                        <from>concat(ora:getPreference('SOA_ERROR_MESSAGE'), $errorCount, 'ErrorMessage:',$globalReturnMessage/ns1:ReturnMessage)</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ErrorDetails/ns9:ErrorText</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('ERROR_TYPE')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ErrorDetails/ns9:ErrorType</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('ERROR_SEVERITY')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ErrorDetails/ns9:ErrorSeverity</to>
                    </copy>
                    <copy>
                        <from>ora:getCurrentDateTime()</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:ErrorDetails/ns9:ErrorDateTime</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('NOTIFICATION_FLAG')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:Notification/ns9:sendNotification</to>
                    </copy>
                    <copy>
                        <from>concat(ora:getPreference('SOA_PROCESS_NAME'),ora:getPreference('SOA_ERROR_SUBJECT'))</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:Notification/ns9:Subject</to>
                    </copy>
                    <copy>
                        <from>ora:getCompositeInstanceId()</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:MessageDetails/ns9:MessageId</to>
                    </copy>
                    <copy>
                        <from>ora:getCurrentDateTime()</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:MessageDetails/ns9:MessageDateTime</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('MESSAGE_TYPE')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:MessageDetails/ns9:MessageType</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('MESSAGE_VERSION')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:MessageDetails/ns9:MessageVersion</to>
                    </copy>
                    <copy>
                        <from>ora:getPreference('SOURCE_SYSTEM_NAME')</from>
                        <to>$InvokeInsertErrorLogger_Produce_Message_InputVariable.body/ns9:MessageDetails/ns9:MessageSourceSystem</to>
                    </copy>
                </assign>
                <invoke name="InvokeInsertErrorLogger"
                        partnerLink="InsertErrorLoggerJMSPublisher"
                        portType="ns8:Produce_Message_ptt"
                        operation="Produce_Message"
                        inputVariable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                        bpelx:invokeAsDetail="no"/>
            </sequence>
        </catchAll>
    </faultHandlers>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="main">
        <!-- Receive input from requestor. (Note: This maps to operation defined in NASupplier_rms_inb.wsdl) -->
        <receive name="receiveInput" partnerLink="nasupplier_rms_inb_client" portType="client:NASupplier_rms_inb" operation="initiate" variable="inputVariable" createInstance="yes"/>

        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <assign name="AssignVariables">
            <copy>
                <from>count($inputVariable.payload/ns3:DataArea/ns3:SupplierPartyMaster)</from>
                <to>$supplierPartyCount</to>
            </copy>
            <copy>
                <from>0</from>
                <to>$outputVariable.payload/ns1:ReturnCode</to>
            </copy>
            <copy>
                <from>0</from>
                <to>$returnCode</to>
            </copy>
            <copy>
                <from>0</from>
                <to>$errorCount</to>
            </copy>
            <copy>
                <from>1</from>
                <to>$supplierPartyCounter</to>
            </copy>
        </assign>
        <invoke name="callbackClient" partnerLink="nasupplier_rms_inb_client"
                portType="client:NASupplier_rms_inbCallback"
                operation="onResult" inputVariable="outputVariable"/>
        <while name="WhileSupplierPArty">
            <condition>$supplierPartyCounter &lt;= $supplierPartyCount</condition>
            <sequence name="SequenceLoopThroughSuppliers">
                <scope name="Scope2">
                    <faultHandlers>
                        <catchAll>
                            <assign name="AssignAppendEmailBody">
                                <copy>
                                    <from>concat($globalReturnMessage,"&lt;BR&gt;",$Invoke1_UpdateSupplierRMS_db_InputVariable.InputParameters/ns5:I_VENDOR_ID,':',$Invoke1_UpdateSupplierRMS_db_OutputVariable.OutputParameters/ns5:O_ERROR_MESSAGE)</from>
                                    <to>$globalReturnMessage/ns1:ReturnMessage</to>
                                </copy>
                                <copy>
                                    <from>-1</from>
                                    <to>$returnCode</to>
                                </copy>
                                <copy>
                                    <from>$errorCount+1</from>
                                    <to>$errorCount</to>
                                </copy>
                            </assign>
                        </catchAll>
                    </faultHandlers>
                    <sequence name="Sequence2">
                        <sequence name="SequenceInvokeEBS">
                            <assign name="AssignCurrentSupplierPartyMaster">
                                <copy>
                                    <from>ora:getElement('inputVariable','payload','/ns3:SyncSupplierPartyMaster/ns3:DataArea/ns3:SupplierPartyMaster',$supplierPartyCounter)</from>
                                    <to>$currentSupplierPartyMaster</to>
                                </copy>
                            </assign>
                            <assign name="TransformEBSToRMS">
                                <bpelx:annotation>
                                    <bpelx:pattern patternName="bpelx:transformation"/>
                                </bpelx:annotation>
                                <copy>
                                    <from>ora:doXSLTransformForDoc("xsl/TransformationEBSToRMS.xsl", $currentSupplierPartyMaster)</from>
                                    <to variable="Invoke1_UpdateSupplierRMS_db_InputVariable"
                                        part="InputParameters"/>
                                </copy>
                            </assign>
                            <invoke name="InvokeUpdateSupplierRMS"
                                    bpelx:invokeAsDetail="no"
                                    partnerLink="UpdateSupplierRMS_db"
                                    portType="ns2:UpdateSupplierRMS_db_ptt"
                                    operation="UpdateSupplierRMS_db"
                                    inputVariable="Invoke1_UpdateSupplierRMS_db_InputVariable"
                                    outputVariable="Invoke1_UpdateSupplierRMS_db_OutputVariable"/>
                            <if name="If1">
                                <documentation>ReturnCode!=0</documentation>
                                <condition>$Invoke1_UpdateSupplierRMS_db_OutputVariable.OutputParameters/ns5:L_RESULT!=0</condition>
                                <assign name="AssignConstructEmailBody">
                                    <copy>
                                        <from>concat($globalReturnMessage,"&lt;BR&gt;",$Invoke1_UpdateSupplierRMS_db_InputVariable.InputParameters/ns5:I_VENDOR_ID,':',$Invoke1_UpdateSupplierRMS_db_OutputVariable.OutputParameters/ns5:O_ERROR_MESSAGE)</from>
                                        <to>$globalReturnMessage/ns1:ReturnMessage</to>
                                    </copy>
                                    <copy>
                                        <from>-1</from>
                                        <to>$returnCode</to>
                                    </copy>
                                    <copy>
                                        <from>$errorCount+1</from>
                                        <to>$errorCount</to>
                                    </copy>
                                </assign>
                            </if>
                        </sequence>
                    </sequence>
                </scope>
                <assign name="AssignIncreamentSupplierCount">
                    <copy>
                        <from>$supplierPartyCounter+1</from>
                        <to>$supplierPartyCounter</to>
                    </copy>
                </assign>
            </sequence>
        </while>
        <sequence name="SequenceThrowError">
            <if name="If2">
                <documentation>OnError</documentation>
                <condition>$returnCode!=0</condition>
                <throw name="ThrowRMSError" faultName="bpelx:RMSFault"/>
            </if>
        </sequence>
    </sequence>
</process>
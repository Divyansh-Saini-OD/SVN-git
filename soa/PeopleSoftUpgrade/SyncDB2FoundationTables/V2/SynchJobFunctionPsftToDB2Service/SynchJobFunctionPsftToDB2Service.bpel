  Oracle JDeveloper BPEL Designer 
  
  Created: Sat Jun 23 17:08:44 IST 2012
  Author:  putrao
  Type: BPEL 1.1 Process
  Purpose: One Way BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="SynchJobFunctionPsftToDB2Service"
               targetNamespace="http://xmlns.oracle.com/OfficeDepotSOAApps/SynchJobFunctionPsftToDB2Service/SynchJobFunctionPsftToDB2Service"
               xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
               xmlns:client="http://xmlns.oracle.com/OfficeDepotSOAApps/SynchJobFunctionPsftToDB2Service/SynchJobFunctionPsftToDB2Service"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/db/Application1/DB2FoundationTbls/InsertUpdateJobFunctionDB2Data"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
          xmlns:ns6="http://www.officedepot.com/officedepot/ODComnErrorStructure/1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/jms/OfficeDepotSOAApps/SynchJobFunctionPsftToDB2Service/CommonErrorLoggingService"
         xmlns:ns5="http://schemas.xmlsoap.org/ws/2003/03/addressing"
         xmlns:ns3="http://xmlns.officedepot.com/EnterpriseObjectLibrary/EBO/Job/V1"
         xmlns:ns4="http://xmlns.officedepot.com/EnterpriseObjects/Core/Common/V3"
         xmlns:ns7="urn:oasis:names:tc:xacml:2.0:context:schema:cd:04"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ns8="http://xmlns.oracle.com/pcbpel/adapter/db/top/InsertUpdateJobFunctionDB2Data"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns9="http://xmlns.oracle.com/Enterprise/Tools/schemas/OD_JOBFUNCTION_SYNC.v1"
         xmlns:aia="http://www.oracle.com/XSL/Transform/java/oracle.apps.aia.core.xpath.AIAFunctions"
         xmlns:ns10="http://xmlns.oracle.com/pcbpel/adapter/jms/10GTo11G/OTMCOSOrderReleaseABCS/InsertErrorLoggerJMSPublisher"
         xmlns:ns11="ODError">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="synchjobfunctionpsfttodb2service_client" partnerLinkType="client:SynchJobFunctionPsftToDB2Service" myRole="SynchJobFunctionPsftToDB2ServiceProvider"/>
    <partnerLink name="InsertUpdateJobFunctionDB2Data"
                 partnerLinkType="ns1:InsertUpdateJobFunctionDB2Data_plt"
                 partnerRole="InsertUpdateJobFunctionDB2Data_role"/>
    <partnerLink name="InsertErrorLoggerJMSPublisher"
                 partnerLinkType="ns10:Produce_Message_plt"
                 partnerRole="Produce_Message_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:SynchJobFunctionPsftToDB2ServiceRequestMessage"/>
    <variable name="Invoke_InsertUpdateJobFunctionDB2Data_merge_InputVariable"
              messageType="ns1:PsJobfunctionTblCollection_msg"/>
    <variable name="InvokeInsertErrorLogger_Produce_Message_InputVariable"
              messageType="ns10:Produce_Message_msg"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="Sequence1">
        <scope name="ScopeSendNotification" variableAccessSerializable="no">
         <variables>
              <variable name="messagePayload" type="xsd:string"/>
              <variable name="emailSubject" type="xsd:string"/>
              <variable name="errorText" type="xsd:string"/>
              <variable name="systemName" type="xsd:string"/>
              <variable name="messageOperation" type="xsd:string"/>
              <variable name="applicationTeamErrorText" type="xsd:string"/>
               <variable name="errorLoopIndex" type="xsd:integer"/>
          <variable name="errorNumberOfJobFunc" type="xsd:integer"/>
          <variable name="jobFuncList" type="xsd:string"/>
            </variables>
          <sequence>
           <assign name="InitLoopIndex">
              <copy>
                <from expression="1"/>
                <to variable="errorLoopIndex"/>
              </copy>
              <copy>
                <from expression="ora:countNodes('inputVariable','payload','/ns9:OD_JOBFUNCTION_SYNC/ns9:MsgData/ns9:Transaction/ns9:JOBFUNCTION_TBL/ns9:JOB_FUNCTION')"/>
                <to variable="errorNumberOfJobFunc"/>
              </copy>
            </assign>
          <while name="ForEachJobFunction"
                   condition="bpws:getVariableData('errorLoopIndex') &lt;= bpws:getVariableData('errorNumberOfJobFunc')">
              <sequence name="Sequence3">
                <assign name="AssignJobFunctionSpecificData">
                  <copy>
                    <from expression="concat(bpws:getVariableData('jobFuncList'),' ',bpws:getVariableData('inputVariable','payload','/ns9:OD_JOBFUNCTION_SYNC/ns9:MsgData/ns9:Transaction[$errorLoopIndex]/ns9:JOBFUNCTION_TBL/ns9:JOB_FUNCTION'),'')"/>
                    <to variable="jobFuncList"/>
                  </copy>
                  <copy>
                    <from expression="bpws:getVariableData('errorLoopIndex') + 1"/>
                    <to variable="errorLoopIndex"/>
                  </copy>
                </assign>
               
              </sequence>
            </while>
            <assign name="AssignInsertErrorLoggerAdminData">
              <copy>
                <from expression="ora:getPreference('SOA_ADMIN_APP_NAME')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ProcessInfo/ns11:BusinessProcessName"/>
              </copy>
              <copy>
                <from expression="ora:getCompositeInstanceId()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ProcessInfo/ns11:BusinessProcessId"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('CATCH_ALL_PROCESS_STEP')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ProcessInfo/ns11:BusinessProcessStep"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('DOMAIN')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ProcessInfo/ns11:BusinessProcessDomain"/>
              </copy>
              <copy>
                <from expression="ora:getCompositeURL()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ProcessInfo/ns11:SystemName"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('SOURCE_SYSTEM_NAME')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ProcessInfo/ns11:TradingPartnerDetails/ns11:TPFrom"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('TP_TO')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ProcessInfo/ns11:TradingPartnerDetails/ns11:TPTo"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('ENTITY_ID')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:Entity/ns11:EntityID"/>
              </copy>
              <copy>
                <from expression="ora:getFaultName()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ErrorDetails/ns11:ErrorCode"/>
              </copy>
              <copy>
                <from expression='concat(ora:getPreference("SOA_ADMIN_ERROR_MESSAGE"),ora:getCurrentDateTime())'/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ErrorDetails/ns11:ErrorDescription"/>
              </copy>
              <copy>
                <from expression="substring(ora:getFaultAsString(),1,4000)"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ErrorDetails/ns11:ErrorText"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('ERROR_TYPE')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ErrorDetails/ns11:ErrorType"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('ERROR_SEVERITY')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ErrorDetails/ns11:ErrorSeverity"/>
              </copy>
              <copy>
                <from expression="ora:getCurrentDateTime()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ErrorDetails/ns11:ErrorDateTime"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('NOTIFICATION_FLAG')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:Notification/ns11:sendNotification"/>
              </copy>
              <copy>
                <from expression='concat(ora:getPreference("SOA_ADMIN_ERROR_MESSAGE"),ora:getCurrentDateTime())'/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:Notification/ns11:Subject"/>
              </copy>
              <copy>
                <from expression="ora:getCompositeInstanceId()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:MessageDetails/ns11:MessageId"/>
              </copy>
              <copy>
                <from expression="ora:getCurrentDateTime()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:MessageDetails/ns11:MessageDateTime"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_TYPE')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:MessageDetails/ns11:MessageType"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_VERSION')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:MessageDetails/ns11:MessageVersion"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_OPERATION')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:MessageDetails/ns11:MessageOperation"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('SOURCE_SYSTEM_NAME')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:MessageDetails/ns11:MessageSourceSystem"/>
              </copy>
            </assign>
            <invoke name="InvokeInsertErrorLogger"
                    inputVariable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    partnerLink="InsertErrorLoggerJMSPublisher"
                    portType="ns10:Produce_Message_ptt"
                    operation="Produce_Message" bpelx:invokeAsDetail="no"/>
            <assign name="AssigninsertErrorLoggerApp">
              <copy>
                <from expression="ora:getPreference('SYSTEM_NAME')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ProcessInfo/ns11:BusinessProcessName"/>
              </copy>
              <copy>
                <from expression="concat(ora:getPreference('APPLICATION_EMAIL_SUBJECT'),ora:getCurrentDateTime())"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ErrorDetails/ns11:ErrorDescription"/>
              </copy>
              <copy>
                <from expression="concat(ora:getPreference('APPLICATION_ERROR_MESSAGE'),&quot; &quot;,bpws:getVariableData('jobFuncList'))"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns11:ErrorData/ns11:ErrorDetails/ns11:ErrorText"/>
              </copy>
            </assign>
            <invoke name="InvokeInsertErrorLoggerApp" bpelx:invokeAsDetail="no"
                    inputVariable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    partnerLink="InsertErrorLoggerJMSPublisher"
                    portType="ns10:Produce_Message_ptt"
                    operation="Produce_Message"/>
          </sequence>
        </scope>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in SynchJobFunctionPsftToDB2Service.wsdl) -->
    <receive name="receiveInput" partnerLink="synchjobfunctionpsfttodb2service_client" portType="client:SynchJobFunctionPsftToDB2Service" operation="synchronizeJobFunctionData" variable="inputVariable" createInstance="yes"/>
    <assign name="XFormPsftJobFunctionToDB2Format">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:doXSLTransformForDoc('xsl/XFormPsftJobFunctionToDB2JobFunctionFormat.xsl', $inputVariable.payload)"/>
        <to variable="Invoke_InsertUpdateJobFunctionDB2Data_merge_InputVariable"
            part="PsJobfunctionTblCollection"/>
      </copy>
    </assign>
    <invoke name="Invoke_InsertUpdateJobFunctionDB2Data"
            inputVariable="Invoke_InsertUpdateJobFunctionDB2Data_merge_InputVariable"
            partnerLink="InsertUpdateJobFunctionDB2Data"
            portType="ns1:InsertUpdateJobFunctionDB2Data_ptt"
            operation="merge" bpelx:invokeAsDetail="no"/>
  </sequence>
</process>
<process name="SynchEmployeePSFTReqABCSImpl"
               targetNamespace="http://xmlns.oracle.com/PeopleSoftMigration/SynchEmployeePSFTReqABCS/SynchEmployeePSFTReqABCSImpl"
               xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
               xmlns:client="http://xmlns.oracle.com/PeopleSoftMigration/SynchEmployeePSFTReqABCS/SynchEmployeePSFTReqABCSImpl"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/jms/PeopleSoftMigration/SynchEmployeePSFTReqABCS/PublishEmployeeDataListJMSProducerV1"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/jms/PeopleSoftMigration/SynchEmployeePSFTReqABCS/CommonErrorLoggingService"
         xmlns:ns3="http://xmlns.oracle.com/PeopleSoftMigration/UpdateEmployeeErrorCallBackPSFTProvABCS/UpdateEmployeeErrorCallBackPSFTProvABCS"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns6="http://xmlns.officedepot.com/EnterpriseObjectLibrary/Common/V1"
         xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns5="http://xmlns.officedepot.com/EnterpriseObjects/Core/EBO/Employee/V1"
         xmlns:ns7="http://www.officedepot.com/officedepot/ODComnErrorStructure/1.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ns4="http://xmlns.oracle.com/Enterprise/Tools/services"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns8="http://xmlns.officedepot.com/PeopleSoftMigration/UpdateEmployeeErrorCallBackPSFTProvABCS/UpdateEmployeeErrorToPSFTEBM"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns9="http://xmlns.officedepot.com/EnterpriseObjects/Core/Common/V3"
         xmlns:ns12="http://xmlns.officedepot.com/EnterpriseObjects/Core/Custom/Common/V3"
         xmlns:ns10="http://schemas.xmlsoap.org/ws/2003/03/addressing"
         xmlns:ns11="urn:oasis:names:tc:xacml:2.0:context:schema:cd:04">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="SynchEmployeePSFTReqABCSImpl_ep" partnerLinkType="client:SynchEmployeePSFTReqABCSImpl" myRole="SynchEmployeePSFTReqABCSImplProvider"/>
    <partnerLink name="PublishEmployeeDataListJMSProducerV1"
                 partnerLinkType="ns1:Produce_Message_plt"
                 partnerRole="Produce_Message_role"/>
    <partnerLink name="CommonErrorLoggingService"
                 partnerLinkType="ns2:Produce_Message_plt"
                 partnerRole="Produce_Message_role"/>
    <partnerLink name="UpdateEmployeeErrorCallBackPSFTProvABCS"
                 partnerLinkType="ns3:UpdateEmployeeErrorCallBackPSFTProvABCS"
                 partnerRole="UpdateEmployeeErrorCallBackPSFTProvABCSProvider"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <!--variable name="inputVariable" messageType="client:SynchEmployeePSFTReqABCSImplRequestMessage"/-->
    <!--variable name="jmsTopicPublishInputVariable"
              messageType="ns1:Produce_Message_msg"/-->
    <variable name="InvokeCommonErrorLoggerToSendErrorData_Produce_Message_InputVariable"
              messageType="ns2:Produce_Message_msg"/>
    <variable name="InvokePeopleSoftCallBackInterface_updateEmployeeErrorDetailsToPSFT_InputVariable"
              messageType="ns3:UpdateEmployeeErrorCallBackPSFTProvABCSRequestMessage"/>
    <variable name="invokeCommonErrorLoggerInputData"
              messageType="ns2:Produce_Message_msg"/>
    <variable name="inputVariable"
              messageType="client:SynchEmployeePSFTReqABCSImplRequestMessage"/>
    <variable name="jmsTopicPublishInputVariable"
              messageType="ns1:Produce_Message_msg"/>
  </variables>
  <faultHandlers>
     <catchAll>
      <sequence name="SequenceLogAndSendNotification">
        <scope name="Scope6">
          <bpelx:annotation>
            <bpelx:general>
              <bpelx:property name="userLabel">ScopeReportErrorToSourceAndLogError</bpelx:property>
            </bpelx:general>
          </bpelx:annotation>
          <variables>
            <variable name="EmployeeList" type="xsd:string"/>
            <variable name="loopIndex" type="xsd:integer"/>
            <variable name="numberOfEmployees" type="xsd:integer"/>
          </variables>
          <sequence name="SequenceReportErrorToSourceAndLog">
            <assign name="TransformToUpdatePeopleSoftCallbackFormat">
              <bpelx:annotation>
                <bpelx:pattern>transformation</bpelx:pattern>
              </bpelx:annotation>
              <copy>
                <from expression="ora:doXSLTransformForDoc('xsl/TransformToUpdatePeopleSoftCallbackFormat.xsl', $inputVariable.payload)"/>
                <to variable="InvokePeopleSoftCallBackInterface_updateEmployeeErrorDetailsToPSFT_InputVariable"
                    part="payload"/>
              </copy>
            </assign>
            <invoke name="InvokePeopleSoftCallBackInterface"
                    bpelx:invokeAsDetail="no"
                    inputVariable="InvokePeopleSoftCallBackInterface_updateEmployeeErrorDetailsToPSFT_InputVariable"
                    partnerLink="UpdateEmployeeErrorCallBackPSFTProvABCS"
                    portType="ns3:UpdateEmployeeErrorCallBackPSFTProvABCS"
                    operation="updateEmployeeErrorDetailsToPSFT"/>
            <scope name="Scope4">
              <bpelx:annotation>
                <bpelx:general>
                  <bpelx:property name="userLabel">ScopeSendNotification</bpelx:property>
                </bpelx:general>
              </bpelx:annotation>
              <variables>
                <variable name="messagePayload" type="xsd:string"/>
                <variable name="emailSubject" type="xsd:string"/>
                <variable name="errorText" type="xsd:string"/>
                <variable name="systemName" type="xsd:string"/>
                <variable name="messageOperation" type="xsd:string"/>
                <variable name="applicationTeamErrorText" type="xsd:string"/>
              </variables>
              <sequence name="SequenceSendNotification">
                <assign name="AssignRunTimeErrorRelatedData">
                  <copy>
                    <from expression="ora:countNodes('InvokePeopleSoftCallBackInterface_updateEmployeeErrorDetailsToPSFT_InputVariable','payload','/ns8:UpdateEmployeeErrorToPSFTEBM/ns8:EmployeeErrorDetails')"/>
                    <to variable="numberOfEmployees"/>
                  </copy>
                  <copy>
                    <from expression='concat(ora:getPreference("EMAIL_SUBJECT"),xp20:current-dateTime())'/>
                    <to variable="emailSubject"/>
                  </copy>
                  <copy>
                    <from expression="substring(ora:getFaultAsString(),1,500)"/>
                    <to variable="errorText"/>
                  </copy>
                  <copy>
                    <from expression='string("")'/>
                    <to variable="messagePayload"/>
                  </copy>
                  <copy>
                    <from expression="ora:getPreference('SOA_ADMIN_APP_NAME')"/>
                    <to variable="systemName"/>
                  </copy>
                </assign>
                <assign name="AssignPopulateCommonErrorLoggingData">
                  <copy>
                    <from expression="ora:getProcessId()"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ProcessInfo/ns7:ProcessName"/>
                  </copy>
                  <copy>
                    <from expression="ora:getPreference('ERROR_TYPE')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorType"/>
                  </copy>
                  <copy>
                    <from expression="ora:getPreference('ERROR_SEVERITY')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorSeverity"/>
                  </copy>
                  <copy>
                    <from expression="bpws:getVariableData('errorText')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorText"/>
                  </copy>
                  <copy>
                    <from expression="ora:getCurrentDateTime()"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorDateTime"/>
                  </copy>
                  <copy>
                    <from expression="ora:getPreference('CATCH_ALL_PROCESS_STEP')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ProcessStep"/>
                  </copy>
                  <copy>
                    <from expression="ora:getFaultName()"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorCode"/>
                  </copy>
                  <copy>
                    <from expression="ora:getDomainId()"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ProcessInfo/ns7:Domain"/>
                  </copy>
                  <copy>
                    <from expression="ora:getCompositeInstanceId()"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageID"/>
                  </copy>
                  <copy>
                    <from expression="ora:getCurrentDateTime()"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageDateTime"/>
                  </copy>
                  <copy>
                    <from expression="ora:getPreference('MESSAGE_TYPE')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageType"/>
                  </copy>
                  <copy>
                    <from expression="ora:getPreference('MESSAGE_VERSION')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageVersion"/>
                  </copy>
                  <copy>
                    <from expression="bpws:getVariableData('messageOperation')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageOperation"/>
                  </copy>
                  <copy>
                    <from expression="ora:getPreference('SOURCE_SYSTEM_NAME')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageSourceSystem"/>
                  </copy>
                  <copy>
                    <from expression="bpws:getVariableData('systemName')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ProcessInfo/ns7:SystemName"/>
                  </copy>
                  <copy>
                    <from expression="bpws:getVariableData('emailSubject')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorDescription"/>
                  </copy>
                  <copy>
                    <from variable="messagePayload"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessagePayload"/>
                  </copy>
                </assign>
                <invoke name="InvokeCommonErrorLoggerToSendErrorData"
                        inputVariable="invokeCommonErrorLoggerInputData"
                        partnerLink="CommonErrorLoggingService"
                        portType="ns2:Produce_Message_ptt"
                        operation="Produce_Message" bpelx:invokeAsDetail="no"/>
                <assign name="AssignErrorDetailsToApplicationTeam">
                  <copy>
                    <from expression="bpws:getVariableData('emailSubject')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorDescription"/>
                  </copy>
                  <copy>
                    <from expression="concat(ora:getPreference('APPLICATION_ERROR_MESSAGE'),&quot; &quot;,bpws:getVariableData('numberOfEmployees'))"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorText"/>
                  </copy>
                  <copy>
                    <from expression="ora:getPreference('SYSTEM_NAME')"/>
                    <to variable="invokeCommonErrorLoggerInputData" part="body"
                        query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ProcessInfo/ns7:SystemName"/>
                  </copy>
                </assign>
                <invoke name="InvokeCommonErrorLoggerToSendEmailToPeopleSoftTeam"
                        partnerLink="CommonErrorLoggingService"
                        portType="ns2:Produce_Message_ptt"
                        operation="Produce_Message"
                        inputVariable="invokeCommonErrorLoggerInputData"
                        bpelx:invokeAsDetail="no"/>
              </sequence>
            </scope>
          </sequence>
        </scope>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in SynchEmployeePSFTReqABCSImpl.wsdl) -->
    <receive name="receiveInput" partnerLink="SynchEmployeePSFTReqABCSImpl_ep" portType="client:SynchEmployeePSFTReqABCSImpl" operation="syncEmployee" variable="inputVariable" createInstance="yes"/>
    <assign name="TransformEmployeeABMToSyncEmployeeEBM">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:doXSLTransformForDoc('xsl/TransformEmployeeABMToSyncEmployeeEBM.xsl', $inputVariable.payload)"/>
        <to variable="jmsTopicPublishInputVariable" part="body"/>
      </copy>
    </assign>
    <assign name="AssignCreationDateAndTime">
      <copy>
        <from expression="xp20:current-dateTime()"/>
        <to variable="jmsTopicPublishInputVariable" part="body"
            query="/ns5:SyncEmployeeEBM/ns5:DataArea/ns5:DateTimeCreated"/>
      </copy>
    </assign>
    <invoke name="InvokeJMSTopicPublishService"
            inputVariable="jmsTopicPublishInputVariable"
            partnerLink="PublishEmployeeDataListJMSProducerV1"
            portType="ns1:Produce_Message_ptt" operation="Produce_Message"
            bpelx:invokeAsDetail="no"/>
  </sequence>
</process>
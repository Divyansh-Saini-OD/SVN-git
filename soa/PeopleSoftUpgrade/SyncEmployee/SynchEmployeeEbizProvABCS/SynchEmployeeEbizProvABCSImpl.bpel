                  <documentation>Yes</documentation>
                  <condition>$Error_Counter &gt; 0 and $ApplicationTeamNotified != true()</condition>
                  <sequence>
                    <assign name="AssignErrorDetailsToApplicationTeam">
                      <copy>
                        <from>concat(ora:getPreference('APPLICATION_ERROR_MESSAGE'),count($Invoke_PSFT_Error_CB_Process_InputVariable/ns16:EmployeeErrorDetails/ns16:EmployeeID))</from>
                        <to>$ErrorLoggerData.body/ns20:ErrorDetails/ns20:ErrorDescription</to>
                      </copy>
                      <copy>
                        <from>$applicationTeamErrorText</from>
                        <to>$ErrorLoggerData.body/ns20:ErrorDetails/ns20:ErrorText</to>
                      </copy>
                      <copy>
                        <from>ora:getPreference('SYSTEM_NAME')</from>
                        <to>$ErrorLoggerData.body/ns20:ProcessInfo/ns20:BusinessProcessName</to>
                      </copy>
                    </assign>
                    <invoke name="InvokeErrorLoggerToSendEmailToPeopleSoftTeam"
                            partnerLink="InsertErrorLogger"
                            portType="ns19:Produce_Message_ptt"
                            operation="Produce_Message"
                            inputVariable="ErrorLoggerData"
                            bpelx:invokeAsDetail="no"/>
                  </sequence>
                  <else>
                    <documentation>No</documentation>
                    <empty name="No_Application_Notification"/>
                  </else>
                </if>
              </sequence>
            </scope>
          </sequence>
        </catchAll>
      </faultHandlers>
      <sequence name="Sequence7">
        <assign name="Intialize_Error_Counter">
          <copy>
            <from><literal>0</literal></from>
            <to>$Error_Counter</to>
          </copy>
        </assign>
        <if name="Determine_If_There_are_Employees_to_Update">
          <documentation>Employees_Exist</documentation>
          <condition>count( $inputVariable.body/ns2:DataArea/ns2:EmployeeList/ns2:Employee) &gt; 0</condition>
          <sequence>
            <forEach parallel="no" counterName="ForEach1Counter"
                     name="ForEachEmployee">
              <startCounterValue>number('1')</startCounterValue>
              <finalCounterValue>count( $inputVariable.body/ns2:DataArea/ns2:EmployeeList/ns2:Employee )</finalCounterValue>
              <scope name="Loop_Scope" exitOnStandardFault="no">
                <bpelx:annotation>
                  <bpelx:general>
                    <bpelx:property name="userLabel">Loop_Scope</bpelx:property>
                  </bpelx:general>
                </bpelx:annotation>
                <variables>
                  <variable name="Invoke_Update_Employee_EBiz_Data_UpdateEmployeeEBizAppsAdaptorV1_InputVariable"
                            messageType="ns9:args_in_msg"/>
                  <variable name="Invoke_Update_Employee_EBiz_Data_UpdateEmployeeEBizAppsAdaptorV1_OutputVariable"
                            messageType="ns9:args_out_msg"/>
                </variables>
                <sequence name="Sequence1">
                  <assign name="Assign_Counter">
                    <copy>
                      <from>$ForEach1Counter</from>
                      <to>$EmpCount/ns4:value</to>
                    </copy>
                  </assign>
                  <assign name="Transform_to_EBiz_Format">
                    <bpelx:annotation>
                      <bpelx:pattern patternName="bpelx:transformation"/>
                    </bpelx:annotation>
                    <copy>
                      <from>ora:doXSLTransformForDoc("xsl/Transformation_Emp_Canonical_Ebiz_Map.xsl", $inputVariable.body, "EmpCount", $EmpCount)</from>
                      <to variable="Invoke_Update_Employee_EBiz_Data_UpdateEmployeeEBizAppsAdaptorV1_InputVariable"
                          part="InputParameters"/>
                    </copy>
                  </assign>
                  <scope name="Scope4">
                    <bpelx:annotation>
                      <bpelx:general>
                        <bpelx:property name="userLabel">Ebiz_Updte_scope</bpelx:property>
                      </bpelx:general>
                    </bpelx:annotation>
                    <faultHandlers>
                      <catchAll>
                        <sequence name="Sequence9">
                          <scope name="Scope5">
                            <variables>
                              <variable name="PS_CB_Non_Business_Faults_List"
                                        element="ns16:UpdateEmployeeErrorToPSFTEBM"/>
                            </variables>
                            <sequence>
                              <assign name="Prepare_Error_List">
                                <bpelx:annotation>
                                  <bpelx:pattern patternName="bpelx:transformation"/>
                                </bpelx:annotation>
                                <copy>
                                  <from>ora:doXSLTransformForDoc("xsl/Cannonical_to_CB_Req_Map.xsl", $inputVariable.body, "EmpCount", $EmpCount)</from>
                                  <to variable="PS_CB_Non_Business_Faults_List"/>
                                </copy>
                              </assign>
                              <assign name="Append_to_existing_List">
                                <extensionAssignOperation>
                                  <bpelx:append>
                                    <bpelx:from>$PS_CB_Non_Business_Faults_List/ns16:EmployeeErrorDetails</bpelx:from>
                                    <bpelx:to>$Invoke_PSFT_Error_CB_Process_InputVariable</bpelx:to>
                                  </bpelx:append>
                                </extensionAssignOperation>
                              </assign>
                              <rethrow name="Rethrow_Fault"/>
                            </sequence>
                          </scope>
                        </sequence>
                      </catchAll>
                    </faultHandlers>
                    <invoke name="Invoke_Update_Employee_EBiz_Data"
                            partnerLink="UpdateEmployeeEBizAppsAdaptorV1"
                            portType="ns9:UpdateEmployeeEBizAppsAdaptorV1_ptt"
                            operation="UpdateEmployeeEBizAppsAdaptorV1"
                            inputVariable="Invoke_Update_Employee_EBiz_Data_UpdateEmployeeEBizAppsAdaptorV1_InputVariable"
                            outputVariable="Invoke_Update_Employee_EBiz_Data_UpdateEmployeeEBizAppsAdaptorV1_OutputVariable"
                            bpelx:invokeAsDetail="no"/>
                  </scope>
                  <if name="If_a_Data_Error_Occurs">
                    <documentation>Error</documentation>
                    <condition>($Invoke_Update_Employee_EBiz_Data_UpdateEmployeeEBizAppsAdaptorV1_OutputVariable.OutputParameters/ns18:P_ERRBUFF != '') or ($Invoke_Update_Employee_EBiz_Data_UpdateEmployeeEBizAppsAdaptorV1_OutputVariable.OutputParameters/ns18:P_RETCODE != '')</condition>
                    <sequence name="Sequence8">
                      <scope name="Scope3">
                        <bpelx:annotation>
                          <bpelx:general>
                            <bpelx:property name="userLabel">EBiz_Sync_Scope</bpelx:property>
                          </bpelx:general>
                        </bpelx:annotation>
                        <variables>
                          <variable name="Emp_Error_Detail"
                                    element="ns16:EmployeeErrorDetails"/>
                        </variables>
                        <sequence>
                          <assign name="Assign_Error_Details">
                            <copy>
                              <from>$Invoke_Update_Employee_EBiz_Data_UpdateEmployeeEBizAppsAdaptorV1_InputVariable.InputParameters/ns18:P_EMPLID</from>
                              <to>$Emp_Error_Detail/ns16:EmployeeID</to>
                            </copy>
                            <copy>
                              <from><literal>E</literal></from>
                              <to>$Emp_Error_Detail/ns16:ProcessFlag</to>
                            </copy>
                            <copy>
                              <from>$Invoke_Update_Employee_EBiz_Data_UpdateEmployeeEBizAppsAdaptorV1_OutputVariable.OutputParameters/ns18:P_ERRBUFF</from>
                              <to>$Emp_Error_Detail/ns16:ErrorDesc</to>
                            </copy>
                            <extensionAssignOperation>
                              <bpelx:append>
                                <bpelx:from>$Emp_Error_Detail</bpelx:from>
                                <bpelx:to>$Invoke_PSFT_Error_CB_Process_InputVariable</bpelx:to>
                              </bpelx:append>
                            </extensionAssignOperation>
                          </assign>
                          <assign name="Increment_Error_Counter">
                            <copy>
                              <from>$Error_Counter  + 1</from>
                              <to>$Error_Counter</to>
                            </copy>
                          </assign>
                        </sequence>
                      </scope>
                    </sequence>
                    <else>
                      <empty name="No_Operation"/>
                    </else>
                  </if>
                </sequence>
              </scope>
            </forEach>
            <if name="Was_Data_Updated_to_EBiz">
              <documentation>Commit_Possible</documentation>
              <condition>$Error_Counter &lt; count($inputVariable.body/ns2:DataArea/ns2:EmployeeList/ns2:Employee)</condition>
              <sequence>
                <assign name="Prepare_Sync_EBiz_Request">
                  <copy>
                    <from>ora:getPreference('CP_USER_NAME')</from>
                    <to>$Invoke_SyncEmployeeEBizAppAdaptorV1_InputVariable.InputParameters/ns17:P_USER_NAME</to>
                  </copy>
                  <copy>
                    <from>ora:getPreference('CP_RESP_NAME')</from>
                    <to>$Invoke_SyncEmployeeEBizAppAdaptorV1_InputVariable.InputParameters/ns17:P_RESP_NAME</to>
                  </copy>
                  <copy>
                    <from>ora:getPreference('CP_APPL_SHORT_NAME')</from>
                    <to>$Invoke_SyncEmployeeEBizAppAdaptorV1_InputVariable.InputParameters/ns17:P_APPL_SHORT_NAME</to>
                  </copy>
                  <copy>
                    <from>ora:getPreference('CP_CONC_SHORT_NAME')</from>
                    <to>$Invoke_SyncEmployeeEBizAppAdaptorV1_InputVariable.InputParameters/ns17:P_CONC_SHORT_NAME</to>
                  </copy>
                </assign>
                <invoke name="Invoke_Sync_Employee_Ebiz_API"
                        partnerLink="SyncEmployeeEBizAppAdaptorV1"
                        portType="ns10:SyncEmployeeEBizAppAdaptorV1_ptt"
                        operation="SyncEmployeeEBizAppAdaptorV1"
                        inputVariable="Invoke_SyncEmployeeEBizAppAdaptorV1_InputVariable"
                        outputVariable="Invoke_SyncEmployeeEBizAppAdaptorV1_OutputVariable"
                        bpelx:invokeAsDetail="no"/>
                <assign name="Reset_Counter">
                  <copy>
                    <from>0</from>
                    <to>$EmpCount/ns4:value</to>
                  </copy>
                </assign>
              </sequence>
              <else>
                <empty name="Nothing_to_Commit"/>
              </else>
            </if>
            <if name="For_Data_Errors">
              <documentation>Error_Count_greater_than_0</documentation>
              <condition>$Error_Counter &gt; 0</condition>
              <sequence name="Sequence11">
                <throw name="Throw_Business_Fault"
                       faultName="client:DataErrors"/>
              </sequence>
              <else>
                <empty name="No_Operation"/>
              </else>
            </if>
          </sequence>
          <else>
            <documentation>No_Employees</documentation>
            <empty name="No_Operation"/>
          </else>
        </if>
      </sequence>
    </scope>
  </sequence>
</process>
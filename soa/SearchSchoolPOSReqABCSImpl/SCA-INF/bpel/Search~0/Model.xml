<?xml version="1.0" encoding="UTF-8"?>
<process name="SearchSchoolIDPOSReqABCSImplProcess" targetNamespace="http://xmlns.oracle.com/BTS/SearchSchoolPOSReqABCSImpl/SearchSchoolIDPOSReqABCSImplProcess" xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/" xmlns:client="http://xmlns.oracle.com/BTS/SearchSchoolPOSReqABCSImpl/SearchSchoolIDPOSReqABCSImplProcess" xmlns:ora="http://schemas.oracle.com/xpath/extension" xmlns:bpelx="http://schemas.oracle.com/bpel/extension" xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/" xmlns:ns1="http://oracle.com/sca/soapservice/BTS/SearchSchoolPOSReqABCSImpl/SearchSchool" xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/BTS/SearchSchoolPOSReqABCSImpl/SearchSchoolPOSDBAdapter" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns3="http://xmlns.officedepot.com/BTS/SearchSchoolIDPOSReqABCSImpl/SearchSchoolIDPOSFault/V1" xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/db/BTS/dbo/stp_search_bts_id/" xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20" xmlns:ns7="http://schemas.xmlsoap.org/ws/2003/03/addressing" xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc" xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue" xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath" xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath" xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions" xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk" xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions" xmlns:ns5="http://xmlns.oracle.com/EnterpriseObjects/Core/EBO/Location/V1" xmlns:ns6="http://xmlns.oracle.com/EnterpriseObjects/Core/Common/V2" xmlns:ns8="urn:oasis:names:tc:xacml:2.0:context:schema:cd:04" xmlns:ns9="http://xmlns.oracle.com/BTS/SearchSchoolIDPOSReqABCSImpl/SearchSchoolIDPOSFault/V1" xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns10="http://xmlns.oracle.com/pcbpel/adapter/jms/BTS/SearchSchoolPOSReqABCSImpl/InsertErrorLoggerJMSProducer" xmlns:ns11="http://www.officedepot.com/officedepot/ODComnErrorStructure/1.0" id="BpPrc0">

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
    <partnerLinks>
        <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
        <partnerLink name="SearchSchool" partnerLinkType="ns1:SearchSchool" myRole="execute_ptt"/>
        <partnerLink name="SearchSchoolPOSDBAdapter" partnerLinkType="ns2:SearchSchoolPOSDBAdapter_plt" partnerRole="SearchSchoolPOSDBAdapter_role"/>
        <partnerLink name="InsertErrorLoggerJMSProducer" partnerLinkType="ns10:Produce_Message_plt" partnerRole="Produce_Message_role"/>
    </partnerLinks>

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <variables>
        <!-- Reference to the message passed as input during initiation -->

        <!-- Reference to the message that will be sent back to the requester during callback -->
        <variable name="receiveSearchInputVariable" messageType="ns1:requestMessage"/>
        <variable name="InvokeSearchSchoolDBAdapter_InputVariable" messageType="ns2:args_in_msg"/>
        <variable name="InvokeSearchSchoolDBAdapter_OutputVariable" messageType="ns2:args_out_msg"/>
        <variable name="FaultMessage" messageType="ns1:faultMessage"/>
        <variable name="SetCompositeTitle" type="xsd:string"/>
        <variable name="ReplySearchOutputVariable" messageType="ns1:replyMessage"/>
        <variable name="InvokeInsertErrorLogger_InputVariable" messageType="ns10:Produce_Message_msg"/>
    </variables>
    <faultHandlers id="BpFhs0">
        <catch faultName="bpelx:timeout" id="BpCFt0">
            <sequence name="Sequence2" id="BpSeq0">
                <assign name="AssignTimeOutMessage" id="BpAss0">
                    <copy>
                        <from expression="200"/>
                        <to variable="FaultMessage" part="SearchFault" query="/ns9:SearchSchoolIDPOSFaultMessage/ns9:ErrorCode"/>
                    </copy>
                    <copy>
                        <from expression="ora:getCompositeInstanceId()"/>
                        <to variable="FaultMessage" part="SearchFault" query="/ns9:SearchSchoolIDPOSFaultMessage/ns9:InstanceId"/>
                    </copy>
                    <copy>
                        <from expression="ora:setCompositeInstanceTitle('TimeOut Error Occured')"/>
                        <to variable="SetCompositeTitle"/>
                    </copy>
                    <copy>
                        <from expression="substring(ora:getFaultAsString(),1,100)"/>
                        <to variable="FaultMessage" part="SearchFault" query="/ns9:SearchSchoolIDPOSFaultMessage/ns9:ErrorMessage"/>
                    </copy>
                </assign>
                <reply name="ReplyTimeOut" variable="FaultMessage" partnerLink="SearchSchool" portType="ns1:execute_ptt" operation="execute" faultName="bpelx:remoteFault" id="BpRpl0"/>
                <terminate name="TerminateTimeOut" id="BpTer0"/>
            </sequence>
        </catch>
        <catch faultName="bpws:selectionFailure" id="BpCFt1">
            <sequence name="Sequence1" id="BpSeq1">
                <assign name="AssignNoDatafoundMessage" id="BpAss1">
                    <copy>
                        <from expression="100"/>
                        <to variable="FaultMessage" part="SearchFault" query="/ns9:SearchSchoolIDPOSFaultMessage/ns9:ErrorCode"/>
                    </copy>
                    <copy>
                        <from expression="ora:getCompositeInstanceId()"/>
                        <to variable="FaultMessage" part="SearchFault" query="/ns9:SearchSchoolIDPOSFaultMessage/ns9:InstanceId"/>
                    </copy>
                    <copy>
                        <from expression="ora:setCompositeInstanceTitle('No DataFound!')"/>
                        <to variable="SetCompositeTitle"/>
                    </copy>
                    <copy>
                        <from expression="'No records found in Back to School Database! '"/>
                        <to variable="FaultMessage" part="SearchFault" query="/ns9:SearchSchoolIDPOSFaultMessage/ns9:ErrorMessage"/>
                    </copy>
                </assign>
                <reply name="ReplyNoDataFound" variable="FaultMessage" partnerLink="SearchSchool" portType="ns1:execute_ptt" operation="execute" faultName="bpelx:remoteFault" id="BpRpl1"/>
                <terminate name="TerminateNoDataFound" id="BpTer1"/>
            </sequence>
        </catch>
        <catchAll id="BpCAl0">
            <sequence name="Sequence3" id="BpSeq2">
                <assign name="AssignCatchAll" id="BpAss2">
                    <copy>
                        <from expression="300"/>
                        <to variable="FaultMessage" part="SearchFault" query="/ns9:SearchSchoolIDPOSFaultMessage/ns9:ErrorCode"/>
                    </copy>
                    <copy>
                        <from expression="ora:getCompositeInstanceId()"/>
                        <to variable="FaultMessage" part="SearchFault" query="/ns9:SearchSchoolIDPOSFaultMessage/ns9:InstanceId"/>
                    </copy>
                    <copy>
                        <from expression="substring(ora:getFaultAsString(),1,200)"/>
                        <to variable="FaultMessage" part="SearchFault" query="/ns9:SearchSchoolIDPOSFaultMessage/ns9:ErrorMessage"/>
                    </copy>
                </assign>
                <reply name="ReplyCatchAll" faultName="bpelx:remoteFault" variable="FaultMessage" partnerLink="SearchSchool" portType="ns1:execute_ptt" operation="execute" id="BpRpl2"/>
                <assign name="AssignInsertErrorLogger" id="BpAss3">
                    <copy>
                        <from expression="ora:getCompositeName()"/>
                        <to variable="InvokeInsertErrorLogger_InputVariable" part="body" query="/ns11:ODComnErrorStructure/ns11:ODComnErrorStructureList/ns11:ProcessInfo/ns11:ProcessName"/>
                    </copy>
                    <copy>
                        <from expression="ora:getFaultAsString()"/>
                        <to variable="InvokeInsertErrorLogger_InputVariable" part="body" query="/ns11:ODComnErrorStructure/ns11:ODComnErrorStructureList/ns11:ErrorDetails/ns11:ErrorText"/>
                    </copy>
                    <copy>
                        <from expression="concat(xp20:current-dateTime(),' :')"/>
                        <to variable="InvokeInsertErrorLogger_InputVariable" part="body" query="/ns11:ODComnErrorStructure/ns11:ODComnErrorStructureList/ns11:ErrorDetails/ns11:ErrorDateTime"/>
                    </copy>
                    <copy>
                        <from expression="ora:getCompositeInstanceId()"/>
                        <to variable="InvokeInsertErrorLogger_InputVariable" part="body" query="/ns11:ODComnErrorStructure/ns11:ODComnErrorStructureList/ns11:ErrorDetails/ns11:RootID"/>
                    </copy>
                </assign>
                <invoke name="InvokeInsertErrorLogger" bpelx:invokeAsDetail="no" inputVariable="InvokeInsertErrorLogger_InputVariable" partnerLink="InsertErrorLoggerJMSProducer" portType="ns10:Produce_Message_ptt" operation="Produce_Message" id="BpInv0"/>
                <terminate name="TerminateCatchAll" id="BpTer2"/>
            </sequence>
        </catchAll>
    </faultHandlers>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="main" id="BpSeq3">
        <!-- Receive input from requestor. (Note: This maps to operation defined in SearchSchoolIDPOSReqABCSImplProcess.wsdl) -->
        <receive name="receiveSearchInput" partnerLink="SearchSchool" portType="ns1:execute_ptt" operation="execute" variable="receiveSearchInputVariable" createInstance="yes" id="BpRcv0"/>

        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <assign name="XformSeachInputToDBAdapterInput" id="BpAss4">
            <bpelx:annotation>
                <bpelx:pattern>transformation</bpelx:pattern>
            </bpelx:annotation>
            <copy>
                <from expression="ora:doXSLTransformForDoc('xsl/XformSeachInputToDBAdapterInput.xsl', $receiveSearchInputVariable.SearchRequest)"/>
                <to variable="InvokeSearchSchoolDBAdapter_InputVariable" part="InputParameters"/>
            </copy>
        </assign>
        <invoke name="InvokeSearchSchoolDBAdapter" bpelx:invokeAsDetail="no" inputVariable="InvokeSearchSchoolDBAdapter_InputVariable" outputVariable="InvokeSearchSchoolDBAdapter_OutputVariable" partnerLink="SearchSchoolPOSDBAdapter" portType="ns2:SearchSchoolPOSDBAdapter_ptt" operation="SearchSchoolPOSDBAdapter" id="BpInv1"/>
        <switch name="Switch1" id="BpSwt0">
            <case condition="bpws:getVariableData('InvokeSearchSchoolDBAdapter_OutputVariable','OutputParameters','/ns4:OutputParameters/ns4:RowSet')=''">
                <bpelx:annotation>
                    <bpelx:general>
                        <bpelx:property name="userLabel">NoDataFound</bpelx:property>
                    </bpelx:general>
                </bpelx:annotation>
                <throw name="ThrowNoDataFound" faultName="bpws:selectionFailure" id="BpThw0"/>
            </case>
            <otherwise>
                <assign name="TransformDBAdapterOutputToReplyOutput" id="BpAss5">
                    <bpelx:annotation>
                        <bpelx:pattern>transformation</bpelx:pattern>
                    </bpelx:annotation>
                    <copy>
                        <from expression="ora:doXSLTransformForDoc('xsl/XTransDBAdapterOutputToReplyOutput.xsl', $InvokeSearchSchoolDBAdapter_OutputVariable.OutputParameters)"/>
                        <to part="SearchResponse" variable="ReplySearchOutputVariable"/>
                    </copy>
                </assign>
            </otherwise>
        </switch>
        <reply name="ReplySearchOutput" partnerLink="SearchSchool" portType="ns1:execute_ptt" operation="execute" variable="ReplySearchOutputVariable" id="BpRpl3"/>
    </sequence>
</process><!--

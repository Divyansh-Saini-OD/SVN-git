<ViewObject
   Name="RequestTableVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.ar.irec.accountDetails.server.RequestTableVORowImpl"
   ComponentClass="oracle.apps.ar.irec.accountDetails.server.RequestTableVOImpl"
   FetchMode="FETCH_AS_NEEDED"
   FetchSize="25"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[

   SELECT cmreq.request_id, 
 cmreq.status,cmreq.creation_date, cmreq.total_amount, ct.trx_number,
ct.customer_trx_id, cmreq.url, cmreq.created_by,
ARI_UTILITIES.get_lookup_meaning('CREDIT_MEMO_REQUEST_STATUS', cmreq.status) as STATUS_MEANING, 
'IrREQDetails' as IR_REQ_Details, ct.invoice_currency_code, ct.bill_to_customer_id as customer_id,ct.bill_to_site_use_id as customer_site_use_id,
ct.trx_date,to_char(ct.trx_date, 'YYYY-MM-DD') StrTrxDate, users.user_name as created_by_name,rct.type transaction_details_class,
lk_reason.meaning reason, 
cmreq.last_update_date last_updated,
mo_global.get_ou_name(ct.org_id) as org_name,
to_char(ct.paying_customer_id) paying_customer_id,
to_char(ct.paying_site_use_id) paying_site_use_id,
ct.BILL_TO_SITE_USE_ID,
ct.SHIP_TO_SITE_USE_ID,
billto_uses.BILL_TO_NAME bill_to_location,
billto_uses.SHIP_TO_NAME ship_to_location,
cust.account_name account_name,
cust.account_number
FROM   
ra_cm_requests cmreq, 
fnd_user users ,
ra_customer_trx ct,
ra_cust_trx_types rct,
ar_lookups lk_reason,
ar_irec_user_acct_sites_all acctsites,
hz_cust_accounts cust,
XX_OM_HEADER_ATTRIBUTES_ALL    billto_uses   										
WHERE  
ct.customer_trx_id = cmreq.customer_trx_id 
AND cust.cust_account_id = acctsites.customer_id
AND rct.cust_trx_type_id = ct.cust_trx_type_id 
AND ct.attribute14 = billto_uses.header_id(+) 
AND cmreq.created_by = users.user_id
AND lk_reason.lookup_type = 'CREDIT_MEMO_REASON'
AND lk_reason.lookup_code = cmreq.cm_reason_code
 AND ct.bill_to_customer_id = acctsites.customer_id
AND(TRUNC(ct.trx_date)) >= trunc(decode( nvl(FND_PROFILE.VALUE('ARI_FILTER_TRXDATE_OLDER'), 0), 0, ct.trx_date, (sysdate-FND_PROFILE.VALUE('ARI_FILTER_TRXDATE_OLDER'))))
 AND acctsites.session_id = :1
 AND acctsites.user_id = :2
AND ct.invoice_currency_code = :3
AND ct.status_trx                = nvl(decode(:4,'OPEN', 'OP', 'CLOSED', 'CL', 
                                                   'PAST_DUE_INVOICE', 'OP', 'OIR_AGING_-99_01', 'OP', 
                                                   'OIR_AGING_1_30', 'OP', 'OIR_AGING_31_60', 'OP', 
                                                   'OIR_AGING_61_90', 'OP', 'OIR_AGING_91_180', 'OP', 
                                                   'OIR_AGING_181_365', 'OP', 'OIR_AGING_366_400', 'OP', 
                                                   'ANY_STATUS', ct.status_trx ),
                                                   ct.status_trx)
 AND cmreq.APPROVAL_DATE             <= decode(:5,'OPEN', cmreq.APPROVAL_DATE, 
                                                 'CLOSED', cmreq.APPROVAL_DATE, 
                                                 'PAST_DUE_INVOICE', sysdate, 
                                                 'OIR_AGING_-99_01', sysdate, 
                                                 'OIR_AGING_1_30', sysdate-30, 
                                                 'OIR_AGING_31_60', sysdate-60, 
                                                 'OIR_AGING_61_90', sysdate-90, 
                                                 'OIR_AGING_91_180', sysdate-180, 
                                                 'OIR_AGING_181_365', sysdate-365, 
                                                 'OIR_AGING_366_400', sysdate-400,
                                                 'ANY_STATUS', cmreq.APPROVAL_DATE )
AND ct.CUSTOMER_SITE_USE_ID = decode(acctsites,CUSTOMER_SITE_USE_ID, -1, ct.CUSTOMER_SITE_USE_ID, acctsites,CUSTOMER_SITE_USE_ID )
AND nvl(billto_uses.COST_CENTER_DEPT ,'-1')       = nvl(:6, nvl(billto_uses.COST_CENTER_DEPT,'-1'))
AND nvl(billto_uses.DESK_DEL_ADDR ,'-1')          = nvl(:7, nvl(billto_uses.DESK_DEL_ADDR,'-1'))
AND nvl(billto_uses.RELEASE_NUMBER ,'-1')         = nvl(:8, nvl(billto_uses.RELEASE_NUMBER,'-1'))
AND nvl(ct.PURCHASE_ORDER ,'-1')                  = nvl(:9, nvl(ct.PURCHASE_ORDER,'-1'))
AND nvl(cmreq.orig_trx_number, '-1')              = nvl(:10, nvl(cmreq.orig_trx_number ,'-1'))
AND cmreq.total_amount between nvl(:11, cmreq.total_amount-1) and nvl(:12, cmreq.total_amount+1)
AND trunc( cmreq.CREATION_DATE ) between trunc(nvl(to_date(:13), cmreq.CREATION_DATE-1)) and trunc(nvl(to_date(:14), cmreq.CREATION_DATE+1)) 
AND trunc( nvl(cmreq.DISPUTE_DATE,sysdate) ) between trunc(nvl(to_date(:15), nvl(cmreq.DISPUTE_DATE,sysdate)-1)) and trunc(nvl(to_date(:16), nvl(cmreq.DISPUTE_DATE,sysdate)+1))
   ]]></SQLQuery>

   Name="DMTableVO"
   Extends="oracle.apps.ar.irec.accountDetails.server.InvoiceTableVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.ar.irec.accountDetails.server.DMTableVORowImpl"
   ComponentClass="oracle.apps.ar.irec.accountDetails.server.DMTableVOImpl"
   FetchMode="FETCH_AS_NEEDED"
   FetchSize="25"
   Passivate="None"
   PageIterMode="Partial"      
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT ArPaymentSchedulesV.TRX_NUMBER, 
       ArPaymentSchedulesV.TRX_DATE,
       to_char(ArPaymentSchedulesV.TRX_DATE, 'YYYY-MM-DD') StrTrxDate,
       ArPaymentSchedulesV.INVOICE_CURRENCY_CODE, 
       ArPaymentSchedulesV.CUSTOMER_ID, 
       ArPaymentSchedulesV.CUSTOMER_SITE_USE_ID, 
       ArPaymentSchedulesV.CUSTOMER_TRX_ID, 
       ArPaymentSchedulesV.CUST_TRX_TYPE_ID, 
       ArPaymentSchedulesV.CLASS AS CLASS1, 
       ArPaymentSchedulesV.STATUS, 
       ArPaymentSchedulesV.TERM_ID, 
       ct.PURCHASE_ORDER AS CT_PURCHASE_ORDER, 
       ct.previous_customer_trx_id as CT_PREVIOUS_CUSTOMER_TRX_ID, 
       ArPaymentSchedulesV.AMOUNT_DUE_ORIGINAL, 
       ArPaymentSchedulesV.AMOUNT_DUE_REMAINING, 
       ArPaymentSchedulesV.PAYMENT_SCHEDULE_ID, 
       ArPaymentSchedulesV.TERMS_SEQUENCE_NUMBER, 
       ArPaymentSchedulesV.DUE_DATE,
       to_char(ArPaymentSchedulesV.DUE_DATE,'YYYY-MM-DD') StrDueDate,
       ArPaymentSchedulesV.CLASS AS CLASS2,
       (
        SELECT nvl
        (
        ( 
        SELECT fnd_message.get_string('AR', 'ARI_STATUS_OVERDUE') FROM dual WHERE ( trunc(sysdate) > ArPaymentSchedulesV.due_date ) AND ArPaymentSchedulesV.STATUS = 'OP'
        )
        , ARI_UTILITIES.get_lookup_meaning('PAYMENT_SCHEDULE_STATUS', ArPaymentSchedulesV.STATUS)
        )
        FROM DUAL
       ) as AR_LOOKUP_CODE_STATUS_MEANING,
       ct.INTERFACE_HEADER_CONTEXT,
       ct.INTERFACE_HEADER_ATTRIBUTE1,
       ct.INTERFACE_HEADER_ATTRIBUTE2,
       ct.INTERFACE_HEADER_ATTRIBUTE3,
       ct.INTERFACE_HEADER_ATTRIBUTE4,
       ct.INTERFACE_HEADER_ATTRIBUTE5,
       ct.INTERFACE_HEADER_ATTRIBUTE6,
       ct.INTERFACE_HEADER_ATTRIBUTE7,
       ct.INTERFACE_HEADER_ATTRIBUTE8,
       ct.INTERFACE_HEADER_ATTRIBUTE9,
       ct.INTERFACE_HEADER_ATTRIBUTE10,
       ct.INTERFACE_HEADER_ATTRIBUTE11,
       ct.INTERFACE_HEADER_ATTRIBUTE12,
       ct.INTERFACE_HEADER_ATTRIBUTE13,
       ct.INTERFACE_HEADER_ATTRIBUTE14,
       ct.INTERFACE_HEADER_ATTRIBUTE15,
       ct.ATTRIBUTE_CATEGORY,
       ct.ATTRIBUTE1,
       ct.ATTRIBUTE2,
       ct.ATTRIBUTE3,
       ct.ATTRIBUTE4,
       ct.ATTRIBUTE5,
       ct.ATTRIBUTE6,
       ct.ATTRIBUTE7,
       ct.ATTRIBUTE8,
       ct.ATTRIBUTE9,
       ct.ATTRIBUTE10,
       ct.ATTRIBUTE11,
       ct.ATTRIBUTE12,
       ct.ATTRIBUTE13,
       ct.ATTRIBUTE14,
       ct.ATTRIBUTE15,
       null account_number, --acct.account_number,
       null account_name, --acct.account_name,
       null selected_flag,
       null ERROR_EXISTS,
       ct.printing_option PRINTING_OPTION,
       to_char(ArPaymentSchedulesV.AMOUNT_IN_DISPUTE) AMOUNT_IN_DISPUTE,
       to_char(ArPaymentSchedulesV.AMOUNT_ADJUSTED_PENDING) AMOUNT_ADJUSTED_PENDING,
       decode(ct.PRINTING_PENDING, 'N', 'Yes', 'No') PRINTED,
       decode(ArPaymentSchedulesV.org_id, 404, 'OU_US', 403, 'OU_CA') as ORG_NAME,       
       to_char(ct.paying_customer_id) PAYING_CUSTOMER_ID,
       to_char(ct.paying_site_use_id) PAYING_SITE_USE_ID,
       ct.SHIP_DATE_ACTUAL SHIP_DATE,
       to_char(ct.SHIP_DATE_ACTUAL,'YYYY-MM-DD') StrShipDate,
       ArPaymentSchedulesV.FREIGHT_ORIGINAL FREIGHT_AMOUNT,
       ArPaymentSchedulesV.TAX_ORIGINAL TAX_AMOUNT,
       ArPaymentSchedulesV.PAYMENT_APPROVAL,
       decode(ArPaymentSchedulesV.CLASS,'PMT',null, 'APPROVED', 'Approved', 'HELD', 'On Hold', 'PENDING','Pending Approval') APPROVAL_STATUS,
       ct.BILL_TO_SITE_USE_ID,
       ct.SHIP_TO_SITE_USE_ID,
       oha.BILL_TO_NAME bill_to_location,
       oha.SHIP_TO_NAME ship_to_location, 
       NVL2(oha.ship_to_name,SUBSTR(oha.ship_to_name, 1, 25) || ', ','') ||
        NVL2(oha.ship_to_address1,SUBSTR(oha.ship_to_address1, 1, 25) || ', ','') || 
        NVL2(oha.ship_to_address2,SUBSTR(oha.ship_to_address2, 1, 25) || ', ','') ||
        NVL2(oha.ship_to_city,SUBSTR(oha.ship_to_city, 1, 25) || ', ','') || 
        NVL2(oha.ship_to_state,SUBSTR(oha.ship_to_state, 1, 25) || ', ','') ||
        oha.ship_to_zip                                SHIP_TO_NAME  
       ,ct.ship_to_site_use_id                         SHIP_TO_ID
FROM AR_PAYMENT_SCHEDULES            ArPaymentSchedulesV
,ra_customer_trx                ct
,ar_irec_user_acct_sites_all    auasa
,XX_OM_HEADER_ATTRIBUTES_ALL    oha   										
,XX_AR_CONSBILL_DELIVERED_V     cbd
WHERE ArPaymentSchedulesV.CLASS               = 'DM'
AND ArPaymentSchedulesV.customer_trx_id       = ct.customer_trx_id 
AND auasa.session_id                          = :1
AND auasa.USER_ID                             = :2
AND ArPaymentSchedulesV.customer_id           = auasa.customer_id
AND ArPaymentSchedulesV.invoice_currency_code = :3
AND ArPaymentSchedulesV.org_id                = auasa.org_id
AND ArPaymentSchedulesV.customer_site_use_id= decode(auasa.customer_site_use_id,-1,ArPaymentSchedulesV.customer_site_use_id, auasa.customer_site_use_id)
AND ct.attribute14                            = oha.header_id(+)                                    
--AND nvl(ct.batch_source_id, -1)               in (1007, 1003, 1002, 1001, -1)
AND cbd.cons_inv_id (+)                       = ArPaymentSchedulesV.cons_inv_id 
AND ArPaymentSchedulesV.status                = nvl(decode(:4,'OPEN', 'OP', 'CLOSED', 'CL', 
                                                   'PAST_DUE_INVOICE', 'OP', 'OIR_AGING_-99_01', 'OP', 
                                                   'OIR_AGING_1_30', 'OP', 'OIR_AGING_31_60', 'OP', 
                                                   'OIR_AGING_61_90', 'OP', 'OIR_AGING_91_180', 'OP', 
                                                   'OIR_AGING_181_365', 'OP', 'OIR_AGING_366_400', 'OP', 
                                                   'ANY_STATUS', ArPaymentSchedulesV.status ),
                                                   ArPaymentSchedulesV.status)
AND ArPaymentSchedulesV.DUE_DATE             <= decode(:5,'OPEN', ArPaymentSchedulesV.DUE_DATE, 
                                                 'CLOSED', ArPaymentSchedulesV.DUE_DATE, 
                                                 'PAST_DUE_INVOICE', sysdate, 
                                                 'OIR_AGING_-99_01', sysdate, 
                                                 'OIR_AGING_1_30', sysdate-30, 
                                                 'OIR_AGING_31_60', sysdate-60, 
                                                 'OIR_AGING_61_90', sysdate-90, 
                                                 'OIR_AGING_91_180', sysdate-180, 
                                                 'OIR_AGING_181_365', sysdate-365, 
                                                 'OIR_AGING_366_400', sysdate-400,
                                                 'ANY_STATUS', ArPaymentSchedulesV.DUE_DATE )
AND nvl(oha.COST_CENTER_DEPT ,'-1')           = nvl(:6, nvl(oha.COST_CENTER_DEPT,'-1'))
AND nvl(oha.DESK_DEL_ADDR ,'-1')              = nvl(:7, nvl(oha.DESK_DEL_ADDR,'-1'))
AND nvl(oha.RELEASE_NUMBER ,'-1')             = nvl(:8, nvl(oha.RELEASE_NUMBER,'-1'))
AND nvl(ct.PURCHASE_ORDER ,'-1')              = nvl(:9, nvl(ct.PURCHASE_ORDER,'-1'))
-- AND nvl(cbd.cons_inv_id, '-1')                = nvl(:10, nvl(cbd.cons_inv_id ,'-1')) -- Commented as per Version 1.2
AND nvl(cbd.cons_billing_number, '-1')        = nvl(:10, nvl(cbd.cons_billing_number ,'-1')) -- Added as per Version 1.2
AND ArPaymentSchedulesV.AMOUNT_DUE_ORIGINAL between nvl(:11, ArPaymentSchedulesV.AMOUNT_DUE_ORIGINAL-1) and nvl(:12, ArPaymentSchedulesV.AMOUNT_DUE_ORIGINAL+1)
AND trunc( ArPaymentSchedulesV.TRX_DATE ) between trunc(nvl(to_date(:13), ArPaymentSchedulesV.TRX_DATE-1)) and trunc(nvl(to_date(:14), ArPaymentSchedulesV.TRX_DATE+1)) 
AND trunc( ArPaymentSchedulesV.DUE_DATE ) between trunc(nvl(to_date(:15), ArPaymentSchedulesV.DUE_DATE-1)) and trunc(nvl(to_date(:16), ArPaymentSchedulesV.DUE_DATE+1))
   ]]></SQLQuery>

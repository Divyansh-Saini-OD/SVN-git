<ViewObject
   Name="UpdateClassVendorVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.wfh.server.UpdateClassVendorVORowImpl"
   ComponentClass="od.oracle.apps.xxmer.wfh.server.UpdateClassVendorVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT ClassVendorEO.ORG_ID, 
       ClassVendorEO.MERCH_CLASS_ID, 
       ClassVendorEO.VENDOR_SITE_ID,
       ClassVendorEO.PLANNER_ID,
       MerchClass.DESCRIPTION AS Merch_Class_Description,
       VendorSites.VENDOR_SITE_CODE AS Vendor_Description,
       EMPL.FIRST_NAME || ' ' || EMPL.LAST_NAME AS Planner_Name
  FROM XX_WFH_CLASS_VENDOR_ALL ClassVendorEO LEFT OUTER JOIN XX_TEMP_WFH_EMPLOYEE EMPL
       ON ClassVendorEO.PLANNER_ID = EMPL.EMPLOYEE_ID
     , XX_WFH_MERCH_CLASS MerchClass
     , PO_VENDOR_SITES_ALL VendorSites
 WHERE ClassVendorEO.ORG_ID = 141
   AND ClassVendorEO.MERCH_CLASS_ID = :1
   AND ClassVendorEO.VENDOR_SITE_ID = :2
   AND ClassVendorEO.MERCH_CLASS_ID = MerchClass.MERCH_CLASS_ID
   AND ClassVendorEO.VENDOR_SITE_ID = VendorSites.VENDOR_SITE_ID
   ]]></SQLQuery>

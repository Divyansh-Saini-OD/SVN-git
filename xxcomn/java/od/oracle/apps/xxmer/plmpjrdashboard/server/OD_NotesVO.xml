   <SQLQuery><![CDATA[
SELECT OD_NotesEO.NOTE_ID, 
       OD_NotesEO.NOTE_NUMBER, 
       OD_NotesEO.PROJECT_ID, 
       OD_NotesEO.TASK_ID, 
       OD_NotesEO.NOTE_DESC, 
       OD_NotesEO.CREATION_DATE, 
       OD_NotesEO.CREATED_BY, 
       OD_NotesEO.LAST_UPDATE_DATE, 
       OD_NotesEO.LAST_UPDATED_BY, 
       OD_NotesEO.LAST_UPDATE_LOGIN, 
       OD_NotesEO.ROWID
,(select full_name from per_all_people_f-- where upper(first_name) like upper('Gaurav')
                      where person_id in(
                            select employee_id from fnd_user
                            where user_id = od_noteseo.created_by
                            )
                            and sysdate between effective_start_Date and effective_end_date
                            ) AS SENT_FROM
FROM OD_PA_NOTES OD_NotesEO
   ]]></SQLQuery>

<ViewObject
   Name="xxGsoPoBagentPoplist"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxmer.xxGsoPlmPOTracking.server.xxGsoPoBagentPoplistImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select short_code from QA_PLAN_CHAR_VALUE_LOOKUPS
where (plan_id,char_id) in (select a.plan_id,b.char_id 
                              from  apps.qa_chars c,
                                    apps.qa_plan_chars b,
                                    apps.qa_plans a
                             where  a.name='OD_GSO_VENDOR_MASTER'
                               and  b.plan_id=a.plan_id
                               and  c.char_id=b.char_id
                               and  c.name='OD_SC_AUDIT_AGENT')
UNION
SELECT '--SELECT FROM LIST--' FROM DUAL
   ]]></SQLQuery>

<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="9.0.3.8.12_1330" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <oa:pageLayout id="PageLayoutRN" amDefName="od.oracle.apps.xxmer.allocation.server.AllocationAM" controllerClass="od.oracle.apps.xxmer.allocation.webui.AllocationCreateCO" windowTitle="Create New Allocation" title="New Allocation">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:contents>
            <oa:messageComponentLayout id="AllocationHeaderVO1">
               <ui:contents>
                  <oa:messageStyledText id="BatchNo" dataType="NUMBER" prompt="BatchNo" viewName="AllocationHeaderVO1" viewAttr="BatchNo"/>
                  <oa:messageStyledText id="BatchDesc" dataType="VARCHAR2" prompt="BatchDesc" viewName="AllocationHeaderVO1" viewAttr="BatchDesc" maximumLength="240"/>
                  <oa:messageStyledText id="ItemId" dataType="NUMBER" prompt="ItemId" viewName="AllocationHeaderVO1" viewAttr="ItemId" maximumLength="40"/>
                  <oa:messageStyledText id="Description" dataType="VARCHAR2" prompt="Description" viewName="AllocationHeaderVO1" viewAttr="Description" maximumLength="240"/>
                  <oa:messageChoice id="PO" pickListViewDef="" pickListViewName="POsCreateVO1" pickListDispAttr="Po" pickListValAttr="PoHeaderId" viewName="AllocationLinesVO1" viewAttr="PoHeaderId" allowBlankValue="true" prompt="PO" serverUnvalidated="true">
                     <ui:primaryClientAction>
                        <ui:firePartialAction event="shipToChange" unvalidated="true"/>
                     </ui:primaryClientAction>
                  </oa:messageChoice>
                  <oa:messageStyledText id="ShipTo" viewName="AllocationLinesVO1" viewAttr="Legacyshipto" prompt="Ship To" rendered="${oa.CreatePVO1.ShipToRender}"/>
                  <oa:messageChoice id="Alloc_Loc" rendered="${oa.CreatePVO1.ShipToRender}" viewName="AllocationLinesVO1" viewAttr="AllocOrganizationId" pickListViewName="StoresVO1" pickListDispAttr="Stores" pickListValAttr="OrganizationId" prompt="Allocation Location" serverUnvalidated="false" dataType="VARCHAR2">
                     <ui:primaryClientAction>
                        <ui:firePartialAction event="locationChange" formSubmitted="true" unvalidated="true"/>
                     </ui:primaryClientAction>
                  </oa:messageChoice>
                  <oa:messageTextInput id="qty" viewName="AllocationLinesVO1" viewAttr="AllocationQty" prompt="qty" rendered="${oa.CreatePVO1.QtyRender}"/>
               </ui:contents>
            </oa:messageComponentLayout>
            <oa:pageButtonBar id="PageButtons">
               <ui:contents>
                  <oa:submitButton id="Apply" text="Apply" use="/oracle/apps/fnd/attributesets/Buttons/Apply" rendered="${oa.CreatePVO1.QtyRender}"/>
                  <oa:submitButton id="Cancel" use="/oracle/apps/fnd/attributesets/Buttons/Cancel"/>
               </ui:contents>
            </oa:pageButtonBar>
         </ui:contents>
      </oa:pageLayout>
   </content>
</page>

<ViewObject
   Name="ODCompareHeaderInfo"
   Extends="oracle.apps.pos.isp.server.CompareHeaderInfo"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxptp.pos.isp.server.ODCompareHeaderInfoRowImpl"
   ComponentClass="oracle.apps.pos.isp.server.CompareHeaderInfoImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select po.segment1 as Po_Number, 
po.revision_num as revision, 
DECODE(PO.TYPE_LOOKUP_CODE, 
       'BLANKET', fnd_message.get_string('POS','POS_POTYPE_BLKT'), 
       'CONTRACT',fnd_message.get_string('POS','POS_POTYPE_CNTR'),
       'STANDARD',fnd_message.get_string('POS','POS_POTYPE_STD'),
       'PLANNED',fnd_message.get_string('POS','POS_POTYPE_PLND')) TYPE ,
po.currency_code as currency, 
hr1.location_code as ship_to_location,
po.ship_via_lookup_code as ship_via, 
po.fob_lookup_code as fob, 
TO_CHAR(POS_TOTALS_PO_SV.GET_PO_ARCHIVE_TOTAL( PO.PO_HEADER_ID, PO.REVISION_NUM,PO.TYPE_LOOKUP_CODE), 
        FND_CURRENCY.SAFE_GET_FORMAT_MASK(PO.CURRENCY_CODE, 30)) as total, 
hr2.location_code as bill_to_location,
terms.name as payment_terms, 
po.freight_terms_lookup_code as feight,
buyers.full_name as buyer, 
po.po_header_id as po_header_id, 
po.ship_to_location_id as ship_to_location_id,
po.bill_to_location_id as bill_to_location_id,
po.agent_id as agent_id,
to_number(null) as po_release_id,po.type_lookup_code,nvl(global_agreement_flag,'N'),po.org_id,
po.attribute_category po_type_code,
fdfc.descriptive_flex_context_name po_type_name
from po_headers_archive_all po, ap_terms_val_v terms, hr_locations_all_po_v hr1,
hr_locations_all_po_v hr2, pos_po_employee_details_v buyers,
fnd_descr_flex_contexts_vl fdfc
where po.terms_id=terms.term_id(+)
and po.ship_to_location_id=hr1.location_id 
and po.bill_to_location_id=hr2.location_id
and buyers.employee_id(+)=po.agent_id
and latest_external_flag='Y'
AND fdfc.descriptive_flex_context_code = po.attribute_category
AND fdfc.application_id = 201 
AND fdfc.descriptive_flexfield_name = 'PO_HEADERS' 
AND fdfc.descriptive_flex_context_code <> 'Global Data Elements'
union all
select
po.segment1||'-'||por.release_num as Po_Number,
por.revision_num,
DECODE(POR.RELEASE_TYPE,
           'BLANKET', FND_MESSAGE.GET_STRING('POS','POS_POTYPE_BLKTR'),
           'SCHEDULED',FND_MESSAGE.GET_STRING('POS','POS_POTYPE_PLNDR'))
      TYPE,
po.currency_code as currency, 
hr1.location_code as ship_to_location,
po.ship_via_lookup_code as ship_via, 
po.fob_lookup_code as fob,
TO_CHAR(POS_TOTALS_PO_SV.GET_RELEASE_ARCHIVE_TOTAL(POR.PO_RELEASE_ID, 
        POR.REVISION_NUM),FND_CURRENCY.SAFE_GET_FORMAT_MASK( PO.CURRENCY_CODE, 30)) as total, 
hr2.location_code as bill_to_location,
terms.name as payment_terms, 
po.freight_terms_lookup_code as feight,
buyers.full_name as buyer, 
po.po_header_id as po_header_id, 
po.ship_to_location_id as ship_to_location_id,
po.bill_to_location_id as bill_to_location_id,
po.agent_id as agent_id,
por.po_release_id as po_release_id,po.type_lookup_code,
nvl(global_agreement_flag,'N'),por.org_id,
po.attribute_category po_type_code,
fdfc.descriptive_flex_context_name po_type_name
from
     PO_HEADERS_ARCHIVE_ALL po, PO_RELEASES_ARCHIVE_ALL por,
     ap_terms_val_v terms, hr_locations_all_po_v hr1,
     hr_locations_all_po_v hr2, pos_po_employee_details_v buyers,
       fnd_descr_flex_contexts_vl fdfc
where po.po_header_id=por.po_header_id
and po.terms_id=terms.term_id(+)
and po.ship_to_location_id=hr1.location_id 
and po.bill_to_location_id=hr2.location_id
and buyers.employee_id(+)=po.agent_id
and por.latest_external_flag='Y'
and po.latest_external_flag='Y'
AND fdfc.descriptive_flex_context_code = po.attribute_category
AND fdfc.application_id = 201 
AND fdfc.descriptive_flexfield_name = 'PO_HEADERS' 
AND fdfc.descriptive_flex_context_code <> 'Global Data Elements'
   ]]></SQLQuery>

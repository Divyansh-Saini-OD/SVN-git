   Name="OD_ReqDetailsVO"
   Extends="oracle.apps.icx.por.reqmgmt.server.ReqDetailsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.icx.por.reqmgmt.server.OD_ReqDetailsVORowImpl"
   ComponentClass="od.oracle.apps.icx.por.reqmgmt.server.OD_ReqDetailsVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
	PRL.LINE_NUM,
	PRL.REQUISITION_LINE_ID,
	PRL.ITEM_DESCRIPTION,
	PRL.NEED_BY_DATE,
        PRL.UNIT_MEAS_LOOKUP_CODE,
	PRL.UNIT_PRICE, 
        PRL.QUANTITY,
	PRL.DELIVER_TO_LOCATION_ID,
	HLT.LOCATION_CODE DELIVER_TO_LOCATION,
	PRL.QUANTITY * PRL.UNIT_PRICE TOTAL,
	PRL.SOURCE_TYPE_CODE,
	PH.PO_HEADER_ID HEADER_ID,
	DECODE(PR.RELEASE_NUM, NULL, PH.CLM_DOCUMENT_NUMBER, PH.CLM_DOCUMENT_NUMBER||'-'||PR.RELEASE_NUM) ORDER_NUMBER,
	PRL.LINE_LOCATION_ID,
	PRL.AUCTION_HEADER_ID,
	PRL.AUCTION_LINE_NUMBER,
	DECODE(PRL.CANCEL_FLAG, 'Y', 'CancelCase',
	 'NoCancelCase') LINE_CANCELLED,
	PRL.REQUISITION_HEADER_ID,
	PRL.AMOUNT FUNCTIONAL_AMOUNT,
	PRL.CURRENCY_AMOUNT,
	PRL.CURRENCY_CODE,
	DECODE(PRL.CURRENCY_AMOUNT, null, null, PRL.CURRENCY_AMOUNT || ' ' || PRL.CURRENCY_CODE) TRXN_AMOUNT_CURRENCY,
        HOU.NAME PURCHASING_ORG,
--POR_VIEW_REQS_PKG.get_purch_org_for_line(PRL.REQUISIT--ION_LINE_ID) PURCHASING_ORG,
	PRL.CURRENCY_UNIT_PRICE,
	IIS.OPERATION_ALLOWED,
po_poapproval_init1.get_formatted_full_name(PRL.CANDIDATE_FIRST_NAME, PRL.CANDIDATE_LAST_NAME) CONTRACTOR_NAME,
	DECODE(PRL.LABOR_REQ_LINE_ID, NULL, FLV.MEANING, NULL) CONTRACTOR_STATUS,
	PRL.ITEM_SOURCE_ID,
	PRL.SUPPLIER_REF_NUMBER,
	DECODE(PRL.CURRENCY_UNIT_PRICE, null, null, PRL.CURRENCY_UNIT_PRICE || ' ' || PRL.CURRENCY_CODE) TRXN_PRICE_CURRENCY,
	PRL.SUGGESTED_VENDOR_PRODUCT_CODE,
	PR.PO_RELEASE_ID RELEASE_ID,
	'PurchaseOrderNumber' ORDER_NUMBER_SWITCHER,
	PRL.CANCEL_FLAG,
	DECODE(PRL.ORDER_TYPE_LOOKUP_CODE,'RATE', TO_NUMBER(NULL), 'FIXED PRICE',TO_NUMBER(NULL), NVL(PRL.QUANTITY_CANCELLED,0)) QUANTITY_CANCELLED,
	PAPF.FULL_NAME BUYER,
	DECODE(PR.PO_RELEASE_ID, 
		NULL, PH.AUTHORIZATION_STATUS,
 		PR.AUTHORIZATION_STATUS) ORDER_STATUS,
	DECODE(PR.PO_RELEASE_ID, 
		NULL, PH.USER_HOLD_FLAG,
 		PR.HOLD_FLAG) ORDER_HOLD_FLAG,
	DECODE(PR.PO_RELEASE_ID, 
		NULL, PH.CANCEL_FLAG,
 		PR.CANCEL_FLAG) ORDER_CANCEL_FLAG,
        DECODE(PRL.ORDER_TYPE_LOOKUP_CODE,'AMOUNT',NULL, UOMVL.UNIT_OF_MEASURE_TL) AS UNIT_OF_MEASURE_TL,
        PRL.MODIFIED_BY_AGENT_FLAG,
        PRL.LINE_NUM_DISPLAY,
        PRL.CLM_INFO_FLAG,
        PRL.PARENT_REQ_LINE_ID,
        PRL.CLM_OPTION_INDICATOR,
        PRL.CLM_BASE_LINE_NUM,
        PRL.GROUP_LINE_ID,
        PRL.CONTRACT_TYPE,
	PRL.UDA_TEMPLATE_ID,
	PRL.CLM_PERIOD_PERF_START_DATE,
	PRL.CLM_PERIOD_PERF_END_DATE,
POR_UTIL_PKG.get_pr_line_status(	PRL.REQUISITION_LINE_ID) as line_status,
        PRL.MATCHING_BASIS,
PRL.CLM_OPTION_TO_DATE,
PRL.CLM_OPTION_FROM_DATE,
PRL.CLM_OPTION_NUM,
PRL.ORDER_TYPE_LOOKUP_CODE,
PRL.CLM_OPTION_EXERCISED,
PRL.CLM_EXERCISED_DATE,
PRL.CONFORMED_LINE_ID,
PRL.CLM_MIPR_OBLIGATION_TYPE,
	DECODE(PRL.ORDER_TYPE_LOOKUP_CODE,'RATE', TO_NUMBER(NULL), 'AMOUNT', TO_NUMBER(NULL), 'FIXED PRICE', TO_NUMBER(NULL),NVL(PRL.QUANTITY_DELIVERED,0)) QUANTITY_DELIVERED,
	DECODE(PRL.ORDER_TYPE_LOOKUP_CODE,'RATE', TO_NUMBER(NULL), 'AMOUNT', TO_NUMBER(NULL), 'FIXED PRICE', TO_NUMBER(NULL),POR_UTIL_PKG.GET_OPEN_QUANTITY(PRL.REQUISITION_LINE_ID)) OPEN_QUANTITY,
    prl.spot_buy_flag,
	(SELECT msi.Segment1
        FROM mtl_system_items_B msi
        WHERE msi.inventory_item_id =  prl.item_id 
        AND msi.organization_id = nvl(prl.source_organization_id,prl.destination_organization_id)) item_name
FROM 	PO_REQUISITION_LINES_ALL PRL,
	PO_LINE_LOCATIONS_ALL PLL,
	HR_LOCATIONS_ALL HL,
	HR_LOCATIONS_ALL_TL HLT,
	PO_HEADERS_ALL PH,
	PO_RELEASES_ALL PR,
	ICX_POR_ITEM_SOURCES IIS,
	FND_LOOKUP_VALUES_VL FLV,
	PER_ALL_PEOPLE_F PAPF,
        HR_ALL_ORGANIZATION_UNITS_VL HOU,
        MTL_UNITS_OF_MEASURE_VL UOMVL
WHERE 	PRL.REQUISITION_HEADER_ID = :1 
AND      PRL.SOURCE_TYPE_CODE = 'VENDOR'
AND      PRL.LINE_LOCATION_ID = PLL.LINE_LOCATION_ID (+)
AND  	 PH.PO_HEADER_ID (+) = PLL.PO_HEADER_ID
AND      PH.ORG_ID = HOU.ORGANIZATION_ID (+)
AND	 PR.PO_RELEASE_ID (+) = PLL.PO_RELEASE_ID
AND	PRL.DELIVER_TO_LOCATION_ID = HL.LOCATION_ID(+) 
AND 	 HL.LOCATION_ID = HLT.LOCATION_ID (+) 
AND 	 HLT.LANGUAGE (+) = userenv('LANG')
AND	 IIS.ITEM_SOURCE_ID(+) = PRL.ITEM_SOURCE_ID
AND 	FLV.LOOKUP_TYPE (+) = 'ICX_POR_CONTRACTOR_STATUS'
AND 	FLV.LOOKUP_CODE (+) = PRL.CONTRACTOR_STATUS
AND 	PH.AGENT_ID = PAPF.PERSON_ID (+) 
AND     trunc(SYSDATE) BETWEEN 
       	trunc(nvl(PAPF.EFFECTIVE_START_DATE, SYSDATE)) AND
       	trunc(nvl(PAPF.EFFECTIVE_END_DATE, SYSDATE+1)) 
AND      UOMVL.UNIT_OF_MEASURE (+) = PRL.UNIT_MEAS_LOOKUP_CODE
UNION ALL
SELECT 
	PRL.LINE_NUM,
	PRL.REQUISITION_LINE_ID,
	PRL.ITEM_DESCRIPTION,
	PRL.NEED_BY_DATE,
	PRL.UNIT_MEAS_LOOKUP_CODE,
	PRL.UNIT_PRICE, 
	PRL.QUANTITY,
	PRL.DELIVER_TO_LOCATION_ID,
	HLT.LOCATION_CODE DELIVER_TO_LOCATION,
	PRL.QUANTITY * PRL.UNIT_PRICE TOTAL,
	PRL.SOURCE_TYPE_CODE,
	 OE_ORDER_IMPORT_INTEROP_PUB.Get_Header_Id ( PSP.order_source_id,PRH.segment1,PRL.requisition_header_id,'H' ,PRL.requisition_line_id) HEADER_ID,
	 TO_CHAR(OE_ORDER_IMPORT_INTEROP_PUB.Get_Order_Number(PSP.order_source_id,PRL.requisition_header_id,PRL.requisition_line_id)) ORDER_NUMBER,
	PRL.LINE_LOCATION_ID,
	PRL.AUCTION_HEADER_ID,
	PRL.AUCTION_LINE_NUMBER,
	DECODE(PRL.CANCEL_FLAG, 'Y', 'CancelCase', 
	'NoCancelCase') LINE_CANCELLED,
	PRL.REQUISITION_HEADER_ID,
	PRL.AMOUNT FUNCTIONAL_AMOUNT,
	PRL.CURRENCY_AMOUNT,
	PRL.CURRENCY_CODE,
	DECODE(PRL.CURRENCY_AMOUNT, null, null, PRL.CURRENCY_AMOUNT || ' ' || PRL.CURRENCY_CODE) TRXN_AMOUNT_CURRENCY,	null PURCHASING_ORG,
	PRL.CURRENCY_UNIT_PRICE,
	IIS.OPERATION_ALLOWED,
	PRL.CANDIDATE_FIRST_NAME || ' ' || PRL.CANDIDATE_LAST_NAME CONTRACTOR_NAME,
	NULL CONTRACTOR_STATUS,
	PRL.ITEM_SOURCE_ID,
	PRL.SUPPLIER_REF_NUMBER,
	DECODE(PRL.CURRENCY_UNIT_PRICE, null, null, PRL.CURRENCY_UNIT_PRICE || ' ' || PRL.CURRENCY_CODE) TRXN_PRICE_CURRENCY,
	PRL.SUGGESTED_VENDOR_PRODUCT_CODE,
	to_number(NULL) RELEASE_ID,
	'InternalOrderNumber' ORDER_NUMBER_SWITCHER,
	PRL.CANCEL_FLAG,
	DECODE(PRL.ORDER_TYPE_LOOKUP_CODE,'RATE', TO_NUMBER(NULL), 'FIXED PRICE', TO_NUMBER(NULL),NVL(PRL.QUANTITY_CANCELLED,0)) QUANTITY_CANCELLED,
	NULL BUYER,	por_view_reqs_pkg.get_so_number_status(prl.requisition_line_id)  ORDER_STATUS,
   	NULL ORDER_HOLD_FLAG,
        NULL ORDER_CANCEL_FLAG,
        DECODE(PRL.ORDER_TYPE_LOOKUP_CODE,'AMOUNT',NULL, UOMVL.UNIT_OF_MEASURE_TL) AS UNIT_OF_MEASURE_TL,
        PRL.MODIFIED_BY_AGENT_FLAG,
        PRL.LINE_NUM_DISPLAY,
        PRL.CLM_INFO_FLAG,
        PRL.PARENT_REQ_LINE_ID,
        PRL.CLM_OPTION_INDICATOR,
        PRL.CLM_BASE_LINE_NUM,
        PRL.GROUP_LINE_ID,
        PRL.CONTRACT_TYPE,
	PRL.UDA_TEMPLATE_ID,
	PRL.CLM_PERIOD_PERF_START_DATE,
	PRL.CLM_PERIOD_PERF_END_DATE,
POR_UTIL_PKG.get_pr_line_status(	PRL.REQUISITION_LINE_ID) as line_status,
        PRL.MATCHING_BASIS,
PRL.CLM_OPTION_TO_DATE,
PRL.CLM_OPTION_FROM_DATE,
PRL.CLM_OPTION_NUM,
PRL.ORDER_TYPE_LOOKUP_CODE,
PRL.CLM_OPTION_EXERCISED,
PRL.CLM_EXERCISED_DATE,
PRL.CONFORMED_LINE_ID,
PRL.CLM_MIPR_OBLIGATION_TYPE,
	DECODE(PRL.ORDER_TYPE_LOOKUP_CODE,'RATE', TO_NUMBER(NULL), 'AMOUNT', TO_NUMBER(NULL), 'FIXED PRICE', TO_NUMBER(NULL),NVL(PRL.QUANTITY_DELIVERED,0)) QUANTITY_DELIVERED,
	DECODE(PRL.ORDER_TYPE_LOOKUP_CODE,'RATE', TO_NUMBER(NULL), 'AMOUNT', TO_NUMBER(NULL), 'FIXED PRICE', TO_NUMBER(NULL),POR_UTIL_PKG.GET_OPEN_QUANTITY(PRL.REQUISITION_LINE_ID)) OPEN_QUANTITY,
    prl.spot_buy_flag,
	(SELECT msi.Segment1
	FROM mtl_system_items_B msi
	WHERE msi.inventory_item_id =  prl.item_id 
	AND msi.organization_id = nvl(prl.source_organization_id,prl.destination_organization_id)) item_name
FROM 	PO_REQUISITION_LINES_ALL PRL,
	PO_REQUISITION_HEADERS_ALL PRH,
	HR_LOCATIONS_ALL HL,
	HR_LOCATIONS_ALL_TL HLT,
	ORG_ORGANIZATION_DEFINITIONS OOD,  
	PO_SYSTEM_PARAMETERS_ALL PSP,
	ICX_POR_ITEM_SOURCES IIS,
        MTL_UNITS_OF_MEASURE_VL UOMVL
WHERE 	PRL.REQUISITION_HEADER_ID = :2 
AND      PRL.REQUISITION_HEADER_ID = PRH.REQUISITION_HEADER_ID
AND      PRL.SOURCE_TYPE_CODE = 'INVENTORY'
AND      OOD.ORGANIZATION_ID = PRL.SOURCE_ORGANIZATION_ID
AND      PSP.ORG_ID(+) = OOD.OPERATING_UNIT
AND	 IIS.ITEM_SOURCE_ID(+) = PRL.ITEM_SOURCE_ID
AND	PRL.DELIVER_TO_LOCATION_ID = HL.LOCATION_ID(+) 
AND 	 HL.LOCATION_ID = HLT.LOCATION_ID (+) 
AND 	 HLT.LANGUAGE (+)= userenv('LANG') 
AND      UOMVL.UNIT_OF_MEASURE = PRL.UNIT_MEAS_LOOKUP_CODE
ORDER BY LINE_NUM_DISPLAY,LINE_NUM ASC
   ]]></SQLQuery>

<ViewObject
   Name="ODConsBillVO"
   OrderBy="due_date desc, location"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxfin.ar.irec.accountDetails.server.ODConsBillVORowImpl"
   ComponentClass="od.oracle.apps.xxfin.ar.irec.accountDetails.server.ODConsBillVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT C.cons_billing_number CONS_BILL_ID,S.location,C.cut_off_date,C.due_date,C.beginning_balance,C.ending_balance
FROM AR_CONS_INV_ALL C LEFT OUTER JOIN HZ_CUST_SITE_USES_ALL S ON C.site_use_id=S.site_use_id
WHERE C.customer_id=:1 AND NVL(:2,C.site_use_id)=C.site_use_id
AND (C.attribute2 IS NOT NULL OR C.attribute4 IS NOT NULL OR C.attribute10 IS NOT NULL OR C.attribute15 IS NOT NULL)
   ]]></SQLQuery>

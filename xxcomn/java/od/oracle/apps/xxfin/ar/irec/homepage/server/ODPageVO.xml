<ViewObject
   Name="ODPageVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   Extends="oracle.apps.ar.irec.homepage.server.PageVO"   
   RowClass="od.oracle.apps.xxfin.ar.irec.homepage.server.ODPageVORowImpl"
   ComponentClass="od.oracle.apps.xxfin.ar.irec.homepage.server.ODPageVOImpl"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select TOTAL_OVERDUE_INVOICES,
       to_char(TOTAL_OVERDUE_INVOICES, fnd_currency_cache.get_format_mask(currency_code, 30))
       AS Overdue_Invoices_Formatted,
       TOTAL_OPEN_INVOICES,
       to_char(TOTAL_OPEN_INVOICES, fnd_currency_cache.get_format_mask(currency_code, 30)) 
       AS Open_Invoices_Formatted,
       UNAPPL_PAYMENTS,
       to_char(UNAPPL_PAYMENTS, fnd_currency_cache.get_format_mask(currency_code, 30)) AS Payments_Formatted,
       UNAPPLIED_CM,
       to_char(UNAPPLIED_CM, fnd_currency_cache.get_format_mask(currency_code, 30)) AS Unapplied_Cm_Formatted,
       PENDING_REQUEST,
       to_char(PENDING_REQUEST, fnd_currency_cache.get_format_mask(currency_code, 30)) 
       AS Pending_Request_Formatted,
       CURRENCY_CODE,
       CUSTOMER_ID,
       (TOTAL_OPEN_INVOICES +UNAPPL_PAYMENTS +UNAPPLIED_CM) AS ACCOUNT_BALANCE,
       to_char( (TOTAL_OPEN_INVOICES +UNAPPL_PAYMENTS +UNAPPLIED_CM),
       fnd_currency_cache.get_format_mask(currency_code, 30)) AS Account_Balance_Formatted,
       trunc(sysdate) as AS_OF_DATE,
       REMAINING_GUARANTEE,
       to_char(REMAINING_GUARANTEE, fnd_currency_cache.get_format_mask(currency_code, 30)) 
       AS Remaining_Guarantee_Formatted
from
(SELECT
  (
  SELECT nvl(SUM ( amount_due_remaining ), 0) AS SUM_AMOUNT 
    FROM AR_PAYMENT_SCHEDULES ArPaymentSchedules, ar_irec_user_acct_sites_all AcctSites
    WHERE 1=1
    AND AcctSites.user_id=:1
    AND AcctSites.session_id=:2  
    AND AcctSites.customer_id = ArPaymentSchedules.customer_id
    AND ArPaymentSchedules.invoice_currency_code = :3 
    AND ArPaymentSchedules.customer_site_use_id= decode(AcctSites.customer_site_use_id,-1,ArPaymentSchedules.customer_site_use_id, AcctSites.customer_site_use_id)
    AND status = 'OP'  
    AND ( class = 'INV' OR class = 'CB' OR class = 'DM' OR class = 'DEP' ) 
    AND (  trunc(sysdate) > due_date )
    AND ArPaymentSchedules.org_id=AcctSites.org_id
  ) AS total_overdue_invoices,
 ( 
   SELECT nvl(SUM ( amount_due_remaining ), 0) AS SUM_AMOUNT 
    FROM AR_PAYMENT_SCHEDULES ArPaymentSchedules, ar_irec_user_acct_sites_all AcctSites
    WHERE 1=1
    AND AcctSites.user_id=:4
    AND AcctSites.session_id=:5  
    AND AcctSites.customer_id = ArPaymentSchedules.customer_id
    AND ArPaymentSchedules.invoice_currency_code = :6 
    AND ArPaymentSchedules.customer_site_use_id= decode(AcctSites.customer_site_use_id,-1,ArPaymentSchedules.customer_site_use_id, AcctSites.customer_site_use_id)
    AND status = 'OP'  
    AND ( class = 'INV' OR class = 'CB' OR class = 'DM' OR class = 'DEP' ) 
    AND ArPaymentSchedules.org_id=AcctSites.org_id
  ) 
  AS total_open_invoices,
 (
    SELECT nvl(SUM ( amount_due_remaining ), 0) AS SUM_AMOUNT 
    FROM AR_PAYMENT_SCHEDULES ArPaymentSchedules, ar_irec_user_acct_sites_all AcctSites
    WHERE 1=1
    AND AcctSites.user_id=:7
    AND AcctSites.session_id=:8  
    AND AcctSites.customer_id = ArPaymentSchedules.customer_id
    AND ArPaymentSchedules.invoice_currency_code = :9 
    AND ArPaymentSchedules.customer_site_use_id= decode(AcctSites.customer_site_use_id,-1,ArPaymentSchedules.customer_site_use_id, AcctSites.customer_site_use_id)
    AND (status = 'OP'  AND amount_due_remaining <> 0 ) 
    AND class = 'PMT' 
    AND ArPaymentSchedules.org_id=AcctSites.org_id
  )
  AS unappl_payments,
 (
    SELECT nvl(SUM ( amount_due_remaining ), 0) AS SUM_AMOUNT 
    FROM AR_PAYMENT_SCHEDULES ArPaymentSchedules, ar_irec_user_acct_sites_all AcctSites
    WHERE 1=1
    AND AcctSites.user_id=:10
    AND AcctSites.session_id=:11  
    AND AcctSites.customer_id = ArPaymentSchedules.customer_id
    AND ArPaymentSchedules.invoice_currency_code = :12 
    AND ArPaymentSchedules.customer_site_use_id= decode(AcctSites.customer_site_use_id,-1,ArPaymentSchedules.customer_site_use_id, AcctSites.customer_site_use_id)
    AND class = 'CM' 
    AND status = 'OP'
  ) 
  AS unapplied_cm,
  ari_utilities.get_pending_cmreq_amount(:13,:14,:15,:16) AS pending_request,
  (
    SELECT nvl(SUM ( amount_due_remaining ), 0) AS SUM_AMOUNT 
    FROM AR_PAYMENT_SCHEDULES ArPaymentSchedules, ar_irec_user_acct_sites_all AcctSites
    WHERE 1=1
    AND AcctSites.user_id=:17
    AND AcctSites.session_id=:18  
    AND AcctSites.customer_id = ArPaymentSchedules.customer_id
    AND ArPaymentSchedules.invoice_currency_code = :19 
    AND ArPaymentSchedules.customer_site_use_id= decode(AcctSites.customer_site_use_id,-1,ArPaymentSchedules.customer_site_use_id, AcctSites.customer_site_use_id)
    AND status = 'OP'
    AND class = 'GUAR' 
  )
  AS Remaining_Guarantee,
  ( select :20 from dual ) AS CURRENCY_CODE,
  ( select to_char(:21) from dual ) AS CUSTOMER_ID
FROM dual)
   ]]></SQLQuery>

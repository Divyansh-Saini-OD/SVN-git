<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODPurchaseOrderVO"
   OrderBy="purchase_order"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxfin.ar.irec.accountDetails.server.ODPurchaseOrderVORowImpl"
   ComponentClass="od.oracle.apps.xxfin.ar.irec.accountDetails.server.ODPurchaseOrderVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT VALS.purchase_order,SITES.location
  FROM (SELECT DISTINCT trx.purchase_order, trx.bill_to_site_use_id
          FROM RA_CUSTOMER_TRX_ALL trx
         WHERE trx.bill_to_customer_id=:1
           AND trx.bill_to_site_use_id = NVL(:2,trx.bill_to_site_use_id)
           AND trx.purchase_order IS NOT NULL) VALS
LEFT OUTER JOIN HZ_CUST_SITE_USES_ALL SITES
ON SITES.site_use_id=VALS.bill_to_site_use_id
   ]]></SQLQuery>

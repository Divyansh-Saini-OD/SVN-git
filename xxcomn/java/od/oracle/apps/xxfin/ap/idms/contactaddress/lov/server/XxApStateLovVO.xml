<ViewObject
   xmlns="http://xmlns.oracle.com/bc4j"
   Name="XxApStateLovVO"
   Version="11.1.1.59.23"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxfin.ap.idms.contactaddress.lov.server.XxApStateLovVOImpl"
   PageIterMode="Partial"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select country_code, geography_name from HZ_GEOGRAPHIES where geography_type='STATE'
       AND COUNTRY_CODE = 'US'
       AND SYSDATE >= START_DATE
       AND SYSDATE <= NVL(END_DATE, SYSDATE)
  ORDER BY GEOGRAPHY_CODE
   ]]></SQLQuery>

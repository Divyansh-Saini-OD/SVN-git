select vv.flex_value,
       vv.description
from fnd_flex_value_sets vs,
     fnd_flex_values_vl vv
where vs.flex_value_set_id = vv.flex_value_set_id 
and   vs.flex_value_set_name = 'OD_AP_INVENTORY_TYPE'
   ]]></SQLQuery>

<ViewObject
   Name="ODTaskSummVO"
   Extends="oracle.apps.jtf.cac.task.server.TaskSummVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.jtf.cac.task.server.ODTaskSummVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.jtf.cac.task.server.ODTaskSummVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT TaskEO.TASK_ID, 
       TaskEO.TASK_TYPE_ID, 
       TaskEO.TASK_STATUS_ID, 
       jtl.TASK_NAME, 
       TaskEO.CALENDAR_START_DATE, 
       TaskEO.CALENDAR_END_DATE, 
       TaskEO.OBJECT_VERSION_NUMBER, 
       TaskEO.SOURCE_OBJECT_TYPE_CODE, 
       TaskEO.SOURCE_OBJECT_ID, 
       TaskEO.PLANNED_END_DATE, 
       TaskEO.SCHEDULED_END_DATE, 
       TaskEO.ACTUAL_END_DATE, 
       TaskEO.DATE_SELECTED, 
       TaskEO.CUSTOMER_ID,
       TaskEO.CUST_ACCOUNT_ID,
       TaskEO.ADDRESS_ID, 
       ts.NAME AS TASK_STATUS_NAME,
       tt.NAME AS TASK_TYPE_NAME,
       TaskEO.PRIVATE_FLAG,
       TaskEO.PLANNED_START_DATE,
       TaskEO.OWNER_ID,
       TaskEO.OWNER_TYPE_CODE,
TaskEO.TASK_NUMBER
FROM  jtf_tasks_b TaskEO,
      jtf_task_statuses_tl ts,
      jtf_task_types_tl tt, 
      jtf_tasks_tl jtl, 
      JTF_OBJECTS_TL Jot  
WHERE TaskEO.source_object_type_code = :1 
  AND TaskEO.source_object_id = :2
  AND TaskEO.task_status_id = ts.task_status_id
  AND ts.language = userenv('LANG')
  AND TaskEO.task_type_id = tt.task_type_id
  AND tt.language = userenv('LANG')
  AND TaskEO.task_id = jtl.task_id 
  AND jtl.language = USERENV('LANG') 
  AND NVL(TaskEO.deleted_flag,'N') = 'N' 
  AND Jot.object_code = TaskEO.source_object_type_code 
  AND Jot.language = jtl.language 
  AND TaskEO.entity = 'TASK'  
  AND (TaskEO.private_flag IS NULL OR TaskEO.private_flag = 'N' OR EXISTS (SELECT 'x' FROM jtf_task_all_assignments WHERE task_id = TaskEO.task_id AND (resource_type_code, resource_id) IN (SELECT 'RS_' || category, resource_id FROM jtf_rs_resource_extns WHERE user_id = fnd_global.user_id)))
   ]]></SQLQuery>

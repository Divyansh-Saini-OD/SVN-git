<ViewObject
   Name="ODHzCustAccountsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.hz.account.customer.server.ODHzCustAccountsVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.hz.account.customer.server.ODHzCustAccountsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select
HCA.cust_account_id,
HCA.party_id,
HCA.account_number,
HCA.account_name,
HCA.orig_system_reference,
HCA.status,
HCA.tax_code,
(select  COUNT(1)
from xx_cdh_cust_acct_ext_b ae,   
     ego_fnd_dsc_flx_ctx_ext edff
where ae.cust_account_id = HCA.cust_account_id
      and ae.attr_group_id = edff.attr_group_id
      and edff.descriptive_flexfield_name = 'XX_CDH_CUST_ACCOUNT'
      and  edff.descriptive_flex_context_code = 'SPC_INFO') numspccard,
(select REQUEST_ID 
from XX_CDH_ACCOUNT_SETUP_REQ
where cust_account_id = HCA.cust_account_id) request_id,
(select BILL_TO_SITE_ID 
from XX_CDH_ACCOUNT_SETUP_REQ
where cust_account_id = HCA.cust_account_id) billtositeid,
DECODE((select REQUEST_ID 
from XX_CDH_ACCOUNT_SETUP_REQ
where cust_account_id = HCA.cust_account_id),NULL,'DN','DY') details
from hz_cust_accounts HCA
where HCA.party_id = :1
   ]]></SQLQuery>

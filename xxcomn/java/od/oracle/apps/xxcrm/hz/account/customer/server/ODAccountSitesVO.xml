   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select hz_cust_acct_sites_all.PARTY_SITE_ID,
       hz_party_sites.party_site_number,
       hz_locations.ORIG_SYSTEM_REFERENCE,
       hz_cust_acct_sites_all.status,
       hz_format_pub.format_address(hz_locations.location_id,null,null,', ') as Address,
       hz_cust_site_uses_all.SITE_USE_CODE ,
       hz_cust_site_uses_all.PRIMARY_FLAG,
       hz_cust_acct_sites_all.cust_account_id cust_id,
       hz_cust_acct_sites_all.CUST_ACCT_SITE_ID CustSid
from hz_cust_acct_sites_all , 
     hz_party_sites,
     hz_locations,
     hz_cust_site_uses_all
where  hz_party_sites.PARTY_SITE_ID = hz_cust_acct_sites_all.PARTY_SITE_ID
and hz_party_sites.LOCATION_ID = hz_locations.LOCATION_ID
and hz_cust_site_uses_all.CUST_ACCT_SITE_ID = hz_cust_acct_sites_all.CUST_ACCT_SITE_ID
and hz_cust_acct_sites_all.CUST_ACCOUNT_ID = :1
   ]]></SQLQuery>

<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODEBillSubTotalFieldsVO"
   OrderBy="field_name"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.cdh.ebl.eblmain.server.ODEBillSubTotalFieldsVORowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT EF.field_id ,
  EF.field_name ,
  EF.include_in_core ,
  EF.include_in_detail ,
  EF.data_format ,
    xcetd.cust_doc_id
  FROM xx_cdh_ebilling_fields_v EF
       ,XX_CDH_EBL_TEMPL_DTL xcetd
  WHERE EF.field_id = xcetd.field_id
  AND   xcetd.attribute1 = 'Y'
  AND   xcetd.attribute20 = 'Y'
AND EF.include_in_standard = 'Y'
AND EF.field_id             IS NOT NULL
AND EF.field_name           IS NOT NULL
UNION
SELECT CAST (xcecf.CONC_FIELD_ID AS VARCHAR2(240)) field_id ,
  xcecf.conc_field_label field_name,
  'N' include_in_core ,
  'N'  include_in_detail,
  NULL data_format,
  xcetd.cust_doc_id
FROM XX_CDH_EBL_TEMPL_DTL xcetd,
  XX_CDH_EBL_CONCAT_FIELDS xcecf
WHERE xcetd.FIELD_ID  = xcecf.CONC_FIELD_ID
AND xcetd.cust_doc_id = xcecf.cust_doc_id
AND xcetd.attribute20 = 'Y'
UNION 
SELECT CAST (xcetd.FIELD_ID AS VARCHAR2(240)) field_id,
  xcetd.label field_name,
  'N' include_in_core  ,
  'N'  include_in_detail,
  NULL data_format,
  xcetd.cust_doc_id
FROM XX_CDH_EBL_TEMPL_DTL xcetd
WHERE base_field_id IS NOT NULL
AND attribute20      = 'Y'
   ]]></SQLQuery>

   RowClass="od.oracle.apps.xxcrm.cdh.ebl.lov.server.ODEBillCustDocIdLOVVORowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT N_EXT_ATTR2 cust_doc_id , cust_account_id
FROM XX_CDH_CUST_ACCT_EXT_B
WHERE attr_group_id =
  (SELECT attr_group_id
  FROM ego_attr_groups_v
  WHERE attr_group_type = 'XX_CDH_CUST_ACCOUNT'
  AND attr_group_name   = 'BILLDOCS'
  )
AND c_ext_attr2  ='N'
AND c_ext_attr16 ='COMPLETE'
AND SYSDATE BETWEEN d_ext_attr1 and NVL(d_ext_attr2, sysdate + 1)
   ]]></SQLQuery>

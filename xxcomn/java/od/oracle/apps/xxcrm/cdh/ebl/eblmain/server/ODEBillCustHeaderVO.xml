<ViewObject
   Name="ODEBillCustHeaderVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.cdh.ebl.eblmain.server.ODEBillCustHeaderVORowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT  HCA.account_name customer_name
      , HCA.account_number customer_number
      , SUBSTR(HCA.orig_system_reference,1,8) AOPS_number 
      , HCA.cust_account_id cust_account_id      
      , XCEB.n_ext_attr2 cust_doc_id
      , XCEB.n_ext_attr19 parent_doc_id
      , XCEB.c_ext_attr1 doc_type
      , XCEB.c_ext_attr2 pay_doc_ind
      , XCEB.c_ext_attr3 delivery_method
      , XCEB.c_ext_attr7 direct_doc
      , FLV.meaning status
      , XCEB.n_ext_attr17 is_parent
      , HCA.party_id
      , HP.party_name
      , DECODE(XCEB.c_ext_attr2,'Y','Yes','N','No') pay_doc_ind_disp
      , XCEB.c_ext_attr16 status_code
      , DECODE(XCEB.c_ext_attr7,'Y','Yes','N','No') direct_doc_disp
   FROM hz_cust_accounts HCA
      , xx_cdh_cust_acct_ext_b XCEB
      , ego_attr_groups_v EAG
      , hz_parties HP
      , fnd_lookup_values FLV
WHERE XCEB.attr_group_id = EAG.attr_group_id
  AND EAG.attr_group_type = 'XX_CDH_CUST_ACCOUNT'
  AND EAG.attr_group_name = 'BILLDOCS'
  AND HCA.cust_account_id = XCEB.cust_account_id
  AND HCA.party_id = HP.party_id
  AND FLV.lookup_code = XCEB.c_ext_attr16
  AND FLV.lookup_type = 'XXOD_EBL_DOCUMENT_STATUS'
  AND FLV.enabled_flag = 'Y'
  AND sysdate BETWEEN nvl(start_date_active,sysdate) AND nvl(end_date_active,sysdate)
   ]]></SQLQuery>

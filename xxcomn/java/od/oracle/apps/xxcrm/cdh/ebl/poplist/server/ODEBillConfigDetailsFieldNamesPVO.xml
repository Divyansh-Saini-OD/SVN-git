   <SQLQuery><![CDATA[
SELECT field_name ,
    field_id ,
    data_type ,
    data_format,
    null cust_doc_id
  FROM xx_cdh_ebilling_fields_v
UNION
SELECT 
  xcetd.label field_name,
  CAST (xcetd.FIELD_ID AS VARCHAR2(240)) field_id,
   NULL data_type,
  NULL data_format,
  xcetd.cust_doc_id
FROM XX_CDH_EBL_TEMPL_DTL xcetd
WHERE not exists (SELECT 1
  FROM xx_cdh_ebilling_fields_v xcefv where xcefv.field_id=xcetd.FIELD_ID)



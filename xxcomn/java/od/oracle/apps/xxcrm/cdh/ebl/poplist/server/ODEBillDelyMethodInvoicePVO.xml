<ViewObject
   Name="ODEBillDelyMethodInvoicePVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.cdh.ebl.poplist.server.ODEBillDelyMethodInvoicePVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT FFVV.FLEX_VALUE CODE,
       FFVV.FLEX_VALUE_MEANING DESCRIPTION
  FROM FND_FLEX_VALUES_VL FFVV,
       FND_FLEX_VALUE_SETS FFVS
WHERE ffvs.flex_value_set_id = ffvv.flex_value_set_id
AND ffvs.flex_value_set_name = 'XXOD_CDH_BILLDOCS_DELIVERY_METHOD_INV'
AND FFVV.ENABLED_FLAG        ='Y'
AND sysdate BETWEEN NVL(ffvv.start_date_active,sysdate) AND NVL(ffvv.end_date_active,sysdate)
   ]]></SQLQuery>

   ComponentClass="od.oracle.apps.xxcrm.cdh.ebl.ebltxtmain.poplist.server.ODEBillDynSplitFieldsVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select dtl.field_id code, NVL(dtl.label, val.source_value2) meaning, cust_doc_id
from XX_CDH_EBL_TEMPL_DTL_TXT dtl,
     xx_fin_translatedefinition def, 
     xx_fin_translatevalues val
WHERE 1=1  
   AND def.translate_id = val.translate_id
   AND def.translation_name = 'XX_CDH_EBL_TXT_DET_FIELDS'
   AND val.enabled_flag = 'Y'
   AND val.source_value1 IS NOT NULL
   AND val.source_value2 IS NOT NULL
   and val.source_value1=dtl.FIELD_ID   
   and dtl.cust_doc_id=:1
   ]]></SQLQuery>

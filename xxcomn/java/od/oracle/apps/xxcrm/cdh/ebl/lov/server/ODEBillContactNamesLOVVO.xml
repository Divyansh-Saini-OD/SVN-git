<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODEBillContactNamesLOVVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT HP.party_name contact_name
     , HOC.org_contact_id
     , HCA.cust_account_id
     , HCP.email_address
 FROM  hz_role_responsibility HRR
     , hz_cust_account_roles HCAR
     , hz_parties HP
     , hz_cust_accounts HCA
     , hz_relationships HR
     , hz_contact_points HCP
     , hz_org_contacts HOC     
 WHERE HCAR.cust_account_role_id = HRR.cust_account_role_id
   AND HCAR.cust_account_id      = HCA.cust_account_id
   AND HR.object_id              = HCA.party_id
   AND HR.relationship_code      = 'CONTACT_OF'
   AND HRR.responsibility_type   = 'BILLING'
   AND HCP.CONTACT_POINT_PURPOSE = 'BILLING'
   AND HCAR.party_id             = HR.party_id
   AND HR.subject_id             = HP.party_id
   AND HP.status                 = 'A'
   AND HCP.owner_table_name      = 'HZ_PARTIES'
   AND HCP.owner_table_id        = HR.party_id
   AND HCP.contact_point_type(+) = 'EMAIL'
   AND HOC.party_relationship_id = HR.relationship_id
   AND HCP.primary_flag          = 'Y'
   AND HCP.email_address IS NOT NULL
   ]]></SQLQuery>

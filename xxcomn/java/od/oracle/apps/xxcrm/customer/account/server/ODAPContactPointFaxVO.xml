<ViewObject
   Name="ODAPContactPointFaxVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select phone_country_code, phone_area_code, phone_number from hz_contact_points 
where 
contact_point_type = 'PHONE' 
and status = 'A' 
and phone_line_type = 'FAX'
and owner_table_id = (select party_id from hz_party_relationships where party_relationship_id = (select party_relationship_id from hz_org_contacts where org_contact_id = :1))
ORDER BY primary_flag DESC, CREATION_DATE DESC
   ]]></SQLQuery>

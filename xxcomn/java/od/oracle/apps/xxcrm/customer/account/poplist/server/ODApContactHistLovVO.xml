<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODApContactHistLovVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.customer.account.poplist.server.ODApContactHistLovVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.customer.account.poplist.server.ODApContactHistLovVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
  hzoc.org_contact_id lookup_code,
  hzp.person_last_name||', '||hzp.person_first_name meaning 
FROM 
  hz_parties hzp,
  hz_party_relationships hzpr,
  hz_org_contacts hzoc
  WHERE hzp.party_id = hzpr.subject_id
  AND hzpr.object_id = :1
  AND hzp.party_type = 'PERSON'
  and hzoc.party_relationship_id = hzpr.party_relationship_id
   ]]></SQLQuery>

   RowClass="od.oracle.apps.xxcrm.customer.account.server.ODContractTemplatesVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.customer.account.server.ODContractTemplatesVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT ODCdhContractTemplateEO.CONTRACT_TEMPLATE_ID, 
       ODCdhContractTemplateEO.TEMPLATE_NAME, 
       ODCdhContractTemplateEO.REVENUE_BAND_CODE, 
       ODCdhContractTemplateEO.CONTRACT_DESCRIPTION, 
       ODCdhContractTemplateEO.PRIORITY, 
       ODCdhContractTemplateEO.CUSTOM, 
       ODCdhContractTemplateEO.CREATED_BY, 
       ODCdhContractTemplateEO.CREATION_DATE, 
       ODCdhContractTemplateEO.LAST_UPDATED_BY, 
       ODCdhContractTemplateEO.LAST_UPDATE_DATE, 
       ODCdhContractTemplateEO.LAST_UPDATE_LOGIN, 
       ODCdhContractTemplateEO.SALES_ROLE, 
       ODCdhContractTemplateEO.CONTRACT_NUMBER, 
       ODCdhContractTemplateEO.CUST_PROD_XREF, 
       ODCdhContractTemplateEO.OFF_CONTRACT_CODE, 
       ODCdhContractTemplateEO.OFF_CONTRACT_PERCENT, 
       ODCdhContractTemplateEO.OFF_WHOLESALE_CODE, 
       ODCdhContractTemplateEO.OFF_WHOLESALE_PERCENT, 
       ODCdhContractTemplateEO.GP_FLOOR_PERCENT, 
       ODCdhContractTemplateEO.FREIGHT_CHARGE, 
       ODCdhContractTemplateEO.PARENT_ID,
       ODCdhContractTemplateEO.TEMPLATE_LOCKED_FLAG,
       ODCdhContractTemplateEO.PRICEPLAN_LOCKED_FLAG,
       ODCdhContractTemplateEO.ASSNCONTR_LOCKED_FLAG,
      ODCdhContractTemplateEO.PRICEPLAN_MANDATORY_FLAG,
	   FFV.FLEX_VALUE REV
FROM XX_CDH_CONTRACT_TEMPLATE ODCdhContractTemplateEO,
  FND_FLEX_VALUES FFV,
  FND_FLEX_VALUE_SETS FFVS
WHERE FFV.FLEX_VALUE_SET_ID = FFVS.FLEX_VALUE_SET_ID
  AND UPPER(FFVS.FLEX_VALUE_SET_NAME) in ( UPPER('XXOD_CUST_US_REVENUE_BAND'), 
      UPPER('XXOD_CUST_CA_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_EUR_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_GB_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_IE_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_IL_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_JP_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_NL_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_US_REVENUE_BAND'))
  AND ENABLED_FLAG = 'Y'  
  AND SYSDATE BETWEEN NVL(START_DATE_ACTIVE,SYSDATE)AND NVL(END_DATE_ACTIVE,SYSDATE + 1)
  AND FFV.FLEX_VALUE_ID = ODCdhContractTemplateEO.REVENUE_BAND_CODE
   ]]></SQLQuery>

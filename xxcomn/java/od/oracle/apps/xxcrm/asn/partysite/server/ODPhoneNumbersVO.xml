<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<!-- $Header: ODPhoneNumbersVO.xml $ -->

<ViewObject
   Name="ODPhoneNumbersVO"
   OrderBy="primary_flag DESC"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.asn.partysite.server.ODPhoneNumbersVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.partysite.server.ODPhoneNumbersVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
  DECODE(ContactPointEO.primary_flag,'Y','P','O') primary_flag,
  ContactPointEO.contact_point_purpose,
  ContactPointEO.Do_NOT_use_flag,
  TZ.TIMEZONE_CODE time_zone,
  ContactPointEO.contact_point_id,
  ContactPointEO.contact_point_type,
  '+' || ContactPointEO.phone_country_code || ' '||
  ContactPointEO.phone_area_code || ' ' ||
  ContactPointEO.phone_number || ' ' ||
  ContactPointEO.phone_extension phone_number,
  ContactPointEO.owner_table_id, ContactPointEO.owner_table_name,
'UEN' UpdateUENUDI,
'RE' RemoveRERD,
lookup.meaning
FROM 
  hz_contact_points ContactPointEO,
  fnd_lookup_values lookup,
  fnd_timezones_vl TZ
WHERE 
  ContactPointEO.contact_point_type = 'PHONE'
  and ContactPointEO.owner_table_name = 'HZ_PARTY_SITES' 
  and ContactPointEO.PHONE_LINE_TYPE = lookup.lookup_code
  and trunc(nvl(lookup.start_date_active, SYSDATE)) <= trunc(SYSDATE)
AND    trunc(nvl(lookup.end_date_active, SYSDATE))  >= trunc(SYSDATE)
AND    lookup.enabled_flag = 'Y'
AND    lookup.language = USERENV('LANG')
AND    lookup.lookup_type ='XX_ASN_ADDR_BK_TYPES'
AND   ContactPointEO.TIMEZONE_ID = TZ.UPGRADE_TZ_ID (+)
and ContactPointEO.owner_table_id = :1
   ]]></SQLQuery>

   Name="ODLeadAccessDetailsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.asn.lead.server.ODLeadAccessDetailsVORowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
  TERR_ENT.NAMED_ACCT_TERR_ENTITY_ID  ACCESS_ID,
  TERR_ENT.ENTITY_ID                  SALES_LEAD_ID,
  TERR_RSC.RESOURCE_ID                SALESFORCE_ID,
  TERR_RSC.GROUP_ID                   SALES_GROUP_ID,
  NVL(TERR_ENT.FULL_ACCESS_FLAG,'Y')  TEAM_LEADER_FLAG,
  NVL(TERR_ENT.FULL_ACCESS_FLAG,'Y')  OWNER_FLAG,
  'N'                                 FREEZE_FLAG,
  JRB.SOURCE_JOB_TITLE,
  JRB.SOURCE_PHONE,
  JRB.SOURCE_EMAIL,
  JRB.SOURCE_NAME AS RESOURCE_NAME,
  HZP.PARTY_ID                       CUSTOMER_ID,
  1                                  OBJECT_VERSION_NUMBER,
  TERR.ATTRIBUTE_CATEGORY,
  TERR.ATTRIBUTE1,
  TERR.ATTRIBUTE2,
  TERR.ATTRIBUTE3,
  TERR.ATTRIBUTE4,
  TERR.ATTRIBUTE5,
  TERR.ATTRIBUTE6,
  TERR.ATTRIBUTE7,
  TERR.ATTRIBUTE8,
  TERR.ATTRIBUTE9,
  TERR.ATTRIBUTE10,
  TERR.ATTRIBUTE11,
  TERR.ATTRIBUTE12,
  TERR.ATTRIBUTE13,
  TERR.ATTRIBUTE14,
  TERR.ATTRIBUTE15
FROM 
  XX_TM_NAM_TERR_DEFN         TERR,
  XX_TM_NAM_TERR_ENTITY_DTLS  TERR_ENT,
  XX_TM_NAM_TERR_RSC_DTLS     TERR_RSC,
  AS_SALES_LEADS              ASLA,
  HZ_PARTIES                  HZP,
  JTF_RS_RESOURCE_EXTNS       JRB
WHERE 
  TERR.NAMED_ACCT_TERR_ID = TERR_ENT.NAMED_ACCT_TERR_ID AND
  TERR.NAMED_ACCT_TERR_ID = TERR_RSC.NAMED_ACCT_TERR_ID AND
  ASLA.CUSTOMER_ID = HZP.PARTY_ID AND
  ASLA.SALES_LEAD_ID = TERR_ENT.ENTITY_ID AND
  TERR_RSC.RESOURCE_ID = JRB.RESOURCE_ID AND
  TERR_ENT.ENTITY_TYPE = 'LEAD' AND
  JRB.CATEGORY = 'EMPLOYEE' AND
  SYSDATE BETWEEN NVL(TERR.START_DATE_ACTIVE,SYSDATE-1) AND
  NVL(TERR.END_DATE_ACTIVE,SYSDATE+1) AND
  SYSDATE BETWEEN NVL(TERR_ENT.START_DATE_ACTIVE,SYSDATE-1) AND
  NVL(TERR_ENT.END_DATE_ACTIVE,SYSDATE+1) AND
  SYSDATE BETWEEN NVL(TERR_RSC.START_DATE_ACTIVE,SYSDATE-1) AND
  NVL(TERR_RSC.END_DATE_ACTIVE,SYSDATE+1)
  AND NVL(TERR_ENT.STATUS,'A') = 'A'
   ]]></SQLQuery>

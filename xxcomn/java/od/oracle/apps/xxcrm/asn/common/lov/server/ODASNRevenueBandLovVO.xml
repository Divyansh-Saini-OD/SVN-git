<ViewObject
   Name="ODASNRevenueBandLovVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.asn.common.lov.server.ODASNRevenueBandLovVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.common.lov.server.ODASNRevenueBandLovVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
  FFV.FLEX_VALUE_ID LOOKUP_CODE,
  FFV.FLEX_VALUE MEANING, 
  FFVS.DESCRIPTION DESCRIPTION
FROM 
  FND_FLEX_VALUES FFV,
  FND_FLEX_VALUE_SETS FFVS
  WHERE FFV.FLEX_VALUE_SET_ID = FFVS.FLEX_VALUE_SET_ID
  AND UPPER(FFVS.FLEX_VALUE_SET_NAME) in ( UPPER('XXOD_CUST_US_REVENUE_BAND'), 
      UPPER('XXOD_CUST_CA_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_EUR_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_GB_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_IE_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_IL_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_JP_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_NL_REVENUE_BAND'), 
	  UPPER('XXOD_CUST_US_REVENUE_BAND'))
  AND ENABLED_FLAG = 'Y'  
  AND SYSDATE BETWEEN NVL (START_DATE_ACTIVE,
  SYSDATE)AND NVL (END_DATE_ACTIVE,  SYSDATE + 1)
   ]]></SQLQuery>

   Name="ODHzPuiDQMSrchResultsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.asn.common.customer.server.ODHzPuiDQMSrchResultsVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.common.customer.server.ODHzPuiDQMSrchResultsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select hzp.party_id, 
       hzp.party_name, 
       hzp.party_type, 
       fnd1.meaning type_lookup, 
       hzp.party_number, 
       hzp.tax_reference,
       hzp.jgzz_fiscal_code taxpayer_id,
       hzp.duns_number_c duns_number, 
       hzp.known_as known_as, 
       hzp.known_as2 known_as2,
       hzp.known_as3 known_as3,
       hzp.known_as4 known_as4,
       hzp.known_as5 known_as5,
       hzp.organization_name_phonetic,
       hzp.person_first_name_phonetic,
       hzp.person_last_name_phonetic,
       hzps.location_id, 
       (select terr.territory_short_name from fnd_territories_vl terr where loc.country =terr.territory_code) country,
       hzp.primary_phone_country_code,
       hzp.primary_phone_area_code,
       hzp.primary_phone_number,
       hzp.primary_phone_line_type, 
       hzp.primary_phone_extension,
       hzp.email_address email,
       hzp.url primary_url, 
       fnd2.meaning certification_level_meaning,
       score, 
       (score||'%') percentage,
       'N' Select_flag, 
       'N' disable_hgrid_flag, 
       'Y' enable_hgrid_flag, 
       '' rel_party_id,
  hz_format_pub.format_address(hzps.location_id,null,null,' ') ADDRESS,
  hzps.party_site_number,
  hzr.subject_id 
from   hz_party_sites hzps, 
hz_parties hzp,  -- Person 
hz_parties hzcp, -- Party_Relationship 
hz_relationships hzr, 
hz_locations loc, 
hz_org_contacts  hoc, 
hz_party_sites_ext_vl ODHzPartySitesExtEO,
     -- fnd_territories_vl terr, 
       hz_matched_parties_gt mpg,
       fnd_lookup_values fnd1,
       fnd_lookup_values fnd2,
       EGO_ATTR_GROUPS_V     eagv
where  mpg.party_id = hzp.party_id     
and    search_context_id = :1
and hzps.party_id = hzr.subject_id 
AND   hzps.location_id =loc.location_id (+) 
and  hzr.subject_type = 'ORGANIZATION' 
AND  hzr.object_id = hzp.party_id 
AND    hzr.party_id = hzcp.party_id 
AND  hzr.relationship_id= hoc.party_relationship_id 
and	   ODHzPartySitesExtEO.ATTR_GROUP_ID = eagv.ATTR_GROUP_ID
and	   eagv.ATTR_GROUP_TYPE = 'HZ_PARTY_SITES_GROUP'
and	   eagv.ATTR_GROUP_NAME = 'SITE_CONTACTS'
AND  hzps.party_site_id = ODHzPartySitesExtEO.party_site_id 
AND  ODHzPartySitesExtEO.language =userenv('LANG') AND 
ODHzPartySitesExtEO.n_ext_attr1 = hzr.relationship_id 
AND  TRUNC(SYSDATE) BETWEEN nvl(TRUNC(hzr.start_date),  TRUNC(sysdate-1)) 
AND nvl(TRUNC(hzr.end_date),   TRUNC(sysdate+1)) 
AND  hzr.status = 'A' 
AND  TRUNC(SYSDATE) BETWEEN nvl(TRUNC(ODHzPartySitesExtEO.d_ext_attr1),  TRUNC(sysdate-1)) 
AND nvl(TRUNC(ODHzPartySitesExtEO.d_ext_attr2), 
TRUNC(sysdate+1)) 
AND  ODHzPartySitesExtEO.c_ext_attr1 = 'A'
and    fnd1.view_application_id(+) = 222
and    fnd1.lookup_type(+) = 'PARTY_TYPE'
and    fnd1.language(+) = userenv('LANG')
and    fnd1.lookup_code(+) = hzp.party_type
and    fnd2.view_application_id(+) = 222
and    fnd2.lookup_type(+) = 'HZ_PARTY_CERT_LEVEL'
and    fnd2.language(+) = userenv('LANG')
and    fnd2.lookup_code(+) = hzp.certification_level
UNION ALL
select hzp.party_id, 
       hzp.party_name, 
       hzp.party_type, 
       fnd1.meaning type_lookup,
       hzp.party_number, 
       hzp.tax_reference,
       hzp.jgzz_fiscal_code taxpayer_id,
       hzp.duns_number_c duns_number, 
       hzp.known_as known_as, 
       hzp.known_as2 known_as2,
       hzp.known_as3 known_as3,
       hzp.known_as4 known_as4,
       hzp.known_as5 known_as5,
       hzp.organization_name_phonetic, 
       hzp.person_first_name_phonetic, 
       hzp.person_last_name_phonetic, 
       hzps.location_id, 
       (select terr.territory_short_name from fnd_territories_vl terr where loc.country =terr.territory_code) country,
       hzp.primary_phone_country_code,
       hzp.primary_phone_area_code,
       hzp.primary_phone_number,
       hzp.primary_phone_line_type, 
       hzp.primary_phone_extension,
       hzp.email_address email, 
       hzp.url primary_url, 
       fnd2.meaning certification_level_meaning,
       100 score, 
       (100||'%') percentage,
       'N' Select_flag, 
       'N' disable_hgrid_flag, 
       'Y' enable_hgird_flag, 
       '' rel_party_id,
  hz_format_pub.format_address(hzps.location_id,null,null,' ') ADDRESS,
  hzps.party_site_number,
  hzr.subject_id
from    hz_party_sites hzps, 
hz_parties hzp,  -- Person 
hz_parties hzcp, -- Party_Relationship 
hz_relationships hzr, 
hz_locations loc, 
hz_org_contacts  hoc, 
hz_party_sites_ext_vl ODHzPartySitesExtEO,
    --   fnd_territories_vl terr,
      fnd_lookup_values fnd1,
       fnd_lookup_values fnd2,
	   EGO_ATTR_GROUPS_V     eagv
where  
hzp.party_id = :2 and    
hzps.party_id = hzr.subject_id 
AND   hzps.location_id =loc.location_id (+) 
and  hzr.subject_type = 'ORGANIZATION' 
AND  hzr.object_id = hzp.party_id 
AND    hzr.party_id = hzcp.party_id 
AND  hzr.relationship_id= hoc.party_relationship_id 
and	   ODHzPartySitesExtEO.ATTR_GROUP_ID = eagv.ATTR_GROUP_ID
and	   eagv.ATTR_GROUP_TYPE = 'HZ_PARTY_SITES_GROUP'
and	   eagv.ATTR_GROUP_NAME = 'SITE_CONTACTS'
AND  hzps.party_site_id = ODHzPartySitesExtEO.party_site_id 
AND  ODHzPartySitesExtEO.language =userenv('LANG') AND 
ODHzPartySitesExtEO.n_ext_attr1 = hzr.relationship_id 
AND  TRUNC(SYSDATE) BETWEEN nvl(TRUNC(hzr.start_date),  TRUNC(sysdate-1)) 
AND nvl(TRUNC(hzr.end_date),   TRUNC(sysdate+1)) 
AND  hzr.status = 'A' 
AND  TRUNC(SYSDATE) BETWEEN nvl(TRUNC(ODHzPartySitesExtEO.d_ext_attr1),  TRUNC(sysdate-1)) 
AND nvl(TRUNC(ODHzPartySitesExtEO.d_ext_attr2), 
TRUNC(sysdate+1)) 
AND  ODHzPartySitesExtEO.c_ext_attr1 = 'A'
and    fnd1.view_application_id(+) = 222
and    fnd1.lookup_type(+) = 'PARTY_TYPE'
and    fnd1.language(+) = userenv('LANG')
and    fnd1.lookup_code(+) = hzp.party_type
and    fnd2.view_application_id(+) = 222
and    fnd2.lookup_type(+) = 'HZ_PARTY_CERT_LEVEL'
and    fnd2.language(+) = userenv('LANG')
and    fnd2.lookup_code(+) = hzp.certification_level
order by score desc
   ]]></SQLQuery>

<ViewObject
   Name="ODSicCodeLovVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.common.customer.lov.server.ODSicCodeLovVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT lookup_code as display_code, description as meaning
  FROM apps.fnd_lookup_values
 WHERE lookup_type = 'XX_SFA_OD_VERTICAL_MARKET'
   AND enabled_flag = 'Y'
   AND TRUNC (SYSDATE) BETWEEN TRUNC (start_date_active)
                           AND TRUNC (NVL (end_date_active, SYSDATE))
   ]]></SQLQuery>

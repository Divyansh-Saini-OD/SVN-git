<?xml version="1.0" encoding='windows-1252'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="OD_MPSPartyListVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.lov.server.OD_MPSPartyListVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select distinct cb.party_id, 
                cb.party_name,
                cb.contract_number
               from xx_cs_mps_device_b cb
              where -- cb.party_id = nvl(p_group_id,cb.party_id)              and 
              cb.program_type in (select meaning
                      from cs_lookups
                      where lookup_type = 'XX_MPS_PROGRAM_TYPES'
                      --and tag in ('BOTH', 'USAGE')
                      and end_date_active is null)
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_version" Value="9.0.3.14.01" />
      <Attr Name="_CodeGenFlagNew" Value="36" />
   </DesignTime>
   <ViewAttribute
      Name="PartyId"
      IsPersistent="false"
      Type="oracle.jbo.domain.Number"
      ColumnType="NUMBER"
      AliasName="PARTY_ID"
      Expression="PARTY_ID"
      SQLType="NUMERIC" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="22" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="PartyName"
      IsPersistent="false"
      Precision="250"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="PARTY_NAME"
      Expression="PARTY_NAME"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="250" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="ContractNumber"
      IsPersistent="false"
      Precision="50"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="CONTRACT_NUMBER"
      Expression="CONTRACT_NUMBER"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="50" />
      </DesignTime>
   </ViewAttribute>
</ViewObject>

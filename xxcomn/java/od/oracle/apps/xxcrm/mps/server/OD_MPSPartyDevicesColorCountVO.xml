<ViewObject
   Name="OD_MPSPartyDevicesColorCountVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.server.OD_MPSPartyDevicesColorCountVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select md.total_black_count MonoLifeCount,  
       md.total_color_count  ColorLifeCount, 
       md.previous_black_count PreviousMonoCount, 
       md.previous_color_count PreviousColorCount, 
       md.Black_count BlkBillCount, 
       (md.black_count * mb.black_cpc) MonoBillAmt,
       md.color_count ColorBillCount,
       (md.color_count * mb.color_cpc) ColorBillAmt,
       md.over_usage BlkOverUsage,
       md.color_over_usage ColorOverUsage,
       md.black_under_usage BlkUnderUsage,
       md.color_under_usage ColorUnderUsage,
       md.previous_bill_count PreBlkBillCount, 
       md.previous_color_bill_count PreColorBillCount ,
     (md.current_count  * mb.service_cost)  Service_cost, mb.serial_no    
from xx_cs_mps_device_details md,
     xx_cs_mps_device_b mb
where md.serial_no = mb.serial_no
and md.supplies_label = 'USAGE'
and mb.serial_no = :0
   ]]></SQLQuery>

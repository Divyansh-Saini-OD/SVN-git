<ViewObject
   Name="XXCSMPSPrematuretonerCngRptVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSPrematuretonerCngRptVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT MB.DEVICE_NAME     AS DEVICE_NAME,
       MB.SERIAL_NO,
       XX_UTIL_SKU_REPORTS.CLOSE_DATE(MB.PARTY_ID,MB.SERIAL_NO,MD.TONER_ORDER_NUMBER) DATE_SWAPPED,
       DECODE(MD.SUPPLIES_LABEL,'TONERLEVEL_CYAN','CYAN','TONERLEVEL_MAGENTA','MAGENTA','TONERLEVEL_YELLOW','YELLOW','TONERLEVEL_BLACK','BLACK',NULL) TONER_TYPE,
       MD.PREVIOUS_LEVEL,
       MD.SUPPLIES_LEVEL,
       MB.MANAGED_STATUS,
       MB.ACTIVE_STATUS
from xx_cs_mps_device_b mb,
     xx_cs_mps_device_details md
where md.serial_no = mb.serial_no
AND MB.PARTY_NAME = NVL(:1,MB.PARTY_NAME)
and md.supplies_label <> 'USAGE'
AND MB.PROGRAM_TYPE IS NOT NULL
AND MB.PARTY_ID IS NOT NULL
AND MD.ATTRIBUTE4 = 'Y'
   ]]></SQLQuery>

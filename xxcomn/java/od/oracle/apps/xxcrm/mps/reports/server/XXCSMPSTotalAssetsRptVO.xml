   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSTotalAssetsRptVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT        
   A.PARTY_NAME     AS CUSTOMER_NAME,
   A.MANUFACTURER   AS MANUFACTURER,
   A.MODEL          AS MODEL,
   A.PROGRAM_TYPE   AS PROGRAM_TYPE,
   A.SERIAL_NO      AS SERIAL_NO,
    A.IP_ADDRESS    AS IP_ADDRESS,
   A.DEVICE_CONTACT AS DEVICE_CONTACT,
   A.DEVICE_PHONE   AS DEVICE_PHONE,
   A.ACTIVE_STATUS  AS ACTIVE_STATUS,
   A.MANAGED_STATUS AS MANAGED_STATUS
FROM                 xx_cs_mps_device_b a
       WHERE A.PARTY_NAME = NVL(:1,A.PARTY_NAME)
 AND A.PROGRAM_TYPE =        NVL(:2,A.PROGRAM_TYPE)
 AND A.MANUFACTURER = NVL(:3,A.MANUFACTURER)
 AND A.MODEL       = NVL(:4,A.MODEL)
   ]]></SQLQuery>

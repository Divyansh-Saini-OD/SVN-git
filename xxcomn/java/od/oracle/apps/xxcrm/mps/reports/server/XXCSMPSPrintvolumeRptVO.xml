<?xml version='1.0' encoding='windows-1252' ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="XXCSMPSPrintvolumeRptVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSPrintvolumeRptVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT mb.party_name,
      mb.model  , 
      MB.PROGRAM_TYPE,
      MB.PERIOD_COVERED_ST_DATE,
      MB.PERIOD_COVERED_END_DATE,
      MB.MANAGED_STATUS,
      MB.ACTIVE_STATUS,
       sum(NVL(total_count,0)) totalcount,
       sum(NVL(total_black_count,0)) totalblackcount,
       sum(NVL(total_color_count,0)) totalcolorcount
from xx_cs_mps_device_b mb,
    xx_cs_mps_device_details md
where md.serial_no = mb.serial_no
and mb.party_name = nvl(:1,mb.party_name)
and mb.program_type = nvl(:2,mb.program_type)
--AND TO_DATE(MB.PERIOD_COVERED_ST_DATE,'dd-mon-yy' )  >= TO_DATE(NVL(:3,mb.PERIOD_COVERED_ST_DATE), 'dd-mon-yy')
--AND TO_DATE(MB.PERIOD_COVERED_ST_DATE,'dd-mon-yy' )  >= TO_DATE(NVL(:3,(select '01-' || to_char(sysdate, 'MON') || '-' || to_char(sysdate, 'YY')from dual)), 'dd-mon-yy')
--AND TO_DATE(MB.PERIOD_COVERED_END_DATE,'dd-mon-yy' ) <= TO_DATE(NVL(:4, MB.PERIOD_COVERED_END_DATE), 'dd-mon-yy')
AND TO_DATE(NVL(MB.PERIOD_COVERED_ST_DATE,TRUNC(SYSDATE-30)),'dd-mon-yy' ) >= TO_DATE(NVL(:3, NVL(MB.PERIOD_COVERED_ST_DATE,TRUNC(SYSDATE-30))), 'dd-mon-yy')
AND TO_DATE(nvl(MB.PERIOD_COVERED_END_DATE,trunc(sysdate+1)),'dd-mon-yy' ) <= TO_DATE(NVL(:4, nvl(MB.PERIOD_COVERED_END_DATE,trunc(sysdate+1))), 'dd-mon-yy')
and md.supplies_label = 'USAGE'
AND MB.PROGRAM_TYPE IS NOT NULL
AND MB.PARTY_ID IS NOT NULL
GROUP BY   MB.PARTY_NAME,MB.MODEL, MB.PROGRAM_TYPE ,MB.PERIOD_COVERED_ST_DATE,MB.PERIOD_COVERED_END_DATE, MB.MANAGED_STATUS, MB.ACTIVE_STATUS
order by sum(NVL(total_count,0)) DESC
   ]]></SQLQuery>

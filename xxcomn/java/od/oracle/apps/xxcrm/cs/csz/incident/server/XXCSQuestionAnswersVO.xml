<?xml version='1.0' encoding='windows-1252' ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="XXCSQuestionAnswersVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.cs.csz.incident.server.XXCSQuestionAnswersVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select ip.panel_name,
       qp.question_label question,
       qd.freeform_string answer,
       qd.transaction_id
from   ies_question_data qd,
       ies_questions qp,
       ies_panels ip
where  ip.panel_id = qp.panel_id
and    qp.question_id = qd.question_id
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.41.57" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess" />
   </DesignTime>
   <ViewAttribute
      Name="PanelName"
      IsPersistent="false"
      IsNotNull="true"
      Precision="256"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="PANEL_NAME"
      Expression="PANEL_NAME"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="256" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="Question"
      IsPersistent="false"
      Precision="3000"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="QUESTION"
      Expression="QUESTION"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="3000" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="Answer"
      IsPersistent="false"
      Precision="4000"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="ANSWER"
      Expression="ANSWER"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="4000" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="TransactionId"
      IsPersistent="false"
      IsNotNull="true"
      Type="oracle.jbo.domain.Number"
      ColumnType="NUMBER"
      AliasName="TRANSACTION_ID"
      Expression="TRANSACTION_ID"
      SQLType="NUMERIC" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="22" />
      </DesignTime>
   </ViewAttribute>
   <AttrArray Name="KeyAttributes">
      <Item Value="PanelName" />
      <Item Value="TransactionId" />
   </AttrArray>
</ViewObject>

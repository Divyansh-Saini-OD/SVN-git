SELECT OD_ServiceRequestMasterEO.REQUEST_NUMBER, 
       OD_ServiceRequestMasterEO.STORE_ID, 
       OD_ServiceRequestMasterEO.LINE_NUMBER, 
       OD_ServiceRequestMasterEO.ITEM_NUMBER, 
       OD_ServiceRequestMasterEO.ITEM_DESCRIPTION, 
       OD_ServiceRequestMasterEO.RMS_SKU, 
       OD_ServiceRequestMasterEO.QUANTITY, 
       OD_ServiceRequestMasterEO.ITEM_CATEGORY, 
       OD_ServiceRequestMasterEO.PURCHASE_PRICE, 
       OD_ServiceRequestMasterEO.SELLING_PRICE, 
       OD_ServiceRequestMasterEO.EXCHANGE_PRICE, 
       OD_ServiceRequestMasterEO.CORE_FLAG, 
       OD_ServiceRequestMasterEO.UOM, 
       OD_ServiceRequestMasterEO.SCHEDULE_DATE, 
       OD_ServiceRequestMasterEO.CREATION_DATE, 
       OD_ServiceRequestMasterEO.CREATED_BY, 
       OD_ServiceRequestMasterEO.LAST_UDATE_DATE, 
       OD_ServiceRequestMasterEO.LAST_UPDATED_BY, 
       OD_ServiceRequestMasterEO.ATTRIBUTE1, 
       OD_ServiceRequestMasterEO.ATTRIBUTE2, 
       OD_ServiceRequestMasterEO.ATTRIBUTE3, 
       OD_ServiceRequestMasterEO.ATTRIBUTE4, 
       OD_ServiceRequestMasterEO.ATTRIBUTE5, 
       OD_ServiceRequestMasterEO.SALES_FLAG, 
       OD_ServiceRequestMasterEO.MANUFACTURER, 
       OD_ServiceRequestMasterEO.MODEL, 
       OD_ServiceRequestMasterEO.SERIAL_NUMBER, 
       OD_ServiceRequestMasterEO.PROBLEM_DESCR, 
       OD_ServiceRequestMasterEO.SPECIAL_INSTR, 
       OD_ServiceRequestMasterEO.INVENTORY_ITEM_ID, 
       OD_ServiceRequestMasterEO.STORE_NUMBER, 
       OD_ServiceRequestMasterEO.QUOTE_NUMBER, 
       OD_ServiceRequestMasterEO.EXPIRE_DATE, 
       OD_ServiceRequestMasterEO.EXCESS_QUANTITY, 
       OD_ServiceRequestMasterEO.EXCESS_FLAG, 
       OD_ServiceRequestMasterEO.RECEIVED_QUANTITY, 
       OD_ServiceRequestMasterEO.RECEIVED_SHIPMENT_FLAG, 
       OD_ServiceRequestMasterEO.COMPLETION_DATE, 
       OD_ServiceRequestMasterEO.ROWID, 
       OD_ServiceRequestMasterEO.TOT_RECEIVED_QTY, (select nvl((select 'Y'
         --into  lc_recpt_flag
         from cs_incident_statuses_tl ct,
             cs_incidents_all_b cb
         where cb.incident_status_id = ct.incident_status_id
         and ct.name not in ('Waiting for Parts' , 'Waiting for Approval','Approved')
         AND CB.INCIDENT_NUMBER = OD_SERVICEREQUESTMASTEREO.REQUEST_NUMBER
        ),'N') from dual) as Recpt_Flag
FROM XX_CS_TDS_PARTS OD_ServiceRequestMasterEO
   ]]></SQLQuery>

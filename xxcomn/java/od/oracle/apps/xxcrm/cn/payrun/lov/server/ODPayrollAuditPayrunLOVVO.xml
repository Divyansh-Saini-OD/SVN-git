   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select 
PAYRUN
from
XX_OIC_PAY_STATUS_HISTORY
WHERE NVL (org_id
           ,NVL (TO_NUMBER (DECODE (SUBSTRB (USERENV ('CLIENT_INFO')
                                            ,1
                                            ,1
                                            )
                                   ,' ', NULL
                                   ,SUBSTRB (USERENV ('CLIENT_INFO')
                                            ,1
                                            ,10
                                            )
                                   )
                           )
                ,-99
                )
           ) =
          NVL (TO_NUMBER (DECODE (SUBSTRB (USERENV ('CLIENT_INFO')
                                          ,1
                                          ,1
                                          )
                                 ,' ', NULL
                                 ,SUBSTRB (USERENV ('CLIENT_INFO')
                                          ,1
                                          ,10
                                          )
                                 )
                         )
              ,-99
              )
   ]]></SQLQuery>

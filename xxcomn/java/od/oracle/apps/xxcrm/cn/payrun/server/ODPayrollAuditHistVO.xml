REASON,
LOG,
CREATION_DATE,
CREATED_BY,
LAST_UPDATE_DATE,
LAST_UPDATED_BY,
LAST_UPDATED_LOGIN,
'Log' as LOGFILE
from XX_OIC_PAY_STATUS_HISTORY
WHERE NVL (org_id
           ,NVL (TO_NUMBER (DECODE (SUBSTRB (USERENV ('CLIENT_INFO')
                                            ,1
                                            ,1
                                            )
                                   ,' ', NULL
                                   ,SUBSTRB (USERENV ('CLIENT_INFO')
                                            ,1
                                            ,10
                                            )
                                   )
                           )
                ,-99
                )
           ) =
          NVL (TO_NUMBER (DECODE (SUBSTRB (USERENV ('CLIENT_INFO')
                                          ,1
                                          ,1
                                          )
                                 ,' ', NULL
                                 ,SUBSTRB (USERENV ('CLIENT_INFO')
                                          ,1
                                          ,10
                                          )
                                 )
                         )
              ,-99
              )   
]]></SQLQuery>

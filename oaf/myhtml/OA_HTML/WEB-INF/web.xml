   <context-param>
      <param-name>OA_RESPONSIBILITY_KEY</param-name>
      <param-value>FWK_TOOLBOX_TUTORIAL_LABS</param-value>
   </context-param>
   <context-param>
      <param-name>JRAD_APPS</param-name>
      <param-value>ak;fnd;dem</param-value>
   </context-param>
   <context-param>
      <param-name>OA_LOGIN_PASSWORD</param-name>
      <param-value>fwkdev</param-value>
   </context-param>
   <context-param>
      <param-name>OA_RESPONSIBILITY_APPS_SHORT_NAME</param-name>
      <param-value>AK</param-value>
   </context-param>
   <context-param>
      <param-name>DBC_FULL_PATH_NAME</param-name>

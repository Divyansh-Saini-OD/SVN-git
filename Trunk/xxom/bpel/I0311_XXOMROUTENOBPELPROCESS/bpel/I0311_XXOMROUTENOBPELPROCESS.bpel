  Purpose: Synchronous BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="I0311_XXOMROUTENOBPELPROCESS"
         targetNamespace="http://xmlns.oracle.com/I0311_XXOMROUTENOBPELPROCESS"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_OM_ROUTENO_PKG/GET_ROUTENO/"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_OM_ROUTENO_PKG/UPDATE_ROUTENO/"
         xmlns:client="http://xmlns.oracle.com/I0311_XXOMROUTENOBPELPROCESS"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ns1="http://www.upslogisticstech.com/UPSLT/TransportationSuite/TransportationWebService"
         xmlns:ehdr="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/db/UpdateRouteNoService/"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/RouteNoService/"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="client"
                 partnerLinkType="client:I0311_XXOMROUTENOBPELPROCESS"
                 myRole="I0311_XXOMROUTENOBPELPROCESSProvider"/>
    <partnerLink name="TransportationWebService"
                 partnerRole="TransportationWebService_Role"
                 partnerLinkType="ns1:TransportationWebService_PL"/>
    <partnerLink name="RouteNoService" partnerRole="RouteNoService_role"
                 partnerLinkType="ns2:RouteNoService_plt"/>
    <partnerLink name="UpdateRouteNoService"
                 partnerRole="UpdateRouteNoService_role"
                 partnerLinkType="ns3:UpdateRouteNoService_plt"/>
  </partnerLinks>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable"
              messageType="client:I0311_XXOMROUTENOBPELPROCESSRequestMessage"/>
    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable"
              messageType="client:I0311_XXOMROUTENOBPELPROCESSResponseMessage"/>
    <variable name="GetRouteDetails_RouteNoService_InputVariable"
              messageType="ns2:args_in_msg"/>
    <variable name="GetRouteDetails_RouteNoService_OutputVariable"
              messageType="ns2:args_out_msg"/>
    <variable name="UpdateRouteNo_UpdateRouteNoService_InputVariable"
              messageType="ns3:args_in_msg"/>
    <variable name="UpdateRouteNo_UpdateRouteNoService_OutputVariable"
              messageType="ns3:args_out_msg"/>
    <variable name="InvokeUPSTransportService_RetrieveLocationByIdentity_InputVariable"
              messageType="ns1:RetrieveLocationByIdentity"/>
    <variable name="InvokeUPSTransportService_RetrieveLocationByIdentity_OutputVariable"
              messageType="ns1:RetrieveLocationByIdentityResponse"/>
    <variable name="bindingFaultVar" messageType="bpelx:RuntimeFaultMessage"/>
    <variable name="remoteFaultVar" messageType="bpelx:RuntimeFaultMessage"/>
  </variables>
  <faultHandlers>
    <catch faultName="bpelx:remoteFault" faultVariable="remoteFaultVar">
      <sequence name="Sequence_1">
        <assign name="Assign_RemoteFault">
          <copy>
            <from variable="remoteFaultVar" part="summary"/>
            <to variable="outputVariable" part="payload"
                query="/client:I0311_XXOMROUTENOBPELPROCESSProcessResponse/client:status"/>
          </copy>
          <copy>
            <from expression="'FAILURE'"/>
            <to variable="outputVariable" part="payload"
                query="/client:I0311_XXOMROUTENOBPELPROCESSProcessResponse/client:result"/>
          </copy>
        </assign>
        <reply name="Reply_RemoteFault" partnerLink="client"
               portType="client:I0311_XXOMROUTENOBPELPROCESS"
               operation="process" variable="outputVariable"
               faultName="client:faultVar"/>
      </sequence>
    </catch>
    <catch faultName="bpelx:bindingFault" faultVariable="bindingFaultVar">
      <sequence name="Sequence_2">
        <assign name="Assign_BindingFault">
          <copy>
            <from variable="bindingFaultVar" part="summary"/>
            <to variable="outputVariable" part="payload"
                query="/client:I0311_XXOMROUTENOBPELPROCESSProcessResponse/client:result"/>
          </copy>
          <copy>
            <from expression="'FAILURE'"/>
            <to variable="outputVariable" part="payload"
                query="/client:I0311_XXOMROUTENOBPELPROCESSProcessResponse/client:status"/>
          </copy>
        </assign>
        <reply name="Reply_bindingFault" partnerLink="client"
               portType="client:I0311_XXOMROUTENOBPELPROCESS"
               operation="process" variable="outputVariable"
               faultName="client:faultVar"/>
      </sequence>
    </catch>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <!-- Receive input from requestor. (Note: This maps to operation defined in I0311_XXOMROUTENOBPELPROCESS.wsdl) -->
    <receive name="receiveInput" partnerLink="client"
             portType="client:I0311_XXOMROUTENOBPELPROCESS" operation="process"
             variable="inputVariable" createInstance="yes"/>
    <!-- Generate reply to synchronous request -->
    <assign name="AssignDeliveryId">
      <copy>
        <from variable="inputVariable" part="payload"
              query="/client:I0311_XXOMROUTENOBPELPROCESSProcessRequest/client:DeliveryId"/>
        <to variable="GetRouteDetails_RouteNoService_InputVariable"
            part="InputParameters" query="/ns4:InputParameters/P_DELIVERY_ID"/>
      </copy>
    </assign>
    <invoke name="GetRouteDetails" partnerLink="RouteNoService"
            portType="ns2:RouteNoService_ptt" operation="RouteNoService"
            inputVariable="GetRouteDetails_RouteNoService_InputVariable"
            outputVariable="GetRouteDetails_RouteNoService_OutputVariable"/>
    <assign name="TransformRouteDetails">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:processXSLT('Transformation_UPS.xsl',bpws:getVariableData('GetRouteDetails_RouteNoService_OutputVariable','OutputParameters'))"/>
        <to variable="InvokeUPSTransportService_RetrieveLocationByIdentity_InputVariable"
            part="parameters"/>
      </copy>
    </assign>
    <invoke name="InvokeUPSTransportService"
            partnerLink="TransportationWebService"
            portType="ns1:TransportationWebService"
            operation="RetrieveLocationByIdentity"
            inputVariable="InvokeUPSTransportService_RetrieveLocationByIdentity_InputVariable"
            outputVariable="InvokeUPSTransportService_RetrieveLocationByIdentity_OutputVariable"/>
    <assign name="AssignPreferredRoute">
      <copy>
        <from variable="inputVariable" part="payload"
              query="/client:I0311_XXOMROUTENOBPELPROCESSProcessRequest/client:DeliveryId"/>
        <to variable="UpdateRouteNo_UpdateRouteNoService_InputVariable"
            part="InputParameters" query="/ns5:InputParameters/P_DELIVERY_ID"/>
      </copy>
      <copy>
        <from variable="InvokeUPSTransportService_RetrieveLocationByIdentity_OutputVariable"
              part="parameters"
              query="/ns1:RetrieveLocationByIdentityResponse/ns1:location/ns1:preferredRouteID"/>
        <to variable="UpdateRouteNo_UpdateRouteNoService_InputVariable"
            part="InputParameters" query="/ns5:InputParameters/P_ROUTE_NO"/>
      </copy>
    </assign>
    <invoke name="UpdateRouteNo" partnerLink="UpdateRouteNoService"
            portType="ns3:UpdateRouteNoService_ptt"
            operation="UpdateRouteNoService"
            inputVariable="UpdateRouteNo_UpdateRouteNoService_InputVariable"
            outputVariable="UpdateRouteNo_UpdateRouteNoService_OutputVariable"/>
    <switch name="Switch_1">
      <case condition="bpws:getVariableData('UpdateRouteNo_UpdateRouteNoService_OutputVariable','OutputParameters','/ns5:OutputParameters/X_ERROR_MSG')  = ''">
        <bpelx:annotation>
          <bpelx:pattern>SUCCESS</bpelx:pattern>
        </bpelx:annotation>
        <assign name="Assign_Success">
          <copy>
            <from expression="'SUCCESS'"/>
            <to variable="outputVariable" part="payload"
                query="/client:I0311_XXOMROUTENOBPELPROCESSProcessResponse/client:status"/>
          </copy>
        </assign>
      </case>
      <otherwise>
        <assign name="Assign_Failure">
          <copy>
            <from variable="UpdateRouteNo_UpdateRouteNoService_OutputVariable"
                  part="OutputParameters"
                  query="/ns5:OutputParameters/X_ERROR_MSG"/>
            <to variable="outputVariable" part="payload"
                query="/client:I0311_XXOMROUTENOBPELPROCESSProcessResponse/client:result"/>
          </copy>
          <copy>
            <from expression="'FAILURE'"/>
            <to variable="outputVariable" part="payload"
                query="/client:I0311_XXOMROUTENOBPELPROCESSProcessResponse/client:status"/>
          </copy>
        </assign>
      </otherwise>
    </switch>
    <reply name="replyOutput" partnerLink="client"
           portType="client:I0311_XXOMROUTENOBPELPROCESS" operation="process"
           variable="outputVariable"/>
  </sequence>
</process>
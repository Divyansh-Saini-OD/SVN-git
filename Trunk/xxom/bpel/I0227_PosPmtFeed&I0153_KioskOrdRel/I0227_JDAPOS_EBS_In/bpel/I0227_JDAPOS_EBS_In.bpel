<process name="I0227_JDAPOS_EBS_In"
         targetNamespace="http://xmlns.oracle.com/I0227_JDAPOS_EBS_In"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns4="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ns7="http://xmlns.oracle.com/pcbpel/adapter/db/svcJDAPOSRead/"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns5="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:client="http://xmlns.oracle.com/I0227_JDAPOS_EBS_In"
         xmlns:ns6="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:ns9="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_BPEL_SVCJDAPOSREAD/XX_OM_POS_SHIP_CONF_PKG-24OD_PO/"
         xmlns:ns1="http://www.thiscompany.com/ns/sales"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/BPEL_SVCJDSPOSREAD/XX_ONT_POS_SHIP_CONF_PKG-24OD_P/"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/svcJDSPOSRead/"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns8="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/BPEL_SVCJDAPOSREAD/XX_ONT_POS_SHIP_CONF_PKG-24OD_P/">
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <partnerLinks>
        <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
        <partnerLink name="client" partnerLinkType="client:I0227_JDAPOS_EBS_In"
                     myRole="I0227_JDAPOS_EBS_InProvider"
                     partnerRole="I0227_JDAPOS_EBS_InRequester"/>
        <partnerLink name="plDbRead" partnerRole="svcJDAPOSRead_role"
                     partnerLinkType="ns7:svcJDAPOSRead_plt"/>
    </partnerLinks>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <variables>
        <!-- Reference to the message passed as input during initiation -->
        <variable name="inputVariable"
                  messageType="client:I0227_JDAPOS_EBS_InRequestMessage"/>
        <!-- Reference to the message that will be sent back to the requester during callback -->
        <variable name="outputVariable"
                  messageType="client:I0227_JDAPOS_EBS_InResponseMessage"/>
        <variable name="AckVar" messageType="client:AckMsg"/>
        <variable name="ErrVar" messageType="client:ErrMsg"/>
        <variable name="invkplDbRead_In" messageType="ns7:args_in_msg"/>
        <variable name="invkplDbRead_Out" messageType="ns7:args_out_msg"/>
        <variable name="asnOutVar" messageType="ns7:args_out_msg"/>
        <variable name="Invoke_1_onErr_InputVariable"
                  messageType="client:ErrMsg"/>
    </variables>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="main">
        <!-- Receive input from requestor. (Note: This maps to operation defined in I0227_JDAPOS_EBS_In.wsdl) -->
        <receive name="receiveInput" partnerLink="client"
                 portType="client:I0227_JDAPOS_EBS_In" operation="initiate"
                 variable="inputVariable" createInstance="yes"/>
        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <assign name="xformPosMsg">
            <bpelx:annotation>
                <bpelx:pattern>transformation</bpelx:pattern>
            </bpelx:annotation>
            <copy>
                <from expression="ora:processXSLT('xformPosMsg.xsl',bpws:getVariableData('inputVariable','payload'))"/>
                <to variable="invkplDbRead_In" part="InputParameters"/>
            </copy>
        </assign>
        <invoke name="invkplDbRead" partnerLink="plDbRead"
                portType="ns7:svcJDAPOSRead_ptt" operation="svcJDAPOSRead"
                inputVariable="invkplDbRead_In"
                outputVariable="invkplDbRead_Out"/>
        <assign name="asnDBOutVar">
            <copy>
                <from variable="invkplDbRead_Out" part="OutputParameters"/>
                <to variable="asnOutVar" part="OutputParameters"/>
            </copy>
        </assign>
        <switch name="swRetStatus">
            <case condition="bpws:getVariableData('invkplDbRead_Out','OutputParameters','/ns9:OutputParameters/X_STATUS')='FAILURE'">
                <bpelx:annotation>
                    <bpelx:pattern>ErrPos</bpelx:pattern>
                </bpelx:annotation>
                <sequence name="Sequence_2">
                    <assign name="xformErrPos">
                        <bpelx:annotation>
                            <bpelx:pattern>transformation</bpelx:pattern>
                        </bpelx:annotation>
                        <copy>
                            <from expression="ora:processXSLT('xformErrPos.xsl',bpws:getVariableData('invkplDbRead_Out','OutputParameters'))"/>
                            <to variable="ErrVar" part="ErrMsg"/>
                        </copy>
                    </assign>
                    <assign name="asnErrPos">
                        <copy>
                            <from expression="ora:getProcessId()"/>
                            <to variable="ErrVar" part="ErrMsg"
                                query="/ns1:Error/BPELProcessID"/>
                        </copy>
                        <copy>
                            <from expression="ora:getInstanceId()"/>
                            <to variable="ErrVar" part="ErrMsg"
                                query="/ns1:Error/TransactionId"/>
                        </copy>
                    </assign>
                    <invoke name="invkErrPos" partnerLink="client"
                            portType="client:I0227_JDAPOS_EBS_InCallback"
                            operation="onErr" inputVariable="ErrVar"/>
                </sequence>
            </case>
            <case condition="bpws:getVariableData('invkplDbRead_Out','OutputParameters','/ns9:OutputParameters/X_STATUS')='SUCCESS'">
                <bpelx:annotation>
                    <bpelx:pattern>AckPos</bpelx:pattern>
                </bpelx:annotation>
                <sequence name="Sequence_1">
                    <assign name="xformAckPos">
                        <bpelx:annotation>
                            <bpelx:pattern>transformation</bpelx:pattern>
                        </bpelx:annotation>
                        <copy>
                            <from expression="ora:processXSLT('xformAckPos.xsl',bpws:getVariableData('invkplDbRead_Out','OutputParameters'))"/>
                            <to variable="AckVar" part="AckMsg"/>
                        </copy>
                    </assign>
                    <invoke name="invkAckPos" partnerLink="client"
                            portType="client:I0227_JDAPOS_EBS_InCallback"
                            operation="onAck" inputVariable="AckVar"/>
                </sequence>
            </case>
        </switch>
    </sequence>
</process>
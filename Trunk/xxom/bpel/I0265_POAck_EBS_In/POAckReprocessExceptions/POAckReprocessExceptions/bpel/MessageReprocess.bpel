<process name="POAckReprocessExceptions"
         targetNamespace="http://xmlns.oracle.com/MessageReprocess"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns4="http://xmlns.oracle.com/ProcessNowDocsReceive"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns5="http://xmlns.oracle.com/SendNowDocsMsgProcess"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:ns9="http://xmlbeans.orderProcessing.externalVendors.officedepot.com"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/aq/DequeueMessage/"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/aq/inbound/"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns10="http://www.openapplications.org/oagis/9"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns13="http://xmlns.oracle.com/POAcknowledgementProcess"
         xmlns:ns7="http://xmlns.oracle.com/I1153_DPSNowPrintEBS_JobConfRel_Child"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns12="http://xmlns.oracle.com/DPSCancel_SendNowDocsMsgProcess"
         xmlns:client="http://xmlns.oracle.com/MessageReprocess"
         xmlns:ns6="http://xmlns.oracle.com/SendSalesOrderInfo"
         xmlns:ns11="http://xmlns.oracle.com/DPSCancel_SendSalesOrderInfo"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ehdr="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/opaque/"
         xmlns:ns14="http://www.openapplications.org/oagis/9/officedepot/1"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:ns8="http://xmlns.oracle.com/I1153_DPSNowPrintEBS_JobConf_Child">
  <bpelx:exec import="java.util.*"/>
  <bpelx:exec import="java.lang.*"/>
  <bpelx:exec import="java.math.*"/>
  <bpelx:exec import="com.collaxa.common.util.*"/>
  <bpelx:exec import="java.io.*"/>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <partnerLink myRole="Dequeue_role" name="DequeueMessage"
                 partnerLinkType="ns1:Dequeue_plt"/>
    <partnerLink name="POAcknowledgementProcess"
                 partnerRole="POAcknowledgementProcessProvider"
                 partnerLinkType="ns13:POAcknowledgementProcess"/>
  </partnerLinks>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <variable name="DequeueHeaderVar" messageType="ns2:Header"/>
    <variable name="CorrIDVar" type="xsd:string"/>
    <variable name="Opaque" type="xsd:string"/>
    <variable name="OpaqueToString" type="xsd:string"/>
    <variable name="ReceiveMessageIn" messageType="ns1:Dequeue_msg"/>
    <variable name="ReceiveMessIn" messageType="ns1:Dequeue_msg"/>
    <variable name="InvokePOAckProcessIn"
              messageType="ns13:POAcknowledgementProcessRequestMessage"/>
  </variables>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <receive name="ReceiveMessage" partnerLink="DequeueMessage"
             portType="ns1:Dequeue_ptt" operation="Dequeue"
             variable="ReceiveMessIn" createInstance="yes"
             bpelx:headerVariable="DequeueHeaderVar"/>
    <assign name="AssignCorrID">
      <copy>
        <from variable="DequeueHeaderVar" part="Header"
              query="/ns2:Header/ns2:Correlation"/>
        <to variable="CorrIDVar"/>
      </copy>
      <copy>
        <from part="opaque" variable="ReceiveMessIn"
              query="/ns3:opaqueElement"/>
        <to variable="Opaque"/>
      </copy>
    </assign>
    <bpelx:exec name="Java_EmbeddingOpaqueToString" language="java"
                version="1.3">
      <![CDATA[/*Write your java code below e.g.  
	System.out.println("Hello, World"); 
*/ 
 
 
String input = (String)getVariableData("Opaque");       
      
Base64Decoder Decoder = new Base64Decoder();       
      
try   
{   
String decoded = Base64Decoder.decode(input);       
      
setVariableData("OpaqueToString",decoded);   
}   
catch(UnsupportedEncodingException uee) {   
uee.printStackTrace();   
}]]>
    </bpelx:exec>
    <switch name="SelectCorrID">
      <case condition="bpws:getVariableData('CorrIDVar') =ora:doc(&quot;${server_url}/xmllib/otc/GlobalParameters.xml&quot;,&quot;/GlobalParameters/OTC/POACK/CorrID/POAckIn&quot;)">
        <sequence name="Sequence_8">
          <assign name="assignIn">
            <copy>
              <from expression="ora:parseEscapedXML(bpws:getVariableData('OpaqueToString'))"/>
              <to variable="InvokePOAckProcessIn" part="payload"
                  query="/ns10:AcknowledgePurchaseOrder"/>
            </copy>
          </assign>
          <invoke name="InvokePOAckProcess"
                  partnerLink="POAcknowledgementProcess"
                  portType="ns13:POAcknowledgementProcess" operation="initiate"
                  inputVariable="InvokePOAckProcessIn"/>
        </sequence>
      </case>
      <otherwise>
        <empty name="Empty"/>
      </otherwise>
    </switch>
  </sequence>
</process>
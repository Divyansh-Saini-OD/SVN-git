<process name="DPSCancel_ClearStaging"
         targetNamespace="http://xmlns.oracle.com/DPSCancel_ClearStaging"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns13="http://xmlns.oracle.com/pcbpel/adapter/aq/outbound/"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_OM_DPSCANCEL_PKG/DELETE_PARENTID_BPEL/"
         xmlns:ns7="http://xmlns.oracle.com/ODErrorLogger"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns12="http://xmlns.oracle.com/pcbpel/adapter/aq/EnqueueMessage/"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/db/DBErrorWrite/"
         xmlns:client="http://xmlns.oracle.com/DPSCancel_ClearStaging"
         xmlns:ns6="http://xmlns.oracle.com/pcbpel/adapter/db/XXOM/XX_OM_GLOBAL_EXCEPTION_PKG/INSERT_EXCEPTION/"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:ns11="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_OM_GLOBAL_EXCEPTION_PKG/INSERT_EXCEPTION/"
         xmlns:ns9="http://xmlns.oracle.com/pcbpel/adapter/db/AppsInitialize/"
         xmlns:ns1="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ehdr="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/db/DBClearStg/"
         xmlns:ns2="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns10="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_OM_DPS_APPS_INIT_PKG/DPS_APPS_INIT/"
         xmlns:ns8="ODError">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="client" partnerLinkType="client:DPSCancel_ClearStaging"
                 myRole="DPSCancel_ClearStagingProvider"/>
    <partnerLink name="DBClearStg" partnerRole="DBClearStg_role"
                 partnerLinkType="ns3:DBClearStg_plt"/>
    <partnerLink name="DBErrorWrite" partnerRole="DBErrorWrite_role"
                 partnerLinkType="ns5:DBErrorWrite_plt"/>
    <partnerLink name="ODErrorLogger" partnerRole="ODErrorLoggerProvider"
                 partnerLinkType="ns7:ODErrorLogger"/>
    <partnerLink name="AppsInitialize" partnerRole="AppsInitialize_role"
                 partnerLinkType="ns9:AppsInitialize_plt"/>
    <partnerLink name="EnqueueMessage" partnerRole="Enqueue_role"
                 partnerLinkType="ns12:Enqueue_plt"/>
  </partnerLinks>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable"
              messageType="client:DPSCancel_ClearStagingRequestMessage"/>
    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable"
              messageType="client:DPSCancel_ClearStagingResponseMessage"/>
    <variable name="InvokeDBClearStgIn" messageType="ns3:args_in_msg"/>
    <variable name="InvokeDBClearStgOut" messageType="ns3:args_out_msg"/>
    <variable name="NowDocsRespondVar" type="xsd:string"/>
    <variable name="RemoteFaultVar" messageType="bpelx:RuntimeFaultMessage"/>
    <variable name="InvokeRemErrorLoggerIn"
              messageType="ns7:ODErrorLoggerRequestMessage"/>
    <variable name="BindingFaultVar" messageType="bpelx:RuntimeFaultMessage"/>
    <variable name="InvokeBindErrorLoggerIn"
              messageType="ns7:ODErrorLoggerRequestMessage"/>
    <variable name="InvokeCatchAllErrorLoggerIn"
              messageType="ns7:ODErrorLoggerRequestMessage"/>
    <variable name="ReplyClientRemoteOut"
              messageType="client:DPSCancel_ClearStagingResponseMessage"/>
    <variable name="ReplyClientBindOut"
              messageType="client:DPSCancel_ClearStagingResponseMessage"/>
    <variable name="ReplyClientCatchAllOut"
              messageType="client:DPSCancel_ClearStagingResponseMessage"/>
    <variable name="activityNameVar" type="xsd:string"/>
    <variable name="InvokeAppsInitIn" messageType="ns9:args_in_msg"/>
    <variable name="InvokeAppsInitOut" messageType="ns9:args_out_msg"/>
    <variable name="activityDescriptionVar" type="xsd:string"/>
    <variable name="InvokeErrorWriteIn" messageType="ns5:args_in_msg"/>
    <variable name="InvokeErrorWriteOut" messageType="ns5:args_out_msg"/>
    <variable name="InvokeEnqueueMessageIn"
              messageType="ns12:DPSCancel_ClearStagingProcessRequest_msg"/>
    <variable name="EnqueueHeader" messageType="ns13:Header"/>
  </variables>
  <faultHandlers>
    <catch faultName="bpelx:remoteFault" faultVariable="RemoteFaultVar">
      <sequence name="Sequence_3">
        <assign name="AssignRemODErrorLogger">
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessDomain"/>
          </copy>
          <copy>
            <from variable="RemoteFaultVar" part="code"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorCode"/>
          </copy>
          <copy>
            <from variable="RemoteFaultVar" part="detail"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorDescription"/>
          </copy>
          <copy>
            <from variable="RemoteFaultVar" part="detail"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorText"/>
          </copy>
          <copy>
            <from expression="'Fatal'"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorSeverity"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorDateTime"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:MessageDetails/ns8:MessageDateTime"/>
          </copy>
          <copy>
            <from expression="ns2:getDefaultRealmName()"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:SystemName"/>
          </copy>
          <copy>
            <from expression="ora:generateGUID()"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessId"/>
          </copy>
          <copy>
            <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/ReplyException")'/>
            <to variable="ReplyClientRemoteOut" part="payload"
                query="/client:DPSCancel_ClearStagingProcessResponse/client:result"/>
          </copy>
          <copy>
            <from variable="activityNameVar"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessStep"/>
          </copy>
          <copy>
            <from expression="ora:getContentAsString(bpws:getVariableData('inputVariable','payload','/client:DPSCancel_ClearStagingProcessRequest'))"/>
            <to variable="InvokeRemErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:MessageDetails/ns8:MessageData"/>
          </copy>
        </assign>
        <invoke name="InvokeRemErrorLogger" partnerLink="ODErrorLogger"
                portType="ns7:ODErrorLogger" operation="initiate"
                inputVariable="InvokeRemErrorLoggerIn"/>
        <reply name="ReplyClientRemote" partnerLink="client"
               portType="client:DPSCancel_ClearStaging" operation="process"
               variable="ReplyClientRemoteOut"/>
      </sequence>
    </catch>
    <catch faultName="bpelx:bindingFault" faultVariable="BindingFaultVar">
      <sequence name="Sequence_4">
        <assign name="AssignBindODErrorLogger">
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessDomain"/>
          </copy>
          <copy>
            <from variable="BindingFaultVar" part="code"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorCode"/>
          </copy>
          <copy>
            <from variable="BindingFaultVar" part="summary"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorDescription"/>
          </copy>
          <copy>
            <from variable="BindingFaultVar" part="detail"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorText"/>
          </copy>
          <copy>
            <from expression="'Fatal'"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorSeverity"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorDateTime"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:MessageDetails/ns8:MessageDateTime"/>
          </copy>
          <copy>
            <from expression="ns2:getDefaultRealmName()"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:SystemName"/>
          </copy>
          <copy>
            <from expression="ora:generateGUID()"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessId"/>
          </copy>
          <copy>
            <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/ReplyException")'/>
            <to variable="ReplyClientRemoteOut" part="payload"
                query="/client:DPSCancel_ClearStagingProcessResponse/client:result"/>
          </copy>
          <copy>
            <from variable="activityNameVar"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessStep"/>
          </copy>
          <copy>
            <from expression="ora:getContentAsString(bpws:getVariableData('inputVariable','payload','/client:DPSCancel_ClearStagingProcessRequest'))"/>
            <to variable="InvokeBindErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:MessageDetails/ns8:MessageData"/>
          </copy>
        </assign>
        <invoke name="InvokeBindErrorLogger" partnerLink="ODErrorLogger"
                portType="ns7:ODErrorLogger" operation="initiate"
                inputVariable="InvokeBindErrorLoggerIn"/>
        <reply name="ReplyClientBind" partnerLink="client"
               portType="client:DPSCancel_ClearStaging" operation="process"
               variable="ReplyClientBindOut"/>
      </sequence>
    </catch>
    <catchAll>
      <sequence name="Sequence_5">
        <assign name="AssignCatchAllEnqueueMsg">
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessDomain"/>
          </copy>
          <copy>
            <from expression="'Fatal'"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorSeverity"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorDateTime"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:MessageDetails/ns8:MessageDateTime"/>
          </copy>
          <copy>
            <from expression="ns2:getDefaultRealmName()"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:SystemName"/>
          </copy>
          <copy>
            <from expression="ora:generateGUID()"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessId"/>
          </copy>
          <copy>
            <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/ReplyException")'/>
            <to variable="ReplyClientCatchAllOut" part="payload"
                query="/client:DPSCancel_ClearStagingProcessResponse/client:result"/>
          </copy>
          <copy>
            <from variable="activityNameVar"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ProcessInfo/ns8:BusinessProcessStep"/>
          </copy>
          <copy>
            <from expression="ora:getContentAsString(bpws:getVariableData('inputVariable','payload','/client:DPSCancel_ClearStagingProcessRequest'))"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:MessageDetails/ns8:MessageData"/>
          </copy>
          <copy>
            <from expression="''"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorCode"/>
          </copy>
          <copy>
            <from expression="concat('An error occurred while performing this acitvity ',bpws:getVariableData('activityNameVar'),bpws:getVariableData('activityDescriptionVar'))"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorDescription"/>
          </copy>
          <copy>
            <from expression="concat('An error occurred while performing this acitvity ',bpws:getVariableData('activityNameVar'),bpws:getVariableData('activityDescriptionVar'))"/>
            <to variable="InvokeCatchAllErrorLoggerIn" part="payload"
                query="/ns8:ErrorData/ns8:ErrorDetails/ns8:ErrorText"/>
          </copy>
        </assign>
        <invoke name="InvokeCatchAllErrorLogger" partnerLink="ODErrorLogger"
                portType="ns7:ODErrorLogger" operation="initiate"
                inputVariable="InvokeCatchAllErrorLoggerIn"/>
        <reply name="ReplyClientCatchAll" partnerLink="client"
               portType="client:DPSCancel_ClearStaging" operation="process"
               variable="ReplyClientCatchAllOut"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <!-- Receive input from requestor. (Note: This maps to operation defined in DPSCancel_ClearStaging.wsdl) -->
    <receive name="receiveInput" partnerLink="client"
             portType="client:DPSCancel_ClearStaging" operation="process"
             variable="inputVariable" createInstance="yes"/>
    <!-- Generate reply to synchronous request -->
    <assign name="AssignSplittedInput">
      <copy>
        <from expression="normalize-space(substring-before(bpws:getVariableData('inputVariable','payload','/client:DPSCancel_ClearStagingProcessRequest/client:input'),'|'))"/>
        <to variable="InvokeDBClearStgIn" part="InputParameters"
            query="/ns4:InputParameters/P_SESSION_ID"/>
      </copy>
      <copy>
        <from expression="normalize-space(substring-after(bpws:getVariableData('inputVariable','payload','/client:DPSCancel_ClearStagingProcessRequest/client:input'),'|'))"/>
        <to variable="NowDocsRespondVar"/>
      </copy>
      <copy>
        <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/UserName")'/>
        <to variable="InvokeDBClearStgIn" part="InputParameters"
            query="/ns4:InputParameters/P_USER_NAME"/>
      </copy>
      <copy>
        <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/RespName")'/>
        <to variable="InvokeDBClearStgIn" part="InputParameters"
            query="/ns4:InputParameters/P_RESP_NAME"/>
      </copy>
      <copy>
        <from expression="'InvokeDBClearStg '"/>
        <to variable="activityNameVar"/>
      </copy>
      <copy>
        <from expression="'(This activity invokes a DB Adpater to clear data corresponding to the current session)'"/>
        <to variable="activityDescriptionVar"/>
      </copy>
    </assign>
    <switch name="Switch_1">
      <case condition="bpws:getVariableData('NowDocsRespondVar') = ora:doc(&quot;${server_url}/xmllib/otc/GlobalParameters.xml&quot;,&quot;/GlobalParameters/OTC/DPS/CancelOrder/NowDocsResponseMissingOrder&quot;)">
        <sequence name="Sequence_6">
          <assign name="AssignEnqueueVar">
            <copy>
              <from expression="substring-before(bpws:getVariableData('inputVariable','payload','/client:DPSCancel_ClearStagingProcessRequest/client:input'),'|')"/>
              <to variable="InvokeEnqueueMessageIn"
                  part="DPSCancel_ClearStagingProcessRequest"
                  query="/client:DPSCancel_ClearStagingProcessRequest/client:input"/>
            </copy>
            <copy>
              <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/EnqueueDelay")'/>
              <to variable="EnqueueHeader" part="Header"
                  query="/ns13:Header/ns13:Delay"/>
            </copy>
          </assign>
          <invoke name="InvokeEnqueueMessage" partnerLink="EnqueueMessage"
                  portType="ns12:Enqueue_ptt" operation="Enqueue"
                  inputVariable="InvokeEnqueueMessageIn"
                  bpelx:inputHeaderVariable="EnqueueHeader"/>
        </sequence>
      </case>
      <case condition="bpws:getVariableData('NowDocsRespondVar') =  ora:doc(&quot;${server_url}/xmllib/otc/GlobalParameters.xml&quot;,&quot;/GlobalParameters/OTC/DPS/CancelOrder/NowDocsResponse&quot;)">
        <sequence name="Sequence_1">
          <invoke name="InvokeDBClearStg" partnerLink="DBClearStg"
                  portType="ns3:DBClearStg_ptt" operation="DBClearStg"
                  inputVariable="InvokeDBClearStgIn"
                  outputVariable="InvokeDBClearStgOut"/>
        </sequence>
      </case>
      <otherwise>
        <sequence name="Sequence_2">
          <assign name="AssignErrVar">
            <copy>
              <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/CancelOrder/ExceptionHeader")'/>
              <to part="InputParameters"
                  query="/ns11:InputParameters/P_REPORT_EXCEPTION/P_EXCEPTION_HEADER"
                  variable="InvokeErrorWriteIn"/>
            </copy>
            <copy>
              <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/CancelOrder/TrackCode")'/>
              <to part="InputParameters"
                  query="/ns11:InputParameters/P_REPORT_EXCEPTION/P_TRACK_CODE"
                  variable="InvokeErrorWriteIn"/>
            </copy>
            <copy>
              <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/CancelOrder/SolutionDomain")'/>
              <to part="InputParameters"
                  query="/ns11:InputParameters/P_REPORT_EXCEPTION/P_SOLUTION_DOMAIN"
                  variable="InvokeErrorWriteIn"/>
            </copy>
            <copy>
              <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/CancelOrder/Function")'/>
              <to part="InputParameters"
                  query="/ns11:InputParameters/P_REPORT_EXCEPTION/P_FUNCTION"
                  variable="InvokeErrorWriteIn"/>
            </copy>
            <copy>
              <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/CancelOrder/ErrorCode")'/>
              <to part="InputParameters"
                  query="/ns11:InputParameters/P_REPORT_EXCEPTION/P_ERROR_CODE"
                  variable="InvokeErrorWriteIn"/>
            </copy>
            <copy>
              <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/CancelOrder/ErrorDescription")'/>
              <to part="InputParameters"
                  query="/ns11:InputParameters/P_REPORT_EXCEPTION/P_ERROR_DESCRIPTION"
                  variable="InvokeErrorWriteIn"/>
            </copy>
            <copy>
              <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/CancelOrder/EntityRef")'/>
              <to part="InputParameters"
                  query="/ns11:InputParameters/P_REPORT_EXCEPTION/P_ENTITY_REF"
                  variable="InvokeErrorWriteIn"/>
            </copy>
            <copy>
              <from expression="substring-before(bpws:getVariableData('inputVariable','payload','/client:DPSCancel_ClearStagingProcessRequest/client:input'),'|')"/>
              <to part="InputParameters"
                  query="/ns11:InputParameters/P_REPORT_EXCEPTION/P_ENTITY_REF_ID"
                  variable="InvokeErrorWriteIn"/>
            </copy>
            <copy>
              <from expression="'InvokeErrorWrite '"/>
              <to variable="activityNameVar"/>
            </copy>
            <copy>
              <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/UserName")'/>
              <to variable="InvokeAppsInitIn" part="InputParameters"
                  query="/ns10:InputParameters/P_USER_NAME"/>
            </copy>
            <copy>
              <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/RespName")'/>
              <to variable="InvokeAppsInitIn" part="InputParameters"
                  query="/ns10:InputParameters/P_RESP_NAME"/>
            </copy>
            <copy>
              <from expression="'(This activity invokes DB Adapter to call Global Exceptional Framework)'"/>
              <to variable="activityDescriptionVar"/>
            </copy>
            <copy>
              <from expression="'OTHERS'"/>
              <to variable="InvokeErrorWriteIn" part="InputParameters"
                  query="/ns11:InputParameters/P_REPORT_EXCEPTION/P_EXCEPTION_HEADER"/>
            </copy>
          </assign>
          <invoke name="InvokeAppsInit" partnerLink="AppsInitialize"
                  portType="ns9:AppsInitialize_ptt" operation="AppsInitialize"
                  inputVariable="InvokeAppsInitIn"
                  outputVariable="InvokeAppsInitOut"/>
          <invoke name="InvokeErrorWrite" partnerLink="DBErrorWrite"
                  portType="ns5:DBErrorWrite_ptt" operation="DBErrorWrite"
                  inputVariable="InvokeErrorWriteIn"
                  outputVariable="InvokeErrorWriteOut"/>
        </sequence>
      </otherwise>
    </switch>
    <reply name="replyOutput" partnerLink="client"
           portType="client:DPSCancel_ClearStaging" operation="process"
           variable="outputVariable"/>
  </sequence>
</process>
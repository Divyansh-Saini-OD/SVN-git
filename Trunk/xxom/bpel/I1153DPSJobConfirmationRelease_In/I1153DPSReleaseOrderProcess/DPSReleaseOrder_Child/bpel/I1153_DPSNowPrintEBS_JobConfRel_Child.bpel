    <sequence name="main">
        <!-- Receive input from requestor. (Note: This maps to operation defined in I1153_DPSNowPrintEBS_JobConfRel_Child.wsdl) -->
        <receive name="receiveJobConfMaster" partnerLink="client"
                 portType="client:I1153_DPSNowPrintEBS_JobConfRel_Child"
                 operation="initiate" variable="receiveInputIn"
                 createInstance="yes">
            <bpelx:annotation>
                <bpelx:documentation>This activity is used to receive the
                                     Release Acknowledgement message from the
                                     Master Process as a part of the Process</bpelx:documentation>
            </bpelx:annotation>
        </receive>
        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <assign name="assignVartoProc">
            <bpelx:annotation>
                <bpelx:documentation>This activity is used to assign the input
                                     message from the Nowdocs to invoke the DB
                                     Package for updation of the Release
                                     acknowedgement</bpelx:documentation>
            </bpelx:annotation>
            <copy>
                <from variable="receiveInputIn" part="payload"
                      query="/ns2:ReleaseOrderRequest/ns2:ReleaseOrderBody/ns2:OrderNumber"/>
                <to variable="invokeDBRelHoldIn" part="InputParameters"
                    query="/ns3:InputParameters/P_ORDER_NUMBER"/>
            </copy>
            <copy>
                <from variable="receiveInputIn" part="payload"
                      query="/ns2:ReleaseOrderRequest/ns2:ReleaseOrderBody/ns2:OrderSubNumber"/>
                <to variable="invokeDBRelHoldIn" part="InputParameters"
                    query="/ns3:InputParameters/P_LINE_ID"/>
            </copy>
            <copy>
                <from expression="'invkDBRelHold - This activity invoke the Database Procedure for Releasing Hold and Updating DPS status in the Order Lines table'"/>
                <to variable="asnActivityName"/>
            </copy>
            <copy>
                <from expression="ora:getContentAsString(bpws:getVariableData('receiveInputIn','payload','/ns2:ReleaseOrderRequest'))"/>
                <to variable="messageDataVar"/>
            </copy>
            <copy>
                <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/UserName")'/>
                <to variable="invokeDBRelHoldIn" part="InputParameters"
                    query="/ns3:InputParameters/P_USER_NAME"/>
            </copy>
            <copy>
                <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/RespName")'/>
                <to variable="invokeDBRelHoldIn" part="InputParameters"
                    query="/ns3:InputParameters/P_RESP_NAME"/>
            </copy>
        </assign>
        <invoke name="invokeDBRelHold" partnerLink="plDBRelHold"
                portType="ns1:plDBRelHold_ptt" operation="plDBRelHold"
                inputVariable="invokeDBRelHoldIn"
                outputVariable="invokeDBRelHoldOut">
            <bpelx:annotation>
                <bpelx:documentation>This activity is used to assign the input
                                     message from the Nowdocs to invoke the DB
                                     Package for updation of the Release
                                     acknowedgement</bpelx:documentation>
            </bpelx:annotation>
        </invoke>
        <assign name="assignOutvartoMaster">
            <bpelx:annotation>
                <bpelx:documentation>This activity is used to assign the output
                                     message from DB Package to the Master
                                     Process</bpelx:documentation>
            </bpelx:annotation>
            <copy>
                <from variable="invokeDBRelHoldOut" part="OutputParameters"
                      query="/ns3:OutputParameters/X_STATUS"/>
                <to variable="callbackClientIn" part="payload"
                    query="/client:I1153_DPSNowPrintEBS_JobConfRel_ChildProcessResponse/client:result"/>
            </copy>
            <copy>
                <from expression="'callbackClient - This activity will Call back the Master'"/>
                <to variable="asnActivityName"/>
            </copy>
        </assign>
        <invoke name="callbackClient" partnerLink="client"
                portType="client:I1153_DPSNowPrintEBS_JobConfRel_ChildCallback"
                operation="onResult" inputVariable="callbackClientIn">
            <bpelx:annotation>
                <bpelx:documentation>This activity is ued to callbcak the Master

   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT xwslt.TRACKING_ID ,
  xwslt.ORDER_NBR ,
  xwslt.ORDER_DT ,
  xwslt.DELIVERY_DT ,
  xwslt.DELIVERY_ID ,
  xwslt.TRIP_ID ,
  xwslt.CONTAINER_ID ,
  xwslt.LANE_NBR ,
  src_loc_hl.location_code source_location_code ,
  ship_to_loc_hl.location_code ship_to_location_code ,
  xwslt.SCAN_IN_TMSTMP ,
  xwslt.SCAN_OUT_TMSTMP ,
  xwslt.ILP ,
  xwslt.STATUSCODE ,
  xwslt.STATUSDESC ,
  xwslt.ATTRIBUTE1 ,
  xwslt.ATTRIBUTE2 ,
  xwslt.ATTRIBUTE3 ,
  xwslt.ATTRIBUTE4 ,
  xwslt.ATTRIBUTE5 ,
  xwslt.ATTRIBUTE_CATEGORY ,
  xwslt.CREATED_BY ,
  xwslt.CREATION_DATE ,
  xwslt.LAST_UPDATED_BY ,
  xwslt.LAST_UPDATE_DATE ,
  xwslt.LAST_UPDATE_LOGIN ,
  xwslt.DELIVERY_DETAIL_ID
FROM XX_WSH_SHIP_LBL_TRACKING xwslt ,
  HR_LOCATIONS src_loc_hl ,
  hr_locations ship_to_loc_hl ,
  oe_order_headers_all OOH ,
  hz_cust_accounts HCA ,
  hz_party_sites HPS ,
  hz_cust_acct_sites_all HCASA ,
  hz_cust_site_uses_all HCSUA ,
  po_location_associations_all PLAA
WHERE 1                        =1
AND src_loc_hl.location_id     = xwslt.src_loc_id
AND OOH.order_number           = xwslt.order_nbr
AND HPS.party_id               = HCA.party_id
AND HCASA.party_site_id        = HPS.party_site_id
AND HCSUA.cust_acct_site_id    = HCASA.cust_acct_site_id
AND PLAA.site_use_id           = HCSUA.site_use_id
AND ship_to_loc_hl.location_id = PLAA.location_id
AND HCSUA.site_use_code        = 'SHIP_TO'
AND HCA.cust_account_id        = OOH.sold_to_org_id
AND HPS.location_id            = xwslt.LOC_ID
AND HCASA.cust_account_id      = OOH.sold_to_org_id
AND HCASA.org_id               = FND_PROFILE.VALUE('ORG_ID')
AND PLAA.org_id                = FND_PROFILE.VALUE('ORG_ID')
AND xwslt.ORDER_NBR            = :1
   ]]></SQLQuery>

<?xml version = '1.0' encoding = 'UTF-8'?>
<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="10.1.3_" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <oa:pageLayout id="region1" controllerClass="od.oracle.apps.xxfin.ap.idms.addresstypes.webui.XxApSupplierAddressPGCO" amDefName="od.oracle.apps.xxfin.ap.idms.addresstypes.server.XxSupplierAddressAM" title="Supplier Address Types" windowTitle="Address Types">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:contents>
            <oa:messageComponentLayout id="XXAddressSearch">
               <ui:contents>
                  <oa:messageLovInput id="AddressType" prompt="Address Type" queryable="true" dataType="NUMBER">
                     <lovContents>
                        <oa:listOfValues id="region2">
                           <ui:contents>
                              <oa:table name="region2" id="region3">
                                 <ui:contents>
                                    <oa:messageStyledText id="AddressNo" viewName="xxsupplieraddresslovVO1" viewAttr="AddressType" queryable="true" prompt="Address Type" dataType="NUMBER"/>
                                    <oa:messageStyledText id="Description" viewName="xxsupplieraddresslovVO1" viewAttr="Description" prompt="Description"/>
                                 </ui:contents>
                              </oa:table>
                           </ui:contents>
                        </oa:listOfValues>
                     </lovContents>
                     <lovMappings>
                        <lovMap id="lovMap1" lovItem="AddressNo" resultTo="AddressType" criteriaFrom="AddressType"/>
                     </lovMappings>
                  </oa:messageLovInput>
                  <oa:messageLayout id="messageLayout1">
                     <ui:contents>
                        <oa:submitButton id="Go" text="Go" prompt="Go"/>
                        <oa:submitButton id="Clear" text="Clear" prompt="Clear"/>
                     </ui:contents>
                  </oa:messageLayout>
               </ui:contents>
            </oa:messageComponentLayout>
            <oa:advancedTable id="SupplierAddressTable" viewName="XXSupAddressVO1" rendered="true">
               <ui:contents>
                  <oa:column id="column1">
                     <ui:columnHeader>
                        <oa:sortableHeader id="sortableHeader1" prompt="Address Type"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageTextInput id="Address_Type" readOnly="${oa.XXSupAddressVO1.DisableTrans}" prompt="Address Type" viewAttr="AddressType" dataType="NUMBER"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="column2">
                     <ui:columnHeader>
                        <oa:sortableHeader id="sortableHeader2" prompt="Description"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageTextInput id="Address_Description" readOnly="${oa.XXSupAddressVO1.DisableTrans}" viewAttr="AddressTypeDesc" required="yes" prompt="Description"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="column42">
                     <ui:columnHeader>
                        <oa:sortableHeader id="sortableHeader42" prompt="Supply Chain Dashboard"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageCheckBox id="DashboardInd" viewAttr="DashboardInd" readOnly="${oa.XXSupAddressVO1.DisableTrans}"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="column41">
                     <ui:columnHeader>
                        <oa:sortableHeader id="sortableHeader41" prompt="Vendor Portal"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageCheckBox id="VendExtrInd" viewAttr="VendorExtranetInd" readOnly="${oa.XXSupAddressVO1.DisableTrans}"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="column4">
                     <ui:columnHeader>
                        <oa:sortableHeader id="sortableHeader4" prompt="Enable Flag"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageCheckBox id="EnableFlag" viewAttr="EnableFlag" readOnly="${oa.XXSupAddressVO1.DisableTrans}"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="column33" rendered="true">
                     <ui:columnHeader>
                        <oa:sortableHeader id="sortableHeader3" prompt="Update"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:image id="Update" prompt="Update" source="updateicon_enabled.gif">
                           <ui:primaryClientAction>
                              <ui:fireAction/>
                           </ui:primaryClientAction>
                        </oa:image>
                     </ui:contents>
                  </oa:column>
               </ui:contents>
               <ui:tableActions>
                  <oa:flowLayout id="region5">
                     <ui:contents>
                        <oa:submitButton id="Create" text="Create" prompt="Create"/>
                        <oa:exportButton id="Export" exportAllRows="true" viewName="XXSupAddressVO1" text="Export"/>
                     </ui:contents>
                  </oa:flowLayout>
               </ui:tableActions>
            </oa:advancedTable>
            <oa:pageButtonBar id="region6" rendered="true">
               <ui:contents>
                  <oa:submitButton id="Save" text="Apply" prompt="Apply"/>
                  <oa:submitButton id="Cancel" text="Cancel" prompt="Cancel"/>
               </ui:contents>
            </oa:pageButtonBar>
         </ui:contents>
      </oa:pageLayout>
   </content>
</page>

<?xml version='1.0' encoding='windows-1252' ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODAPVendorRTVChoiceVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxfin.ap.pos.supplier.server.ODAPVendorRTVChoiceVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select 
(POV.VENDOR_NAME 
||' : '||POVS.VENDOR_SITE_CODE) as RTV_SITE_CODE,POVS.VENDOR_SITE_ID, POVS.VENDOR_ID
from ap_supplier_sites_all POVS, ap_suppliers POV
WHERE POVS.PAY_SITE_FLAG = 'N'
AND   POVS.PURCHASING_SITE_FLAG = 'N'
AND   POVS.VENDOR_SITE_CODE LIKE '%RTV%'
AND   POVS.ATTRIBUTE8 LIKE '%RTV%'
AND    POVS.VENDOR_ID = POV.VENDOR_ID
ORDER BY POV.VENDOR_NAME
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.41.57" />

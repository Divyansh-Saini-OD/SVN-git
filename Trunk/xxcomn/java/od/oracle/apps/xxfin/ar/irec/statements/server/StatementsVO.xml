<?xml version="1.0" encoding='windows-1252'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="StatementsVO"
   OrderBy="billing_dt desc, location, file_name"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxfin.ar.irec.statements.server.StatementsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT F.file_id, ' ' location, T.billing_dt, F.file_name, NVL(F.file_length,NVL(DBMS_LOB.getlength(F.file_data),0)) file_size, null selected_flag
  FROM XX_AR_EBL_FILE F
  JOIN XX_AR_EBL_TRANSMISSION T
    ON F.transmission_id=T.transmission_id
 WHERE T.customer_id=:1
   AND (F.file_data IS NOT NULL OR F.file_length IS NOT NULL)
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_version" Value="9.0.3.14.00" />
      <Attr Name="_CodeGenFlagNew" Value="32" />
   </DesignTime>

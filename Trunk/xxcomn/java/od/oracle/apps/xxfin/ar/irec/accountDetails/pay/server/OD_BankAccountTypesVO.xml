<ViewObject
   Name="OD_BankAccountTypesVO"
   Extends="oracle.apps.ar.irec.accountDetails.pay.server.BankAccountTypesVO"
   BindingStyle="Oracle"
   CustomQuery="true"
    RowClass="od.oracle.apps.xxfin.ar.irec.accountDetails.pay.server.OD_BankAccountTypesVORowImpl"
   ComponentClass="od.oracle.apps.xxfin.ar.irec.accountDetails.pay.server.OD_BankAccountTypesVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select irec.lookup_code account_type_code,
       irec.meaning account_type
from ar_lookups irec,
     fnd_lookup_values_vl iPayment
where irec.lookup_type = 'AR_IREC_BANK_ACCOUNT_TYPES'
and irec.enabled_flag = 'Y'
AND TRUNC(SYSDATE)    
      BETWEEN TRUNC(NVL(irec.START_DATE_ACTIVE,SYSDATE)) 
      AND     TRUNC(NVL(irec.END_DATE_ACTIVE,SYSDATE))
and iPayment.lookup_code = irec.lookup_code
and iPayment.lookup_type = 'IBY_BANKACCT_TYPES'
and iPayment.enabled_flag = 'Y'
AND TRUNC(SYSDATE)    
      BETWEEN TRUNC(NVL(iPayment.START_DATE_ACTIVE,SYSDATE)) 
      AND     TRUNC(NVL(iPayment.END_DATE_ACTIVE,SYSDATE)) 
	  AND irec.lookup_code not in ('CHECKING','SAVINGS','MONEYMRKT') /*Added for R12 Upgrade - E1356 CR868 */
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_version" Value="9.0.3.12.53" />
      <Attr Name="_CodeGenFlagNew" Value="36" />
   </DesignTime>

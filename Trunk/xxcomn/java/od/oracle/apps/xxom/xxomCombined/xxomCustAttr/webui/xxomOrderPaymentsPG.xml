<?xml version = '1.0' encoding = 'UTF-8'?>
<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="10.1.3_1312" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <oa:pageLayout id="PagelayoutRN" windowTitle="Payments" title="Payments" amDefName="od.oracle.apps.xxom.xxomCombined.xxomCustAttr.server.xxomCustAttrAM" controllerClass="od.oracle.apps.xxom.xxomCombined.xxomCustAttr.webui.xxomOrderPaymentsCO">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:contents>
            <oa:tableLayout id="xxomOrderPaymentsTBLLT" hAlign="center">
               <ui:contents>
                  <oa:rowLayout id="xxomOrderPaymentsROW1">
                     <ui:contents>
                        <oa:cellFormat id="xxomOrderPaymentsCELL1">
                           <ui:contents>
                              <oa:header id="xxomOrderPaymentsHDR" text="Order Details">
                                 <ui:contents>
                                    <oa:messageComponentLayout id="xxomOrderPaymentsRN">
                                       <ui:contents>
                                          <oa:messageStyledText id="OrderNumber" dataType="NUMBER" prompt="Order Number" viewName="xxomOrderHeaderAttrVO1" viewAttr="OrderNumber" styleClass="OraDataText"/>
                                          <oa:messageStyledText id="OrigSysDocumentRef" dataType="VARCHAR2" prompt="Orig Sys Document Ref" viewName="xxomOrderHeaderAttrVO1" viewAttr="OrigSysDocumentRef" maximumLength="50" styleClass="OraDataText"/>
                                          <oa:messageStyledText id="OrderDate" viewName="xxomOrderHeaderAttrVO1" viewAttr="OrderedDate" prompt="Order Date" styleClass="OraDataText"/>
                                          <oa:messageStyledText id="OrderType" dataType="VARCHAR2" prompt="Order Type" viewName="xxomOrderHeaderAttrVO1" viewAttr="OrderType" maximumLength="30" styleClass="OraDataText"/>
                                          <oa:messageStyledText id="OrderStatus" viewName="xxomOrderHeaderAttrVO1" viewAttr="FlowStatusCode" prompt="Status" styleClass="OraDataText"/>
                                          <oa:messageStyledText id="OrderTotal" viewName="xxomOrderHeaderAttrVO1" viewAttr="OrderTotal" prompt="Order Total" styleClass="OraDataText"/>
                                       </ui:contents>
                                    </oa:messageComponentLayout>
                                 </ui:contents>
                              </oa:header>
                           </ui:contents>
                        </oa:cellFormat>
                     </ui:contents>
                  </oa:rowLayout>
               </ui:contents>
            </oa:tableLayout>
            <oa:header id="PaymentDetailsRN" text="Payment Details">
               <ui:contents>
                  <oa:table id="xxomOrderPaymentsVO1" width="100%">
                     <ui:contents>
                        <oa:messageStyledText id="PaymentNumber" dataType="NUMBER" prompt="Payment NO" viewName="xxomOrderPaymentsVO1" viewAttr="PaymentNumber"/>
                        <oa:messageStyledText id="PaymentTrxId" dataType="NUMBER" prompt="Payment Trx NO" viewName="xxomOrderPaymentsVO1" viewAttr="PaymentTrxId"/>
                        <oa:messageStyledText id="PaymentLevelCode1" dataType="VARCHAR2" prompt="Payment Level" viewName="xxomOrderPaymentsVO1" viewAttr="PaymentLevelCode" maximumLength="30"/>
                        <oa:messageStyledText id="PaymentTypeCode" dataType="VARCHAR2" prompt="Payment Type" viewName="xxomOrderPaymentsVO1" viewAttr="PaymentTypeCode" maximumLength="30"/>
                        <oa:messageStyledText id="PrepaidAmount" dataType="NUMBER" prompt="Prepaid Amount" viewName="xxomOrderPaymentsVO1" viewAttr="PrepaidAmount"/>
                        <oa:messageStyledText id="PaymentAmount" dataType="NUMBER" prompt="Payment Amount" viewName="xxomOrderPaymentsVO1" viewAttr="PaymentAmount" totalValue="true"/>
                     </ui:contents>
                  </oa:table>
                  <oa:link id="ReturnToSearchLink" text="Return To Order Search" destination="OA.jsp?page=/od/oracle/apps/xxom/xxomCombined/xxomCustAttr/webui/xxomArchiveCustAttrSearchPG&amp;retainAM=Y"/>
                  <oa:spacer id="spaceItem1" width="5"/>
                  <oa:link id="HeaderAttributesLink" text="Header Attributes" destination="OA.jsp?page=/od/oracle/apps/xxom/xxomCombined/xxomCustAttr/webui/xxomOrderHeaderAttPG&amp;HeaderId={@HeaderId}&amp;retainAM=Y&amp;addBreadCrumb=Y"/>
                  <oa:spacer id="spaceItem2" width="5"/>
                  <oa:link id="OrderLinesLink" text="Order Lines" destination="OA.jsp?page=/od/oracle/apps/xxom/xxomCombined/xxomCustAttr/webui/xxomOrderLinesPG&amp;HeaderId={@HeaderId}&amp;retainAM=Y&amp;addBreadCrumb=Y"/>
               </ui:contents>
            </oa:header>
         </ui:contents>
      </oa:pageLayout>
   </content>
</page>

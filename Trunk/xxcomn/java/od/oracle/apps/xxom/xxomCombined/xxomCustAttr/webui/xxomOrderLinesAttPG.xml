                                          <oa:messageStyledText id="FlowStatusCode" dataType="VARCHAR2" prompt="Status" viewName="xxomOrderHeaderAttrVO1" viewAttr="FlowStatusCode" maximumLength="30" styleClass="OraDataText"/>
                                          <oa:messageStyledText id="OrderTotal" dataType="NUMBER" prompt="Order Total" viewName="xxomOrderHeaderAttrVO1" viewAttr="OrderTotal" styleClass="OraDataText"/>
                                       </ui:contents>
                                    </oa:messageComponentLayout>
                                 </ui:contents>
                              </oa:header>
                           </ui:contents>
                        </oa:cellFormat>
                     </ui:contents>
                  </oa:rowLayout>
                  <oa:rowLayout id="xxomOrderLinesAttROW2">
                     <ui:contents>
                        <oa:cellFormat id="xxomOrderLinesAttCELL2" vAlign="top">
                           <ui:contents>
                              <oa:header id="xxomOrderLinesAttHDR2" text="Line Details">
                                 <ui:contents>
                                    <oa:messageComponentLayout id="xxomOrderLinesAttRID2">
                                       <ui:contents>
                                          <oa:messageStyledText id="LineNumber" dataType="NUMBER" prompt="Line Number" viewName="xxomOrderLinesAttVO1" viewAttr="LineNumber" styleClass="OraDataText"/>

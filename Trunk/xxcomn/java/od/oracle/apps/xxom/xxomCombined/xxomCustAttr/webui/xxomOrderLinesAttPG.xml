                  <oa:link id="OrderLinesLink" destination="OA.jsp?page=/od/oracle/apps/xxom/xxomCombined/xxomCustAttr/webui/xxomOrderLinesPG&amp;HeaderId={@HeaderId}&amp;retainAM=Y&amp;addBreadCrumb=Y" text="Order Lines"/>

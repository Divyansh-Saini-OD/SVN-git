                  <oa:spacer id="spaceritem4" width="10"/>
                  <oa:link id="ReturnToSearchLink" destination="OA.jsp?page=/od/oracle/apps/xxom/xxomCombined/xxomCustAttr/webui/xxomArchiveCustAttrSearchPG&amp;retainAM=Y" text="Return To Search Page"/>

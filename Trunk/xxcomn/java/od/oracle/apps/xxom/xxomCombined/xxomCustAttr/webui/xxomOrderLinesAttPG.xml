                                       </ui:contents>
                                    </oa:messageComponentLayout>
                                 </ui:contents>
                              </oa:header>
                           </ui:contents>
                        </oa:cellFormat>
                     </ui:contents>
                  </oa:rowLayout>
               </ui:contents>
            </oa:tableLayout>
            <oa:spacer id="spaceer" height="10"/>

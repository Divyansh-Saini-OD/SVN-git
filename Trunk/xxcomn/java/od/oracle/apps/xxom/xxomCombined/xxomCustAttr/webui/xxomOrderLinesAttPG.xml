                        <oa:messageStyledText id="TaxableFlag" dataType="VARCHAR2" prompt="Taxable Flag" viewName="xxomOrderLinesAttVO1" viewAttr="TaxableFlag" maximumLength="1"/>
                        <oa:messageStyledText id="SkuDept" dataType="VARCHAR2" prompt="Sku Dept" viewName="xxomOrderLinesAttVO1" viewAttr="SkuDept" maximumLength="240"/>
                        <oa:messageStyledText id="AverageCost" dataType="NUMBER" prompt="Average Cost" viewName="xxomOrderLinesAttVO1" viewAttr="AverageCost"/>
                        <oa:messageStyledText id="CostCenterDept" dataType="VARCHAR2" prompt="Cost Center Dept" viewName="xxomOrderLinesAttVO1" viewAttr="CostCenterDept" maximumLength="30"/>
                        <oa:messageStyledText id="ItemSource" dataType="VARCHAR2" prompt="Item Source" viewName="xxomOrderLinesAttVO1" viewAttr="ItemSource" maximumLength="30"/>
                        <oa:messageStyledText id="VendorSiteId" dataType="NUMBER" prompt="Vendor Site" viewName="xxomOrderLinesAttVO1" viewAttr="VendorSiteId"/>

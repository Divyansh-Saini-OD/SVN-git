<ViewObject
   Name="ODRepSrchLOV"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.stl.admin.lov.server.ODRepSrchLOVImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT  mem.RESOURCE_ID , mem.GROUP_ID ,rol.ROLE_ID ,rol.ROLE_NAME, r.resource_name, g.group_name
    ,rrl.START_DATE_ACTIVE ,rrl.END_DATE_ACTIVE ,rrl.attribute15 legarcy_rep
  from jtf_rs_group_members mem, jtf_rs_role_relations rrl, jtf_rs_roles_vl rol, jtf_rs_groups_vl g, jtf_rs_resource_extns_vl r
  WHERE mem.group_member_id = rrl.role_resource_id and nvl(rrl.delete_flag, 'N') <> 'Y' and nvl(mem.delete_flag, 'N') <> 'Y' and rrl.role_resource_type = 'RS_GROUP_MEMBER' and rrl.role_id = rol.role_id
    and rol.member_flag='Y' and r.resource_id=mem.RESOURCE_ID and g.group_id=mem.group_id
   ]]></SQLQuery>

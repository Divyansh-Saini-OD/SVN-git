<ViewObject
   Name="ODCurrencyVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.addattr.tempcl.server.ODCurrencyVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.addattr.tempcl.server.ODCurrencyVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT pa.currency_code, 
       a.account_number, 
       a.cust_account_id,
       p.cust_account_profile_id, 
       pa.cust_acct_profile_amt_id
  FROM hz_cust_accounts a, 
       hz_customer_profiles p, 
       hz_cust_profile_amts pa
 WHERE 1 = 1
   AND a.cust_account_id = p.cust_account_id
   AND p.cust_account_profile_id = pa.cust_account_profile_id
--   AND p.status = 'A'
--   AND a.status = 'A'
   AND p.cust_account_id = :1
   AND p.cust_account_profile_id = :2
   ]]></SQLQuery>

   Name="ODSCSOpportunityDtlsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.scs.fdk.server.ODSCSOpportunityDtlsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_ALL"
   FetchSize="10"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT    OpportunityEO.lead_id,
          OpportunityEO.description,
          OpportunityEO.status,
          OpportunityEO.sales_stage_id,
          OpportunityEO.total_amount,
          OpportunityEO.currency_code,
          OpportunityEO.decision_date,
          OpportunityEO.win_probability,
          OpportunityEO.close_reason,
          OpportunityEO.sales_methodology_id,
          OpportunityEO.channel_code,
          DECODE(OpportunityEO.sales_methodology_id, NULL, 'N','Y') 
                AS  sales_meth_flag,
          OpportunityEO.customer_id,
          hp.party_name,
          hp.party_type,
          OpportunityEO.attribute1,
          OpportunityEO.attribute2,
          OpportunityEO.attribute3,
          OpportunityEO.attribute4,
          OpportunityEO.attribute5,
          OpportunityEO.attribute6,
          OpportunityEO.attribute7,
          OpportunityEO.attribute8,
          OpportunityEO.attribute9,
          OpportunityEO.attribute10,
          OpportunityEO.attribute11,
          OpportunityEO.attribute12,
          OpportunityEO.attribute13,
          OpportunityEO.attribute14,
          OpportunityEO.attribute15,
          OpportunityEO.attribute_category,
 OpportunityEO.source_promotion_id,
OpportunityEO.object_version_number,
DECODE(OpportunityEO.sales_stage_id, NULL, 'N','Y')   AS  sales_stage_flag,
OpportunityEO.address_id,
hz_format_pub.format_address(hl.location_id, null, null, ', ', null, null,null, null)   as address   ,
OpportunityEO.prm_referral_code,
OpportunityEO.lead_number,
OpportunityEO.vehicle_response_code
,hps.orig_system_reference
,SUBSTR(hps.orig_system_reference, 1, INSTR(hps.orig_system_reference,'-', 1, 1)-1) customer
,SUBSTR(hps.orig_system_reference, INSTR(hps.orig_system_reference,'-', 1, 1)+1,(INSTR(hps.orig_system_reference,'-', 1, 2))-(INSTR(hps.orig_system_reference,'-', 1, 1)+1)
) sequence
,cav.resource_id
,cav.role_id
,cav.group_id
,cav.legcy_id
FROM     as_leads_all OpportunityEO,
          hz_parties hp,
          hz_party_sites hps,
          hz_locations hl,
          fnd_territories_tl ftt,
          XX_TM_CURR_ASSIGN_LEGCY_RPID_V cav
 WHERE    OpportunityEO.customer_id = hp.party_id
AND    OpportunityEO.address_id = hps.party_site_id(+)
AND    OpportunityEO.customer_id = hps.party_id (+)
AND    hps.location_id = hl.location_id (+)
AND    hl.country = ftt.territory_code (+)
AND    ftt.language (+) = USERENV('LANG')
and OpportunityEO.lead_id=cav.entity_id(+)
and cav.entity_type(+)='OPPORTUNITY'
AND      OpportunityEO.lead_id = :1
   ]]></SQLQuery>

<ViewObject
   Name="ODSCSFDKHdrDtlsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.scs.fdk.server.ODSCSFDKHdrDtlsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select h.FDBK_ID,h.creation_date LogCreateDt,customer_account_id,address_id ,h.last_updated_emp, h.sales_territory_id SALES_PERSON_ID,  
  (select PHONE from 
 XX_SCS_ORG_CONTACT_INFO_V c , XXCRM.XXSCS_FDBK_LINE_DTL d where c.contact_id=d.fdk_txt 
    and d.FDK_CODE= 'CONTACT_ID' and d.FDBK_ID=h.FDBK_ID) PHONE
   ,(select EMAIL_ADDRESS from 
 XX_SCS_ORG_CONTACT_INFO_V c , XXCRM.XXSCS_FDBK_LINE_DTL d where c.contact_id=d.fdk_txt 
    and d.FDK_CODE= 'CONTACT_ID' and d.FDBK_ID=h.FDBK_ID) EMAIL_ADDRESS
   ,(select LEGCY_ID from 
 XX_SCS_ORG_CONTACT_INFO_V c , XXCRM.XXSCS_FDBK_LINE_DTL d where c.contact_id=d.fdk_txt 
    and d.FDK_CODE= 'CONTACT_ID' and d.FDBK_ID=h.FDBK_ID) LEGCY_ID
   ,(select CONTACT_FULL_NAME from 
 XX_SCS_ORG_CONTACT_INFO_V c , XXCRM.XXSCS_FDBK_LINE_DTL d where c.contact_id=TO_NUMBER (d.fdk_txt) 
    and d.FDK_CODE= 'CONTACT_ID' and d.FDBK_ID=h.FDBK_ID) CONTACT_FULL_NAME
 from
  XXCRM.XXSCS_FDBK_HDR h
 where h.FDBK_ID=:1
   ]]></SQLQuery>

<ViewObject
   Name="ODCustomerSearchResultsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.asn.custsrch.common.customer.server.ODCustomerSearchResultsVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.custsrch.common.customer.server.ODCustomerSearchResultsVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT /*+ USE_NL(ps,hp,terr,hl) PUSH_PRED(sitedemov) */ 
  ps.party_site_id,
  hp.party_id,
  hp.party_name,
  (CASE (
         	SELECT COUNT(1)	
         	FROM DUAL	
         	WHERE EXISTS
		 	(SELECT 1
         		FROM hz_cust_accounts hca
         		WHERE hca.party_id = hp.party_id
         		AND ROWNUM = 1
         		AND status = 'A'))
   	   	WHEN 0
        	THEN hp.party_number
   		ELSE (SELECT orig_system_reference
         		FROM hz_cust_accounts
         		WHERE party_id = hp.party_id
		 	AND ROWNUM = 1
         	AND status = 'A')
   END) prospect,
  (CASE (
         	SELECT COUNT(1)	
         	FROM DUAL	
         	WHERE EXISTS
		 	(SELECT 1
         		FROM hz_cust_accounts hca
         		WHERE hca.party_id = hp.party_id
         		AND ROWNUM = 1))
   		WHEN 0
        	THEN 'Prospect'
   		ELSE 'Customer'
  END) prospect_cust,
 (SELECT customer_type
	FROM hz_cust_accounts hca
	WHERE hca.party_id = hp.party_id
	AND ROWNUM = 1) customer_type,
  ps.party_site_number,
  hz_utility_v2pub.get_all_purposes(ps.party_site_id) site_use_type,
  ' ' address_key,
            (SELECT (                
                     CASE NVL(address_style,'-9X9Y9Z') WHEN 'AS_DEFAULT' THEN                
                            address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address4
                          || '.'
                          || state
                          || '.'
                          || county
                          || '.'
                          || city
                          || '.'
                          || postal_code                                       
                     WHEN '-9X9Y9Z' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address4
                          || '.'
                          || state
                          || '.'
                          || county
                          || '.'
                          || city
                          || '.'
                          || postal_code
                     WHEN 'JP' THEN
                             postal_code
                          || '.'
                          || state
                          || '.'
                          || city
                          || '.'
                          || address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address_lines_phonetic
                     WHEN 'NE' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || state
                          || '.'
                          || postal_code
                          || '.'
                          || city
                     WHEN 'POSTAL_ADDR_DEF' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address4
                          || '.'
                          || city
                          || '.'
                          || county
                          || '.'
                          || state
                          || '.'
                          || province
                          || '.'
                          || postal_code
                     WHEN 'POSTAL_ADDR_US' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address4
                          || '.'
                          || city
                          || '.'
                          || county
                          || '.'
                          || state
                          || '.'
                          || postal_code
                     WHEN 'SA' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || city
                          || '.'
                          || province
                          || '.'
                          || state
                          || '.'
                          || county
                          || '.'
                          || postal_code
                     WHEN 'SE' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || postal_code
                          || '.'
                          || city
                          || '.'
                          || state
                     WHEN 'UAA' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || city
                          || '.'
                          || state
                          || '.'
                          || postal_code
                 WHEN 'AS_DEFAULT_CA' THEN
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || city
                          || '.'
                          || province
                          || '.'
                          || postal_code
                 ELSE
                             address1
                          || '.'
                          || address2
                          || '.'
                          || address3
                          || '.'
                          || address4
                          || '.'
                          || state
                          || '.'
                          || county
                          || '.'
                          || city
                          || '.'
                          || postal_code
                     END )
            FROM hz_locations
            WHERE location_id = ps.location_id) address,
(CASE 
(SELECT COUNT(1) FROM DUAL WHERE EXISTS (select 1 from hz_cust_accounts hca where hca.party_id =    hp.party_id and rownum = 1 and status = 'A'))
 WHEN 0 THEN  ps.party_site_number  
 ELSE 
 (REGEXP_REPLACE((REGEXP_SUBSTR(ps.orig_system_reference,'-[^-]+-')),'[-]',''))
 END) site_seq_num,
  (select email_address
   FROM hz_contact_points HZCP
   WHERE  ps.party_site_id = HZCP.OWNER_TABLE_ID  AND
          HZCP.PRIMARY_FLAG = 'Y' and HZCP.OWNER_TABLE_NAME = 'HZ_PARTY_SITES' and ((email_address is not null) or (email_address <> '')) and rownum = 1) email,
  (select (PHONE_COUNTRY_CODE||PHONE_AREA_CODE||PHONE_NUMBER||PHONE_EXTENSION)pphone    
   from  hz_contact_points HZCP   
   where ps.party_site_id =  HZCP.OWNER_TABLE_ID AND
         HZCP.PRIMARY_FLAG = 'Y'         and
       HZCP.OWNER_TABLE_NAME = 'HZ_PARTY_SITES' and
         PHONE_LINE_TYPE = 'GEN' and ((PHONE_NUMBER is not null) or (PHONE_NUMBER <> '')) and rownum = 1) pphone, 
                     (CASE
                        SIGN((SELECT COUNT(1) FROM DUAL WHERE EXISTS
						          (SELECT 1
                                   FROM
                                        jtf_rs_resource_extns resource_det,
                                        (SELECT 
											terr.named_acct_terr_id, terr_ent.entity_type, terr_ent.entity_id,
											terr_rsc.resource_id, terr_rsc.resource_role_id, terr_rsc.GROUP_ID,
											terr_ent.full_access_flag
										FROM xx_tm_nam_terr_rsc_dtls terr_rsc,
											xx_tm_nam_terr_defn terr,
											xx_tm_nam_terr_entity_dtls terr_ent
										WHERE terr.named_acct_terr_id = terr_rsc.named_acct_terr_id
											AND terr_ent.named_acct_terr_id = terr_rsc.named_acct_terr_id
											AND terr_ent.named_acct_terr_id = terr.named_acct_terr_id
											AND SYSDATE BETWEEN NVL (terr.start_date_active, SYSDATE - 1)
									                   AND NVL (terr.end_date_active, SYSDATE + 1)
											AND SYSDATE BETWEEN NVL (terr_ent.start_date_active, SYSDATE - 1)
									                   AND NVL (terr_ent.end_date_active, SYSDATE + 1)
											AND SYSDATE BETWEEN NVL (terr_rsc.start_date_active, SYSDATE - 1)
									                   AND NVL (terr_rsc.end_date_active, SYSDATE + 1)
											AND NVL (terr.status, 'A') = 'A'
											AND NVL (terr_ent.status, 'A') = 'A'
											AND NVL (terr_rsc.status, 'A') = 'A') terr_vw
                                  WHERE  terr_vw.resource_id = resource_det.resource_id
                                  AND terr_vw.entity_id = ps.party_site_id
                                  AND terr_vw.entity_type = 'PARTY_SITE'
                                  AND EXISTS (SELECT 1
                                                FROM jtf_rs_roles_b        role_det
                                                    ,jtf_rs_role_relations rassign
                                                WHERE role_det.role_id = rassign.role_id
                                                AND   role_det.attribute15 = 'BSD'
                                                AND   rassign.role_resource_type = 'RS_INDIVIDUAL'
                                                AND   rassign.role_resource_id = resource_det.resource_id 
                                               )                              
                                  AND  rownum = 1)
                                ))
                        WHEN 1  
                        THEN (SELECT resource_det.source_name s_name
                                   FROM
                                        jtf_rs_resource_extns resource_det,
                                        (SELECT 
											terr.named_acct_terr_id, terr_ent.entity_type, terr_ent.entity_id,
											terr_rsc.resource_id, terr_rsc.resource_role_id, terr_rsc.GROUP_ID,
											terr_ent.full_access_flag
										FROM xx_tm_nam_terr_rsc_dtls terr_rsc,
											xx_tm_nam_terr_defn terr,
											xx_tm_nam_terr_entity_dtls terr_ent
										WHERE terr.named_acct_terr_id = terr_rsc.named_acct_terr_id
											AND terr_ent.named_acct_terr_id = terr_rsc.named_acct_terr_id
											AND terr_ent.named_acct_terr_id = terr.named_acct_terr_id
											AND SYSDATE BETWEEN NVL (terr.start_date_active, SYSDATE - 1)
									                   AND NVL (terr.end_date_active, SYSDATE + 1)
											AND SYSDATE BETWEEN NVL (terr_ent.start_date_active, SYSDATE - 1)
									                   AND NVL (terr_ent.end_date_active, SYSDATE + 1)
											AND SYSDATE BETWEEN NVL (terr_rsc.start_date_active, SYSDATE - 1)
									                   AND NVL (terr_rsc.end_date_active, SYSDATE + 1)
											AND NVL (terr.status, 'A') = 'A'
											AND NVL (terr_ent.status, 'A') = 'A'
											AND NVL (terr_rsc.status, 'A') = 'A') terr_vw
                                  WHERE  terr_vw.resource_id = resource_det.resource_id
                                  AND terr_vw.entity_id = ps.party_site_id
                                  AND terr_vw.entity_type = 'PARTY_SITE'
                                  AND EXISTS (SELECT 1
                                                FROM jtf_rs_roles_b        role_det
                                                    ,jtf_rs_role_relations rassign
                                                WHERE role_det.role_id = rassign.role_id
                                                AND   role_det.attribute15 = 'BSD'
                                                AND   rassign.role_resource_type = 'RS_INDIVIDUAL'
                                                AND   rassign.role_resource_id = resource_det.resource_id 
                                               )                              
                                  AND  rownum = 1)
                     ELSE
                     (SELECT resource_det.source_name s_name
                                   FROM
                                        jtf_rs_resource_extns resource_det,
                                        (SELECT 
											terr.named_acct_terr_id, terr_ent.entity_type, terr_ent.entity_id,
											terr_rsc.resource_id, terr_rsc.resource_role_id, terr_rsc.GROUP_ID,
											terr_ent.full_access_flag
										FROM xx_tm_nam_terr_rsc_dtls terr_rsc,
											xx_tm_nam_terr_defn terr,
											xx_tm_nam_terr_entity_dtls terr_ent
										WHERE terr.named_acct_terr_id = terr_rsc.named_acct_terr_id
											AND terr_ent.named_acct_terr_id = terr_rsc.named_acct_terr_id
											AND terr_ent.named_acct_terr_id = terr.named_acct_terr_id
											AND SYSDATE BETWEEN NVL (terr.start_date_active, SYSDATE - 1)
									                   AND NVL (terr.end_date_active, SYSDATE + 1)
											AND SYSDATE BETWEEN NVL (terr_ent.start_date_active, SYSDATE - 1)
									                   AND NVL (terr_ent.end_date_active, SYSDATE + 1)
											AND SYSDATE BETWEEN NVL (terr_rsc.start_date_active, SYSDATE - 1)
									                   AND NVL (terr_rsc.end_date_active, SYSDATE + 1)
											AND NVL (terr.status, 'A') = 'A'
											AND NVL (terr_ent.status, 'A') = 'A'
											AND NVL (terr_rsc.status, 'A') = 'A') terr_vw                                        
                                  WHERE  terr_vw.resource_id = resource_det.resource_id
                                  AND terr_vw.entity_id = ps.party_site_id
                                  AND terr_vw.entity_type = 'PARTY_SITE'
                                  AND EXISTS (SELECT 1
                                                FROM jtf_rs_roles_b        role_det
                                                    ,jtf_rs_role_relations rassign
                                                WHERE role_det.role_id = rassign.role_id
                                                AND   role_det.attribute14 = 'BDM'
                                                AND   rassign.role_resource_type = 'RS_INDIVIDUAL'
                                                AND   rassign.role_resource_id = resource_det.resource_id 
                                               )                              
                                  AND  rownum = 1)      
                     END
                    ) primary_sales_person,
                    (CASE NVL(sitedemov.sitedemo_cancross,'-9X9Y9Z')
                        WHEN '-9X9Y9Z'
                           THEN '*' || hp.attribute1
                        ELSE sitedemov.sitedemo_cancross
                     END
                    ) can_cross_sell,
                    (CASE NVL(sitedemov.sitedemo_contactfreq,'-9X9Y9Z')
                        WHEN '-9X9Y9Z'
                           THEN '*' || hp.attribute2
                        ELSE sitedemov.sitedemo_contactfreq
                     END
                    ) conct_freq,
                    (CASE NVL(to_char(sitedemov.SITEDEMO_WCW),'-9X9Y9Z')
                        WHEN '-9X9Y9Z'
                           THEN '*' || hp.attribute10
                        ELSE TO_CHAR (sitedemov.sitedemo_wcw)
                     END
                    ) white_collar_wrk,
hp.party_type,  
hp.party_number,  
hp.tax_reference,  
hp.jgzz_fiscal_code taxPayer_id,  
hp.duns_number_c duns_number,  
hp.known_as known_as,  
hp.known_as2 known_as2,  
hp.known_as3 known_as3,  
hp.known_as4 known_as4,  
hp.known_as5 known_as5,  
hp.organization_name_phonetic,  
hp.person_first_name_phonetic,  
hp.person_last_name_phonetic,  
ps.location_id,  
terr.territory_short_name country,  
hp.url primary_url,  
'100%' percentage,  
'N' Select_flag,  
'N' disable_hgrid_flag,  
'Y' enable_hgrid_flag,
' ' source_system,
hl.city,
hl.state,
hl.province,
hl.postal_code,
hp.sic_code,
hp.sic_code_type,
'' customer_category,
(select XX_CDH_DQM_ACQUIRE_PKG.Get_Sales_Channel(hp.party_id,null,null,null) from dual) sales_channel,
(select XX_CDH_DQM_ACQUIRE_PKG.Get_Micr_Num(hp.party_id,null,null,null) from dual) micr_number,
'' od_customer_type,
'' ship_to_sequence_num,
'' billing_number,
'' classification,
'' relationship_role,
ps.status site_status,
hp.status party_status,
ps.identifying_address_flag,
hp.category_code,
(select PHONE_COUNTRY_CODE    
   from  hz_contact_points HZCP   
   where ps.party_site_id =  HZCP.OWNER_TABLE_ID AND
         HZCP.PRIMARY_FLAG = 'Y'         and
       HZCP.OWNER_TABLE_NAME = 'HZ_PARTY_SITES' and
         PHONE_LINE_TYPE = 'GEN' and ((PHONE_NUMBER is not null) or (PHONE_NUMBER <> '')) and rownum = 1) PHONE_COUNTRY_CODE,
(select PHONE_AREA_CODE    
   from  hz_contact_points HZCP   
   where ps.party_site_id =  HZCP.OWNER_TABLE_ID AND
         HZCP.PRIMARY_FLAG = 'Y'         and
       HZCP.OWNER_TABLE_NAME = 'HZ_PARTY_SITES' and
         PHONE_LINE_TYPE = 'GEN' and ((PHONE_NUMBER is not null) or (PHONE_NUMBER <> '')) and rownum = 1) PHONE_AREA_CODE,
(select PHONE_NUMBER    
   from  hz_contact_points HZCP   
   where ps.party_site_id =  HZCP.OWNER_TABLE_ID AND
         HZCP.PRIMARY_FLAG = 'Y'         and
       HZCP.OWNER_TABLE_NAME = 'HZ_PARTY_SITES' and
         PHONE_LINE_TYPE = 'GEN' and ((PHONE_NUMBER is not null) or (PHONE_NUMBER <> '')) and rownum = 1) PHONE_NUMBER,
(select PHONE_EXTENSION
   from  hz_contact_points HZCP   
   where ps.party_site_id =  HZCP.OWNER_TABLE_ID AND
         HZCP.PRIMARY_FLAG = 'Y'         and
       HZCP.OWNER_TABLE_NAME = 'HZ_PARTY_SITES' and
         PHONE_LINE_TYPE = 'GEN' and ((PHONE_NUMBER is not null) or (PHONE_NUMBER <> '')) and rownum = 1) PHONE_EXTENSION
FROM 
hz_parties hp,  
hz_party_sites ps,  
fnd_territories_vl terr,  
XX_CDH_S_EXT_SITEDEMO_V sitedemov,
hz_locations hl 
WHERE   
hp.party_type in ('PERSON', 'ORGANIZATION') and
ps.party_id = hp.party_id and
ps.party_site_id = sitedemov.party_site_id(+) and
hp.country = terr.territory_code and
ps.location_id = hl.location_id
   ]]></SQLQuery>

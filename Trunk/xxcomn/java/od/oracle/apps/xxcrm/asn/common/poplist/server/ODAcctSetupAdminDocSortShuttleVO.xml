   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT LKP.lookup_code, LKP.lookup_code || ' = ' ||LKP.meaning meaning 
FROM fnd_lookups LKP, XX_CDH_DOC_TEMPLATE_ATRRIBUTES TEMPATTR
WHERE  TEMPATTR.DOC_ATTRIB_VALUE=LKP.lookup_code
AND LKP.lookup_type='XXOD_ASN_ACCT_SETUP_DOC_SORTS'
AND TEMPATTR.DOC_ATTRIB_TYPE_CODE='SORT'
AND TEMPATTR.DOC_TEMPLATE_ID=:1
   ]]></SQLQuery>

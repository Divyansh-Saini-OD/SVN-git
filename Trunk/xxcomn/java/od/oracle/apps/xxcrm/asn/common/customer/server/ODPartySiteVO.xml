   <SQLQuery><![CDATA[
select 
hzps.party_site_id, 
hzps.party_id,
hzps.location_id,
hzps.party_site_number,
hzl.country,
hzl.address1,
hzl.address2,
hzl.address3,
hzl.address4,
hzl.city,
hzl.state,
hzl.county,
hzl.postal_code,
DECODE(hzps.location_id,null,null,hz_format_pub.format_address(hzps.location_id,null,null,',')) primary_address
from
hz_party_sites hzps,
hz_locations hzl
where hzps.location_id = hzl.location_id
   ]]></SQLQuery>

   <SQLQuery><![CDATA[
SELECT OpportunityEO.lead_id as LeadId, OpportunityEO.lead_number as leadNumber ,  hp.party_name as PartyName,OpportunityEO.TOTAL_AMOUNT as Amount, OpportunityEO.Decision_Date  as  CloseDate, OpportunityEO.win_probability as winProbability, SUBSTRB(OpportunityEO.description, 1,240) as Description, ast.meaning as OpptyStatusName,
fnt.name as CurrencyName, 
OpportunityEO.currency_code as CurrencyCode
from as_leads_all  OpportunityEO  , hz_parties hp ,
as_statuses_tl ast,fnd_currencies_tl fnt 
WHERE   OpportunityEO.customer_id = hp.party_id   
 AND  OpportunityEO.status = ast.status_code    
AND  ast.language = USERENV('LANG')
and OpportunityEO.currency_code = fnt.currency_code 
and fnt.language = USERENV('LANG')
   ]]></SQLQuery>

<ViewObject
   Name="ODSalesrepsLovVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.common.customer.lov.server.ODSalesrepsLovVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT r.source_name as name, g.group_name, g.GROUP_ID, ro.role_name, ro.role_id,
       r.resource_id, r.source_job_title, r.source_email, r.source_phone,
       rr.attribute15
  FROM jtf_rs_resource_extns r,
       jtf_rs_role_relations rr,
       jtf_rs_roles_vl ro,
       jtf_rs_group_usages u,
       jtf_rs_group_members jgm,
       jtf_rs_groups_vl g,
       fnd_lookups l
 WHERE rr.role_resource_type = 'RS_GROUP_MEMBER'
   AND rr.role_resource_id = jgm.group_member_id
   AND rr.role_id = ro.role_id
   AND ro.role_type_code = 'SALES'
   AND u.USAGE = 'SALES'
   AND l.lookup_type = 'XX_CRM_CUST_CREATE_ROLE_LOV'
   AND l.lookup_code = ro.role_name
   AND r.resource_id = jgm.resource_id
   AND jgm.GROUP_ID = g.GROUP_ID
   AND u.GROUP_ID = g.GROUP_ID
   AND NVL (jgm.delete_flag, 'N') = 'N'
   AND ro.attribute15 = 'BSD'
   AND NVL (rr.delete_flag, 'N') = 'N'
   AND ro.member_flag = 'Y'
   AND ro.attribute14 <> 'OT'
   AND rr.start_date_active <= SYSDATE
   AND (rr.end_date_active >= SYSDATE OR rr.end_date_active IS NULL)
   AND r.start_date_active <= SYSDATE
   AND (r.end_date_active >= SYSDATE OR r.end_date_active IS NULL)
   AND g.start_date_active <= SYSDATE
   AND (g.end_date_active >= SYSDATE OR g.end_date_active IS NULL)
   AND l.start_date_active <= SYSDATE
   AND (l.end_date_active >= SYSDATE OR l.end_date_active IS NULL)
   AND NVL (l.enabled_flag, 'Y') = 'Y'
   ]]></SQLQuery>

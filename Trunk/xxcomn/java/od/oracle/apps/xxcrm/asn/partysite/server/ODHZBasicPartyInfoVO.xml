<ViewObject
   Name="ODHZBasicPartyInfoVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.partysite.server.ODHZBasicPartyInfoVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT
  p.party_id,
  p.party_name  org_name,
  DECODE(ca.account_number, null,'P'||p.party_number,ca.account_number) account_number,
  hz_format_phone_v2pub.get_formatted_phone(cpp.contact_point_id) primary_phone,
  decode(ps.location_id,null,null,hz_format_pub.format_address(ps.location_id,null,null,',')) primary_address,
  p.attribute24  revenue_band,
  p.curr_fy_potential_revenue,
  p.next_fy_potential_revenue,
  p.attribute12      od_white_collar_worker,
  p.attribute10      white_collar_worker,
  hope.n_ext_attr1   potential_spend
FROM
  hz_organization_profiles_v op,
  hz_parties p,
  hz_cust_accounts ca,
  hz_party_sites ps,
  hz_contact_points cpp,
  fnd_currencies_vl ct,
  hz_locations l,
  ar_lookups al,
  hz_org_profiles_ext_vl hope,
  ego_attr_groups_v egoag
WHERE
  p.party_id = :1 and
  op.party_id(+) = p.party_id and
  ca.party_id(+) = p.party_id and
  ps.party_id = p.party_id and
  ps.identifying_address_flag = 'Y' and
  cpp.owner_table_id(+) = p.party_id and
  cpp.owner_table_name(+) = 'HZ_PARTIES' and
  cpp.contact_point_type(+) = 'PHONE' and
  cpp.primary_flag(+) = 'Y' and
  cpp.status(+) = 'A' and
  op.pref_functional_currency = ct.currency_code(+) and
  ps.location_id = l.location_id(+) and
  al.lookup_type(+) = 'MONTH' and
  al.lookup_code(+) = op.fiscal_yearend_month and
  hope.organization_profile_id(+) = op.organization_profile_id and
  egoag.attr_group_id(+) = hope.attr_group_id and
  egoag.attr_group_type(+) = 'HZ_ORG_PROFILES_GROUP' and
  egoag.attr_group_name(+) = 'SPEND'
   ]]></SQLQuery>

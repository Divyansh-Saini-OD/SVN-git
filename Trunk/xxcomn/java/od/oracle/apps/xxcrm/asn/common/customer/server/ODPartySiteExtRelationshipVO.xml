<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODPartySiteExtRelationshipVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.common.customer.server.ODPartySiteExtRelationshipVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT  
    party_site_id,
    c_ext_attr1,
    n_ext_attr1,
    d_ext_attr1,
    d_ext_attr2
FROM
    HZ_PARTY_SITES_EXT_B hzpsb,
    ego_attr_groups_v egoag
WHERE
    egoag.attr_group_id = hzpsb.attr_group_id AND 
    egoag.attr_group_type = 'HZ_PARTY_SITES_GROUP'AND
    egoag.attr_group_name = 'SITE_CONTACTS'AND
    party_site_id = :partySiteId AND 
    n_ext_attr1 = :relationshipId AND
    c_ext_attr1 = 'A'  AND 
    TRUNC(SYSDATE) BETWEEN TRUNC(d_ext_attr1) AND NVL(TRUNC(d_ext_attr2),TRUNC(SYSDATE+1))
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_version" Value="9.0.3.13.93" />
      <Attr Name="_CodeGenFlagNew" Value="36" />
   </DesignTime>

<ViewObject
   Name="ODPartySiteListVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.asn.common.customer.server.ODPartySiteListVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.common.customer.server.ODPartySiteListVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
  hzl.location_id,
  hzp.party_id,
  hzps.party_site_id, 
  hzl.address1,
  hzl.city,
  hzl.state,
  hzl.postal_code,
  hzp.party_name,   DECODE(hzps.location_id,null,null,hz_format_pub.format_address(hzps.location_id,null,null,',')) primary_address,
   hzps.identifying_address_flag
FROM
  hz_parties hzp,
  hz_party_sites hzps, 
  hz_locations hzl
WHERE
  hzp.party_id = hzps.party_id
  AND hzps.location_id = hzl.location_id
  AND hzp.party_id = :partyid
  AND hzps.status = 'A'
ORDER BY identifying_address_flag DESC
   ]]></SQLQuery>

<ViewObject
   Name="ODHzPartySiteContactsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.asn.common.customer.server.ODHzPartySiteContactsVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.common.customer.server.ODHzPartySiteContactsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select HzPuiRelationshipsEO.relationship_id,
       HzPuiRelationshipsEO.party_id,
       SUBJECT_ID, 
       HZ_PARTIES.party_id org_party_id,
       HZ_PARTIES.party_name,
       RelationshipParty.party_id contact_id,
       TRIM(HZ_PARTIES.person_first_name||' '||HZ_PARTIES.person_last_name) CONTACT_NAME,
       HZ_PARTIES.known_as, 
       HZ_PARTIES.party_number,       HZ_RELATIONSHIP_GROUPINGS_V.role_meaning_singular,
       HZ_ORG_CONTACTS.job_title, 
       fnd1.meaning job_title_code, 
       HZ_ORG_CONTACTS.department,
       fnd2.meaning department_code,        hz_format_phone_v2pub.get_formatted_phone(RelationshipParty.primary_phone_contact_pt_id) primary_phone,       decode(RelationshipParty.primary_phone_contact_pt_id,
              null, null,
              hz_format_phone_v2pub.get_formatted_phone (
                RelationshipParty.primary_phone_country_code, RelationshipParty.primary_phone_area_code,
                RelationshipParty.primary_phone_number, RelationshipParty.primary_phone_extension,
                RelationshipParty.primary_phone_line_type))   phone,
       DECODE(HZ_LOCATIONS.location_id,null,null,hz_format_pub.format_address(HZ_LOCATIONS.location_id,null,null,',')) primary_address,
       RelationshipParty.primary_phone_country_code,
       RelationshipParty.primary_phone_area_code,
       RelationshipParty.primary_phone_number,
       RelationshipParty.primary_phone_extension,
       RelationshipParty.primary_phone_line_type,
       decode(RelationshipParty.primary_phone_contact_pt_id,
              null, 'NoRestriction',
              decode(hz_utility_v2pub.is_restriction_exist(
                       'HZ_CONTACT_POINTS',
                       RelationshipParty.primary_phone_contact_pt_id,
                       'DO_NOT'),
                     'Y', 'HasRestriction',
                     'N', 'NoRestriction')) AS CONTACT_RESTRICTION,
       RelationshipParty.email_address email,
       REL_GROUP_CODE, 
       ROLE,       
       HZ_LOCATIONS.address1,
       HZ_LOCATIONS.address2,
       HZ_LOCATIONS.address3,
       HZ_LOCATIONS.address4,
       HZ_LOCATIONS.city,
       HZ_LOCATIONS.country,
       HZ_LOCATIONS.state,
       HZ_LOCATIONS.postal_code,
       HZ_PARTIES.PARTY_TYPE, 
       HzPuiRelationshipsEO.SUBJECT_TYPE, 
       HzPuiRelationshipsEO.OBJECT_TYPE, 
       HzPuiRelationshipsEO.STATUS,
       HZ_ORG_CONTACTS.org_contact_id,
       HZ_PARTY_SITES.location_id,
       HzPuiRelationshipsEO.actual_content_source,
       'N' Select_flag,
       'N' Is_Function_called,       
       HzPuiRelationshipsEO.directional_flag,
       decode(to_char(HzPuiRelationshipsEO.END_DATE,'DD-MM-YYYY') , 
              '31-12-4712', null, 
              HzPuiRelationshipsEO.END_DATE) END_DATE,    
       HZ_PARTIES.person_first_name, 
       HZ_PARTIES.person_last_name,
       HzPuiRelationshipsEO.start_date,
       OBJECT_ID
from   HZ_RELATIONSHIPS HzPuiRelationshipsEO, 
       HZ_RELATIONSHIP_GROUPINGS_V, 
       HZ_PARTIES, 
       HZ_PARTY_SITES, 
       HZ_ORG_CONTACTS, 
       HZ_LOCATIONS, 
       HZ_PARTIES RelationshipParty,
       fnd_lookup_values fnd1, 
       fnd_lookup_values fnd2 
where  HZ_PARTY_SITES.location_id = HZ_LOCATIONS.location_id (+)
and    HzPuiRelationshipsEO.relationship_type = 
         HZ_RELATIONSHIP_GROUPINGS_V.relationship_type
and    HzPuiRelationshipsEO.relationship_code = 
         HZ_RELATIONSHIP_GROUPINGS_V.backward_rel_code
and    HZ_PARTIES.party_id = HzPuiRelationshipsEO.object_id
and    HZ_PARTY_SITES.party_id(+) = HzPuiRelationshipsEO.party_id
and    HZ_PARTY_SITES.identifying_address_flag(+) = 'Y'
and    HzPuiRelationshipsEO.relationship_id =
         hz_org_contacts.party_relationship_id(+)
and    HzPuiRelationshipsEO.subject_id <> HzPuiRelationshipsEO.object_id
and    HzPuiRelationshipsEO.object_type = 
         HZ_RELATIONSHIP_GROUPINGS_V.subject_type
and    HzPuiRelationshipsEO.subject_type = 
         HZ_RELATIONSHIP_GROUPINGS_V.object_type
and    HzPuiRelationshipsEO.party_id = RelationshipParty.party_id
and    fnd1.view_application_id(+) = 222 
and    fnd1.language(+) = userenv('LANG') 
and    fnd1.lookup_type(+) = 'RESPONSIBILITY' 
and    fnd1.lookup_code(+) = HZ_ORG_CONTACTS.job_title_code 
and    fnd2.view_application_id(+) = 222 
and    fnd2.lookup_type(+) = 'DEPARTMENT_TYPE' 
and    fnd2.language(+) = userenv('LANG') 
and    fnd2.lookup_code(+) = HZ_ORG_CONTACTS.department_code
and    subject_id = :1  
AND REL_GROUP_CODE = 'PARTY_REL_GRP_CONTACTS'
and not exists (select 'Y' 
from HZ_PARTY_SITES_EXT_B hzpsxt,ego_attr_groups_v egoag 
where hzpsxt.attr_group_id =egoag.attr_group_id
and hzpsxt.party_site_id = :2
and hzpsxt.n_ext_attr1 = HzPuiRelationshipsEO.relationship_id and 
SYSDATE BETWEEN nvl(hzpsxt.d_ext_attr1,  sysdate -1) AND nvl(hzpsxt.d_ext_attr2,   sysdate + 1) 
and c_ext_attr1 = 'A')
   ]]></SQLQuery>

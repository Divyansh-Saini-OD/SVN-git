<ViewObject
   Name="ODAttachPartySiteVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asn.partysite.server.ODAttachPartySiteVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
  PartySitesEO.party_site_id,
  PartySitesEO.party_id,
  PartySitesEO.location_id,
  PartySitesEO.actual_content_source,
  PartySitesEO.identifying_address_flag,
  PartySitesEO.status status,
  PartySitesEO.orig_system_reference OrigSystemReference,
  PartySitesEO.party_site_number,
  PartySitesEO.party_site_name,
  DECODE(PartySitesEO.location_id,null,null,hz_format_pub.format_address(PartySitesEO.location_id,null,null,',')) address,
  loc.country as country_code,
  terr.territory_short_name as country,
  loc.state,
  loc.province,
  loc.county,
  loc.postal_code,
  hz_utility_v2pub.get_all_purposes( PartySitesEO.party_site_id) all_purposes,
  hz_utility_v2pub.get_site_use_purpose( PartySitesEO.party_site_id) use,
'Yes' as UpdateYesNo,
'Yes1' as RemoveYes1No1,
'N' as Selected,
hzp.party_name
FROM 
  hz_party_sites PartySitesEO,
  hz_parties hzp,
  hz_locations loc,
  fnd_territories_vl terr
WHERE 
  hzp.party_id = PartySitesEO.party_id and
  PartySitesEO.location_id = loc.location_id and
  PartySitesEO.status = 'A' and
  terr.territory_code = loc.country  
  and hzp.party_id = :1
  and PartySitesEO.party_site_id = :2
   ]]></SQLQuery>

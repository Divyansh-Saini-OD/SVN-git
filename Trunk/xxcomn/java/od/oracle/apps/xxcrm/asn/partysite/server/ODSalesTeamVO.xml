   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT     RscDtls.NAMED_ACCT_TERR_RSC_ID,
  RscDtls.RESOURCE_ID,
  RscDtls.GROUP_ID,
  RscDtls.RESOURCE_ROLE_ID,
  RscDtls.NAMED_ACCT_TERR_ID,
  jrb.category,
  jrb.source_job_title,
  jrb.source_phone,
  jrb.source_email,
  jrb.source_name as resource_name,
  entitydtls.entity_type,
  entitydtls.entity_ID,
  NVL(entitydtls.Full_access_flag,'Y')
FROM     XX_TM_NAM_TERR_RSC_DTLS RscDtls,
  jtf_rs_resource_extns jrb,
  XX_TM_NAM_TERR_ENTITY_DTLS  entitydtls
WHERE 
  RscDtls.RESOURCE_ID = jrb.resource_id  AND
  jrb.category = 'EMPLOYEE'  AND
  entitydtls.entity_type = 'PARTY_SITE'  and
  NVL(entitydtls.status,'A') = 'A'  and
  RscDtls.NAMED_ACCT_TERR_ID = entitydtls.NAMED_ACCT_TERR_ID 
and entitydtls.entity_id = :1
   ]]></SQLQuery>

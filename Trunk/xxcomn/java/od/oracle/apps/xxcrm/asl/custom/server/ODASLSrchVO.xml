<ViewObject
   Name="ODASLSrchVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.asl.custom.server.ODASLSrchVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
       REQ.REQUEST_ID,
        HZP.PARTY_ID,
        HZP.PARTY_NUMBER,
        HZP.PARTY_NAME,
        HZPS.PARTY_SITE_ID,
        HZPS.PARTY_SITE_NUMBER,        
        HZL.COUNTRY,
		DECODE(hzps.location_id,null,null,hz_format_pub.format_address(hzps.location_id,null,null,',')) BILL_TO_ADDRESS,
        REQ.AP_CONTACT,
DECODE(substrb(hzpp.person_last_name,1,50),NULL,substrb(hzpp.person_first_name,1,40),substrb(hzpp.person_last_name,1,50))||' '||		DECODE(substrb(hzpp.person_first_name,1,50),NULL,substrb(hzpp.person_last_name,1,40),substrb(hzpp.person_first_name,1,50)) CONTACT,
		REQ.COMMENTS,
        initcap(REQ.STATUS) STATUS,
        REQ.CREATION_DATE,
        REQ.STATUS_TRANSITION_DATE,
        decode(REQ.OFFLINE_CREATED_FLAG,'Y','Yes','No') OFFLINE_CREATED_FLAG
    FROM
        XX_CDH_ACCOUNT_SETUP_REQ REQ,
        HZ_PARTIES               HZP,
        HZ_PARTIES               HZPP,
        HZ_LOCATIONS             HZL,
        HZ_PARTY_SITES           HZPS,
		HZ_RELATIONSHIPS         HZR,
		HZ_ORG_CONTACTS          HZOC
    WHERE
            REQ.PARTY_ID = HZP.PARTY_ID
        AND REQ.AP_CONTACT = HZOC.ORG_CONTACT_ID 
        AND HZOC.PARTY_RELATIONSHIP_ID = HZR.RELATIONSHIP_ID 
        AND HZR.SUBJECT_ID = HZPP.PARTY_ID(+) 
		AND HZR.SUBJECT_TYPE = 'PERSON'    
        AND REQ.BILL_TO_SITE_ID = HZPS.PARTY_SITE_ID
        AND HZPS.location_id = HZL.location_id
        AND NVL(DELETE_FLAG,'N')='N'
        AND REQ.Created_by = :1
		ORDER BY REQ.STATUS_TRANSITION_DATE desc
   ]]></SQLQuery>

       ODDashboardQuickLinksEO.CATEGORY_ID,
       null AS RESP_KEY_FV,
       fl.meaning,
       R.RESPONSIBILITY_NAME,
       DECODE(ODDashboardQuickLinksEO.PATH,'Relative','http://'||ODDashboardQuickLinksEO.FULL_PATH,' '||ODDashboardQuickLinksEO.FULL_PATH) as ACT_PATH
FROM XXBI_QUICK_LINKS ODDashboardQuickLinksEO,FND_LOOKUPS FL,FND_RESPONSIBILITY_VL R
where fl.lookup_type='XXBI_QUICK_LINK_CATEGORIES' and fl.enabled_flag='Y'
and fl.lookup_code=ODDashboardQuickLinksEO.CATEGORY_ID
AND R.RESPONSIBILITY_KEY = ODDashboardQuickLinksEO.RESPONSIBILITY_KEY

   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT phone_line_type||': '||phone_country_code||phone_area_code||phone_number||phone_extension phoneNumber,  
primary_flag PrimaryFlag,
hz_format_phone_v2pub.GET_FORMATTED_PHONE(CONTACT_POINT_ID) formattedPhone
FROM hz_contact_points
WHERE contact_point_type = 'PHONE' 
AND owner_table_name = 'HZ_PARTIES'
AND owner_table_id = :1
   ]]></SQLQuery>

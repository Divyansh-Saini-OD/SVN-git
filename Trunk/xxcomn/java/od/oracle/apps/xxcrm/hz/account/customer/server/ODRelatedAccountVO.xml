   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT acc.account_number,
  acc.account_name,
  acc.orig_system_reference,
  acc.status,
  accrel.relationship_type,
  accrel.bill_to_flag,
  accrel.ship_to_flag
FROM hz_cust_acct_relate_all accrel,
  hz_cust_accounts acc
WHERE accrel.cust_account_id = :1
 AND accrel.related_cust_account_id = acc.cust_account_id
   ]]></SQLQuery>

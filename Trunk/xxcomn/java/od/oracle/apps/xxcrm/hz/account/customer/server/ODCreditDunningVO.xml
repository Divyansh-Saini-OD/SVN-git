   Name="ODCreditDunningVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.hz.account.customer.server.ODCreditDunningVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.hz.account.customer.server.ODCreditDunningVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select hcpa.OVERALL_CREDIT_LIMIT,hcpa.CURRENCY_CODE,
       hcp.CREDIT_ANALYST_ID,
       ac.name AS Collection_Agent, ac.RESOURCE_ID,
       jrre.SOURCE_PHONE,
       jrre.SOURCE_EMAIL,
       jrre.SOURCE_NAME,
       jrres.SOURCE_NAME creditagentname,
       jrres.SOURCE_PHONE creditagentphone,
       jrres.SOURCE_EMAIL creditagentmail,
       hcp.cust_account_id,
       NULL billsiteID
from hz_cust_profile_amts hcpa,
     hz_customer_profiles hcp,
     ar_collectors ac,
     jtf_rs_resource_extns jrre,
     jtf_rs_resource_extns jrres
where hcp.CUST_ACCOUNT_PROFILE_ID = hcpa.CUST_ACCOUNT_PROFILE_ID
and   ac.COLLECTOR_ID = hcp.COLLECTOR_ID
and hcpa.OVERALL_CREDIT_LIMIT > 0
and jrre.RESOURCE_ID(+) = ac.RESOURCE_ID
and jrres.RESOURCE_ID(+) = hcp.CREDIT_ANALYST_ID
and hcpa.site_use_id is null
and hcpa.currency_code in
         (select sob.currency_code
            from hz_cust_acct_sites   cas,
                 ar_system_parameters asp,
                 gl_sets_of_books         sob
           where cas.org_id          = asp.org_id
             and asp.set_of_books_id = sob.set_of_books_id
             and cas.status          = 'A'
             and cas.bill_to_flag    = 'P'
             and cas.cust_account_id = hcp.cust_account_id)
and  hcp.CUST_ACCOUNT_ID = :1
   ]]></SQLQuery>

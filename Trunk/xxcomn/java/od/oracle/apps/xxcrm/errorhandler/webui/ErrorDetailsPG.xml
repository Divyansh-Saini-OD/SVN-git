<?xml version = '1.0' encoding = 'UTF-8'?>
<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="9.0.3.8.13_1550" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <oa:pageLayout id="MainRN" amDefName="od.oracle.apps.xxcrm.errorhandler.server.ErrorDetailsAM" windowTitle="Error Search" title="Error Search">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:contents>
            <oa:query id="QueryRN" mode="none" dispSimple="true" dispCustomized="false" dispAdvanced="false">
               <oa:simpleSearchPanel>
                  <oa:header id="SimpleSearchHeader" text="" rendered="true">
                     <ui:contents>
                        <oa:messageComponentLayout id="CustomSimpleSearch">
                           <ui:contents>
                              <oa:messageLovInput id="SearchModuleName" selectiveSearchCriteria="false" prompt="Module Name" shortDesc="Module" styleClass="OraFieldText" columns="10" maximumLength="240" externalListOfValues="/od/oracle/apps/xxcrm/errorhandler/lov/webui/RiceIDLovRN" unvalidated="true" required="yes">
                                 <lovMappings>
                                    <lovMap id="lovMap2" lovItem="ModuleName" resultTo="SearchModuleName" criteriaFrom="SearchModuleName"/>
                                 </lovMappings>
                              </oa:messageLovInput>
                              <oa:messageLovInput id="SearchProgramType" styleClass="OraFieldText" externalListOfValues="/od/oracle/apps/xxcrm/errorhandler/lov/webui/ProgramTypeRN" shortDesc="SearchProgramType" prompt="Rice ID">
                                 <lovMappings>
                                    <lovMap id="lovMap3" lovItem="ProgramType" resultTo="SearchProgramType" criteriaFrom="SearchProgramType"/>
                                    <lovMap id="lovMap4" lovItem="ModuleNameForm" criteriaFrom="SearchModuleName" requiredForLOV="true" programmaticQuery="true"/>
                                 </lovMappings>
                              </oa:messageLovInput>
                              <oa:messageTextInput id="SearchFromDate" prompt="From Date" styleClass="OraFieldText" dataType="DATE" columns="10" translatableDefaultValue="SYSDATE"/>
                              <oa:messageTextInput id="SearchToDate" dataType="DATE" prompt="To Date" shortDesc="To Date" translatableDefaultValue="SYSDATE"/>
                              <oa:messageLayout id="ButtonLayout">
                                 <ui:contents>
                                    <oa:submitButton id="Go" text="Go" prompt="Go"/>
                                 </ui:contents>
                              </oa:messageLayout>
                           </ui:contents>
                        </oa:messageComponentLayout>
                     </ui:contents>
                  </oa:header>
               </oa:simpleSearchPanel>
               <ui:contents>
                  <oa:table id="ErrorDetailsRN" text="Error Details" shortDesc="Error Details" controllerClass="od.oracle.apps.xxcrm.errorhandler.webui.ErrorDetailsCO" styleClass="OraHeaderText">
                     <ui:contents>
                        <oa:messageStyledText id="ProgramType" dataType="VARCHAR2" prompt="Rice ID" viewName="ErrorDetailsVO1" viewAttr="ProgramType" styleClass="OraFieldText"/>
                        <oa:messageStyledText id="ErrorMessageCode" dataType="VARCHAR2" prompt="Error Code" viewName="ErrorDetailsVO1" viewAttr="ErrorMessageCode" maximumLength="255" styleClass="OraLinkText" cellNoWrapFormat="true" destination="OA.jsp?page=/od/oracle/apps/xxcrm/errorhandler/webui/ErrorResolutionPG&amp;messageName={@ErrorMessageCode}&amp;retainAM=Y&amp;addBreadCrumb=Y"/>
                        <oa:messageStyledText id="ErrorMessage" dataType="VARCHAR2" prompt="Error Message" viewName="ErrorDetailsVO1" viewAttr="ErrorMessage" maximumLength="4000" styleClass="OraFieldText" cellNoWrapFormat="false" columns="30"/>
                        <oa:messageStyledText id="ErrorMessageSeverity" dataType="VARCHAR2" prompt="Severity" viewName="ErrorDetailsVO1" viewAttr="ErrorMessageSeverity" styleClass="OraFieldText" columns="10"/>
                        <oa:messageStyledText id="ErrorStatusFlag" dataType="VARCHAR2" prompt="Status" viewName="ErrorDetailsVO1" viewAttr="ErrorStatusFlag" maximumLength="30" styleClass="OraFieldText"/>
                        <oa:messageStyledText id="CreationDate" dataType="DATE" prompt="Run Date" viewName="ErrorDetailsVO1" viewAttr="CreationDate" queryable="true" sortState="descending" required="no" styleClass="OraFieldText"/>
                     </ui:contents>
                  </oa:table>
               </ui:contents>
            </oa:query>
         </ui:contents>
      </oa:pageLayout>
   </content>
</page>

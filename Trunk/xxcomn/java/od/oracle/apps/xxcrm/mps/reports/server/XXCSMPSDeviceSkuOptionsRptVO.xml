<ViewObject
   Name="XXCSMPSDeviceSkuOptionsRptVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSDeviceSkuOptionsRptVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT
      distinct
      XB.MODEL,
      XD.SERIAL_NO,      
      xblack.sku_option_1 black1,
      xblack.sku_option_2 black2,
      xblack.sku_option_3 black3,
      xmagenta.sku_option_1 magenta1,
      xmagenta.sku_option_2 magenta2,
      xmagenta.sku_option_3 magenta3,
      xcyan.sku_option_1 cyan1,
      xcyan.sku_option_2 cyan2,
      xcyan.sku_option_3 cyan3,
      xyellow.sku_option_1 yellow1,
      xyellow.sku_option_2 yellow2,
      xyellow.sku_option_3 yellow3,
      XB.MPS_REP_COMMENTS,
      XB.MANAGED_STATUS,
      XB.ACTIVE_STATUS   
    FROM  xx_cs_mps_device_details xd,
          xx_cs_mps_device_b xb,
          (select sku_option_1, sku_option_2, sku_option_3, serial_no
          from   xx_cs_mps_device_details xa
          where  xa.SUPPLIES_LABEL != 'USAGE'
          and    xa.SUPPLIES_LABEL = 'TONERLEVEL_BLACK') xblack,
          (select sku_option_1, sku_option_2, sku_option_3, serial_no
          from   xx_cs_mps_device_details xa
          where  xa.SUPPLIES_LABEL != 'USAGE'
          and    xa.SUPPLIES_LABEL = 'TONERLEVEL_MAGENTA') xmagenta,
          (select sku_option_1, sku_option_2, sku_option_3, serial_no
          from   xx_cs_mps_device_details xa
          where  xa.SUPPLIES_LABEL != 'USAGE'
          and    xa.SUPPLIES_LABEL = 'TONERLEVEL_CYAN') xcyan,
          (select sku_option_1, sku_option_2, sku_option_3, serial_no
          from   xx_cs_mps_device_details xa
          where  xa.SUPPLIES_LABEL != 'USAGE'
          and    xa.SUPPLIES_LABEL = 'TONERLEVEL_YELLOW') xyellow 
    WHERE xb.serial_no     = xd.serial_no
    AND   xb.party_id = :1
    AND   xb.serial_no = nvl(:2, xb.serial_no) 
    AND   xd.serial_no = xblack.serial_no
    AND   xd.serial_no = xmagenta.serial_no
    AND   xd.serial_no = xcyan.serial_no
    AND   xd.serial_no = xyellow.serial_no
   ]]></SQLQuery>

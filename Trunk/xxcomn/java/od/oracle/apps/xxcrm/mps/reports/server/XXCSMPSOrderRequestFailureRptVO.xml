<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="XXCSMPSOrderRequestFailureRptVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSOrderRequestFailureRptVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select distinct mb.party_name, 
        mb.serial_no as serialno,
        cb.creation_date as orderdate,
       jt.attribute4 item,
       jt.attribute5 label, substr(jv.notes,1,200) Notes, DBMS_LOB.substr(jv.notes_detail, 300) as Details,
       cb.incident_id, mb.party_id,
       cb.incident_number srnumber,
       mb.managed_status managedstatus,
       mb.active_status  activestatus
from xx_cs_mps_device_b mb,
     cs_incident_statuses_tl st,
     cs_incidents_all_b cb,
     cs_incident_types_tl ct,
     jtf_tasks_b jt,
     jtf_notes_vl jv
where jv.source_object_id = cb.incident_id
and jt.source_object_id = cb.incident_id
and st.incident_status_id = cb.incident_status_id
--and st.name = 'Order Rejected'
and  ct.incident_type_id = cb.incident_type_id
and  ct.name = 'MPS Supplies Request'
and jt.source_object_type_code = 'SR'
and  cb.customer_id = mb.party_id
and  cb.incident_attribute_3 = mb.serial_no
--and   cb.creation_date between to_date('08/1/13','mm/dd/yy') and to_date('08/31/13','mm/dd/yy')
and st.source_lang = 'US'
and jv.source_object_code = 'SR'
and jv.notes like 'Order Rejected%'
AND party_id = nvl(:0, party_id)
AND serial_no = nvl(:1,serial_no)
 AND to_date(cb.CREATION_DATE,'dd-MON-yy') >=(nvl(:2,to_date(cb.CREATION_DATE,'dd-MON-yy')))
  AND to_date(cb.CREATION_DATE,'dd-MON-yy') <=(nvl(:3,to_date(cb.CREATION_DATE,'dd-MON-yy')))
   ]]></SQLQuery>

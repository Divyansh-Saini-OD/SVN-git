<?xml version = '1.0' encoding = 'UTF-8'?>
<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="10.1.3_1312" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <oa:pageLayout id="PageLayoutRN" amDefName="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSDeviceThresholdAM" controllerClass="od.oracle.apps.xxcrm.mps.reports.webui.XXCSMPSDevicThresholdReportCO" windowTitle="MPS Device Threshold Report" title="MPS Device Threshold Report">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:contents>
            <oa:messageComponentLayout id="CriteriaRN">
               <ui:contents>
                  <oa:messageLovInput id="CustomerName" prompt="Customer Name">
                     <lovContents>
                        <oa:listOfValues id="region2">
                           <ui:contents>
                              <oa:table name="region2" id="XXCSMPSPartyNameVO1">
                                 <ui:contents>
                                    <oa:messageStyledText id="PartyName" dataType="VARCHAR2" prompt="PartyName" viewName="XXCSMPSPartyNameVO1" viewAttr="PartyName" maximumLength="250" queryable="true"/>
                                    <oa:formValue id="PartyId" dataType="NUMBER" prompt="PartyId" viewName="XXCSMPSPartyNameVO1" viewAttr="PartyId"/>
                                    <oa:messageStyledText id="AopsCustNumber" dataType="VARCHAR2" prompt="AopsCustNumber" viewName="XXCSMPSPartyNameVO1" viewAttr="AopsCustNumber" maximumLength="40"/>
                                 </ui:contents>
                              </oa:table>
                           </ui:contents>
                        </oa:listOfValues>
                     </lovContents>
                     <lovMappings>
                        <lovMap id="partyNameMap" lovItem="PartyName" resultTo="CustomerName" criteriaFrom="CustomerName"/>
                        <lovMap id="partyIdMap" lovItem="PartyId" resultTo="CustPartyId"/>
                     </lovMappings>
                  </oa:messageLovInput>
                  <oa:messageTextInput id="ActiveStatus" prompt="Active Status"/>
               </ui:contents>
            </oa:messageComponentLayout>
            <oa:rowLayout id="SearchButtonRN" hAlign="center">
               <ui:contents>
                  <oa:submitButton id="SearchBtn" text="Search" prompt="Search"/>
                  <oa:spacer id="item1" width="10"/>
                  <oa:submitButton id="ClearBtn" text="Clear" prompt="Clear"/>
               </ui:contents>
            </oa:rowLayout>
            <oa:spacer id="item2" height="20" width="600"/>
            <oa:advancedTable id="searchResultsRN" viewName="XXCSMPSDeviceThresholdVO1">
               <ui:contents>
                  <oa:column id="SerialNumberCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="SerialNumberColHdr" prompt="Serial Number"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="SerialNumber" viewAttr="SerialNumber"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="CustomerNameCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="CustomerNameColHdr" prompt="Customer Name"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="CustName" viewAttr="CustomerName"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="DeviceLevelCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="DeviceLevelColHdr" prompt="Device Level"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="DeviceLevel" viewAttr="DeviceLevel"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="BlackLevelCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="BlackLevelColHdr" prompt="Black Threshold"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="BlackLevel" viewAttr="BlackThreshold"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="CyanLevelCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="CyanLevelColHdr" prompt="Cyan Threshold"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="CyanThreshold" viewAttr="CyanThreshold"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="MagentaLevelCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="MagentaLevelColHdr" prompt="Magenta Threshold"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="MagentaThreshold" viewAttr="MagentaThreshold"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="YellowLevelCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="YellowLevelColHdr" prompt="Yellow Threshold"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="YellowThreshold" viewAttr="YellowThreshold"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="AOPSNumberCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="AOPSNumberColHdr" prompt="AOPS Number"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="AOPSNumber" viewAttr="AopsNumber"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="ProgramTypeCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="ProgramTypeColHdr" prompt="Program Type"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="ProgramType" viewAttr="ProgramType"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="SuppliesLabelCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="SuppliesLabelColHdr" prompt="Supplies Label"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="SuppliesLabel"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="ActiveStatusCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="ActiveStatusColHdr" prompt="Active Status"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="ActiveSts" viewAttr="ActiveStatus"/>
                     </ui:contents>
                  </oa:column>
                  <oa:column id="ManagedStatusCol">
                     <ui:columnHeader>
                        <oa:sortableHeader id="ManagedStatusColHdr" prompt="Managed Status"/>
                     </ui:columnHeader>
                     <ui:contents>
                        <oa:messageStyledText id="ManagedStatus" viewAttr="ManagedStatus"/>
                     </ui:contents>
                  </oa:column>
               </ui:contents>
               <ui:tableActions>
                  <oa:flowLayout id="region1">
                     <ui:contents>
                        <oa:exportButton id="exportBtn" exportAllRows="true" text="Export to CSV" viewName="XXCSMPSDeviceThresholdVO1"/>
                     </ui:contents>
                  </oa:flowLayout>
               </ui:tableActions>
            </oa:advancedTable>
            <oa:formValue id="CustPartyId"/>
         </ui:contents>
      </oa:pageLayout>
   </content>
</page>

<ViewObject
   Name="XXCSMPSStalePrinterRptVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSStalePrinterRptVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT	
    A.PARTY_NAME	  AS CUSTOMER_NAME,
    A.MANUFACTURER  AS MANUFACTURER,
    A.MODEL         AS MODEL,
    A.SERIAL_NO     AS SERIAL_NO,
    A.IP_ADDRESS    AS IP_ADDRESS,
		A.DEVICE_NAME	  AS DEVICE_NAME,	
		B.CDATE         AS LAST_ACTIVE,
    b.sr_number     AS sr_number,
    a.managed_status AS MANAGED_STATUS,
    a.active_status  AS ACTIVE_STATUS
FROM 		xx_cs_mps_device_b a,
	(select	max(last_update_date) cdate,
			device_id, max(request_number) sr_number
		from 		xx_cs_mps_device_details
		group by 	device_id) b
	WHERE B.DEVICE_ID	=	A.DEVICE_ID
  AND a.party_name = nvl(:0,a.party_name)
AND 	B.CDATE	<	SYSDATE	-	NVL(:1,7)
ORDER BY 	a.device_id
   ]]></SQLQuery>

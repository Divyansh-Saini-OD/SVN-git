<ViewObject
   Name="XXCSMPSTOPCustomerVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSTOPCustomerVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSTOPCustomerVOImpl"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select mb.party_name, 
    mb.program_type, 
    mb.managed_status,
    mb.active_status,
   sum(NVL(total_count,0)) as totalcount, 
   sum(NVL(total_black_count,0)) as total_black_count, 
   sum(NVL(total_color_count,0)) as total_color_count
from xx_cs_mps_device_b mb,
     xx_cs_mps_device_details md
where md.serial_no = mb.serial_no
and md.supplies_label = 'USAGE'
and mb.program_type = nvl(:1,mb.program_type)
and mb.program_type is not null
and mb.party_id is not null
group by  mb.party_name, mb.program_type, mb.managed_status, mb.active_status
order by sum(NVL(total_count,0)) DESC
   ]]></SQLQuery>

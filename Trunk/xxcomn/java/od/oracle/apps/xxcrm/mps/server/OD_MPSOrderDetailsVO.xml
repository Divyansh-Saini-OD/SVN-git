<ViewObject
   Name="OD_MPSOrderDetailsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.mps.server.OD_MPSOrderDetailsVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.server.OD_MPSOrderDetailsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select cb.incident_attribute_1 order_no, cb.incident_attribute_2 order_date ,
     cb.incident_number, jt.attribute4 item, cb.incident_attribute_3, jt.attribute5 item_description,
     cb.close_date , fnd_profile.value('XX_CS_GMILL_LINK') as GMILL_LINK
from cs_incidents_all_b cb,
     jtf_tasks_b jt
where jt.source_object_id = cb.incident_id
and jt.source_object_type_code = 'SR'
and cb.incident_attribute_3 = :1 --'CNCCB9N32M' 
and cb.customer_id = :2 --'14047142'
order by cb.creation_date desc
   ]]></SQLQuery>

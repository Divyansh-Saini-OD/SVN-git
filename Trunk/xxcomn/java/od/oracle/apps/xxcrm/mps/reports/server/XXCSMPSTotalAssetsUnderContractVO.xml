<ViewObject
   Name="XXCSMPSTotalAssetsUnderContractVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSTotalAssetsUnderContractVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT        
   A.PARTY_NAME     AS CUSTOMER_NAME,
   A.MANUFACTURER   AS MANUFACTURER,
   A.MODEL          AS MODEL,
   A.PROGRAM_TYPE   AS PROGRAM_TYPE,
   A.SERIAL_NO      AS SERIAL_NO,
   A.PERIOD_COVERED_ST_DATE AS START_DATE, 
   A.EXPIRED_DATE AS EXPIRED_DATE,
   A.CONTRACT_NUMBER AS CONTRACT_NUMBER,
   A.ACTIVE_STATUS AS  ACTIVE_STATUS,
   A.MANAGED_STATUS AS MANAGED_STATUS,
   sum(NVL(total_count,0)) as totalcount, 
   SUM(NVL(TOTAL_BLACK_COUNT,0)) AS TOTAL_BLACK_COUNT, 
   sum(NVL(total_color_count,0)) as total_color_count
FROM  xx_cs_mps_device_b a ,
      xx_cs_mps_device_details md
       WHERE MD.SERIAL_NO = A.SERIAL_NO
        AND MD.SUPPLIES_LABEL = 'USAGE'
        AND A.PROGRAM_TYPE IS NOT NULL
        AND A.PARTY_ID IS NOT NULL
        AND A.PARTY_NAME = NVL(:1,A.PARTY_NAME)
        AND A.PROGRAM_TYPE =        NVL(:2,A.PROGRAM_TYPE)
        AND TO_DATE(A.PERIOD_COVERED_ST_DATE,'dd-mon-yy' ) >= TO_DATE(NVL(:3, A.PERIOD_COVERED_ST_DATE), 'dd-mon-yy')
        AND TO_DATE(A.PERIOD_COVERED_END_DATE,'dd-mon-yy' ) <= TO_DATE(NVL(:4, A.PERIOD_COVERED_END_DATE), 'dd-mon-yy')
        GROUP BY  A.PARTY_NAME, A.MANUFACTURER ,A.MODEL,   A.PROGRAM_TYPE,A.SERIAL_NO,A.PERIOD_COVERED_ST_DATE ,A.EXPIRED_DATE, A.CONTRACT_NUMBER,A.ACTIVE_STATUS, A.MANAGED_STATUS
        order by sum(NVL(total_count,0)) DESC
   ]]></SQLQuery>

<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="XXCSMPSTonerOrderVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSTonerOrderVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.mps.reports.server.XXCSMPSTonerOrderVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select mb.party_name, 
       nvl(cb.customer_ticket_number,cb.incident_attribute_1) order_no, 
       cb.incident_attribute_2 Delivery_date, 
       jt.attribute4 item,
       md.supplies_label
from apps.xx_cs_mps_device_details md,
     apps.xx_cs_mps_device_b mb,
     apps.cs_incidents_all_b cb,
     apps.cs_incident_types_tl ct,
     apps.jtf_tasks_b jt
where md.serial_no = mb.serial_no
and md.sku_option_1 = jt.attribute4
and  jt.source_object_id = cb.incident_id
and  ct.incident_type_id = cb.incident_type_id 
and  ct.name = 'MPS Supplies Request'
and jt.source_object_type_code = 'SR'
and  cb.customer_id = mb.party_id
and  cb.incident_attribute_3 = mb.serial_no
and   cb.incident_attribute_1 is not null  
order by mb.party_name, md.supplies_label
   ]]></SQLQuery>

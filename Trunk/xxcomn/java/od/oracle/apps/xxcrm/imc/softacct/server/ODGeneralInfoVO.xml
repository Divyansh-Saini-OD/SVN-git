   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT   p.party_name ,  act.account_number,  act.account_name ,  lu.meaning as status,  duns_number_c,  p.JGZZ_fiscal_code as taxpayer_id ,  to_char(act.creation_date) as creation_date,  p.tax_reference as tax_registration_number,  arc.name collector ,  act.primary_salesrep_id as salesperson ,  rat.name as payment_terms,  pc.name as profile_class,  (select party_name FROM   hz_cust_accounts c,  hz_cust_account_roles s,  hz_relationships r ,  hz_parties p WHERE   c.cust_Account_id= (select hcas.cust_account_id from HZ_CUST_ACCT_SITES_all hcas where cust_acct_site_id =:0)   and  s.cust_Account_id = c.cust_account_id   and  s.role_type='CONTACT' and  s.primary_flag= 'Y' and  s.party_id = r.party_id and  subject_type = 'PERSON' and  directional_flag='F' and  subject_table_name='HZ_PARTIES' and  p.party_id=r.subject_id) as primary_account_contact,  (select hz_format_pub.format_address(c.location_id,  null,  null,  ',  ',  null,  null,  null,  null)FROM   hz_cust_accounts a,  hz_party_sites b,  hz_locations c WHERE   cust_account_id= (select hcas.cust_account_id from HZ_CUST_ACCT_SITES_all hcas where cust_acct_site_id =:1)   and  a.party_id=b.party_id and  b.location_id = c.location_id and  identifying_address_flag='Y') as primary_address from hz_cust_accounts act, hz_parties p, hz_customer_profiles cp, HZ_CUST_PROFILE_CLASSES pc, ar_lookups lu , ar_collectors arc , ra_terms rat, RA_SALESREPS ras   WHERE act.CUST_ACCOUNT_ID = (select hcas.cust_account_id from HZ_CUST_ACCT_SITES_all hcas where cust_acct_site_id =:2) AND  act.cust_account_id = cp.cust_account_id AND  act.party_id = p.party_id AND  cp.collector_id = arc.collector_id AND  cp.profile_class_id = pc.profile_class_id (+) AND  cp.SITE_USE_ID IS NULL AND  cp.standard_terms = rat.term_id (+) AND  act.primary_salesrep_id = ras.salesrep_id(+) and  lu.lookup_type= 'CODE_STATUS' and  lu.lookup_code = act.status
   ]]></SQLQuery>

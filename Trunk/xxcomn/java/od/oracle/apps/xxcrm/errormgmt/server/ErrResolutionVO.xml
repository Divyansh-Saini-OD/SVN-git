<ViewObject
   Name="ErrResolutionVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.errormgmt.server.ErrResolutionVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.errormgmt.server.ErrResolutionVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT RESOLUTION_ID,resolution,APPLICATION_ID,APPLICATION_NAME,LANGUAGE_CODE,MESSAGE_NAME,resolution_step 
FROM
( SELECT ErrResolutionEO.RESOLUTION_ID, 
         ErrResolutionEO.APPLICATION_ID,
         fav.APPLICATION_NAME, 
         ErrResolutionEO.LANGUAGE_CODE, 
         ErrResolutionEO.MESSAGE_NAME,
         ('Step '||r.r) resolution_step, 
         decode( r, 1, ErrResolutionEO.RESOLUTION_STEP1, 2, ErrResolutionEO.RESOLUTION_STEP2, 3, ErrResolutionEO.RESOLUTION_STEP3,
                    4, ErrResolutionEO.RESOLUTION_STEP4, 5, ErrResolutionEO.RESOLUTION_STEP5, 6, ErrResolutionEO.RESOLUTION_STEP6,
                    7, ErrResolutionEO.RESOLUTION_STEP7, 8, ErrResolutionEO.RESOLUTION_STEP8, 9, ErrResolutionEO.RESOLUTION_STEP9,
                   10, ErrResolutionEO.RESOLUTION_STEP10,11,ErrResolutionEO.RESOLUTION_STEP11,12,ErrResolutionEO.RESOLUTION_STEP12,
                   13, ErrResolutionEO.RESOLUTION_STEP13,14,ErrResolutionEO.RESOLUTION_STEP14,15,ErrResolutionEO.RESOLUTION_STEP15
               ) RESOLUTION
  FROM   XX_COMN_ERROR_RESOLUTION ErrResolutionEO,
         FND_APPLICATION_VL fav,
         ( select 1 r from sys.dual
           union
           select 2 r from sys.dual
           union
           select 3 r from sys.dual
           union
           select 4 r from sys.dual
           union
           select 5 r from sys.dual
           union
           select 6 r from sys.dual
           union
           select 7 r from sys.dual
           union
           select 8 r from sys.dual
           union
           select 9 r from sys.dual
           union
           select 10 r from sys.dual
           union
           select 11 r from sys.dual
           union
           select 12 r from sys.dual
           union
           select 13 r from sys.dual
           union
           select 14 r from sys.dual
           union
           select 15 r from sys.dual
         ) r
  WHERE  fav.application_id = ErrResolutionEO.application_id 
) 
WHERE resolution is not null
   ]]></SQLQuery>

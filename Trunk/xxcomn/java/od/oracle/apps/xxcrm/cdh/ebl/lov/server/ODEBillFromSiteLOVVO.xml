<ViewObject
   Name="ODEBillFromSiteLOVVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.cdh.ebl.lov.server.ODEBillFromSiteLOVVORowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT asites.orig_system_reference ,
  asites.cust_acct_site_id
  ,asites.cust_account_id
  ,sites.party_site_id
  ,uses.site_use_code
  , loc.address1 || ', ' || loc.address2 || ', ' || loc.state address
FROM hz_cust_acct_sites_all asites,
  hz_party_sites sites,
  hz_cust_site_uses_all uses,
  hz_locations loc
WHERE asites.party_site_id   = sites.party_site_id
AND asites.cust_acct_site_id = uses.cust_acct_site_id
AND sites.location_id        = loc.location_id
AND uses.site_use_code       = 'SHIP_TO'
   ]]></SQLQuery>

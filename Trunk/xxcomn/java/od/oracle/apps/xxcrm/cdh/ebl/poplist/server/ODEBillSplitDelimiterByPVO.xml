<?xml version="1.0" encoding='windows-1252'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODEBillSplitDelimiterByPVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
	  v.source_value1 Delimiter, v.source_value2 Description
 FROM apps.XX_FIN_TRANSLATEDEFINITION D
JOIN apps.XX_FIN_TRANSLATEVALUES V
ON D.translate_id=V.translate_id
WHERE D.translation_name='XXCRM_CDH_EBL_DELIMITER'
AND V.enabled_flag='Y'
AND TRUNC(SYSDATE) BETWEEN V.start_date_active AND NVL(V.end_date_active,SYSDATE)
]]></SQLQuery>
   <DesignTime>
      <Attr Name="_version" Value="9.0.3.12.53" />
      <Attr Name="_CodeGenFlagNew" Value="4" />
   </DesignTime>
   <ViewAttribute
      Name="Delimiter"
      IsPersistent="false"
      Type="java.lang.String"
      AliasName="Delimiter"
      ColumnType="VARCHAR2"
      Expression="DELIMITER"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="22" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="Description"
      IsPersistent="false"
      Type="java.lang.String"
      AliasName="Description"
      ColumnType="VARCHAR2"
      Expression="DESCRIPTION"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="23" />
      </DesignTime>
   </ViewAttribute>
</ViewObject>

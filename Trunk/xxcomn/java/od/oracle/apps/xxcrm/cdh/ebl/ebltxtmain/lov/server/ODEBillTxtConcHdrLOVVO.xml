   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select hdr.field_id code, NVL(hdr.label, val.source_value2) meaning, hdr.cust_doc_id, hdr.seq
from XX_CDH_EBL_TEMPL_HDR_TXT hdr,
     xx_fin_translatedefinition def, 
     xx_fin_translatevalues val
WHERE 1=1
   AND def.translate_id = val.translate_id
   AND def.translation_name = 'XX_CDH_EBL_TXT_HDR_FIELDS'
   AND val.enabled_flag = 'Y'
   AND val.source_value1 IS NOT NULL
   AND val.source_value2 IS NOT NULL
   and val.source_value1=hdr.FIELD_ID   
and hdr.cust_doc_id=:1
   ]]></SQLQuery>

   RowClass="od.oracle.apps.xxcrm.cdh.ebl.eblmain.server.ODEBillContactsVORowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT ODEBillContactsEO.EBL_DOC_CONTACT_ID, 
       ODEBillContactsEO.CUST_DOC_ID, 
       ODEBillContactsEO.ORG_CONTACT_ID, 
       ODEBillContactsEO.CUST_ACCT_SITE_ID, 
       ODEBillContactsEO.ATTRIBUTE1, 
       ODEBillContactsEO.ATTRIBUTE2, 
       ODEBillContactsEO.ATTRIBUTE3, 
       ODEBillContactsEO.ATTRIBUTE4, 
       ODEBillContactsEO.ATTRIBUTE5, 
       ODEBillContactsEO.ATTRIBUTE6, 
       ODEBillContactsEO.ATTRIBUTE7, 
       ODEBillContactsEO.ATTRIBUTE8, 
       ODEBillContactsEO.ATTRIBUTE9, 
       ODEBillContactsEO.ATTRIBUTE10, 
       ODEBillContactsEO.ATTRIBUTE11, 
       ODEBillContactsEO.ATTRIBUTE12, 
       ODEBillContactsEO.ATTRIBUTE13, 
       ODEBillContactsEO.ATTRIBUTE14, 
       ODEBillContactsEO.ATTRIBUTE15, 
       ODEBillContactsEO.ATTRIBUTE16, 
       ODEBillContactsEO.ATTRIBUTE17, 
       ODEBillContactsEO.ATTRIBUTE18, 
       ODEBillContactsEO.ATTRIBUTE19, 
       ODEBillContactsEO.ATTRIBUTE20,
       (SELECT HP.party_name 
          FROM hz_org_contacts HOC
            ,  hz_relationships HR
            ,  hz_parties HP
        WHERE  HOC.party_relationship_id = HR.relationship_id
          AND  HR.subject_id = HP.party_id
          AND  HR.relationship_code = 'CONTACT_OF'
          AND  HOC.org_contact_id = ODEBillContactsEO.ORG_CONTACT_ID) contact_name,
        (SELECT  arp_addr_pkg.format_address( HL.address_style ,HL.address1
                                    , HL.address2 ,HL.address3
                                    , HL.address4 ,HL.city
                                    , HL.county ,HL.state
                                    , HL.province ,HL.postal_code
                                    , HTV.territory_short_name )    
          FROM fnd_territories_vl HTV
             , hz_cust_acct_sites_all HCAS
             , hz_party_sites HPS
             , hz_locations HL  
          WHERE HL.country = HTV.territory_code(+) 
            AND HCAS.party_site_id = HPS.party_site_id 
            AND HL.location_id = HPS.location_id 
            AND HCAS.status = 'A' 
            AND HCAS.cust_acct_site_id = ODEBillContactsEO.CUST_ACCT_SITE_ID ) Location                           
        ,(SELECT DECODE(c_ext_attr2, 'Y', 'BILL_TO', 'SHIP_TO')
            FROM XX_CDH_CUST_ACCT_EXT_B DOC
               , ego_attr_groups_v EAG
           WHERE DOC.n_ext_attr2 = ODEBillContactsEO.CUST_DOC_ID
             AND DOC.cust_account_id= ODEBillContactsEO.attribute1
             AND DOC.attr_group_id = EAG.attr_group_id
             AND EAG.attr_group_type = 'XX_CDH_CUST_ACCOUNT'
             AND EAG.attr_group_name = 'BILLDOCS') site_use_code
FROM XX_CDH_EBL_CONTACTS ODEBillContactsEO
   ]]></SQLQuery>

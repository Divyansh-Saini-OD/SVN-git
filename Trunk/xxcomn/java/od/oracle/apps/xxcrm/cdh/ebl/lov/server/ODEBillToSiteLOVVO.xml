      ColumnType="VARCHAR2"
      Expression="ORIG_SYSTEM_REFERENCE"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="240" />
      </DesignTime>
   </ViewAttribute>

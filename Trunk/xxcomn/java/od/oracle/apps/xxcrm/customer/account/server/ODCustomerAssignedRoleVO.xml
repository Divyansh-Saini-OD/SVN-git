<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ODCustomerAssignedRoleVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxcrm.customer.account.server.ODCustomerAssignedRoleVORowImpl"
   ComponentClass="od.oracle.apps.xxcrm.customer.account.server.ODCustomerAssignedRoleVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 
  JRE.user_name,
  JRR.ROLE_CODE
FROM 
  XX_TM_NAM_TERR_CURR_ASSIGN_V ASAA,
  jtf_rs_resource_extns JRE,
  JTF_RS_ROLES_B JRR,
  HZ_PARTY_SITES HZPS
WHERE 
  ASAA.entity_type = 'PARTY_SITE'  and
  ASAA.resource_id = JRE.resource_id  and
  HZPS.party_site_id = ASAA.entity_id  and
  JRR.role_id = ASAA.RESOURCE_ROLE_ID and
  HZPS.party_id = :1  and
  JRE.user_name=:2
   ]]></SQLQuery>

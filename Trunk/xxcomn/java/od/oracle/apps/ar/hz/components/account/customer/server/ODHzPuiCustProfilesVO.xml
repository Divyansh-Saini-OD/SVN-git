decode(HzPuiCustProfileEO.SITE_USE_ID, NULL, HzPuiCustProfileEO.CONS_BILL_LEVEL,
(select act.CONS_BILL_LEVEL from HZ_CUSTOMER_PROFILES act WHERE act.CUST_ACCOUNT_ID = HzPuiCustProfileEO.CUST_ACCOUNT_ID
AND act.SITE_USE_ID is NULL AND act.PARTY_ID = HzPuiCustProfileEO.PARTY_ID)) as DISPLAY_BILL_LEVEL,
decode(HzPuiCustProfileEO.SITE_USE_ID, NULL, HzPuiCustProfileEO.CONS_INV_FLAG,
(select act.CONS_INV_FLAG from HZ_CUSTOMER_PROFILES act WHERE act.CUST_ACCOUNT_ID = HzPuiCustProfileEO.CUST_ACCOUNT_ID
AND act.SITE_USE_ID is NULL AND act.PARTY_ID = HzPuiCustProfileEO.PARTY_ID)) as ACT_BFB_FLAG,
(SELECT vl.AUTOMATCH_SET_NAME FROM ar_cash_auto_rule_sets_vl vl WHERE vl.automatch_set_id = HzPuiCustProfileEO.automatch_set_id) AS AUTOMATCH_SET_NAME,
(select hcpc.NAME  from hz_cust_profile_classes hcpc 
 where hcpc.PROFILE_CLASS_ID = HzPuiCustProfileEO.PROFILE_CLASS_ID ) profile_class_name,
  (SELECT ar.meaning
  FROM ar_lookups ar
 WHERE ar.lookup_code = hzpuicustprofileeo.credit_rating
   AND ar.lookup_type = 'CREDIT_RATING')credit_rating_meaning,
  (SELECT ar.meaning
  FROM ar_lookups ar
 WHERE ar.lookup_code = hzpuicustprofileeo.credit_classification
   AND ar.lookup_type = 'AR_CMGT_CREDIT_CLASSIFICATION')credit_class_meaning,
(SELECT ar.meaning
  FROM ar_lookups ar
 WHERE ar.lookup_code = hzpuicustprofileeo.review_cycle
   AND ar.lookup_type = 'PERIODIC_REVIEW_CYCLE')review_cycle_meaning,
  (SELECT ar.meaning
  FROM ar_lookups ar
 WHERE ar.lookup_code = hzpuicustprofileeo.account_status
   AND ar.lookup_type = 'ACCOUNT_STATUS')act_Status_Meaning,
  (SELECT ar.meaning
  FROM ar_lookups ar
 WHERE ar.lookup_code = hzpuicustprofileeo.risk_code
   AND ar.lookup_type = 'RISK_CODE')risk_code_meaning,
(SELECT c.resource_name
  FROM jtf_rs_resource_extns_vl c
 WHERE c.resource_id = hzpuicustprofileeo.credit_analyst_id)credit_anylst_name,
(SELECT ar.name
     FROM ar_collectors ar
        WHERE ar.collector_id = HzPuiCustProfileEO.collector_id)Collector_Name,
(SELECT t.NAME
  FROM ra_terms_vl t
 WHERE t.term_id = hzpuicustprofileeo.standard_terms) payment_term_name,
(SELECT ar.hierarchy_name
     FROM ar_autocash_hierarchies ar
        WHERE ar.autocash_hierarchy_id = HzPuiCustProfileEO.AUTOCASH_HIERARCHY_ID) cash_ruleSet_Name,
(SELECT ar.hierarchy_name
     FROM ar_autocash_hierarchies ar
        WHERE ar.autocash_hierarchy_id = HzPuiCustProfileEO.AUTOCASH_HIERARCHY_ID_FOR_ADR) reminder_ruleSet_Name,
(SELECT ext.c_ext_attr1 
     from xx_cdh_cust_acct_ext_b ext, EGO_ATTR_GROUPS_V grp 
     where ext.attr_group_id=grp.attr_group_id 
     and grp.application_id=222 
     and  grp.attr_group_name = 'OMX_DNB_RATING' 
     and grp.attr_group_type='XX_CDH_CUST_ACCOUNT' 
     and ext.cust_account_id =HzPuiCustProfileEO.cust_account_id) as Dnb_Rating
FROM HZ_CUSTOMER_PROFILES HzPuiCustProfileEO

<ViewObject
   Name="ODArPartySiteInfosVO"
   Extends="oracle.apps.ar.cusstd.acctSite.server.ArPartySiteInfosVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.ar.cusstd.acctSite.server.ODArPartySiteInfosVORowImpl"

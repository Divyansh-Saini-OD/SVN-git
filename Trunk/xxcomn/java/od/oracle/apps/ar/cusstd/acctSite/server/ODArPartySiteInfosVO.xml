   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select ArHzPartySitesEO.party_site_id,
       ArHzPartySitesEO.party_site_number, 
       ArHzPartySitesEO.party_site_name, 
       decode(ArHzPartySitesEO.status, 'A', 'Y', 'N') Active,
       hz_format_pub.format_address(ArHzPartySitesEO.location_id, null, null, ' ') address,
       ArHzLocationsEO.address_lines_phonetic AlternateName,
       ArHzLocationsEO.location_id,
       ArHzPartySitesEO.status,
       CASE WHEN (instr(ArHzPartySitesEO.orig_system_reference,'-OMX') > 0)  THEN REGEXP_REPLACE(ArHzPartySitesEO.orig_system_reference,'-OMX', '') ELSE null  END  OMX_Consignee_ID 
from hz_party_sites ArHzPartySitesEO,
     hz_locations ArHzLocationsEO
where ArHzPartySitesEO.party_site_id = :1
and ArHzPartySitesEO.location_id = ArHzLocationsEO.location_id
   ]]></SQLQuery>

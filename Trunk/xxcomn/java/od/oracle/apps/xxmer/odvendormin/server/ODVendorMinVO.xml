<ViewObject
   Name="ODVendorMinVO"
   OrderBy="VENDOR_SITE_ID"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.odvendormin.server.ODVendorMinVORowImpl"
   ComponentClass="od.oracle.apps.xxmer.odvendormin.server.ODVendorMinVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT vs.vendor_site_id
      ,vs.vendor_site_code
      ,vs.invoice_currency_code
      ,kf.min_prepaid_code
      ,decode(kf.min_prepaid_code,'DL',to_char(kf.vendor_min_amount,'999,999,999'),kf.vendor_min_amount) vendor_min_amount
      ,vs.attribute9 legacyid
  FROM xx_po_vendor_sites_kff_v kf,
       po_vendor_sites_all      vs
 WHERE vs.vendor_site_id = kf.vendor_site_id
     AND nvl(inactive_date,sysdate+1) > sysdate
     AND fnd_profile.value_specific('ORG_ID')  = vs.org_id
   ]]></SQLQuery>

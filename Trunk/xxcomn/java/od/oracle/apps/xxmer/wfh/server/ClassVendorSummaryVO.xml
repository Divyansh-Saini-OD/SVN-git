<ViewObject
   Name="ClassVendorSummaryVO"
   OrderBy="MERCH_CLASS_ID"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.wfh.server.ClassVendorSummaryVORowImpl"
   ComponentClass="od.oracle.apps.xxmer.wfh.server.ClassVendorSummaryVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT ClassVendor.MERCH_CLASS_ID, 
       MerchClass.DESCRIPTION AS MERCH_CLASS_DESCRIPTION,
       ClassVendor.VENDOR_SITE_ID AS VENDOR_ID,
       VendorSites.VENDOR_SITE_CODE AS VENDOR_DESCRIPTION,
       ClassVendor.PLANNER_ID AS PLANNER_ID,
       EMPL.FIRST_NAME || ' ' || EMPL.LAST_NAME AS PLANNER_NAME
  FROM XX_WFH_CLASS_VENDOR_ALL ClassVendor LEFT OUTER JOIN XX_TEMP_WFH_EMPLOYEE EMPL
       ON ClassVendor.PLANNER_ID = EMPL.EMPLOYEE_ID
     , XX_WFH_MERCH_CLASS MerchClass
     , PO_VENDOR_SITES_ALL VendorSites
 WHERE ClassVendor.ORG_ID = 141
   AND ClassVendor.MERCH_CLASS_ID = MerchClass.MERCH_CLASS_ID
   AND ClassVendor.VENDOR_SITE_ID = VendorSites.VENDOR_SITE_ID
   ]]></SQLQuery>

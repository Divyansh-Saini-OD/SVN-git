   ComponentClass="od.oracle.apps.xxmer.wfh.server.UpdateClassVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT ClassEO.ORG_ID, 
       ClassEO.MERCH_CLASS_ID, 
       MerchClass.DESCRIPTION AS Merch_Class_Description,
       ClassEO.PLANNER_ID,
       EMPL.FIRST_NAME || ' ' || EMPL.LAST_NAME AS Planner_Name
  FROM XX_WFH_CLASS_ALL ClassEO LEFT OUTER JOIN XX_TEMP_WFH_EMPLOYEE EMPL
       ON ClassEO.PLANNER_ID = EMPL.EMPLOYEE_ID
     , XX_WFH_MERCH_CLASS MerchClass
 WHERE ClassEO.ORG_ID = 141
   AND ClassEO.MERCH_CLASS_ID = :1
   AND ClassEO.MERCH_CLASS_ID = MerchClass.MERCH_CLASS_ID
   ]]></SQLQuery>

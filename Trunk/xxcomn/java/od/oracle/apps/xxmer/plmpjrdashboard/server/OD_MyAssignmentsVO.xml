<ViewObject
   Name="OD_MyAssignmentsVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="od.oracle.apps.xxmer.plmpjrdashboard.server.OD_MyAssignmentsVORowImpl"
   ComponentClass="od.oracle.apps.xxmer.plmpjrdashboard.server.OD_MyAssignmentsVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT PPA.PROJECT_ID, PPA.NAME PROJ_NAME, PPA.SEGMENT1 PROJ_NUMBER,PT.TASK_ID,PT.TASK_NAME,PT.TASK_NUMBER,PPS.PROJECT_STATUS_NAME TASK_STATUS
,Ppvs.Scheduled_start_Date,Ppvs.Scheduled_Finish_Date,Pt.Parent_Task_Id,
ppeb.c_ext_attr1 department,ppeb.c_ext_attr2 class,ppeb.c_ext_attr5 division,pt.Task_Manager_Person_Id
,NULL select_checkbox
,(Select 
                          decode(count(1),0,'Disable','Enable')
                          from Od_Pa_Notes opn  where opn.project_id = ppa.project_id and opn.task_id = pt.task_id) Notes
,PT.ATTRIBUTE10 TASK_GROUP
  , CASE  WHEN PPVS.ACTUAL_FINISH_DATE <= PPVS.SCHEDULED_FINISH_DATE THEN 'Green'
          WHEN PPVS.ACTUAL_FINISH_DATE  > PPVS.SCHEDULED_FINISH_DATE THEN ''
          WHEN (PPVS.ACTUAL_FINISH_DATE is null AND PPVS.SCHEDULED_FINISH_DATE < SYSDATE) THEN 'Red'
      end
     STATUSFLAG
  ,PPVS.ACTUAL_FINISH_DATE
, (SELECT FULL_NAME FROM PER_ALL_PEOPLE_F PAPF 
    WHERE PAPF.PERSON_ID = PT.TASK_MANAGER_PERSON_ID AND SYSDATE BETWEEN NVL(PAPF.EFFECTIVE_START_DATE,SYSDATE) AND NVL(PAPF.EFFECTIVE_END_DATE,SYSDATE) ) OWNER_NAME
From 
Pa_Projects_All Ppa,
Pa_Tasks Pt,
Pa_Proj_Elements Ppe,
Pa_Project_Statuses Pps,
Pa_Proj_Elem_Ver_Schedule Ppvs,
Pa_Projects_Erp_Ext_B Ppeb,
Ego_Attr_Groups_V Eag,
Fnd_Application fa
Where 
Ppa.Project_Id = Pt.Project_Id
And Pt.Task_Id = Ppe.Proj_Element_Id
And Ppe.Status_Code = Pps.Project_Status_Code
and pps.status_type = 'TASK' 
-- Comment not showing records after comp/uncompAND pps.project_status_name = 'Not Started'
and ppe.object_type='PA_TASKS'
and ppa.attribute_category = 'Private Brand'
AND nvl(ppa.attribute10,'No') = 'Yes'
AND ppa.enabled_flag = 'Y'
and ppa.template_flag = 'N'
And Pt.Task_Id = Ppvs.Proj_Element_Id
and pt.project_id = ppvs.project_id
and ppeb.attr_group_id = eag.attr_group_id 
and eag.attr_group_name = 'PB_GEN_INFO'
and eag.application_id = fa.application_id 
AND FA.APPLICATION_SHORT_NAME = 'PA' 
AND PPEB.PROJECT_ID = PPA.PROJECT_ID
   ]]></SQLQuery>

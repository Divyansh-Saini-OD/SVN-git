   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
Select ffvl.flex_value_meaning, ffvl.description From Fnd_Flex_Value_Sets Ffvs, Fnd_Flex_Values_Vl Ffvl
Where Ffvs.Flex_Value_Set_Id = Ffvl.Flex_Value_Set_Id
And Ffvs.Flex_Value_Set_Name Like 'XX_GI_CLASS_VS'
And Ffvl.Enabled_Flag = 'Y'
and sysdate between nvl(ffvl.start_date_active,sysdate - 1) and nvl(ffvl.end_date_active,sysdate + 1)
   ]]></SQLQuery>

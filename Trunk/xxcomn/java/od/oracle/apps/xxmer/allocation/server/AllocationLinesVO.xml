       AllocationLinesEO.ALLOCATION_LINE_ID, 
       AllocationLinesEO.PO_HEADER_ID, 
       AllocationLinesEO.PO_LINE_ID, 
       AllocationLinesEO.LINE_LOCATION_ID, 
       AllocationLinesEO.LOCKED_IN, 
       AllocationLinesEO.ALLOC_ORGANIZATION_ID, 

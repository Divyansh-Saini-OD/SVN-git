       AllocationLinesEO.LAST_UPDATED_BY, 
       AllocationLinesEO.CREATION_DATE, 
       AllocationLinesEO.CREATED_BY, 
       AllocationLinesEO.LAST_UPDATE_LOGIN,
	po.segment1 as "PO",	
	AH.BATCH_NO,
	adv.RUTL,
	adv.RTC as "ReplenTypeCd",
	adv.QOH,
	adv.ONORDER,
	adv.INTRANSIT,	
	adv.AWS,
	adv.WEEKS_SUPPLY,
	adv.INNER_PK,
	adv.CASE_PK,
	' ' as "LegacyShipTO",
        ' ' as "LegacyLocation",
	'N' as "DeleteAllocation"
       

	
	
FROM xx_po_ALLOCATION_LINES AllocationLinesEO, 
	 po_headers_all po, 
	xx_po_ALLOCATION_HEADER AH,
	XX_PO_ALLOCATION_DETAILS_V adv
	 
	 
where po.po_header_id = AllocationLinesEO.po_header_id
AND AH.ALLOCATION_HEADER_ID = AllocationLinesEO.ALLOCATION_HEADER_ID
and AllocationLinesEO.ALLOCATION_LINE_ID = adv.ALLOCATION_LINE_ID
   ]]></SQLQuery>

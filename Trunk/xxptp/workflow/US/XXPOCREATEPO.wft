      ARROW_GEOMETRY = "0;0;0;0;0.522727;48,96:48,400:"
    END ACTIVITY_TRANSITION
  END PROCESS_ACTIVITY

  BEGIN PROCESS_ACTIVITY "PO_DOC_CREATED"
    ACTIVITY_ITEM_TYPE = "CREATEPO"
    ACTIVITY_NAME = "PO_DOC_CREATED"
    ICON_GEOMETRY = "-448,96"
    VALUE_TYPE = "ITEMATTR"
    PERFORM_ROLE = "BUYER_USERNAME"

    BEGIN ACTIVITY_TRANSITION "*" "SHOULD_DOC_BE_APPROVED"
      ARROW_GEOMETRY = "1;0;0;0;0.500000;-448,96:-208,96:"

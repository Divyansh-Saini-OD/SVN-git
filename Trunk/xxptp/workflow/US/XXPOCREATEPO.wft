    ACTIVITY_NAME = "END"
    START_END = "END"
    DEFAULT_RESULT = "DOCUMENT_PROCESSED"
    ICON_GEOMETRY = "288,400"
    VALUE_TYPE = "CONSTANT"
  END PROCESS_ACTIVITY

  BEGIN PROCESS_ACTIVITY "LAUNCH_PO_APPROVAL"

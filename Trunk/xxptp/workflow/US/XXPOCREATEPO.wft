    VALUE_TYPE = "CONSTANT"

    BEGIN ACTIVITY_TRANSITION "CREATE_FAILED" "CONTINUEFLOW-1"
      ARROW_GEOMETRY = "0;0;8;0;0.312500;-272,400:48,400:"
    END ACTIVITY_TRANSITION

    BEGIN ACTIVITY_TRANSITION "CREATE_OK" "PO_DOC_CREATED"
      ARROW_GEOMETRY = "0;0;6;0;0.500063;-272,400:-448,96:"

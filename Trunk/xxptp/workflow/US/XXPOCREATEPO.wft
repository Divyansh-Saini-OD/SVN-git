      VALUE_TYPE = "CONSTANT"
    END ACTIVITY_ATTRIBUTE_VALUE

    BEGIN ACTIVITY_TRANSITION "*" "END-9"
      ARROW_GEOMETRY = "1;0;0;0;0.500000;48,400:288,400:"
    END ACTIVITY_TRANSITION
  END PROCESS_ACTIVITY

  BEGIN PROCESS_ACTIVITY "CREATE_DOC"

         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/db/top/InsertUpdateDepartmentDB2Data"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns7="http://xmlns.officedepot.com/EnterpriseObjects/Core/Common/V3"
         xmlns:ns8="http://schemas.xmlsoap.org/ws/2003/03/addressing"
         xmlns:ns9="urn:oasis:names:tc:xacml:2.0:context:schema:cd:04"
         xmlns:ns10="http://xmlns.oracle.com/Enterprise/Tools/schemas/DEPT_SYNC.VERSION_1"
         xmlns:aia="http://www.oracle.com/XSL/Transform/java/oracle.apps.aia.core.xpath.AIAFunctions"
         xmlns:ns11="http://xmlns.oracle.com/pcbpel/adapter/jms/10GTo11G/OTMCOSOrderReleaseABCS/InsertErrorLoggerJMSPublisher"
         xmlns:ns12="ODError">


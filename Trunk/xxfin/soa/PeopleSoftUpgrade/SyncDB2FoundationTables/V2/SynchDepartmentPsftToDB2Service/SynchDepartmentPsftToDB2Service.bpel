    <variable name="InvokeInsertErrorLogger_Produce_Message_InputVariable"
              messageType="ns11:Produce_Message_msg"/>
    <variable name="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
              messageType="ns11:Produce_Message_msg"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="Sequence1">
        <scope name="ScopeSendNotification">
        <variables>
              <variable name="messagePayload" type="xsd:string"/>
              <variable name="emailSubject" type="xsd:string"/>
              <variable name="errorText" type="xsd:string"/>
              <variable name="systemName" type="xsd:string"/>
              <variable name="messageOperation" type="xsd:string"/>
              <variable name="applicationTeamErrorText" type="xsd:string"/>
               <variable name="errorLoopIndex" type="xsd:integer"/>
          <variable name="errorNumberOfDepts" type="xsd:integer"/>
          <variable name="deptList" type="xsd:string"/>
          </variables>
          <sequence>
           <assign name="InitLoopIndex">
              <copy>
                <from expression="1"/>
                <to variable="errorLoopIndex"/>
              </copy>
              <copy>
                <from expression="ora:countNodes('inputVariable','payload','/ns10:DEPT_SYNC/ns10:MsgData/ns10:Transaction/ns10:DEPT_TBL/ns10:DEPTID')"/>
                <to variable="errorNumberOfDepts"/>
              </copy>
            </assign>
          <while name="ForEachDepartment"
                   condition="bpws:getVariableData('errorLoopIndex') &lt;= bpws:getVariableData('errorNumberOfDepts')">
              <sequence name="Sequence3">
                <assign name="AssignDeptSpecificData">
                  <copy>
                    <from expression="concat(bpws:getVariableData('deptList'),' ',bpws:getVariableData('inputVariable','payload','/ns10:DEPT_SYNC/ns10:MsgData/ns10:Transaction[$errorLoopIndex]/ns10:DEPT_TBL/ns10:DEPTID'),'')"/>
                    <to variable="deptList"/>
                  </copy>
                  <copy>
                    <from expression="bpws:getVariableData('errorLoopIndex') + 1"/>
                    <to variable="errorLoopIndex"/>
                  </copy>
                </assign>
               
              </sequence>
            </while>
            <assign name="AssignInsertErrorLoggerData">
              <copy>
                <from expression="ora:getPreference('SOA_ADMIN_APP_NAME')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:BusinessProcessName"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('SOURCE_SYSTEM_NAME')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:TradingPartnerDetails/ns12:TPFrom"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('TP_TO')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:TradingPartnerDetails/ns12:TPTo"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('ENTITY_ID')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:Entity/ns12:EntityID"/>
              </copy>
              <copy>
                <from expression="ora:getFaultName()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorCode"/>
              </copy>
              <copy>
                <from expression='concat(ora:getPreference("SOA_ADMIN_ERROR_MESSAGE"),ora:getCurrentDateTime())'/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorDescription"/>
              </copy>
              <copy>
                <from expression="substring(ora:getFaultAsString(),1,4000)"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorText"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('ERROR_TYPE')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorType"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('ERROR_SEVERITY')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorSeverity"/>
              </copy>
              <copy>
                <from expression="ora:getCurrentDateTime()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorDateTime"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('NOTIFICATION_FLAG')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:Notification/ns12:sendNotification"/>
              </copy>
              <copy>
                <from expression='concat(ora:getPreference("SOA_ADMIN_ERROR_MESSAGE"),ora:getCurrentDateTime())'/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:Notification/ns12:Subject"/>
              </copy>
              <copy>
                <from expression="ora:getCompositeInstanceId()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageId"/>
              </copy>
              <copy>
                <from expression="ora:getCurrentDateTime()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageDateTime"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_TYPE')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageType"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_VERSION')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageVersion"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_OPERATION')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageOperation"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('SOURCE_SYSTEM_NAME')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageSourceSystem"/>
              </copy>
              <copy>
                <from expression="ora:getCompositeInstanceId()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:BusinessProcessId"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('CATCH_ALL_PROCESS_STEP')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:BusinessProcessStep"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('DOMAIN')"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:BusinessProcessDomain"/>
              </copy>
              <copy>
                <from expression="ora:getCompositeURL()"/>
                <to variable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:SystemName"/>
              </copy>
            </assign>
            <invoke name="InvokeInsertErrorLogger"
                    inputVariable="InvokeInsertErrorLogger_Produce_Message_InputVariable"
                    partnerLink="InsertErrorLoggerJMSPublisher"
                    portType="ns11:Produce_Message_ptt"
                    operation="Produce_Message" bpelx:invokeAsDetail="no"/>
            <assign name="AssignInsertErrorLoggerApp">
              <copy>
                <from expression="ora:getPreference('SYSTEM_NAME')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:BusinessProcessName"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('SOURCE_SYSTEM_NAME')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:TradingPartnerDetails/ns12:TPFrom"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('TP_TO')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:TradingPartnerDetails/ns12:TPTo"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('ENTITY_ID')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:Entity/ns12:EntityID"/>
              </copy>
              <copy>
                <from expression="ora:getFaultName()"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorCode"/>
              </copy>
              <copy>
                <from expression="concat(ora:getPreference('APPLICATION_EMAIL_SUBJECT'),ora:getCurrentDateTime())"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorDescription"/>
              </copy>
              <copy>
                <from expression="concat(ora:getPreference('APPLICATION_ERROR_MESSAGE'),' ', bpws:getVariableData('deptList'))"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorText"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('ERROR_TYPE')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorType"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('ERROR_SEVERITY')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorSeverity"/>
              </copy>
              <copy>
                <from expression="ora:getCurrentDateTime()"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ErrorDetails/ns12:ErrorDateTime"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('NOTIFICATION_FLAG')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:Notification/ns12:sendNotification"/>
              </copy>
              <copy>
                <from expression='concat(ora:getPreference("SOA_ADMIN_ERROR_MESSAGE"),ora:getCurrentDateTime())'/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:Notification/ns12:Subject"/>
              </copy>
              <copy>
                <from expression="ora:getCompositeInstanceId()"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageId"/>
              </copy>
              <copy>
                <from expression="ora:getCurrentDateTime()"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageDateTime"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_TYPE')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageType"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_VERSION')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageVersion"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_OPERATION')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageOperation"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('SOURCE_SYSTEM_NAME')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:MessageDetails/ns12:MessageSourceSystem"/>
              </copy>
              <copy>
                <from expression="ora:getCompositeURL()"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:SystemName"/>
              </copy>
              <copy>
                <from expression="ora:getCompositeInstanceId()"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:BusinessProcessId"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('CATCH_ALL_PROCESS_STEP')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:BusinessProcessStep"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('DOMAIN')"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:BusinessProcessDomain"/>
              </copy>
              <copy>
                <from expression="ora:getCompositeURL()"/>
                <to variable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    part="body"
                    query="/ns12:ErrorData/ns12:ProcessInfo/ns12:SystemName"/>
              </copy>
            </assign>
            <invoke name="InvokeInsertErrorLoggerApp"
                    inputVariable="InvokeInsertErrorLoggerApp_Produce_Message_InputVariable"
                    partnerLink="InsertErrorLoggerJMSPublisher"
                    portType="ns11:Produce_Message_ptt"
                    operation="Produce_Message" bpelx:invokeAsDetail="no"/>
          </sequence>
        </scope>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in SynchDepartmentPsftToDB2Service.wsdl) -->
    <receive name="receiveInput" partnerLink="synchdepartmentpsfttodb2service_client" portType="client:SynchDepartmentPsftToDB2Service" operation="synchronizeDepartmentData" variable="inputVariable" createInstance="yes"/>
    <assign name="XFormPSFTDeptToDB2DeptFormat">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:doXSLTransformForDoc('xsl/XFormPSFTDeptToDB2DeptFormat.xsl', $inputVariable.payload)"/>
        <to variable="Invoke_InsertUpdateDepartmentDB2Data_merge_InputVariable"
            part="PsDeptTblCollection"/>
      </copy>
    </assign>
    <invoke name="Invoke_InsertUpdateDepartmentDB2Data"
            inputVariable="Invoke_InsertUpdateDepartmentDB2Data_merge_InputVariable"
            partnerLink="InsertUpdateDepartmentDB2Data"
            portType="ns1:InsertUpdateDepartmentDB2Data_ptt" operation="merge"
            bpelx:invokeAsDetail="no"/>
  </sequence>
</process>
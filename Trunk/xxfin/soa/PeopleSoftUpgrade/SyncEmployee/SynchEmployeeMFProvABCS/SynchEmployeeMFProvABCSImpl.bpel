                <from expression="concat(ora:getPreference('SOA_ADMIN_ERROR_MESSAGE'),bpws:getVariableData('receiveInput_Consume_Message_InputVariable','body','/ns2:SyncEmployeeEBM/ns2:DataArea/ns2:DateTimeCreated'))"/>

            <variable name="applicationTeamErrorText" type="xsd:string"/>
            <variable name="employeeError"
                      element="ns11:UpdateEmployeeErrorToPSFTEBM"/>
          </variables>
          <sequence name="ErrorHandler">
            <switch name="Switch4">
              <case condition="contains(ora:getFaultAsString(),'employeeRemoteFault')">
                <bpelx:annotation>
                  <bpelx:pattern>In case of remote fault add remaining employees to the error list</bpelx:pattern>
                  <bpelx:general>
                    <bpelx:property name="userLabel">LoopThroughRestOfEmployees</bpelx:property>
                  </bpelx:general>
                </bpelx:annotation>
                <while name="LoopRemainingEmployees"
                       condition="bpws:getVariableData('loopIndex')  &lt;= bpws:getVariableData('numberOfEmployees')">
                  <sequence name="Sequence7">
                    <assign name="AssignRemainingEmployees">
                      <copy>
                        <from expression="concat(bpws:getVariableData('employeeList'),bpws:getVariableData('receiveInput_Consume_Message_InputVariable','body','/ns2:SyncEmployeeEBM/ns2:DataArea/ns2:EmployeeList/ns2:Employee[$loopIndex]/ns2:EmployeeIdentification/ns2:EmployeeID'),&quot;,&quot;)"/>
                        <to variable="employeeList"/>
                      </copy>
                    </assign>
                    <assign name="AssignEmployeeRemoteErrorDetails">
                      <copy>
                        <from expression="bpws:getVariableData('receiveInput_Consume_Message_InputVariable','body','/ns2:SyncEmployeeEBM/ns2:DataArea/ns2:EmployeeList/ns2:Employee[$loopIndex]/ns2:EmployeeIdentification/ns2:EmployeeID')"/>
                        <to variable="employeeError"
                            query="/ns11:UpdateEmployeeErrorToPSFTEBM/ns11:EmployeeErrorDetails/ns11:EmployeeID"/>
                      </copy>
                      <copy>
                        <from expression="string('N')"/>
                        <to variable="employeeError"
                            query="/ns11:UpdateEmployeeErrorToPSFTEBM/ns11:EmployeeErrorDetails/ns11:ProcessFlag"/>
                      </copy>
                      <copy>
                        <from expression="xp20:current-dateTime()"/>
                        <to variable="employeeError"
                            query="/ns11:UpdateEmployeeErrorToPSFTEBM/ns11:EmployeeErrorDetails/ns11:ProcessDateTime"/>
                      </copy>
                    </assign>
                    <assign name="AppendEmployeeRemoteErrorData">
                      <bpelx:append>
                        <bpelx:from variable="employeeError"
                                    query="/ns11:UpdateEmployeeErrorToPSFTEBM/ns11:EmployeeErrorDetails"/>
                        <bpelx:to variable="EmployeeErrorData"
                                  query="/ns11:UpdateEmployeeErrorToPSFTEBM"/>
                      </bpelx:append>
                      <copy>
                        <from expression="bpws:getVariableData('employeeErrorIndex') + 1"/>
                        <to variable="employeeErrorIndex"/>
                      </copy>
                      <copy>
                        <from expression=" bpws:getVariableData('loopIndex') + 1"/>
                        <to variable="loopIndex"/>
                      </copy>
                    </assign>
                  </sequence>
                </while>
              </case>
            </switch>
            <assign name="AssignInsertCommonErrorHandler">

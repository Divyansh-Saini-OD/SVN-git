  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in SynchEmployeeMFProvABCSImpl.wsdl) -->
    <receive name="receiveInput"
             variable="receiveInput_Consume_Message_InputVariable" createInstance="yes"
             partnerLink="MFUpdateEmployeeJMSConsumerV1"
             portType="ns1:Consume_Message_ptt" operation="Consume_Message"/>
    <!--<receive name="receiveInput"
             variable="receiveInput_Consume_Message_InputVariable" createInstance="yes"
             partnerLink="MFUpdateEmployeeService"
             portType="ns1:Consume_Message_ptt" operation="Consume_Message"/> -->
    <assign name="InitLoopIndex">
      <copy>
        <from expression="1"/>
        <to variable="loopIndex"/>
      </copy>
      <copy>
        <from expression="0"/>
        <to variable="employeeErrorIndex"/>
      </copy>
      <copy>
        <from expression="ora:countNodes('receiveInput_Consume_Message_InputVariable','body','/ns2:SyncEmployeeEBM/ns2:DataArea/ns2:EmployeeList/ns2:Employee')"/>
        <to variable="numberOfEmployees"/>
      </copy>
    </assign>
    <while name="LoopEmployee"
           condition="bpws:getVariableData('loopIndex')  &lt;= bpws:getVariableData('numberOfEmployees')">
      <sequence>
        <sequence>
          <switch name="Switch1">
            <case condition="bpws:getVariableData('receiveInput_Consume_Message_InputVariable','body','/ns2:SyncEmployeeEBM/ns2:DataArea/ns2:EmployeeList/ns2:Employee[$loopIndex]/ns2:EmploymentDetails/ns2:PersonRelationWithOrganisation')='EMP'">
              <bpelx:annotation>
                <bpelx:pattern>Selects only EMP Employee types</bpelx:pattern>
                <bpelx:general>
                  <bpelx:property name="userLabel">EmployeeTypeFilter</bpelx:property>
                </bpelx:general>
              </bpelx:annotation>
              <scope name="Scope2">
                <variables>
                  <variable name="employeeFaultMessage"
                            messageType="ns16:SyncEmployeeMFTablesABCSImplFaultMessage"/>
                </variables>
                <faultHandlers>
                  <catch faultName="ns16:employeeFault"
                         faultVariable="employeeFaultMessage">
                    <switch name="Switch3">
                      <case condition="contains(ora:getFaultAsString(),'remoteFault')">
                        <bpelx:annotation>
                          <bpelx:pattern>Check if there is a remote fault</bpelx:pattern>
                          <bpelx:general>
                            <bpelx:property name="userLabel">RemoteFault</bpelx:property>
                          </bpelx:general>
                        </bpelx:annotation>
                        <sequence name="Sequence5">
                          <assign name="AssignEmployeeRemoteErrorData">
                            <bpelx:append>
                              <bpelx:from variable="employeeFaultMessage"
                                          part="payload"
                                          query="/ns11:UpdateEmployeeErrorToPSFTEBM/ns11:EmployeeErrorDetails"/>
                              <bpelx:to variable="EmployeeErrorData"
                                        query="/ns11:UpdateEmployeeErrorToPSFTEBM"/>
                            </bpelx:append>
                            <copy>
                              <from expression="bpws:getVariableData('employeeErrorIndex') + 1"/>
                              <to variable="employeeErrorIndex"/>
                            </copy>
                            <copy>
                              <from expression=" bpws:getVariableData('loopIndex') + 1"/>
                              <to variable="loopIndex"/>
                            </copy>
                            <copy>
                              <from expression="concat(bpws:getVariableData('employeeList'),bpws:getVariableData('InvokeSyncEmployeeMFTablesABCSImpl_process_InputVariable','payload','/ns2:Employee/ns2:EmployeeIdentification/ns2:EmployeeID'),&quot;,&quot;)"/>
                              <to variable="employeeList"/>
                            </copy>
                          </assign>
                          <throw name="ThrowRemoteFault"
                                 faultName="bpelx:employeeRemoteFault"/>
                        </sequence>
                      </case>
                      <otherwise>
                        <sequence name="Sequence6">
                          <assign name="AssignEmployeeSpecificErrorData">
                            <bpelx:append>
                              <bpelx:from variable="employeeFaultMessage"
                                          part="payload"
                                          query="/ns11:UpdateEmployeeErrorToPSFTEBM/ns11:EmployeeErrorDetails"/>
                              <bpelx:to variable="EmployeeErrorData"
                                        query="/ns11:UpdateEmployeeErrorToPSFTEBM"/>
                            </bpelx:append>
                            <copy>
                              <from expression="bpws:getVariableData('employeeErrorIndex') +1"/>
                              <to variable="employeeErrorIndex"/>
                            </copy>
                            <copy>
                              <from expression="concat(bpws:getVariableData('employeeList'),bpws:getVariableData('InvokeSyncEmployeeMFTablesABCSImpl_process_InputVariable','payload','/ns2:Employee/ns2:EmployeeIdentification/ns2:EmployeeID'),&quot;,&quot;)"/>
                              <to variable="employeeList"/>
                            </copy>
                          </assign>
                        </sequence>
                      </otherwise>
                    </switch>
                  </catch>
                </faultHandlers>
                <sequence name="Sequence1">
                  <assign name="AssignCurrentEmployee">
                    <copy>
                      <from expression="ora:getElement('receiveInput_Consume_Message_InputVariable','body','/ns2:SyncEmployeeEBM/ns2:DataArea/ns2:EmployeeList/ns2:Employee',$loopIndex)"/>
                      <to variable="InvokeSyncEmployeeMFTablesABCSImpl_process_InputVariable"
                          part="payload" query="/ns2:Employee"/>
                    </copy>
                  </assign>
                  <invoke name="InvokeSyncEmployeeMFTablesABCSImpl"
                          bpelx:invokeAsDetail="no"
                          inputVariable="InvokeSyncEmployeeMFTablesABCSImpl_process_InputVariable"
                          outputVariable="InvokeSyncEmployeeMFTablesABCSImpl_process_OutputVariable"
                          partnerLink="SyncEmployeeMFTablesABCSImpl.syncemployeemftablesabcsimpl_client"
                          portType="ns16:SyncEmployeeMFTablesABCSImpl"
                          operation="process"/>
                </sequence>
              </scope>
            </case>
          </switch>
          <assign name="AssignIncreamentLoopIndex">
            <copy>
              <from expression=" bpws:getVariableData('loopIndex') + 1"/>
              <to variable="loopIndex"/>
            </copy>
          </assign>
        </sequence>
      </sequence>
    </while>
    <switch name="Switch2">
      <case condition="bpws:getVariableData('EmployeeErrorData','/ns11:UpdateEmployeeErrorToPSFTEBM/ns11:EmployeeErrorDetails[1]/ns11:ProcessFlag')!=''">
        <bpelx:annotation>
          <bpelx:pattern>Test if any error has occured during MF tabel sync</bpelx:pattern>
          <bpelx:general>
            <bpelx:property name="userLabel">OnErrorUpdatingMFTables</bpelx:property>
          </bpelx:general>
        </bpelx:annotation>
        <throw name="ThrowError"
               faultName="bpelx:employeeSyncFault"/>
      </case>
    </switch>
  </sequence>
</process>
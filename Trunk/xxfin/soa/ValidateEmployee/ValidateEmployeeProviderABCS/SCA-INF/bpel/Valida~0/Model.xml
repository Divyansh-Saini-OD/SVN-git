                <to variable="invokeCommonErrorLoggerInputData" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ErrorDetails/ns6:ProcessStep"/>
              </copy>
              <copy>
                <from expression="ora:getFaultName()"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ErrorDetails/ns6:ErrorCode"/>
              </copy>
              <copy>

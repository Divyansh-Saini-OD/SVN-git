      <sequence name="SequenceCatchAll" id="BpSeq0">
        <scope name="Scope4" id="BpScp0">
          <bpelx:annotation>
            <bpelx:general>
              <bpelx:property name="userLabel">ScopePopulateFaultErrorMessageAndReplyBack</bpelx:property>
            </bpelx:general>
          </bpelx:annotation>
          <variables>
            <variable name="employeeValidationFault" messageType="client:EmployeeValidationFault"/>
          </variables>
          <sequence name="SequencePopulateAndReplyFaultDetails" id="BpSeq1">
            <switch name="DetermineErrorType" id="BpSwt0">
              <case condition="contains(ora:getFaultAsString(),'remoteFault') and contains(ora:getFaultAsString(),ora:getPreference('DATA_NOT_FOUND_PEOPLESOFT_RESPONSE')) ">

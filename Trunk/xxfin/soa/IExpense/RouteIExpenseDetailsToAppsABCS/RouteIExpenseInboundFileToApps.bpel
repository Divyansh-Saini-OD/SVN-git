<!--
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  Oracle JDeveloper BPEL Designer 
  
  Created: Tue Jan 24 14:23:39 IST 2012
  Type: BPEL 1.1 Process
  Purpose: One Way BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="RouteIExpenseInboundFileToApps"
               targetNamespace="http://xmlns.oracle.com/IExpense/RouteIExpenseDetailsToAppsABCS/RouteIExpenseInboundFileToApps"
               xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
               xmlns:client="http://xmlns.oracle.com/IExpense/RouteIExpenseDetailsToAppsABCS/RouteIExpenseInboundFileToApps"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/file/IExpense/RouteIExpenseDetailsToAppsABCS/PollForIExpenseInboundFile"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/apps/IExpense/RouteIExpenseDetailsToAppsABCS/InvokeIExpenseConcurrentProgram"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/apps/IExpense/RouteIExpenseDetailsToAppsABCS/IExpenseConcurrentProgram"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/opaque/"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_MASTER_CARD_TRAN_VALID_PKG/CALL_MAIN/"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns6="http://xmlns.oracle.com/pcbpel/adapter/jms/IExpense/RouteIExpenseDetailsToAppsABCS/CommonErrorLoggingService"
         xmlns:ns7="http://www.officedepot.com/officedepot/ODComnErrorStructure/1.0"
         xmlns:ns8="http://xmlns.oracle.com/pcbpel/adapter/file/IExpense/RouteIExpenseDetailsToAppsABCS/testread"
         xmlns:ns9="http://xmlns.oracle.com/pcbpel/adapter/db/IExpense/RouteIExpenseDetailsToAppsABCS/ReadCardProgramID"
         xmlns:ns10="http://xmlns.oracle.com/pcbpel/adapter/db/top/ReadCardProgramID"
         xmlns:ns11="http://xmlns.oracle.com/pcbpel/adapter/file/IExpense/RouteIExpenseDetailsToAppsABCS/ArchivePolledFile"
         xmlns:ns12="http://xmlns.oracle.com/pcbpel/adapter/file/IExpense/RouteIExpenseDetailsToAppsABCS/ArchiveFile"
         xmlns:ns13="http://xmlns.oracle.com/pcbpel/adapter/file/IExpense/RouteIExpenseDetailsToAppsABCS/CopyFile">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="PollForIExpenseInboundFile"
                 partnerLinkType="ns1:Read_plt" myRole="Read_role"/>
    <partnerLink name="CommonErrorLoggingService"
                 partnerLinkType="ns6:Produce_Message_plt"
                 partnerRole="Produce_Message_role"/>
    <partnerLink name="IExpenseConcurrentProgram"
                 partnerLinkType="ns3:IExpenseConcurrentProgram_plt"
                 partnerRole="IExpenseConcurrentProgram_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <variable name="fileName" type="xsd:string"/>
    <variable name="directoryPath" type="xsd:string"/>
    <variable name="inputVariable" messageType="ns1:Read_msg"/>
    <variable name="IExpenseConcurrentProgramInputVariable"
              messageType="ns3:args_in_msg"/>

    <variable name="invokeCommonErrorLoggerInputData"
              messageType="ns6:Produce_Message_msg"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="SequenceSendNotification">
        <scope name="Scope4">
          <bpelx:annotation>
            <bpelx:general>
              <bpelx:property name="userLabel">ScopeSendNotification</bpelx:property>
            </bpelx:general>
          </bpelx:annotation>
          <variables>
            <variable name="messagePayload" type="xsd:string"/>
            <variable name="emailSubject" type="xsd:string"/>
            <variable name="errorText" type="xsd:string"/>
            <variable name="systemName" type="xsd:string"/>
          </variables>
          <sequence name="SequenceSendNotification">
            <assign name="AssignRunTimeErrorRelatedData">
              <copy>
                <from expression="concat(ora:getPreference(&quot;SOA_ADMIN_ERROR_MESSAGE&quot;),bpws:getVariableData('fileName'))"/>
                <to variable="emailSubject"/>
              </copy>
              <copy>
                <from expression="substring(ora:getFaultAsString(),1,500)"/>
                <to variable="errorText"/>
              </copy>
              <copy>
                <from expression='string("")'/>
                <to variable="messagePayload"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('SOA_ADMIN_APP_NAME')"/>
                <to variable="systemName"/>
              </copy>
            </assign>
            <assign name="AssignPopulateCommonErrorLoggingData">
              <copy>
                <from expression="ora:getProcessId()"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ProcessInfo/ns7:ProcessName"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('ERROR_TYPE')"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorType"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('ERROR_SEVERITY')"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorSeverity"/>
              </copy>
              <copy>
                <from expression="bpws:getVariableData('errorText')"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorText"/>
              </copy>
              <copy>
                <from expression="ora:getCurrentDateTime()"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorDateTime"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('CATCH_ALL_PROCESS_STEP')"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ProcessStep"/>
              </copy>
              <copy>
                <from expression="ora:getFaultName()"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorCode"/>
              </copy>
              <copy>
                <from expression="ora:getDomainId()"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ProcessInfo/ns7:Domain"/>
              </copy>
              <copy>
                <from expression="ora:getCompositeInstanceId()"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageID"/>
              </copy>
              <copy>
                <from expression="ora:getCurrentDateTime()"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageDateTime"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_TYPE')"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageType"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_VERSION')"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageVersion"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('MESSAGE_OPERATION')"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageOperation"/>
              </copy>
              <copy>
                <from expression="ora:getPreference('SOURCE_SYSTEM_NAME')"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessageSourceSystem"/>
              </copy>
              <copy>
                <from expression="bpws:getVariableData('systemName')"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ProcessInfo/ns7:SystemName"/>
              </copy>
              <copy>
                <from expression="bpws:getVariableData('emailSubject')"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:ErrorDetails/ns7:ErrorDescription"/>
              </copy>
              <copy>
                <from variable="messagePayload"/>
                <to variable="invokeCommonErrorLoggerInputData" part="body"
                    query="/ns7:ODComnErrorStructure/ns7:ODComnErrorStructureList/ns7:MessageDetails/ns7:MessagePayload"/>
              </copy>
            </assign>
            <invoke name="InvokeCommonErrorLoggerToSendErrorData"
                    inputVariable="invokeCommonErrorLoggerInputData"
                    partnerLink="CommonErrorLoggingService"
                    portType="ns6:Produce_Message_ptt"
                    operation="Produce_Message" bpelx:invokeAsDetail="no"/>
          </sequence>
        </scope>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in RouteIExpenseInboundFileToApps.wsdl) -->
    <receive name="receiveInput" partnerLink="PollForIExpenseInboundFile"
             portType="ns1:Read_ptt" operation="Read" variable="inputVariable" createInstance="yes">
      <bpelx:property name="jca.file.FileName" variable="fileName"/>
      <bpelx:property name="jca.file.Directory" variable="directoryPath"/>
    </receive>
    <scope name="Scope3">
      <bpelx:annotation>
        <bpelx:general>
          <bpelx:property name="userLabel">SetHeader</bpelx:property>
        </bpelx:general>
      </bpelx:annotation>
      <variables>
        <variable name="header" type="xsd:string"/>
      </variables>
      <sequence name="SequenceSetHeader">
        <assign name="setHeader">
          <copy>
            <from expression="bpws:getVariableData('fileName')"/>
            <to variable="header"/>
          </copy>
        </assign>      
    <bpelx:exec name="setTitle" version="1.5" language="java">
          <![CDATA[/* Set the Title for the process      
*/      
          
try                 
{                 
               
  String title      = "";                 
  title = (String)getVariableData("header");                 
  setCompositeInstanceTitle(title) ;                 
                 
}                 
catch (Exception ex)                 
{                 
  addAuditTrailEntry(ex.toString());                 
 ex.printStackTrace();                 
}]]>
        </bpelx:exec>
      </sequence>
    </scope>
    <scope name="Scope2">
      <bpelx:annotation>
        <bpelx:general>
          <bpelx:property name="userLabel">ScopeInvokeIExpenseConcurrentProgram</bpelx:property>
        </bpelx:general>
      </bpelx:annotation>
      <sequence name="SequenceInvokeConcurrentProgram">
        <assign name="AssignConcurrentProgramInputs">
          <copy>
            <from expression="concat(bpws:getVariableData('directoryPath'),'/',bpws:getVariableData('fileName'))"/>
            <to variable="IExpenseConcurrentProgramInputVariable"
                part="InputParameters"
                query="/ns5:InputParameters/ns5:P_DATA_FILE"/>
          </copy>
        </assign>
        <invoke name="InvokeIExpenseConcurrentProgram"
                inputVariable="IExpenseConcurrentProgramInputVariable" 
                partnerLink="IExpenseConcurrentProgram"
                portType="ns3:IExpenseConcurrentProgram_ptt"
                operation="IExpenseConcurrentProgram"
                bpelx:invokeAsDetail="no"/>
      </sequence>
    </scope>
  </sequence>
</process>
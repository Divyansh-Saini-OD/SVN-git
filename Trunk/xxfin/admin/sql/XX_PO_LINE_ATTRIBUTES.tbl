CREATE TABLE XXFIN.XX_PO_LINE_ATTRIBUTES
(PO_NUMBER		VARCHAR2(30)
,LINE_NUMBER		NUMBER
,ATTRIBUTE1             VARCHAR2(150)  
,ATTRIBUTE2             VARCHAR2(150)  
,CREATED_BY          	NUMBER
,CREATION_DATE       	DATE 

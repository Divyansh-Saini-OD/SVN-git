,LAST_UPDATE_DATE    	DATE
,LAST_UPDATE_LOGIN   	NUMBER
,CONSTRAINT XX_PO_LINE_ATTRIBUTES_PK PRIMARY KEY(PO_NUMBER,LINE_NUMBER));
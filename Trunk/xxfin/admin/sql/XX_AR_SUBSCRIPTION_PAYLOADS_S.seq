-- Create Custom Sequence XX_AR_SUBSCRIPTION_PAYLOADS_S
-- ----------------------------------------------------------------------------

BEGIN
  EXECUTE IMMEDIATE 'DROP SEQUENCE XXFIN.XX_AR_SUBSCRIPTION_PAYLOADS_S';
EXCEPTION
  WHEN OTHERS THEN NULL;

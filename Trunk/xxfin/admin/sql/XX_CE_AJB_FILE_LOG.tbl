SET SHOW         OFF
SET VERIFY       OFF
SET ECHO         OFF
SET TAB          OFF
SET FEEDBACK     ON

CREATE TABLE xxfin.xx_ce_ajb_file_log
        ( log_seq_id NUMBER
        , file_type VARCHAR2(20)
        , file_name VARCHAR2(200)
        , bpel_instance_id NUMBER
        , creation_date DATE);


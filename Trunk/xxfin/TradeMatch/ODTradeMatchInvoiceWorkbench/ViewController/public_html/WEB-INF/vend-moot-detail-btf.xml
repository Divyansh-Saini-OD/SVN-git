<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="vend-moot-detail-btf">
    <default-activity>setDefaultOrgId</default-activity>
    <transaction>
      <new-transaction/>
    </transaction>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__4">
      <name>pSupplierName</name>
      <value>#{pageFlowScope.pSupplierName}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <input-parameter-definition id="__15">
      <name>invoiceNo</name>
      <value>#{pageFlowScope.invoiceNo}</value>
      <class>java.lang.Number</class>
    </input-parameter-definition>
    <managed-bean id="__1">
      <managed-bean-name>MyDate</managed-bean-name>
      <managed-bean-class>java.util.Date</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__2">
      <managed-bean-name>customQueryBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.customQueryBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__3">
      <managed-bean-name>vendorMootBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.managedBeans.vendorMootBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__14">
      <managed-bean-name>InvoiceDetailsBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.managedBeans.InvoiceDetailsBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__16">
      <managed-bean-name>ExcelExportBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.ExcelExporter</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="VendMootDetail">
      <page>/VendorMootDetailsC.jsff</page>
    </view>
    <method-call id="setDefaultOrgId">
      <method>#{bindings.setDefaultOrgId.execute}</method>
      <outcome>
        <fixed-outcome>setDefaultOrgId</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
        <commit/>
      </outcome>
    </task-flow-return>
    <method-call id="methodCall1">
      <method>#{pageFlowScope.InvoiceDetailsBean.invoiceDetails}</method>
      <outcome>
        <to-string/>
      </outcome>
    </method-call>
    <view id="InvDetail">
      <page>/InvoiceDetails.jsff</page>
    </view>
    <control-flow-rule id="__6">
      <from-activity-id>setDefaultOrgId</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>setDefaultOrgId</from-outcome>
        <to-activity-id>VendMootDetail</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>VendMootDetail</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>Home</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__9">
        <from-outcome>Step3</from-outcome>
        <to-activity-id>methodCall1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__10">
      <from-activity-id>methodCall1</from-activity-id>
      <control-flow-case id="__11">
        <from-outcome>Step4</from-outcome>
        <to-activity-id>InvDetail</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__17">
        <from-outcome>Step5</from-outcome>
        <to-activity-id>VendMootDetail</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>InvDetail</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome>return3</from-outcome>
        <to-activity-id>VendMootDetail</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

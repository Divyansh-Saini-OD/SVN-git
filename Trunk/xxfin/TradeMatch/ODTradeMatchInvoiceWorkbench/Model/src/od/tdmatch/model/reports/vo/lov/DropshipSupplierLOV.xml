AND APS.VENDOR_ID = ASSA.VENDOR_ID 
AND EXISTS (SELECT 1 FROM XX_AP_DROPSHIP_NON_DEDUCTIONS 
WHERE LTRIM(VENDOR_NUM, '0') = LTRIM(ASSA.VENDOR_SITE_CODE_ALT, '0'))]]></SQLQuery>

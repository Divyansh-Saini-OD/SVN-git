<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SupplierNoLovInvoiceVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.lov.SupplierNoLovInvoiceVORowImpl"
  ComponentClass="od.tdmatch.model.lov.SupplierNoLovInvoiceVOImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.lov.SupplierNoLovInvoiceVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT distinct sup.segment1 supplier_no, sup.vendor_name supplier_name,sup.vendor_id   
  FROM ap_supplier_sites_all site,   
       ap_suppliers sup   
 WHERE EXISTS (SELECT 'x'   
                 FROM ap_invoices_all ai   
                WHERE ai.validation_request_id=-9999999999     
                  AND ai.vendor_id=sup.vendor_id   
                  AND ai.vendor_site_id=site.vendor_site_id   
                  AND EXISTS (SELECT 'x'   
                                  FROM  xx_fin_translatevalues tv   
                                     ,xx_fin_translatedefinition td   
                               WHERE td.TRANSLATION_NAME = 'XX_AP_TR_MATCH_INVOICES'   
                                 AND tv.TRANSLATE_ID  = td.TRANSLATE_ID   
                                 AND tv.enabled_flag='Y'   
                                 AND SYSDATE BETWEEN tv.start_date_active and NVL(tv.end_date_active,sysdate)   
                                 AND tv.target_value1=ai.source   
                              )   
			  )   
   AND site.vendor_id=sup.vendor_id                     
   AND exists (SELECT 'x'           
         FROM xx_fin_translatevalues tv,                                                                            
              xx_fin_translatedefinition td                            
        WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                             
          AND tv.translate_id  = td.translate_id                                                                            
          AND tv.enabled_flag = 'Y'                                                                            
          AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)           
          AND tv.target_value1 = site.attribute8) 
          and sup.end_date_active is null 
          and site.inactive_date is null]]></SQLQuery>
  <ViewAttribute
    Name="SupplierNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUPPLIER_NO"
    Expression="SUPPLIER_NO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.lov.SupplierNoLovInvoiceVO.SupplierNo_LABEL"/>
        <DISPLAYWIDTH
          Value="25"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="SupplierName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUPPLIER_NAME"
    Expression="SUPPLIER_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.lov.SupplierNoLovInvoiceVO.SupplierName_LABEL"/>
        <DISPLAYWIDTH
          Value="25"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="VendorId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="VENDOR_ID"
    Expression="VENDOR_ID"
    SQLType="NUMERIC">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="od.tdmatch.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

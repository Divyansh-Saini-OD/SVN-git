<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SupplierTermsVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.lov.SupplierTermsVORowImpl"
  ComponentClass="od.tdmatch.model.lov.SupplierTermsVOImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.lov.SupplierTermsVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT sup.segment1 supplier_no, sup.vendor_name supplier_name        
  FROM          
       ap_suppliers sup 
       WHERE EXISTS (SELECT /*+ LEADING (ai) INDEX(ai AP_INVOICES_N3) */ 'x'       
                 FROM ap_invoices_all ai       
                WHERE ai.payment_status_flag IN ('N','P')       
                  AND ai.vendor_id=sup.vendor_id       
                  AND ai.vendor_site_id=site.vendor_site_id       
                  AND EXISTS (SELECT 'x'       
                                  FROM  xx_fin_translatevalues tv       
                                     ,xx_fin_translatedefinition td       
                               WHERE td.TRANSLATION_NAME = 'XX_AP_TR_MATCH_INVOICES'       
                                 AND tv.TRANSLATE_ID  = td.TRANSLATE_ID       
                                 AND tv.enabled_flag='Y'       
                                 AND SYSDATE BETWEEN tv.start_date_active and NVL(tv.end_date_active,sysdate)       
                                 AND tv.target_value1=ai.source       
                             )       
				  AND  'APPROVED'=AP_INVOICES_PKG.GET_APPROVAL_STATUS(ai.invoice_id,        
                                           ai.invoice_amount,       
                                           ai.payment_status_flag,       
                                           ai.invoice_type_lookup_code       
                                          )       
                                                )                                    
   AND site.vendor_id=sup.vendor_id                         
   AND exists (SELECT 'x'           
         FROM xx_fin_translatevalues tv,                                                                            
              xx_fin_translatedefinition td                            
        WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES'                                                                             
          AND tv.translate_id  = td.translate_id                                                                            
          AND tv.enabled_flag = 'Y'                                                                            
          AND sysdate between tv.start_date_active and nvl(tv.end_date_active,sysdate)           
          AND tv.target_value1 = site.attribute8)]]></SQLQuery>
  <ViewAttribute
    Name="SupplierNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUPPLIER_NO"
    Expression="SUPPLIER_NO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.lov.SupplierTermsVO.SupplierNo_LABEL"/>
        <DISPLAYWIDTH
          Value="15"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="SupplierName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUPPLIER_NAME"
    Expression="SUPPLIER_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.lov.SupplierTermsVO.SupplierName_LABEL"/>
        <DISPLAYWIDTH
          Value="15"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="od.tdmatch.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

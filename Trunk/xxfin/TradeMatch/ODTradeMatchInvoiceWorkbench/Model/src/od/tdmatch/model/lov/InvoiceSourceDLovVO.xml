<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="InvoiceSourceDLovVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.lov.InvoiceSourceDLovVORowImpl"
  ComponentClass="od.tdmatch.model.lov.InvoiceSourceDLovVOImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.lov.InvoiceSourceDLovVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[select  tv.target_value1 invoice_source          
  from  xx_fin_translatevalues tv          
       ,xx_fin_translatedefinition td          
where 1=1     
   AND td.TRANSLATION_NAME = 'XX_AP_TR_MATCH_INVOICES'       
   AND tv.TRANSLATE_ID  = td.TRANSLATE_ID          
   AND tv.enabled_flag='Y'          
   AND SYSDATE BETWEEN tv.start_date_active and NVL(tv.end_date_active,sysdate)  
   AND tv.target_value1='US_OD_DROPSHIP']]></SQLQuery>
  <ViewAttribute
    Name="InvoiceSource"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="INVOICE_SOURCE"
    Expression="INVOICE_SOURCE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="od.tdmatch.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

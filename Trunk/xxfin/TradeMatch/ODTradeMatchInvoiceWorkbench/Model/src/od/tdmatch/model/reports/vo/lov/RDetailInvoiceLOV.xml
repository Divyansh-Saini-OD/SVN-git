<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="RDetailInvoiceLOV"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.reports.vo.lov.RDetailInvoiceLOVRowImpl"
  ComponentClass="od.tdmatch.model.reports.vo.lov.RDetailInvoiceLOVImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.reports.vo.lov.RDetailInvoiceLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[select INVOICE_NUM,invoice_id from AP_INVOICES_ALL aia where     
      EXISTS    
    (SELECT 1    
    FROM xx_fin_translatevalues tv ,    
      xx_fin_translatedefinition td    
    WHERE td.TRANSLATION_NAME = 'XX_AP_TR_MATCH_INVOICES'    
    AND tv.TRANSLATE_ID       = td.TRANSLATE_ID    
    AND tv.enabled_flag       ='Y'    
    AND sysdate BETWEEN TV.START_DATE_ACTIVE AND NVL(TV.END_DATE_ACTIVE,sysdate)    
    and TV.TARGET_VALUE1 = AIA.source    
    )]]></SQLQuery>
  <ViewAttribute
    Name="InvoiceNum"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="INVOICE_NUM"
    Expression="INVOICE_NUM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.reports.vo.lov.RDetailInvoiceLOV.InvoiceNum_LABEL"/>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="InvoiceId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="15"
    Scale="0"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="INVOICE_ID"
    Expression="INVOICE_ID"
    SQLType="NUMERIC"
    IsQueriable="false"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="od.tdmatch.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

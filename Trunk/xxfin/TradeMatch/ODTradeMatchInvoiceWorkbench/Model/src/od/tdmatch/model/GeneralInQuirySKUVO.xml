  <Variable
    Name="b_sku_id"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select a.segment1 sku,a.description,u.unit_of_measure uom,e.flex_value||' ('||e.description||')' dept,matr.package_size,latr.primary_supp , XX_AP_TRADE_MATCH_UTL_PKG.xx_primary_vendor_name(latr.primary_supp) primary_supp_name     
  from xxptp.xx_inv_item_org_attributes latr,    
       xx_inv_item_master_attributes matr,     
       mtl_units_of_measure_tl u,    
       fnd_flex_values_vl e,    
       fnd_flex_value_sets d,    
       mtl_categories_b c,    
       mtl_item_categories b,    
       inv.mtl_system_items_b a     
 where a.organization_id+0=441     
   and a.segment1=:b_sku_id    
   and b.inventory_item_id=a.inventory_item_id    
   and b.organization_id=a.organization_id    
   and c.category_id=b.category_id    
   and c.structure_id=101    
   and d.flex_value_set_name='XX_GI_DEPARTMENT_VS'    
   and e.flex_value_set_id=d.flex_value_set_id    
   and e.flex_value=c.segment3    
   and u.uom_code=a.primary_uom_code    
   and matr.inventory_item_id=a.inventory_item_id    
   and latr.inventory_item_id=a.inventory_item_id    
   and latr.rms_timestamp in (select max(rms_timestamp)     
                                from xxptp.XX_INV_ITEM_ORG_ATTRIBUTES     
                               where inventory_item_id=a.inventory_item_id)]]></SQLQuery>

<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ConsignmentSKULovVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.lov.ConsignmentSKULovVORowImpl"
  ComponentClass="od.tdmatch.model.lov.ConsignmentSKULovVOImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="od.tdmatch.model.lov.ConsignmentSKULovVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[ Select /*+ INDEX(msi MTL_SYSTEM_ITEMS_B_N1) */ msi.segment1 SKU_Number, msi.inventory_item_id         
  FROM mtl_system_items_b msi  
WHERE 1=1  
AND  Nvl(Enabled_Flag,'Y') = 'Y'
AND   Nvl(End_Date_Active,Sysdate +1 ) > Sysdate 
AND EXISTS ( SELECT 'x'  
FROM mtl_material_transactions mmt 
    WHERE 1=1  
    AND msi.organization_id=441     
 AND mmt.inventory_item_id = msi.inventory_item_id    
 AND mmt.transaction_source_name = 'OD CONSIGNMENT RTV')]]></SQLQuery>
  <ViewAttribute
    Name="SkuNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SKU_NUMBER"
    Expression="SKU_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="od.tdmatch.model.lov.ConsignmentSKULovVO.SkuNumber_LABEL"/>
        <DISPLAYWIDTH
          Value="10"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="InventoryItemId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="INVENTORY_ITEM_ID"
    Expression="INVENTORY_ITEM_ID"
    SQLType="NUMERIC"
    IsQueriable="false"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="od.tdmatch.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

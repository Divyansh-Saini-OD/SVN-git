  <SQLQuery><![CDATA[SELECT VENDOR_SITE_CODE supplier_site,  VENDOR_SITE_ID vendor_site_id 
FROM APPS.AP_SUPPLIER_SITES_ALL SIT 
WHERE 1                          =1 
AND NVL(inactive_date,sysdate+1) > sysdate 
AND EXISTS 
  (SELECT 1 
  FROM APPS.XX_FIN_TRANSLATEVALUES TV, 
    APPS.xx_fin_translatedefinition td 
  WHERE td.translation_name = 'XX_AP_TRADE_CATEGORIES' 
  AND tv.translate_id       = td.translate_id 
  AND tv.enabled_flag       = 'Y' 
  AND sysdate BETWEEN tv.start_date_active AND NVL(tv.end_date_active,sysdate) 
  AND Tv.Target_Value1 = Sit.Attribute8 ||''  )]]></SQLQuery>

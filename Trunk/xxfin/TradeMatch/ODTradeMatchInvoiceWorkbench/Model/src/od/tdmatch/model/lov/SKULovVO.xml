<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SKULovVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="od.tdmatch.model.lov.SKULovVORowImpl"
  ComponentClass="od.tdmatch.model.lov.SKULovVOImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <SQLQuery><![CDATA[Select /*+ INDEX(msi MTL_SYSTEM_ITEMS_B_N1) */  Msi.Segment1 SKU_Number,      
 msi.inventory_item_id       
 From  Mtl_System_Items_B Msi        
 Where Msi.Organization_Id =441      
 And  Nvl(Enabled_Flag,'Y') = 'Y'      
 And   Nvl(End_Date_Active,Sysdate +1 ) > Sysdate]]></SQLQuery>

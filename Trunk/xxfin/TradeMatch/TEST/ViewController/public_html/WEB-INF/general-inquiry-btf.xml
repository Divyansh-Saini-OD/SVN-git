<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="general-inquiry-btf">
    <default-activity>methodCall1</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>GeneralInquiryBean</managed-bean-name>
      <managed-bean-class>od.tdmatch.view.managedBeans.GeneralInquiryBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__4">
      <managed-bean-name>MyDate</managed-bean-name>
      <managed-bean-class>java.util.Date</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="GeneralInquiry">
      <page>/GeneralInquiry.jsff</page>
    </view>
    <method-call id="methodCall1">
      <method>#{pageFlowScope.GeneralInquiryBean.createVendorUpdate}</method>
      <outcome>
        <fixed-outcome>vendorinformation</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__2">
      <from-activity-id>methodCall1</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>vendorinformation</from-outcome>
        <to-activity-id>GeneralInquiry</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

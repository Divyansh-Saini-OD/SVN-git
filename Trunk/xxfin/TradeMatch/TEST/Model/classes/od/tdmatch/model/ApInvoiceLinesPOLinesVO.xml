  <SQLQuery><![CDATA[select line_number,quantity_invoiced,unit_price,amount,line_type_lookup_code,po_line_id,po_line_location_id,(select segment1 from po_headers_all a where a.po_header_id=:poHdrId) po_num,     
(select SUBSTR(location_code,1,6) from hr_locations_all where location_id=ApInvoiceLinesEO.ship_to_location_id) location,(select shipment_num from po_line_locations_all where po_line_id=ApInvoiceLinesEO.po_line_id) shipment_num,     
(select line_num from po_lines_all a where a.po_line_id=ApInvoiceLinesEO.po_line_id) po_line_num,     
ApInvoiceLinesEO.po_line_id po_line_id,(select segment1 from mtl_system_items_b a where a.inventory_item_id=ApInvoiceLinesEO.inventory_item_id and a.organization_id=441) sku,item_description sku_desc,     
(select quantity from po_lines_all a where a.po_line_id=ApInvoiceLinesEO.po_line_id) po_qty,     
(select unit_price from po_lines_all a where po_line_id=ApInvoiceLinesEO.po_line_id) po_price,     
(select g.segment1 ||'.'||g.segment2||'.'||g.segment3||'.'||g.segment4||'.'||g.segment5||'.'||g.segment6||'.'||g.segment7   from gl_code_combinations g,        po.po_distributions_all d where d.po_line_id=ApInvoiceLinesEO.po_line_id     and g.code_combination_id=d.code_combination_id) charge_account,     
'N' unmatch_po_flag,(select decode(count(hold_id),0,'N','Y') from ap_holds_all a where a.line_location_id=ApInvoiceLinesEO.po_line_location_id) hold_exists_flag,ApInvoiceLinesEO.inventory_item_id    
from ap_invoice_lines_all ApInvoiceLinesEO     
where invoice_id=:invId and line_type_lookup_code in ('ITEM','FREIGHT')      
union all      
select line_num,null,null,null,null,po_line_id,null,(select segment1 from po_headers_all a where a.po_header_id=:poHdrId) po_num,     
(SELECT SUBSTR(location_code,1,6)  
  FROM hr_locations_all a, po_line_locations_all b  
  WHERE b.po_line_id=PoLinesAllEO.po_line_id  
  and a.location_id=b.ship_to_location_id  
  ) location,(select shipment_num from po_line_locations_all where po_line_id=PoLinesAllEO.po_line_id) shipment_num,     
line_num po_line_num,     
po_line_id,(select segment1 from mtl_system_items_b a where a.inventory_item_id=PoLinesAllEO.item_id and a.organization_id=441) sku,item_description sku_desc,     
quantity po_qty,     
unit_price po_price,     
(select g.segment1 ||'.'||g.segment2||'.'||g.segment3||'.'||g.segment4||'.'||g.segment5||'.'||g.segment6||'.'||g.segment7   from gl_code_combinations g,        po.po_distributions_all d where d.po_line_id=PoLinesAllEO.po_line_id     and g.code_combination_id=d.code_combination_id) charge_account,     
'Y','Y',PoLinesAllEO.item_id     
from po_lines_all PoLinesAllEO where po_header_id=:poHdrId and po_line_id not in (select po_line_id from ap_invoice_lines_all where invoice_id=:invId and po_line_id is not null)]]></SQLQuery>

  SQL_STATEMENT_TEXT =
 "SELECT DISTINCT a.FULL_NAME Employee,\n\
      a.EMPLOYEE_NUMBER EMPLOYEE_NO,\n\
      C .FULL_NAME SUPERVISOR,\n\
      a.effective_end_date Terminated,\n\
      a.last_update_date Last_Update\n\
Into   &FULL_NAME\n\
\n\
       &EMPLOYEE\n\
\n\
       &SUPERVISOR\n\
\n\
       &Terminated\n\
\n\
       &last_update\n\
\n\
FROM per_all_people_f c,\n\
  per_all_assignments_f pa,\n\
  ap_cards_all b,\n\
  PER_ALL_PEOPLE_F a\n\
WHERE b.EMPLOYEE_ID            =a.PERSON_ID\n\
AND pa.person_id               =b.employee_id\n\
and C.PERSON_ID                =PA.SUPERVISOR_ID\n\
and a.person_type_id=6\n\
and exists (select 'x'\n\
         from per_all_people_f\n\
        where person_id=a.person_id\n\
          and person_type_id=9)\n\
and PA.ASSIGNMENT_SEQUENCE=(select max(ASSIGNMENT_SEQUENCE)\n\
        from per_all_assignments_f\n\
       where person_id=a.person_id)\n\
AND pa.person_id=b.employee_id\n\
AND c.person_id=pa.supervisor_id\n\
AND TRUNC(a.effective_end_date) BETWEEN TRUNC(SYSDATE-7) AND TRUNC(SYSDATE)\n\
order by a.last_update_date desc\n\
\n\
"

  START_DATE_ACTIVE = "2012/09/20 00:00:00"
  DESCRIPTION = "Terminated Employee With Credit card"
  FREQUENCY_TYPE = "B"
  DAYS_BETWEEN_CHECKS = "1"
  DATE_LAST_CHECKED = "2012/09/20 16:54:38"
  MAINTAIN_HISTORY_DAYS = "0"

<definitions
     name="GetUserID"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/db/GetUserID/"
     xmlns="http://schemas.xmlsoap.org/wsdl/"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/db/GetUserID/"
     xmlns:db="http://xmlns.oracle.com/pcbpel/adapter/db/GetUserID"
     xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
    >
    <types>
      <schema xmlns="http://www.w3.org/2001/XMLSchema" >
        <import namespace="http://xmlns.oracle.com/pcbpel/adapter/db/GetUserID" schemaLocation="GetUserID.xsd" />
      </schema>
    </types>
    <message name="GetUserIDInput_msg">
        <part name="GetUserIDInput_msg" element="db:GetUserIDInput"/>
    </message>
    <message name="GetUserIDOutputCollection_msg">
        <part name="GetUserIDOutputCollection" element="db:GetUserIDOutputCollection"/>
    </message>
    <portType name="GetUserID_ptt">
        <operation name="GetUserID">
            <input message="tns:GetUserIDInput_msg"/>
            <output message="tns:GetUserIDOutputCollection_msg"/>
        </operation>
    </portType>
    <binding name="GetUserID_binding" type="tns:GetUserID_ptt">
    <jca:binding  />
        <operation name="GetUserID">
      <jca:operation
          SqlString="SELECT user_id FROM FND_USER WHERE USER_NAME = #p_User_name"
          InteractionSpec="oracle.tip.adapter.db.DBPureSQLInteractionSpec" >
      </jca:operation>
            <input/>
            <output/>
        </operation>
    </binding>
    <service name="GetUserID">
        <port name="GetUserID_pt" binding="tns:GetUserID_binding">
<!--Your runtime connection is declared in 
J2EE_HOME/application-deployments/default/DbAdapter/oc4j-ra.xml 
These mcf properties here are from your design time connection and 
save you from having to edit that file and restart the application server 
if eis/db/EBSApps is missing. 
These mcf properties are safe to remove.-->
      <jca:address location="eis/db/EBSAppsFin" UIConnectionName="EIS_APPS_EBS_APPS_DBConn"
          ManagedConnectionFactory="oracle.tip.adapter.db.DBManagedConnectionFactory"
      />
        </port>
    </service>
  <plt:partnerLinkType name="GetUserID_plt" >
    <plt:role name="GetUserID_role" >
      <plt:portType name="tns:GetUserID_ptt" />
    </plt:role>
  </plt:partnerLinkType>
</definitions>

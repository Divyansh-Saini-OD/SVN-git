<?xml version = '1.0' encoding = 'UTF-8'?>
<BPELSuitcase>
   <BPELProcess id="FTPTDMSupplier" src="FTPTDMSupplier.bpel">
      <partnerLinkBindings>
         <partnerLinkBinding name="ReadTDMFile">
            <property name="wsdlLocation">ReadTDMFile.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="WriteTDMOutbound">
            <property name="wsdlLocation">WriteTDMOutbound.wsdl</property>
            <property name="Outbound_Path" type="LogicalDirectory"></property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="WriteToFTPServer">
            <property name="wsdlLocation">WriteToFTPServer.wsdl</property>
            <property name="Outbound_Path" type="LogicalDirectory"></property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODTransactionLogger">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/default/ODTransactionLogger/ODTransactionLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODErrorLogger">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/default/ODErrorLogger/ODErrorLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="BPELInvokeESP">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/fin/BPELInvokeESP/BPELInvokeESP?wsdl</property>
         </partnerLinkBinding>
      </partnerLinkBindings>
      <activationAgents>
         <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="ReadTDMFile">
            <property name="Inbound_Path" type="LogicalDirectory"></property>
            <property name="portType">Get_ptt</property>
            <property name="Archive_Path" type="LogicalArchiveDirectory"></property>
         </activationAgent>
      </activationAgents>
   </BPELProcess>
</BPELSuitcase>

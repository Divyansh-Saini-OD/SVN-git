          </copy>
        </assign>
        <invoke name="invkESP" partnerLink="BPELInvokeESP"
                portType="ns7:BPELInvokeESP" operation="initiate"
                inputVariable="invkESP_initiate_InputVariable"/>
      </sequence>
    </scope>
    -->
  </sequence>
</process>
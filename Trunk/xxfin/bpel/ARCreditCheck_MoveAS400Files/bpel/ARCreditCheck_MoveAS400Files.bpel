<process name="ARCreditCheck_MoveAS400Files"
         targetNamespace="http://xmlns.oracle.com/ARCreditCheck_Move_CCO317_FAR410"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/file/attachment/"
         xmlns:ns7="http://xmlns.oracle.com/BPELInvokeESP"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns5="http://xmlns.oracle.com/ODTransactionLogger"
         xmlns:client="http://xmlns.oracle.com/ARCreditCheck_Move_CCO317_FAR410"
         xmlns:ns6="http://xmlns.oracle.com/TransactionData"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ns9="http://xmlns.oracle.com/ODErrorLogger"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/ftp/ServiceFileRead/"
         xmlns:ehdr="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/ftp/ServiceFileWrite/"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/ftp/"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns10="ODError" xmlns:ns8="http://xmlns.oracle.com/GetFileInfo">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <partnerLink myRole="Get_role" name="ServiceFileRead"
                 partnerLinkType="ns1:Get_plt"/>
    <partnerLink name="ServiceFileWrite" partnerRole="Put_role"
                 partnerLinkType="ns3:Put_plt"/>
    <partnerLink myRole="ODErrorLoggerRequester" name="ODErrorLogger"
                 partnerRole="ODErrorLoggerProvider"
                 partnerLinkType="ns9:ODErrorLogger"/>
    <partnerLink myRole="ODTransactionLoggerRequester"
                 name="ODTransactionLogger"
                 partnerRole="ODTransactionLoggerProvider"
                 partnerLinkType="ns5:ODTransactionLogger"/>
    <partnerLink myRole="BPELInvokeESPRequester" name="BPELInvokeESP"
                 partnerRole="BPELInvokeESPProvider"
                 partnerLinkType="ns7:BPELInvokeESP"/>
    <partnerLink name="GetFileInfo" partnerRole="GetFileInfoProvider"
                 partnerLinkType="ns8:GetFileInfo"/>
  </partnerLinks>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <variable name="Receive_FileRead_Get_In"
              messageType="ns1:attachmentElement_msg"/>
    <variable name="File_Name_In" messageType="ns2:InboundHeader_msg"/>
    <variable name="Switch_File_Name" type="xsd:string"/>
    <variable name="Invoke_FileWrite_Put_In"
              messageType="ns3:attachmentElement_msg"/>
    <variable name="File_Name_Out" messageType="ns2:OutboundHeader_msg"/>
    <variable name="invkCatchAll_initiate"
              messageType="ns9:ODErrorLoggerRequestMessage"/>
    <variable name="File_Name" type="xsd:string"/>
    <variable name="ErrorMessageText" type="xsd:string"/>
    <variable name="ProcessHeader" type="xsd:string"/>
    <variable name="logTransaction_initiate_InputVariable"
              messageType="ns5:ODTransactionLoggerRequestMessage"/>
    <variable name="logErrorTransaction_initiate_InputVariable"
              messageType="ns5:ODTransactionLoggerRequestMessage"/>
    <variable name="invkESP_initiate_InputVariable"
              messageType="ns7:BPELInvokeESPRequestMessage"/>
    <variable name="stepName" type="xsd:string"/>
    <variable name="InvokeESPIn" messageType="ns7:BPELInvokeESPRequestMessage"/>
    <variable name="ESP_File_Name" type="xsd:string"/>
    <variable name="ReleaseESPJob" type="xsd:string"/>
    <variable name="ESPJobReleased" type="xsd:string"/>
    <variable name="GetFileInfo_process_InputVariable"
              messageType="ns8:GetFileInfoRequestMessage"/>
    <variable name="GetFileInfo_process_OutputVariable"
              messageType="ns8:GetFileInfoResponseMessage"/>
    <variable name="getFileInfoPath" type="xsd:string"/>
    <variable name="xmlFragment" type="xsd:string"/>
    <variable name="fileInfoXmlFragment" type="xsd:string"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="Sequence_4">
        <scope name="logTransactionErrorData">
          <sequence name="Sequence_6">
            <assign name="copyTransactionErrorData">
              <copy>
                <from expression="'Error'"/>
                <to variable="logTransaction_initiate_InputVariable"
                    part="payload"
                    query="/ns6:TransactionData/ns6:ProcessInfo/ns6:ProcessStatus"/>
              </copy>
            </assign>
            <invoke name="logErrorTransaction" partnerLink="ODTransactionLogger"
                    portType="ns5:ODTransactionLogger" operation="initiate"
                    inputVariable="logTransaction_initiate_InputVariable"/>
          </sequence>
        </scope>
        <assign name="asnCatchAll">
          <copy>
            <from expression="concat(&quot;Error encountered in Process ARCreditCheck_MoveAS400Files, while copying file &quot;, bpws:getVariableData('File_Name_In','inboundHeader','/ns2:InboundFTPHeaderType/ns2:fileName'), '. ', bpws:getVariableData('fileInfoXmlFragment'))"/>
            <to variable="ErrorMessageText"/>
          </copy>
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getInstanceId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessId"/>
          </copy>
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessDomain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessURL()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:SystemName"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorDateTime"/>
          </copy>
          <copy>
            <from variable="stepName"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessStep"/>
          </copy>
          <copy>
            <from expression="ora:getFaultName()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorCode"/>
          </copy>
          <copy>
            <from variable="ErrorMessageText"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorDescription"/>
          </copy>
          <copy>
            <from expression="ora:getFaultAsString()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorText"/>
          </copy>
          <copy>
            <from expression="'system'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorType"/>
          </copy>
          <copy>
            <from expression="'fatal'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorSeverity"/>
          </copy>
          <copy>
            <from expression="'Y'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:Notification/ns10:sendNotification"/>
          </copy>
        </assign>
        <invoke name="invkCatchAll" partnerLink="ODErrorLogger"
                portType="ns9:ODErrorLogger" operation="initiate"
                inputVariable="invkCatchAll_initiate"/>
        <terminate name="Terminate"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <receive name="Receive_FileRead" partnerLink="ServiceFileRead"
             portType="ns1:Get_ptt" operation="Get"
             variable="Receive_FileRead_Get_In" createInstance="yes"
             bpelx:headerVariable="File_Name_In"/>
    <scope name="GetFileInfo">
      <sequence name="Sequence_10">
        <assign name="Assign_GetFileInfo_Input">
          <copy>
            <from expression="ora:doc('${server_url}/xmllib/fin/GlobalParameters.xml','/GlobalParameters/GetFileName/FilePath')"/>
            <to variable="getFileInfoPath"/>
          </copy>
          <copy>
            <from expression="concat(bpws:getVariableData('getFileInfoPath') ,'outbound/', bpws:getVariableData('File_Name_In','inboundHeader','/ns2:InboundFTPHeaderType/ns2:fileName'))"/>
            <to variable="GetFileInfo_process_InputVariable" part="payload"
                query="/ns8:GetFileInfoProcessRequest/ns8:fileName"/>
          </copy>
        </assign>
        <invoke name="GetFileInfo" partnerLink="GetFileInfo"
                portType="ns8:GetFileInfo" operation="process"
                inputVariable="GetFileInfo_process_InputVariable"
                outputVariable="GetFileInfo_process_OutputVariable"/>
      </sequence>
    </scope>
    <scope name="LogTransactionStart">
      <sequence name="Sequence_5">
        <assign name="copyTransactionData">
          <copy>
            <from expression="ora:getDomainId() "/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:ProcessInfo/ns6:Domain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:ProcessInfo/ns6:ProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getInstanceId()"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:ProcessInfo/ns6:InstanceId"/>
          </copy>
          <copy>
            <from expression="'Start'"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:ProcessInfo/ns6:ProcessStatus"/>
          </copy>
          <copy>
            <from expression="'FTP'"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:MessageDetails/ns6:MessageOperation"/>
          </copy>
          <copy>
            <from expression="'Concurrent Program Short Name: XXARCREDITCHECK, Concurrent Manager Description: OD: AR Credit Check Extract'"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:MessageDetails/ns6:MessageSourceSystemComponent"/>
          </copy>
          <copy>
            <from expression="'Opaque'"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:MessageDetails/ns6:MessageType"/>
          </copy>
          <copy>
            <from expression="concat('&lt;EntityID EntityType=&quot;SourceFileName&quot;>',bpws:getVariableData('File_Name_In','inboundHeader','/ns2:InboundFTPHeaderType/ns2:fileName'),'&lt;/EntityID>&lt;EntityID EntityType=&quot;ArchiveFileName&quot;>',bpws:getVariableData('GetFileInfo_process_OutputVariable','payload','/ns8:GetFileInfoProcessResponse/ns8:fileName'),'&lt;/EntityID>&lt;EntityID EntityType=&quot;LastModified&quot;>',bpws:getVariableData('GetFileInfo_process_OutputVariable','payload','/ns8:GetFileInfoProcessResponse/ns8:lastModified'),'&lt;/EntityID>&lt;EntityID EntityType=&quot;FileSize&quot;>',bpws:getVariableData('GetFileInfo_process_OutputVariable','payload','/ns8:GetFileInfoProcessResponse/ns8:fileSize'),'&lt;/EntityID>&lt;EntityID EntityType=&quot;NumberOfLines&quot;>',bpws:getVariableData('GetFileInfo_process_OutputVariable','payload','/ns8:GetFileInfoProcessResponse/ns8:numberOfLines'),'&lt;/EntityID>&lt;EntityID EntityType=&quot;LongestLineLength&quot;>',bpws:getVariableData('GetFileInfo_process_OutputVariable','payload','/ns8:GetFileInfoProcessResponse/ns8:longestLineLength'),'&lt;/EntityID>')"/>
            <to variable="fileInfoXmlFragment"/>
          </copy>
          <copy>
            <from expression="concat('&lt;EntityList xmlns=&quot;http://xmlns.oracle.com/TransactionData&quot;>', bpws:getVariableData('fileInfoXmlFragment'), '&lt;/EntityList>')"/>
            <to variable="xmlFragment"/>
          </copy>          
          <copy>
            <from expression="ora:parseEscapedXML(bpws:getVariableData('xmlFragment'))"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:EntiltyList"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:ProcessInfo/ns6:transactionStartTime"/>
          </copy>
          <copy>
            <from expression="'I1026_ARCRDCHK_TMP - EBS AR Outbound'"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:ProcessInfo/ns6:SystemName"/>
          </copy>
          <copy>
            <from expression="'EBS'"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:ProcessInfo/ns6:TradingPartnerDetails/ns6:TPFrom"/>
          </copy>
          <copy>
            <from expression="'EBS'"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:MessageDetails/ns6:MessageSourceSystem"/>
          </copy>
          <copy>
            <from expression="'AS400'"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:ProcessInfo/ns6:TradingPartnerDetails/ns6:TPTo"/>
          </copy>
          <copy>
            <from expression="'After receive file and Before inserting the data for Audit Log Start'"/>
            <to variable="stepName"/>
          </copy>
        </assign>
        <invoke name="logTransaction" partnerLink="ODTransactionLogger"
                portType="ns5:ODTransactionLogger" operation="initiate"
                inputVariable="logTransaction_initiate_InputVariable"/>
      </sequence>
    </scope>
    <scope name="SetProcessHeader">
      <sequence name="Sequence_9">
        <assign name="initializeInstanceTitle">
          <copy>
            <from expression="substring(concat(&quot;Processing: &quot;, bpws:getVariableData('File_Name_In','inboundHeader','/ns2:InboundFTPHeaderType/ns2:fileName')),1,50)"/>
            <to variable="ProcessHeader"/>
          </copy>
          <copy>
            <from expression="'After audit log start and efore Initialize Instance Title'"/>
            <to variable="stepName"/>
          </copy>
        </assign>
        <bpelx:exec name="setTitleCode" language="java" version="1.5">
          <![CDATA[try {                  
  String title = (String)getVariableData("ProcessHeader");                  
  setTitle(title);                  
  addAuditTrailEntry(title) ;                  
} catch (Exception ex) {                  
  addAuditTrailEntry(ex.toString());                  
  ex.printStackTrace();                  
}]]>
        </bpelx:exec>
      </sequence>
    </scope>
    <assign name="Assign_File_Name">
      <copy>
        <from variable="Receive_FileRead_Get_In" part="attachmentElement"
              query="/ns4:attachmentElement"/>
        <to variable="Invoke_FileWrite_Put_In" part="attachmentElement"
            query="/ns4:attachmentElement"/>
      </copy>
      <copy>
        <from variable="File_Name_In" part="inboundHeader"
              query="/ns2:InboundFTPHeaderType/ns2:fileName"/>
        <to variable="Switch_File_Name"/>
      </copy>
      <copy>
        <from expression="substring(bpws:getVariableData('Switch_File_Name'),1,24)"/>
        <to variable="Switch_File_Name"/>
      </copy>
      <copy>
        <from expression="ora:doc('${server_url}/xmllib/fin/GlobalParameters.xml','/GlobalParameters/ARCreditCheck_MoveAS400Files/Interface[@name=bpws:getVariableData(&quot;Switch_File_Name&quot;)]/TargetFileName')"/>
        <to variable="File_Name"/>
      </copy>
      <copy>
        <from variable="File_Name"/>
        <to variable="File_Name_Out" part="outboundHeader"
            query="/ns2:OutboundFTPHeaderType/ns2:fileName"/>
      </copy>
      <copy>
        <from expression="'After Assigning the File Name and before invoking the file PUT'"/>
        <to variable="stepName"/>
      </copy>
      <copy>
        <from expression="ora:doc('${server_url}/xmllib/fin/GlobalParameters.xml','/GlobalParameters/ARCreditCheck_MoveAS400Files/Interface[@name=bpws:getVariableData(&quot;Switch_File_Name&quot;)]/ESPFilePattern')"/>
        <to variable="ESP_File_Name"/>
      </copy>
      <copy>
        <from expression="'No'"/>
        <to variable="ReleaseESPJob"/>
      </copy>
      <copy>
        <from expression="'Error'"/>
        <to variable="ESPJobReleased"/>
      </copy>
    </assign>
    <invoke name="Invoke_FileWrite" partnerLink="ServiceFileWrite"
            portType="ns3:Put_ptt" operation="Put"
            inputVariable="Invoke_FileWrite_Put_In"
            bpelx:inputHeaderVariable="File_Name_Out"/>
    <scope name="ScopeESPInvoke">
      <sequence name="SeqESPInvoke">
        <assign name="AssignESPParameters">
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="InvokeESPIn" part="payload"
                query="/ns7:BPELInvokeESPProcessRequest/ns7:processDomain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="InvokeESPIn" part="payload"
                query="/ns7:BPELInvokeESPProcessRequest/ns7:processName"/>
          </copy>
          <copy>
            <from variable="ESP_File_Name"/>
            <to variable="InvokeESPIn" part="payload"
                query="/ns7:BPELInvokeESPProcessRequest/ns7:filePattern"/>
          </copy>
        </assign>
        <invoke name="InvokeESP" partnerLink="BPELInvokeESP"
                portType="ns7:BPELInvokeESP" operation="initiate"
                inputVariable="InvokeESPIn"/>
        <assign name="AssignReleaseESPJob">
          <copy>
            <from expression="'Yes'"/>
            <to variable="ReleaseESPJob"/>
          </copy>
          <copy>
            <from expression="'Success'"/>
            <to variable="ESPJobReleased"/>
          </copy>
        </assign>
      </sequence>
    </scope>
    <scope name="logTransactionEnd">
      <sequence name="Sequence_7">
        <assign name="copyTransactionEndData">
          <copy>
            <from expression="'Success'"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:ProcessInfo/ns6:ProcessStatus"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:ProcessInfo/ns6:transactionEndTime"/>
          </copy>
          
          <copy>
            <from expression="concat('&lt;EntityID EntityType=&quot;TargetFileName&quot;>',bpws:getVariableData('File_Name'),'&lt;/EntityID>&lt;EntityID EntityType=&quot;ESPJobRequired&quot;>',bpws:getVariableData('ReleaseESPJob'),'&lt;/EntityID>&lt;EntityID EntityType=&quot;ReleaseESPJobStatus&quot;>',bpws:getVariableData('ESPJobReleased'),'&lt;/EntityID>')"/>
            <to variable="xmlFragment"/>
          </copy>
          
          <copy>
            <from expression="concat('&lt;EntityList xmlns=&quot;http://xmlns.oracle.com/TransactionData&quot;>', bpws:getVariableData('fileInfoXmlFragment'), bpws:getVariableData('xmlFragment'), '&lt;/EntityList>')"/>
            <to variable="xmlFragment"/>
          </copy>          
          <copy>
            <from expression="ora:parseEscapedXML(bpws:getVariableData('xmlFragment'))"/>
            <to variable="logTransaction_initiate_InputVariable" part="payload"
                query="/ns6:TransactionData/ns6:EntiltyList"/>
          </copy>
          
          <copy>
            <from expression="'After Invoke PUT. Before audit Log Success'"/>
            <to variable="stepName"/>
          </copy>
        </assign>
        <invoke name="logTransactionEnd" partnerLink="ODTransactionLogger"
                portType="ns5:ODTransactionLogger" operation="initiate"
                inputVariable="logTransaction_initiate_InputVariable"/>
      </sequence>
    </scope>
    <!-- 
    <scope name="logESP">
      <sequence name="Sequence_8">
        <assign name="copyESPData">
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="invkESP_initiate_InputVariable" part="payload"
                query="/ns7:BPELInvokeESPProcessRequest/ns7:processDomain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="invkESP_initiate_InputVariable" part="payload"
                query="/ns7:BPELInvokeESPProcessRequest/ns7:processName"/>
          </copy>
          <copy>
            <from variable="File_Name_In" part="inboundHeader"
                  query="/ns2:InboundFTPHeaderType/ns2:fileName"/>

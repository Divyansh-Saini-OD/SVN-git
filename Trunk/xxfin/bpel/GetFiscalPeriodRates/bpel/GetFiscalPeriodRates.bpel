    <variable name="Invoke_GetFiscalPeriodRate_Teradata_GetFiscalPeriodRate_Teradata_OutputVariable"
              messageType="ns1:GetFiscalPeriodRate_TeradataOutputCollection_msg"/>
    <variable name="ProcessHeader" type="xsd:string"/>
  </variables>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <!-- Receive input from requestor. (Note: This maps to operation defined in GetFiscalPeriodRates.wsdl) -->
    <receive name="receiveInput" partnerLink="client"
             portType="client:GetFiscalPeriodRates" operation="initiate"
             variable="inputVariable" createInstance="yes"/>
    <scope name="setProcessHeader">
      <sequence name="Sequence_1">
        <assign name="initializeInstanceTitle">
          <copy>
            <from expression="substring( concat( 'DateTime: ', bpws:getVariableData('inputVariable','payload','/client:GetFiscalPeriodRatesProcessRequest/client:DateTime')),1,50)"/>
            <to variable="ProcessHeader"/>
          </copy>
        </assign>
        <bpelx:exec name="setTitle" language="java" version="1.5">
          <![CDATA[try {              
  String title = (String)getVariableData("ProcessHeader");              
  setTitle(title);              
  addAuditTrailEntry(title) ;              
} catch (Exception ex) {              
  addAuditTrailEntry(ex.toString());              
  ex.printStackTrace();              
}]]>
        </bpelx:exec>
      </sequence>
    </scope>
    <!-- Generate reply to synchronous request -->
    <assign name="Input_to_FiscalPeriodRate_Transform">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:processXSLT('Input_to_FiscalPeriodRate_Transformation.xsl',bpws:getVariableData('inputVariable','payload'))"/>
        <to variable="Invoke_GetFiscalPeriodRate_Teradata_GetFiscalPeriodRate_Teradata_InputVariable"
            part="GetFiscalPeriodRate_TeradataInput_msg"/>
      </copy>
    </assign>
    <invoke name="Invoke_GetFiscalPeriodRate_Teradata"
            partnerLink="GetFiscalPeriodRate_Teradata"
            portType="ns1:GetFiscalPeriodRate_Teradata_ptt"
            operation="GetFiscalPeriodRate_Teradata"
            inputVariable="Invoke_GetFiscalPeriodRate_Teradata_GetFiscalPeriodRate_Teradata_InputVariable"
            outputVariable="Invoke_GetFiscalPeriodRate_Teradata_GetFiscalPeriodRate_Teradata_OutputVariable"/>
    <assign name="FiscalPeriodRate_to_Output_Transform">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:processXSLT('FiscalPeriodRate_to_output.xsl',bpws:getVariableData('Invoke_GetFiscalPeriodRate_Teradata_GetFiscalPeriodRate_Teradata_OutputVariable','GetFiscalPeriodRate_TeradataOutputCollection'))"/>
        <to variable="outputVariable" part="payload"/>
      </copy>
    </assign>
     <!-- Commented for defect 4980-->

    <!--<reply name="replyOutput" partnerLink="client"
           portType="client:GetFiscalPeriodRates" operation="process"
           variable="outputVariable"/>-->
           
     <!-- Added for defect 4980-->
        <invoke name="callbackClient" partnerLink="client"
            portType="client:GetFiscalPeriodRatesCallback"
            operation="onResult" inputVariable="outputVariable"/>
    <!-- End -->            
  </sequence>

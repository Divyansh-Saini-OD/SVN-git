////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="I0437_ACHPmt_Out"
         targetNamespace="http://xmlns.oracle.com/I0437_ACHPmt_Out"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns4="http://xmlns.oracle.com/ias/pcbpel/NotificationService"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns16="http://xmlns.oracle.com/BPELInvokeESP"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/ftp/"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:ns9="http://xmlns.oracle.com/ODErrorLogger"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/ftp/ACHFileGet/"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/opaque/"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns15="http://xmlns.oracle.com/TransactionData"
         xmlns:ns10="ODError"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns13="http://xmlns.oracle.com/Recipient6"
         xmlns:ns7="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns12="http://xmlns.oracle.com/pcbpel/adapter/db/top/test"
         xmlns:client="http://xmlns.oracle.com/I0437_ACHPmt_Out"
         xmlns:ns18="http://schemas.oracle.com/service/bpel/common"
         xmlns:ns6="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ns11="http://xmlns.oracle.com/pcbpel/adapter/db/test/"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/ftp/ACHFilePut/"
         xmlns:ns14="http://xmlns.oracle.com/ODTransactionLogger"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:ns8="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <partnerLink myRole="Get_role" name="ACHFileGet"
                 partnerLinkType="ns1:Get_plt"/>
    <partnerLink name="ACHFilePut" partnerRole="Put_role"
                 partnerLinkType="ns3:Put_plt"/>
    <partnerLink name="NotificationService"
                 partnerRole="NotificationServiceProvider"
                 partnerLinkType="ns4:NotificationServiceLink"/>
    <partnerLink myRole="ODErrorLoggerRequester" name="ODErrorLogger"
                 partnerRole="ODErrorLoggerProvider"
                 partnerLinkType="ns9:ODErrorLogger"/>
    <partnerLink myRole="ODTransactionLoggerRequester"
                 name="ODTransactionLogger"
                 partnerRole="ODTransactionLoggerProvider"
                 partnerLinkType="ns14:ODTransactionLogger"/>
    <partnerLink myRole="BPELInvokeESPRequester" name="BPELInvokeESP"
                 partnerRole="BPELInvokeESPProvider"
                 partnerLinkType="ns16:BPELInvokeESP"/>
  </partnerLinks>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <variable name="File_Name_In" messageType="ns5:InboundHeader_msg"/>
    <variable name="File_Name_Out" messageType="ns5:OutboundHeader_msg"/>
    <variable name="invkCatchAll_initiate"
              messageType="ns9:ODErrorLoggerRequestMessage"/>
    <variable name="Email_Address" type="xsd:string"/>
    <variable name="TMP_File_Name" type="xsd:string"/>
    <variable name="ESP_File_Name" type="xsd:string"/>
    <variable name="ErrorMessageText" type="xsd:string"/>
    <variable name="invkTransactionStart_initiate_InputVariable"
              messageType="ns14:ODTransactionLoggerRequestMessage"/>
    <variable name="ProcessHeader" type="xsd:string"/>
    <variable name="invkESP_initiate_InputVariable"
              messageType="ns16:BPELInvokeESPRequestMessage"/>
    <variable name="bankName" type="xsd:string"/>
    <variable name="AS2Name" type="xsd:string"/>
    <variable name="rcvACHFileGet" messageType="ns1:attachmentElement_msg"/>
    <variable name="invkACHFilePut" messageType="ns3:attachmentElement_msg"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="Sequence_4">
        <scope name="logTransactionError">
          <sequence name="Sequence_6">
            <assign name="asnLogTransactionError">
              <copy>
                <from expression="'Error'"/>
                <to variable="invkTransactionStart_initiate_InputVariable"
                    part="payload"
                    query="/ns15:TransactionData/ns15:ProcessInfo/ns15:ProcessStatus"/>
              </copy>
            </assign>
            <invoke name="invkLogTransactionError"
                    partnerLink="ODTransactionLogger"
                    portType="ns14:ODTransactionLogger" operation="initiate"
                    inputVariable="invkTransactionStart_initiate_InputVariable"/>
          </sequence>
        </scope>
        <assign name="asnCatchAll">
          <copy>
            <from expression="concat(&quot;The interface I0438_EFTPmt - EBS AP Outbound to Banks failed while FTP file &quot;,bpws:getVariableData('File_Name_In','inboundHeader','/ns5:InboundFTPHeaderType/ns5:fileName'),&quot; to B2B. The file name sent to B2B is: &quot;, bpws:getVariableData('File_Name_Out','outboundHeader','/ns5:OutboundFTPHeaderType/ns5:fileName'), &quot;.&quot;)"/>
            <to variable="ErrorMessageText"/>
          </copy>
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getInstanceId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessId"/>
          </copy>
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessDomain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessURL()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:SystemName"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorDateTime"/>
          </copy>
          <copy>
            <from expression="'Catch All on the Main Sequence'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessStep"/>
          </copy>
          <copy>
            <from expression="ora:getFaultName()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorCode"/>
          </copy>
          <copy>
            <from variable="ErrorMessageText"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorDescription"/>
          </copy>
          <copy>
            <from expression="ora:getFaultAsString()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorText"/>
          </copy>
          <copy>
            <from expression="'system'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorType"/>
          </copy>
          <copy>
            <from expression="'fatal'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorSeverity"/>
          </copy>
          <copy>
            <from expression="ora:doc('${server_url}/xmllib/fin/GlobalParameters.xml','/GlobalParameters/FaultNotification/SendNotification')"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:Notification/ns10:sendNotification"/>
          </copy>
        </assign>
        <invoke name="invkCatchAll" partnerLink="ODErrorLogger"
                portType="ns9:ODErrorLogger" operation="initiate"
                inputVariable="invkCatchAll_initiate"/>
        <terminate name="Terminate"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <receive name="rcvACHFileGet" partnerLink="ACHFileGet"
             portType="ns1:Get_ptt" operation="Get" variable="rcvACHFileGet"
             createInstance="yes" bpelx:headerVariable="File_Name_In"/>
    <scope name="logTransactionStart">
      <variables>
        <variable name="xmlFragment" type="xsd:string"/>
      </variables>
      <sequence name="Sequence_5">
        <assign name="asnTransactionStart">
          <copy>
            <from expression="ora:getDomainId() "/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:ProcessInfo/ns15:Domain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:ProcessInfo/ns15:ProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getInstanceId()"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:ProcessInfo/ns15:InstanceId"/>
          </copy>
          <copy>
            <from expression="'Start'"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:ProcessInfo/ns15:ProcessStatus"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:ProcessInfo/ns15:transactionStartTime"/>
          </copy>
          <copy>
            <from expression="'ACH EFT File Processing'"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:MessageDetails/ns15:MessageSourceSystem"/>
          </copy>
          <copy>
            <from expression="'Concurrent Program Short Name: XXAPPBP1, Concurrent Manager Description: OD: AP EFT Expense/Trade Payment Batch Process'"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:MessageDetails/ns15:MessageSourceSystemComponent"/>
          </copy>
          <copy>
            <from expression="'FTP to B2B'"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:MessageDetails/ns15:MessageOperation"/>
          </copy>
          <copy>
            <from expression="'Attachment'"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:MessageDetails/ns15:MessageType"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:MessageDetails/ns15:MessageDateTime"/>
          </copy>
          <copy>
            <from>
              <EntiltyList xmlns="http://xmlns.oracle.com/TransactionData">
                <EntityID EntityType="SourceFileName"/>
              </EntiltyList>
            </from>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload" query="/ns15:TransactionData/ns15:EntiltyList"/>
          </copy>
          <copy>
            <from expression="concat('&lt;EntityID EntityType=&quot;SourceFileName&quot;>', bpws:getVariableData('File_Name_In','inboundHeader','/ns5:InboundFTPHeaderType/ns5:fileName'), '&lt;/EntityID>')"/>
            <to variable="xmlFragment"/>
          </copy>
          <copy>
            <from expression="ora:parseEscapedXML(bpws:getVariableData('xmlFragment'))"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:EntiltyList/ns15:EntityID[1]"/>
          </copy>
          <copy>
            <from expression="'I0438_EFTPmt - EBS AP Outbound to Banks'"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:ProcessInfo/ns15:SystemName"/>
          </copy>
          <copy>
            <from expression="'EBS'"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:ProcessInfo/ns15:TradingPartnerDetails/ns15:TPFrom"/>
          </copy>
          <copy>
            <from expression="'B2B/EDI through AS2 to the Banks'"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:ProcessInfo/ns15:TradingPartnerDetails/ns15:TPTo"/>
          </copy>
          <copy>
            <from expression="'EBS'"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:MessageDetails/ns15:MessageSourceSystem"/>
          </copy>
        </assign>
        <invoke name="invkTransactionStart" partnerLink="ODTransactionLogger"
                portType="ns14:ODTransactionLogger" operation="initiate"
                inputVariable="invkTransactionStart_initiate_InputVariable"/>
      </sequence>
    </scope>
    <scope name="setTitle">
      <sequence name="Sequence_9">
        <assign name="asnSetTitle">
          <copy>
            <from expression="substring(concat('Processing ', bpws:getVariableData('File_Name_In','inboundHeader','/ns5:InboundFTPHeaderType/ns5:fileName')),1,50)"/>
            <to variable="ProcessHeader"/>
          </copy>
        </assign>
        <bpelx:exec name="setTitle" language="java" version="1.5">
          <![CDATA[/*Write your java code below e.g.  
	System.out.println("Hello, World"); 
*/ 
 
try {                      
  String title = (String)getVariableData("ProcessHeader");                      
  setTitle(title);                      
  addAuditTrailEntry(title) ;                      
} catch (Exception ex) {                      
  addAuditTrailEntry(ex.toString());                      
  ex.printStackTrace();                      
}]]>
        </bpelx:exec>
      </sequence>
    </scope>
    <assign name="asnFile">
      <copy>
        <from variable="File_Name_In" part="inboundHeader"
              query="/ns5:InboundFTPHeaderType/ns5:fileName"/>
        <to variable="TMP_File_Name"/>
      </copy>
      <copy>
        <from expression="concat(substring-before(bpws:getVariableData('TMP_File_Name'), 'NACHA_'), 'NACHA_')"/>
        <to variable="TMP_File_Name"/>
      </copy>
      <copy>
        <from variable="rcvACHFileGet" part="attachmentElement"/>
        <to variable="invkACHFilePut" part="attachmentElement"/>
      </copy>
    </assign>
    <assign name="AssignFileNameOut">
      <copy>
        <from expression="ora:doc('${server_url}/xmllib/fin/GlobalParameters.xml','/GlobalParameters/FtpACHEFTPaymentAP_Out/Files/File[@name=bpws:getVariableData(&quot;TMP_File_Name&quot;)]/Bank')"/>
        <to variable="bankName"/>
      </copy>
      <copy>
        <from expression="ora:doc('${server_url}/xmllib/fin/GlobalParameters.xml','/GlobalParameters/FtpACHEFTPaymentAP_Out/Files/File[@name=bpws:getVariableData(&quot;TMP_File_Name&quot;)]/ESPFilePattern')"/>
        <to variable="ESP_File_Name"/>
      </copy>
      <copy>
        <from expression="ora:doc('${server_url}/xmllib/fin/GlobalParameters.xml','/GlobalParameters/FtpACHEFTPaymentAP_Out/Banks/Bank[@name=bpws:getVariableData(&quot;bankName&quot;)]/AS2Name')"/>
        <to variable="AS2Name"/>
      </copy>
      <copy>
        <from variable="AS2Name"/>
        <to variable="File_Name_Out" part="outboundHeader"
            query="/ns5:OutboundFTPHeaderType/ns5:fileName"/>
      </copy>
      <copy>
        <from expression="ora:doc('${server_url}/xmllib/fin/GlobalParameters.xml','/GlobalParameters/FtpACHEFTPaymentAP_Out/EmailAddress')"/>
        <to variable="Email_Address"/>
      </copy>
    </assign>
    <invoke name="InvkACHFilePut" partnerLink="ACHFilePut"
            portType="ns3:Put_ptt" operation="Put"
            inputVariable="invkACHFilePut"
            bpelx:inputHeaderVariable="File_Name_Out"/>
    <scope name="Email_1">
      <bpelx:annotation>
        <bpelx:pattern patternName="bpelx:email"></bpelx:pattern>
      </bpelx:annotation>
      <variables>
        <variable name="varNotificationReq"
                  messageType="ns4:EmailNotificationRequest"/>
        <variable name="varNotificationResponse"
                  messageType="ns4:ArrayOfResponse"/>
        <variable name="NotificationServiceFaultVariable"
                  messageType="ns4:NotificationServiceErrorMessage"/>
      </variables>
      <sequence name="Sequence_1">
        <assign name="EmailParamsAssign">
          <copy>
            <from expression="string('SOAEmailNotify')"/>
            <to variable="varNotificationReq" part="EmailPayload"
                query="/EmailPayload/ns4:FromAccountName"/>
          </copy>
          <copy>
            <from expression="string('')"/>
            <to variable="varNotificationReq" part="EmailPayload"
                query="/EmailPayload/ns4:Bcc"/>
          </copy>
          <copy>
            <from expression="string('')"/>
            <to variable="varNotificationReq" part="EmailPayload"
                query="/EmailPayload/ns4:Cc"/>
          </copy>
          <copy>
            <from expression="string('')"/>
            <to variable="varNotificationReq" part="EmailPayload"
                query="/EmailPayload/ns4:ReplyToAddress"/>
          </copy>
          <copy>
            <from expression="concat(string('File '), bpws:getVariableData('File_Name_In','inboundHeader','/ns5:InboundFTPHeaderType/ns5:fileName'), string(' has been sent'))"/>
            <to variable="varNotificationReq" part="EmailPayload"
                query="/EmailPayload/ns4:Subject"/>
          </copy>
          <copy>
            <from expression="bpws:getVariableData('Email_Address')"/>
            <to variable="varNotificationReq" part="EmailPayload"
                query="/EmailPayload/ns4:To"/>
          </copy>
          <copy>
            <from expression="concat(string('The file '), bpws:getVariableData('File_Name_In','inboundHeader','/ns5:InboundFTPHeaderType/ns5:fileName'), string('
has been sent to B2B server for further processing.
'))"/>
            <to variable="varNotificationReq" part="EmailPayload"
                query="/EmailPayload/ns4:Content/ns4:ContentBody"/>
          </copy>
          <copy>
            <from expression="string('text/html; charset=UTF-8')"/>
            <to variable="varNotificationReq" part="EmailPayload"
                query="/EmailPayload/ns4:Content/ns4:MimeType"/>
          </copy>
        </assign>
        <invoke name="InvokeNotificationService"
                partnerLink="NotificationService"
                portType="ns4:NotificationService"
                operation="sendEmailNotification"
                inputVariable="varNotificationReq"
                outputVariable="varNotificationResponse"/>
      </sequence>
    </scope>
    <scope name="callESP">
      <sequence name="Sequence_7">
        <assign name="asnESP">
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="invkESP_initiate_InputVariable" part="payload"
                query="/ns16:BPELInvokeESPProcessRequest/ns16:processDomain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="invkESP_initiate_InputVariable" part="payload"
                query="/ns16:BPELInvokeESPProcessRequest/ns16:processName"/>
          </copy>
          <copy>
            <from variable="ESP_File_Name"/>
            <to variable="invkESP_initiate_InputVariable" part="payload"
                query="/ns16:BPELInvokeESPProcessRequest/ns16:filePattern"/>
          </copy>
        </assign>
        <invoke name="invkESP" partnerLink="BPELInvokeESP"
                portType="ns16:BPELInvokeESP" operation="initiate"
                inputVariable="invkESP_initiate_InputVariable"/>
      </sequence>
    </scope>
    <scope name="logTransactionEnd">
      <variables>
        <variable name="xmlFragment" type="xsd:string"/>
      </variables>
      <sequence name="Sequence_8">
        <assign name="asnLogTransactionEnd">
          <copy>
            <from expression="'Success'"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:ProcessInfo/ns15:ProcessStatus"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:ProcessInfo/ns15:transactionEndTime"/>
          </copy>
          <copy>
            <from>
              <EntiltyList xmlns="http://xmlns.oracle.com/TransactionData">
                <EntityID EntityType="SourceFileName"/>
                <EntityID EntityType="TargetFileName"/>
                <EntityID EntityType="InkoveESP"/>
              </EntiltyList>
            </from>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload" query="/ns15:TransactionData/ns15:EntiltyList"/>
          </copy>
          <copy>
            <from expression="concat('&lt;EntityID EntityType=&quot;SourceFileName&quot;>', bpws:getVariableData('File_Name_In','inboundHeader','/ns5:InboundFTPHeaderType/ns5:fileName'), '&lt;/EntityID>')"/>
            <to variable="xmlFragment"/>
          </copy>
          <copy>
            <from expression="ora:parseEscapedXML(bpws:getVariableData('xmlFragment'))"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:EntiltyList/ns15:EntityID[1]"/>
          </copy>
          <copy>
            <from expression="concat('&lt;EntityID EntityType=&quot;TargetFileName&quot;>', bpws:getVariableData('AS2Name'), '&lt;/EntityID>')"/>
            <to variable="xmlFragment"/>
          </copy>
          <copy>
            <from expression="ora:parseEscapedXML(bpws:getVariableData('xmlFragment'))"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:EntiltyList/ns15:EntityID[2]"/>
          </copy>
          <copy>
            <from expression="'YES'"/>
            <to variable="invkTransactionStart_initiate_InputVariable"
                part="payload"
                query="/ns15:TransactionData/ns15:EntiltyList/ns15:EntityID[3]"/>
          </copy>
        </assign>
        <invoke name="invkLogTransactionEnd" partnerLink="ODTransactionLogger"
                portType="ns14:ODTransactionLogger" operation="initiate"
                inputVariable="invkTransactionStart_initiate_InputVariable"/>
      </sequence>
    </scope>
  </sequence>
</process>
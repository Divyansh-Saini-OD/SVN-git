<definitions
     name="TDSettlementFTPRead"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/ftp/TDSettlementFTPRead/"
     xmlns="http://schemas.xmlsoap.org/wsdl/"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/ftp/TDSettlementFTPRead/"
     xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
     xmlns:pc="http://xmlns.oracle.com/pcbpel/"
     xmlns:imp1="http://www.openapplications.org/oagis/9"
     xmlns:hdr="http://xmlns.oracle.com/pcbpel/adapter/ftp/"
    >
    <import namespace="http://xmlns.oracle.com/pcbpel/adapter/ftp/" location="ftpAdapterInboundHeader.wsdl"/>
    <types>
      <schema xmlns="http://www.w3.org/2001/XMLSchema" >
        <import namespace="http://www.openapplications.org/oagis/9" schemaLocation="http://chilsoa01d.na.odcorp.net:7778/orabpel/xmllib/eaixml/oagis/v91/message/bod/ProcessInvoice.xsd" />
      </schema>
    </types>
    <message name="ProcessInvoice_msg">
        <part name="ProcessInvoice" element="imp1:ProcessInvoice"/>
    </message>
    <portType name="Get_ptt">
        <operation name="Get">
            <input message="tns:ProcessInvoice_msg"/>
        </operation>
    </portType>
    <binding name="Get_binding" type="tns:Get_ptt">
    <pc:inbound_binding  />
        <operation name="Get">
      <jca:operation
          FileType="ascii"
          LogicalDirectory="TDSettlementInbound"
          ActivationSpec="oracle.tip.adapter.ftp.inbound.FTPActivationSpec"
          LogicalArchiveDirectory="TDSettlementArchive"
          DeleteFile="true"
          IncludeFiles=".*Settlement.*\.xml"
          PollingFrequency="3600"
          MinimumAge="20"
          FileModificationTime="FileSystem"
          ModificationTimeFormat="4,18,yyyyMMddHHmmSS"
          OpaqueSchema="false" >
      </jca:operation>
      <input>
        <jca:header message="hdr:InboundHeader_msg" part="inboundHeader"/>
      </input>
        </operation>
    </binding>
    <service name="TDSettlementFTPRead">
        <Documentation>This service reads the Tech depot settlement files from $CUSTOM_DATA/ xxfin/ftp/in directory.</Documentation>
        <port name="Get_pt" binding="tns:Get_binding">
      <jca:address location="eis/ftp/EBSCentralExternal" UIincludeWildcard="*Settlement*.xml" />
        </port>
    </service>
  <plt:partnerLinkType name="Get_plt" >
    <plt:role name="Get_role" >
      <plt:portType name="tns:Get_ptt" />
    </plt:role>
  </plt:partnerLinkType>
</definitions>

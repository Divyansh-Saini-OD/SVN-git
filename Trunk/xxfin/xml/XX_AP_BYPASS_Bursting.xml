<?xml version='1.0' encoding='UTF-8'?>
<xapi:requestset xmlns:xapi="http://xmlns.oracle.com/oxp/xapi">
<xapi:request select="/XX_AP_BYPASSINVXML/LIST_G_EMAIL_DETAILS/G_EMAIL_DETAILS">
<xapi:delivery>
<xapi:email server="${SMTP_SERVER}" port="25" from="AccountsPayable@officedepot.com">
<!--set the id for the delivery method -->
<!--use the supplier site email address element to email the document-->
<xapi:message id="123" to="${DISTRIBUTION_LIST}" attachment="true" 
subject="${EMAIL_SUBJECT}">"${EMAIL_CONTENT}"
</xapi:message>
</xapi:email>	
</xapi:delivery>
<xapi:document output="BYPASSINV.pdf" output-type="pdf" delivery="123">
<!--Specify the delivery method id to be used -->
<xapi:template type="rtf" location="xdo://XXFIN.XXAPBYPASSINV.en.US?getSource=true" filter="">
</xapi:template>
</xapi:document>
</xapi:request>
</xapi:requestset>


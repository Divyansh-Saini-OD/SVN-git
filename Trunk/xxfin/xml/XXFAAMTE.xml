<?xml version="1.0" encoding="WINDOWS-1252" ?>
<dataTemplate name="XXFAAMTE" description="OD FA Mass Amortization Exception Report" Version="1.0">
<parameters>
 <parameter name="p_request_id" datatype="number" />
</parameters>
<dataQuery>
<sqlStatement name="Q_AMTE">
<![CDATA[
   SELECT 
	Asset_id,
	Asset_no,
	Description,
	Book_type_code,
	Process_mode,
	Company,
	Building,
	CostCenter,
	Lob,
	Major,
	Minor,
	Subminor,
	FA_period,
	creation_date cdate,
	date_placed_in_service,
      asset_service_days,
	asset_service_months,
	life_in_months,
	rem_life_months,
	Cost,
	deprn_amount,
	ytd_deprn,
	deprn_reserve,
	NBV,
	life_to_process,
	amrtz_to_process,
	amrtz_date_to_process,
	current_months,
	current_amrtz,
	current_amrtz_date,
	future_months,
	future_amrtz,
	future_amrtz_date,
	error_message,
	new_deprn,
	months_to_adjust,
	catch_up_adjmnt,
	deprn_expense,
	new_rem_life,
	new_adj_nbv
   FROM xx_fa_amortz_stg
  WHERE request_id=:p_request_id
    AND error_flag='Y'
    AND error_message IS NOT NULL
order by asset_id]]>
</sqlStatement>
</dataQuery>

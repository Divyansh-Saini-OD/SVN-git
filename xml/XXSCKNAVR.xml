<dataTemplate name="XXSCKNAVR" description="OD SC K &amp; N Active Vendor Report" Version="1.0">
<dataQuery>
<sqlStatement name="Q_XXSCKNAVR">
<![CDATA[
select OD_SC_VENDOR_NUMBER
,OD_SC_VENDOR_NAME
,OD_SC_VEND_TYPE
,OD_SC_VENDOR_STATUS
,OD_SC_ACTIVATION_DATE
,OD_SC_INACTIVE_DATE
,OD_SC_REACTIVATE_DATE
,OD_SC_AUDIT_AGENT
,OD_SC_VEND_ADDRESS
,OD_SC_VEND_CONT_NAME 
,OD_SC_VEND_PHONE        
,OD_SC_VEND_FAX       
,OD_SC_VEND_EMAIL 
,OD_SC_FACTORY_NUMBER
,OD_SC_FACTORY_NAME
,OD_SC_FACTORY_ADDRESS
 ,OD_SC_VEND_REGION         
from
Q_od_pb_sc_vendor_master_v
where OD_SC_VEND_TYPE='DI'
and OD_SC_VENDOR_STATUS='Active'
ORDER BY 1
]]>
</sqlStatement>
</dataQuery>

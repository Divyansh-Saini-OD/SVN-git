<?xml version="1.0" encoding="WINDOWS-1252" ?>
<dataTemplate name="XXPAGSPE" description="OD GSO PO Import Exceptions Report" Version="1.0">
<parameters>
 <parameter name="p_batch_id" datatype="number" />
</parameters>
<dataQuery>
<sqlStatement name="Q_VEND">
<![CDATA[
SELECT distinct 							
	 vendor_no,
	 vendor_name,
	 po_number,
	 country_cd,
	 agent,
	 dept
  FROM xx_gso_po_stg
 WHERE load_batch_id=:p_batch_id
   AND vendor_process_flag=3
ORDER BY vendor_name
]]>
</sqlStatement>
<sqlStatement name="Q_DEPT">
<![CDATA[
SELECT distinct 							
	 dept,
	 vendor_no,
	 vendor_name,
	 po_number,
	 country_cd,
	 agent
  FROM xx_gso_po_stg
 WHERE load_batch_id=:p_batch_id
   AND dept_process_flag=3
ORDER BY dept,vendor_name
]]>
</sqlStatement>
<sqlStatement name="Q_SKU">
<![CDATA[
SELECT distinct 							
	 vendor_no,
	 vendor_name,
	 sku,
	 description,
	 po_number,
	 country_cd,
	 mo,
	 agent,
       dept
  FROM xx_gso_po_stg
 WHERE load_batch_id=:p_batch_id
   AND sku_process_flag=3
ORDER BY vendor_no,sku
]]>
</sqlStatement>
<sqlStatement name="Q_EOF">
<![CDATA[
SELECT distinct 							
	 vendor_no,
	 vendor_name,
	 sku,
	 description,
	 po_number,
	 country_cd,
	 mo,
	 agent,
	 dept
  FROM xx_gso_po_stg
 WHERE load_batch_id=:p_batch_id
   AND eol_Flag='Y'
ORDER BY vendor_no,sku
]]>
</sqlStatement>
</dataQuery>

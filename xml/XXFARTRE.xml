<?xml version="1.0" encoding="WINDOWS-1252" ?>
<dataTemplate name="XXFARTRE" description="OD FA Mass Retirement Exception Report" Version="1.0">
<parameters>
 <parameter name="p_request_id" datatype="number" />
</parameters>
<dataQuery>
<sqlStatement name="Q_RTRE"> 
<![CDATA[
   SELECT 
	Asset_id,
	Description,
	Book_type_code,
	Company,
	Building,
	location_description,
      legacy_asset_id,
	tax_asset_id,
	CostCenter,
	Lob,
	Major,
	Minor,
	Subminor,
	date_placed_in_service,
	life_in_months,
      fa_retire_period,
	retirement_date,
 	Retirement_Type,
	Retire_convention,
	asset_prorate_con,
	cost_to_be_retired,
      asset_cost_retired,
	pos_to_be,
	deprn_method_code,
 	retire_convention,
	fa_current_Cost,
	units_retired,
	nbv_retired,
	proceeds_of_sale,
	gain_loss_amount,
	sold_to,
	comments,
	error_message,
      cost_of_removal
   FROM xx_fa_retire_stg
  WHERE request_id=:p_request_id
    AND error_flag='Y'
order by asset_id]]>
</sqlStatement>
</dataQuery>

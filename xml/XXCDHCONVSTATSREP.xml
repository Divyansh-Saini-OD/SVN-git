   <![CDATA[   
            SELECT NVL2(error_id, 6, 7)  EID
                 , COUNT(1)              EC
            FROM   HZ_IMP_PARTIES_INT 
            WHERE  batch_id = :EBI
            GROUP BY NVL2(ERROR_ID, 6, 7)]]>
   </sqlStatement>

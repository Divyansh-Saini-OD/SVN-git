<dataQuery>
   <sqlStatement name="Q_BATCH">
   <![CDATA[
            SELECT hibs.batch_id         EBI
                 , hibs.original_system  EBN
                 , hibs.batch_name       BATCH_NAME
                 , hosv.orig_system_name ORIG_SYSTEM_NAME
            FROM   hz_imp_batch_summary hibs
                 , HZ_ORIG_SYSTEMS_VL hosv
            WHERE  hosv.orig_system = hibs.original_system
                   and hibs.creation_date
                   BETWEEN TO_DATE(:P_FROM_DATE,'YYYY/MM/DD HH24:MI:SS')
                   AND TO_DATE(:P_TO_DATE,'YYYY/MM/DD HH24:MI:SS')
            ORDER BY hibs.original_system,hibs.creation_date]]>
   </sqlStatement>
   <sqlStatement name="Q_PARTIES_CNT">
   <![CDATA[
            SELECT COUNT(1)            PC 
            FROM   xxod_hz_imp_parties_int 
            WHERE  batch_id = :EBI]]>
   </sqlStatement>

<?xml version="1.0" encoding="WINDOWS-1252" ?>
<dataTemplate name="XXPLMSTRAVVREP" description="OD PLM STR Audit_Viol Validation Report" Version="1.0">
<parameters>
 <parameter name="P_VENDOR_NAME" datatype="character" />
 <parameter name="P_FACTORY_NAME" datatype="character" />
</parameters>
<dataQuery>
<sqlStatement name="Q_STRAVVREP">
<![CDATA[
 SELECT od_sc_transmission_status
     , od_sc_inspection_no
     , d.od_sc_inspection_type od_sc_inspection_type
     , od_sc_scheduled_date
     , od_sc_inspection_month
     , od_sc_inspection_year
     , od_sc_od_vendor_id
     , od_sc_vendor
     , od_sc_factory_id
     , od_sc_factory
     , od_sc_grade
     , od_sc_region
     , od_sc_region_sub
     , od_sc_agent
     , od_sc_violation_text
     , od_sc_violation_question
     , od_sc_auditor_comments_viol
     , od_sc_vendor_number
     , od_sc_vendor_name
     , od_sc_factory_number
     , od_sc_factory_name
     , od_sc_europe_rgn
     , od_sc_eu_sub_rgn
     , od_sc_mexico_region
     , od_sc_mx_sub_rgn
     , od_sc_asia_region
     , od_sc_as_sub_rgn
     , od_sc_vend_region
     , od_sc_straudt_schd_d
     , od_sc_str_audit_result
     , od_sc_question_code
     , od_sc_question_applicable
     , od_sc_violation_code
     , od_sc_violation_section
     , od_sc_violation_sub_section
     , od_sc_inspect_date
  FROM q_od_pb_sc_vendor_master_v m
     , q_od_sc_vendor_audit_v d
WHERE upper(m.od_sc_vendor_name )= upper(d.od_sc_vendor)
  AND upper(m.od_sc_factory_name) = upper(d.od_sc_factory)
  AND d.od_sc_violation_code IS NOT NULL
  AND m.od_sc_vendor_name = NVL (:P_VENDOR_NAME, od_sc_vendor_name)
  AND m.od_sc_factory_name = NVL (:P_FACTORY_NAME, od_sc_factory_name)
ORDER BY 1

]]>
</sqlStatement>
</dataQuery>

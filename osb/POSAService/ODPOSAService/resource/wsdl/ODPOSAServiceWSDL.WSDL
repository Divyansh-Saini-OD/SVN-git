 <wsdl:binding name="POSAServiceSOAP11Binding" type="tns:POSAService">
  <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
  <wsdl:operation name="activateToken">
   <soap:operation style="document" soapAction="http://www.officedepot.com/officedepot/V1/ODPOSAServiceSchema/activateToken"/>
   <wsdl:input>
    <soap:body use="literal" parts="payload"/>
   </wsdl:input>
   <wsdl:output>
    <soap:body use="literal" parts="payload"/>
   </wsdl:output>
   <wsdl:fault name="PartnerServiceFaultFault">
    <soap:fault name="PartnerServiceFaultFault" use="literal"/>
   </wsdl:fault>
  </wsdl:operation>
  <wsdl:operation name="deactivateToken">
   <soap:operation style="document" soapAction="http://www.officedepot.com/officedepot/V1/ODPOSAServiceSchema/deactivateToken"/>
   <wsdl:input>
    <soap:body use="literal" parts="payload"/>
   </wsdl:input>
   <wsdl:output>
    <soap:body use="literal" parts="payload"/>
   </wsdl:output>
   <wsdl:fault name="PartnerServiceFaultFault">
    <soap:fault name="PartnerServiceFaultFault" use="literal"/>
   </wsdl:fault>
  </wsdl:operation>
  <wsdl:operation name="checkStatus">
   <soap:operation style="document" soapAction="http://www.officedepot.com/officedepot/V1/ODPOSAServiceSchema/checkStatus"/>
   <wsdl:input>
    <soap:body use="literal" parts="payload"/>
   </wsdl:input>
   <wsdl:output>
    <soap:body use="literal" parts="payload"/>
   </wsdl:output>
   <wsdl:fault name="PartnerServiceFaultFault">
    <soap:fault name="PartnerServiceFaultFault" use="literal"/>
   </wsdl:fault>
  </wsdl:operation>
 </wsdl:binding>
<wsdl:service name="POSAServService">
		<wsdl:port name="POSAServicePort" binding="tns:POSAServiceSOAP11Binding">
			<soap:address location="http://www.od.com/"/>
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>
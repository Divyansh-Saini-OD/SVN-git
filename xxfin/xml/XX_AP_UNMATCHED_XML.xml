<?xml version="1.0" encoding="UTF-8" ?>
<dataTemplate name ="invoiceData" description="Invoice Data" defaultPackage="XX_AP_XXAUNMATCHRECEIPT_PKG" Version="1.0" >
<parameters>
<parameter name="p_date" dataType="char"/>
<parameter name="p_currency_code" dataType="string"/>
<parameter name="p_segment_from" dataType="string"/>
<parameter name="p_segment_to" dataType="string"/>
<parameter name="p_sup_site_cd_from" dataType="string"/>
<parameter name="p_sup_site_cd_to" dataType="string"/>
<parameter name="p_po_type" dataType="string"/>
</parameters>
<dataQuery>
<sqlStatement name="Q1">
	<![CDATA[SELECT PO_NUMBER  ,
    PO_CURRENCY   ,
    C_LOCATION ,
    RECEIPT_NUM ,
    SHIPMENT_HEADER_ID,
    RECEIPT_DATE       ,
    VENDOR_ID          ,
    VENDOR_NAME        ,
    VENDOR_SITE_CODE   ,
    SUPPLIER_SITE_CATEGORY ,
    VENDOR_ASSISTANT       ,
    BUC_D_0                ,
    BUC_D_1                ,
    BUC_D_2                ,
    BUC_D_3                ,
    BUC_D_4                ,
    BUC_D_5                ,
    BUC_D_6                ,
    BUC_D_7
	FROM TABLE (XX_AP_XXAUNMATCHRECEIPT_PKG.xx_ap_unmatch_detail(
      :p_date ,
      :p_segment_from ,
      :p_segment_to ,
      :p_sup_site_cd_from ,
      :p_sup_site_cd_to ,
      :p_currency_Code ))
	  ORDER BY VENDOR_NAME, PO_NUMBER, RECEIPT_NUM]]>
</sqlStatement>
</dataQuery>
<dataTrigger name="beforeReport" 
source="XX_AP_XXAUNMATCHRECEIPT_PKG.beforeReport()"/>
<dataStructure>
<group name="XX_AP_UNMATCH" source="Q1">
<element name="PO_NUMBER" value="PO_NUMBER" />
<element name="PO_CURRENCY" value="PO_CURRENCY" />
<element name="C_LOCATION" value="C_LOCATION" />
<element name="RECEIPT_NUM" value="RECEIPT_NUM" />
<element name="RECEIPT_DATE" value="RECEIPT_DATE" />
<element name="VENDOR_ID" value="VENDOR_ID" />
<element name="VENDOR_NAME" value="VENDOR_NAME" />
<element name="VENDOR_SITE_CODE" value="VENDOR_SITE_CODE" />
<element name="SUPPLIER_SITE_CATEGORY" value="SUPPLIER_SITE_CATEGORY" />
<element name="VENDOR_ASSISTANT" value="VENDOR_ASSISTANT" />
<element name="BUC_D_0" value="BUC_D_0" />
<element name="BUC_D_1" value="BUC_D_1" />
<element name="BUC_D_2" value="BUC_D_2" />
<element name="BUC_D_3" value="BUC_D_3" />
<element name="BUC_D_4" value="BUC_D_4" />
<element name="BUC_D_5" value="BUC_D_5" />
<element name="BUC_D_6" value="BUC_D_6" />
<element name="BUC_D_7" value="BUC_D_7" />
</group>
</dataStructure>
</dataTemplate>
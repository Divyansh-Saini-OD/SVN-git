<definitions
     name="PcardChunkRead"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/file/PcardChunkRead/"
     xmlns="http://schemas.xmlsoap.org/wsdl/"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/file/PcardChunkRead/"
     xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
     xmlns:imp1="http://TargetNamespace.com/Pcard"
     xmlns:hdr="http://xmlns.oracle.com/pcbpel/adapter/file/"
    >
    <import namespace="http://xmlns.oracle.com/pcbpel/adapter/file/" location="fileAdapterOutboundHeader.wsdl"/>
    <types>
      <schema targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/file/PcardChunkRead/"
              xmlns="http://www.w3.org/2001/XMLSchema" >
        <import namespace="http://TargetNamespace.com/Pcard" schemaLocation="Pcard.xsd" />
        <element name="empty"><complexType/></element>
      </schema>
    </types>
    <message name="Empty_msg">
        <part name="Empty" element="tns:empty"/>
    </message>
    <message name="container_msg">
        <part name="container" element="imp1:container"/>
    </message>
    <portType name="SynchRead_ptt">
        <operation name="SynchRead">
            <input message="tns:Empty_msg"/>
            <output message="tns:container_msg"/>
        </operation>
    </portType>
    <binding name="SynchRead_binding" type="tns:SynchRead_ptt">
    <jca:binding  />
        <operation name="SynchRead">
      <jca:operation
          LogicalDirectory="Inbound_Path"
          InteractionSpec="oracle.tip.adapter.file.outbound.FileReadInteractionSpec"
          DeleteFile="true"
          FileName="File_Name_In"
          OpaqueSchema="false" >
      </jca:operation>
      <input>
        <jca:header message="hdr:OutboundHeader_msg" part="outboundHeader"/>
      </input>
        </operation>
    </binding>
    <service name="PcardChunkRead">
        <port name="SynchRead_pt" binding="tns:SynchRead_binding">
      <jca:address location="eis/FileAdapter" />
        </port>
    </service>
  <plt:partnerLinkType name="SynchRead_plt" >
    <plt:role name="SynchRead_role" >
      <plt:portType name="tns:SynchRead_ptt" />
    </plt:role>
  </plt:partnerLinkType>
</definitions>

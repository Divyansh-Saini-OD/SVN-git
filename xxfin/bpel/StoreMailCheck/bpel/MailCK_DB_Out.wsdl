<?xml version="1.0" encoding="UTF-8"?>
<definitions 
    name="MailCK_DB_Out"
    targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/db/MailCK_DB_Out/"
    xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/db/MailCK_DB_Out/"
    xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
    xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
    xmlns:pc="http://xmlns.oracle.com/pcbpel/"
    xmlns:top="http://xmlns.oracle.com/pcbpel/adapter/db/top/MailCKDBOut"
    xmlns:hdr="http://xmlns.oracle.com/pcbpel/adapter/db/"
    xmlns="http://schemas.xmlsoap.org/wsdl/">

    <import namespace="http://xmlns.oracle.com/pcbpel/adapter/db/" location="DBAdapterOutboundHeader.wsdl"/>

    <types>
        <schema xmlns="http://www.w3.org/2001/XMLSchema">
            <import namespace="http://xmlns.oracle.com/pcbpel/adapter/db/top/MailCKDBOut"
                schemaLocation="MailCKDBOut_table.xsd"/>
        </schema>
    </types>

    <message name="XxArMailCheckHoldsCollection_msg">
        <part name="XxArMailCheckHoldsCollection" element="top:XxArMailCheckHoldsCollection"/>
    </message>


    <portType name="MailCK_DB_Out_ptt">
        <operation name="insert">
            <input message="tns:XxArMailCheckHoldsCollection_msg"/>
        </operation>
    </portType>

    <binding name="MailCK_DB_Out_binding" type="tns:MailCK_DB_Out_ptt">
        <jca:binding />
        <operation name="insert">
            <jca:operation
                InteractionSpec="oracle.tip.adapter.db.DBWriteInteractionSpec"

                DescriptorName="MailCKDBOut.XxArMailCheckHolds"
                DmlType="insert"
                DetectOmissions="true"
                OptimizeMerge="true"
                MappingsMetaDataURL="MailCKDBOut_toplink_mappings.xml" />
            <input>
                <jca:header message="hdr:OutboundHeader_msg" part="outboundHeader"/>
            </input>
        </operation>
    </binding>

    <!-- Your runtime connection is declared in
        J2EE_HOME/application-deployments/default/DbAdapter/oc4j-ra.xml
        These 'mcf' properties here are from your design time connection and 
        save you from having to edit that file and restart the application server
        if eis/db/EBSApps is missing.
        These 'mcf' properties are safe to remove.
    -->
    <service name="MailCK_DB_Out">
        <port name="MailCK_DB_Out_pt" binding="tns:MailCK_DB_Out_binding">
            <jca:address location="eis/db/EBSAppsFin"
                UIConnectionName="null"
                ManagedConnectionFactory="oracle.tip.adapter.db.DBManagedConnectionFactory"
            />
        </port>
    </service>

    <plt:partnerLinkType name="MailCK_DB_Out_plt" >
        <plt:role name="MailCK_DB_Out_role" >
            <plt:portType name="tns:MailCK_DB_Out_ptt" />
        </plt:role>
    </plt:partnerLinkType>

</definitions>

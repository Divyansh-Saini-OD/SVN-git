<?xml version = '1.0' encoding = 'UTF-8'?>
<BPELSuitcase>
   <BPELProcess id="FtpAJBSettlementProcess_Out" src="FtpAJBSettlementProcess_Out.bpel">
      <partnerLinkBindings>
         <partnerLinkBinding name="AJBFileGet">
            <property name="wsdlLocation">AJBFileGet.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="AJBFilePut">
            <property name="wsdlLocation">AJBFilePut.wsdl</property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODErrorLogger">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/default/ODErrorLogger/ODErrorLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="PutTriggerFileAJB">
            <property name="wsdlLocation">PutTriggerFileAJB.wsdl</property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
      </partnerLinkBindings>
      <activationAgents>
         <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="AJBFileGet">
            <property name="ArchivalFilePath" type="LogicalArchiveDirectory"></property>
            <property name="IncomingFilePath" type="LogicalDirectory"></property>
            <property name="portType">Get_ptt</property>
         </activationAgent>
      </activationAgents>
   </BPELProcess>
</BPELSuitcase>

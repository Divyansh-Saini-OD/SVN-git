<definitions
     name="ExtensityInvoiceRead"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/ftp/ExtensityInvoiceRead/"
     xmlns="http://schemas.xmlsoap.org/wsdl/"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/ftp/ExtensityInvoiceRead/"
     xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
     xmlns:pc="http://xmlns.oracle.com/pcbpel/"
     xmlns:imp1="http://www.na.odcorp.com/ExtensityInvoiceRead"
     xmlns:hdr="http://xmlns.oracle.com/pcbpel/adapter/ftp/"
    >
    <import namespace="http://xmlns.oracle.com/pcbpel/adapter/ftp/" location="ftpAdapterInboundHeader.wsdl"/>
    <types>
      <schema xmlns="http://www.w3.org/2001/XMLSchema" >
        <import namespace="http://www.na.odcorp.com/ExtensityInvoiceRead" schemaLocation="ExtensityInvoice.xsd" />
      </schema>
    </types>
    <message name="Extensity_msg">
        <part name="Extensity" element="imp1:Extensity"/>
    </message>
    <portType name="Get_ptt">
        <operation name="Get">
            <input message="tns:Extensity_msg"/>
        </operation>
    </portType>
    <binding name="Get_binding" type="tns:Get_ptt">
    <pc:inbound_binding  />
        <operation name="Get">
      <jca:operation
          FileType="binary"
          LogicalDirectory="FTPInboundDir"
          ActivationSpec="oracle.tip.adapter.ftp.inbound.FTPActivationSpec"
          LogicalArchiveDirectory="FTPArchiveDIR"
          DeleteFile="true"
          IncludeFiles="AP_EXPENSE_NA_EXTENSITY.*\.txt"
          PollingFrequency="1800"
          MinimumAge="30"
          FileModificationTime="FileSystem"
          ModificationTimeFormat="4,18,yyyyMMddHHmmSS"
          OpaqueSchema="false" >
      </jca:operation>
      <input>
        <jca:header message="hdr:InboundHeader_msg" part="inboundHeader"/>
      </input>
        </operation>
    </binding>
    <service name="ExtensityInvoiceRead">
        <port name="Get_pt" binding="tns:Get_binding">
      <jca:address location="eis/ftp/EBSCentralExternal" UIincludeWildcard="AP_EXPENSE_NA_EXTENSITY*.txt" />
        </port>
    </service>
  <plt:partnerLinkType name="Get_plt" >
    <plt:role name="Get_role" >
      <plt:portType name="tns:Get_ptt" />
    </plt:role>
  </plt:partnerLinkType>
</definitions>

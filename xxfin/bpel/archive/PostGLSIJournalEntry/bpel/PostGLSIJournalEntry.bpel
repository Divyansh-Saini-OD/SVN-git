<process name="PostGLSIJournalEntry"
         targetNamespace="http://xmlns.oracle.com/PostGLSIJournalEntry"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns4="http://xmlns.oracle.com/PostJournalEntryGLInsert"
         xmlns:ns7="http://schemas.oracle.com/service/bpel/common"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns5="http://www.openapplications.org/oagis/9/officedepot/1"
         xmlns:client="http://xmlns.oracle.com/PostGLSIJournalEntry"
         xmlns:ns6="http://xmlns.oracle.com/ODErrorLogger"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/ftp/FTPGLJournalInvoice/"
         xmlns:ehdr="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ns3="http://www.openapplications.org/oagis/9"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/ftp/"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:ns15="ODError"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc">
    <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
    <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
    <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
    <partnerLinks>
        <partnerLink myRole="Get_role" name="FTPGLJournalInvoice"
                     partnerLinkType="ns1:Get_plt"/>
        <partnerLink name="PostJournalEntryGLInsert"
                     partnerRole="PostJournalEntryGLInsertProvider"
                     partnerLinkType="ns4:PostJournalEntryGLInsert"/>
        <partnerLink name="ODErrorLogger" partnerRole="ODErrorLoggerProvider"
                     partnerLinkType="ns6:ODErrorLogger"/>
    </partnerLinks>
    <variables>
        <variable name="FTPInHeader" messageType="ns2:InboundHeader_msg"/>
        <variable name="FTPFileIn" messageType="ns1:GLJournalInvoice_msg"/>
        <variable name="Insert_GL_Interface_NA_Stg_In"
                  messageType="ns4:PostJournalEntryGLInsertRequestMessage"/>
        <variable name="Invoke_ODErrorLogger_In"
                  messageType="ns6:ODErrorLoggerRequestMessage"/>
        <variable name="XSLT_Param" element="ns7:parameters"/>
        <variable name="Variable_1" type="xsd:string"/>
    </variables>
    <sequence name="main">
        <receive name="receive" createInstance="yes"
                 partnerLink="FTPGLJournalInvoice" portType="ns1:Get_ptt"
                 operation="Get" variable="FTPFileIn"
                 bpelx:headerVariable="FTPInHeader"/>
        <assign name="Assign_FileNameParameter">
            <copy>
                <from expression="'GLFileName'"/>
                <to variable="XSLT_Param"
                    query="/ns7:parameters/ns7:item/ns7:name"/>
            </copy>
            <copy>
                <from variable="FTPInHeader" part="inboundHeader"
                      query="/ns2:InboundFTPHeaderType/ns2:fileName"/>
                <to variable="XSLT_Param"
                    query="/ns7:parameters/ns7:item/ns7:value"/>
            </copy>
        </assign>
        <assign name="TransformGLSIToOAGIS">
            <bpelx:annotation>
                <bpelx:pattern>transformation</bpelx:pattern>
            </bpelx:annotation>
            <copy>
                <from expression="ora:processXSLT('TransformGLSIToOAGIS.xsl',bpws:getVariableData('FTPFileIn','GLJournalInvoice'), bpws:getVariableData('XSLT_Param'))"/>
                <to variable="Insert_GL_Interface_NA_Stg_In" part="payload"/>
            </copy>
        </assign>
        <!--assign name="Assign_FileName">
            <copy>
                <from variable="FTPInHeader" part="inboundHeader"
                      query="/ns2:InboundFTPHeaderType/ns2:fileName"/>
                <to variable="Insert_GL_Interface_NA_Stg_In" part="payload"
                    query="/ns3:PostJournalEntry/ns3:DataArea/ns3:JournalEntry/ns3:JournalEntryHeader/ns5:Batch/ns3:FileName"/>
            </copy>
        </assign-->
        <scope name="ScopeInvokePostJournalEntry">
            <variables>
                <variable name="RemoteFaultVar"
                          messageType="bpelx:RuntimeFaultMessage"/>
            </variables>
            <faultHandlers>
                <catch faultName="bpelx:remoteFault"
                       faultVariable="RemoteFaultVar">
                    <sequence name="Sequence_9">
                        <assign name="Assign_Error_Data">
                            <copy>
                                <from expression="concat(ora:getProcessId(), ora:getProcessVersion())"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ProcessInfo/ns15:BusinessProcessName"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="ora:getInstanceId()"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ProcessInfo/ns15:BusinessProcessId"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="ora:getDomainId()"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ProcessInfo/ns15:BusinessProcessDomain"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="ora:getProcessURL()"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ProcessInfo/ns15:SystemName"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from variable="RemoteFaultVar" part="code"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ErrorDetails/ns15:ErrorCode"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="xp20:current-dateTime()"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ErrorDetails/ns15:ErrorDateTime"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from variable="RemoteFaultVar" part="detail"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ErrorDetails/ns15:ErrorDescription"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from variable="RemoteFaultVar" part="summary"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ErrorDetails/ns15:ErrorText"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="'1'"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ErrorDetails/ns15:ErrorSeverity"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="'Step 1'"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ProcessInfo/ns15:BusinessProcessStep"
                                    part="payload"/>
                            </copy>
                        </assign>
                        <invoke name="Invoke_ODErrorLogger"
                                partnerLink="ODErrorLogger"
                                portType="ns6:ODErrorLogger"
                                operation="initiate"
                                inputVariable="Invoke_ODErrorLogger_In"/>
                    </sequence>
                </catch>
                <catchAll>
                    <sequence name="Sequence_10">
                        <assign name="Assign_Error_Data">
                            <copy>
                                <from expression="concat(ora:getProcessId(), ora:getProcessVersion())"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ProcessInfo/ns15:BusinessProcessName"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="ora:getInstanceId()"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ProcessInfo/ns15:BusinessProcessId"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="ora:getDomainId()"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ProcessInfo/ns15:BusinessProcessDomain"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="ora:getProcessURL()"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ProcessInfo/ns15:SystemName"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="'01'"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ErrorDetails/ns15:ErrorCode"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="xp20:current-dateTime()"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ErrorDetails/ns15:ErrorDateTime"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="'Error while FTP the file. Get the BPEL process instance and check the Audit trail.'"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ErrorDetails/ns15:ErrorDescription"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="'Error occured in FTP'"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ErrorDetails/ns15:ErrorText"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="'1'"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ErrorDetails/ns15:ErrorSeverity"
                                    part="payload"/>
                            </copy>
                            <copy>
                                <from expression="'Step 1'"/>
                                <to variable="Invoke_ODErrorLogger_In"
                                    query="/ns15:ErrorData/ns15:ProcessInfo/ns15:BusinessProcessStep"
                                    part="payload"/>
                            </copy>
                        </assign>
                        <invoke name="Invoke_ODErrorLogger"
                                partnerLink="ODErrorLogger"
                                portType="ns6:ODErrorLogger"
                                operation="initiate"
                                inputVariable="Invoke_ODErrorLogger_In"/>
                    </sequence>
                </catchAll>
            </faultHandlers>
            <sequence name="sequence_1">
                <invoke name="Invoke_GL_Insert_Service"
                        partnerLink="PostJournalEntryGLInsert"
                        portType="ns4:PostJournalEntryGLInsert"
                        operation="initiate"
                        inputVariable="Insert_GL_Interface_NA_Stg_In"/>
            </sequence>
        </scope>
    </sequence>
</process>
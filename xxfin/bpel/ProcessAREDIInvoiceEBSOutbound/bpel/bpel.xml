<?xml version = '1.0' encoding = 'UTF-8'?>
<BPELSuitcase>
   <BPELProcess id="ProcessAREDIInvoiceEBSOutbound" src="ProcessAREDIInvoiceEBSOutbound.bpel">
      <partnerLinkBindings>
         <partnerLinkBinding name="ServiceFileRead">
            <property name="wsdlLocation">ServiceFileRead.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="B2BCustomerInvoiceOUtbound">
            <property name="wsdlLocation">http://esbdev02.na.odcorp.net/orabpel/b2b/B2BCustomerInvoiceOUtbound/B2BCustomerInvoiceOUtbound?wsdl</property>
            <property name="optSoapShortcut">false</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODErrorLogger">
            <property name="wsdlLocation">http://esbdev02.na.odcorp.net/orabpel/default/ODErrorLogger/ODErrorLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="PutReprocessFile">
            <property name="wsdlLocation">PutReprocessFile.wsdl</property>
            <property name="retryInterval">60</property>
            <property name="archive_path" type="LogicalDirectory"></property>
         </partnerLinkBinding>
         <partnerLinkBinding name="PutFileReprocess">
            <property name="wsdlLocation">PutFileReprocess.wsdl</property>
            <property name="input_path" type="LogicalDirectory"></property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
      </partnerLinkBindings>
      <activationAgents>
         <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="ServiceFileRead">
            <property name="oracle.tip.adapter.file.debatching.rejection.quantum">EOF</property>
            <property name="Logical_Path" type="LogicalDirectory"></property>
            <property name="portType">Get_ptt</property>
            <property name="Archive_Path" type="LogicalArchiveDirectory"></property>
         </activationAgent>
      </activationAgents>
   </BPELProcess>
</BPELSuitcase>

<process name="ProcessODAmexL3CreditSettlement"
         targetNamespace="http://xmlns.oracle.com/ProcessODAmexL3CreditSettlement"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns4="http://xmlns.officedepot.com/B2BAmexCreditSettlementOutbound"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns5="http://www.officedepot.com/GetODAmexL3CpcSrvc"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:ns9="http://xmlns.oracle.com/ODErrorLogger"
         xmlns:ns1="http://www.openapplications.org/oagis/9"
         xmlns:ns2="http://www.openapplications.org/oagis/9/officedepot/1"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns10="ODError"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns13="http://xmlns.oracle.com/pcbpel/adapter/file/PutReprocessFile/"
         xmlns:ns7="http://xmlns.oracle.com/pcbpel/adapter/ftp/"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns12="http://xmlns.oracle.com/TransactionData"
         xmlns:client="http://xmlns.oracle.com/ProcessODAmexL3CreditSettlement"
         xmlns:ns18="http://schemas.oracle.com/service/bpel/common"
         xmlns:ns6="http://xmlns.oracle.com/pcbpel/adapter/ftp/FilePut/"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ehdr="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ns14="http://xmlns.oracle.com/pcbpel/adapter/file/"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/ftp/GetODAmexL3CpcSrvc/"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:ns8="http://xmlns.oracle.com/ODTransactionLogger">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <partnerLink myRole="Get_role" name="GetODAmexL3CpcSrvc"
                 partnerLinkType="ns3:Get_plt"/>
    <partnerLink myRole="ODErrorLoggerRequester" name="ODErrorLogger"
                 partnerRole="ODErrorLoggerProvider"
                 partnerLinkType="ns9:ODErrorLogger"/>
    <partnerLink myRole="B2BAmexCreditSettlementOutboundRequester"
                 name="B2BAmexCreditSettlementOutbound"
                 partnerRole="B2BAmexCreditSettlementOutboundProvider"
                 partnerLinkType="ns4:B2BAmexCreditSettlementOutbound"/>
    <partnerLink name="FilePutBack" partnerRole="Put_role"
                 partnerLinkType="ns6:Put_plt"/>
    <partnerLink name="PutReprocessFile" partnerRole="Write_role"
                 partnerLinkType="ns13:Write_plt"/>
  </partnerLinks>
  <variables>
    <variable name="ProcessInvVar" messageType="ns3:ProcessInvMsg"/>
    <variable name="InvokeB2BSrvc_initiate_InputVariable"
              messageType="ns4:B2BAmexCreditSettlementOutboundRequestMessage"/>
    <variable name="invkCatchAll_initiate"
              messageType="ns9:ODErrorLoggerRequestMessage"/>
    <variable name="invkBindingFault_initiate"
              messageType="ns9:ODErrorLoggerRequestMessage"/>
    <variable name="Invoke_1_Put_InputVariable"
              messageType="ns6:container_msg"/>
    <variable name="File_Name_In" messageType="ns7:InboundHeader_msg"/>
    <variable name="File_Name_out" messageType="ns7:OutboundHeader_msg"/>
    <variable name="ReceiveODAmexL3Cpc_Get_InputVariable"
              messageType="ns3:container_msg"/>
    <variable name="ProcessHeader" type="xsd:string"/>
    <variable name="XSLT_PARAM" element="ns18:parameters"/>
    <variable name="ErrorMessageText" type="xsd:string"/>
    <variable name="xmlFragment" type="xsd:string"/>
    <variable name="InvokeFilePutBack_In" messageType="ns13:container_msg"/>
    <variable name="ReprocessHeader" element="ns14:OutboundFileHeaderType"/>
    <variable name="TimeStamp" type="xsd:string"/>
    <variable name="ReceiveB2BSrvc_Response_In"
              messageType="ns4:B2BAmexCreditSettlementOutboundResponseMessage"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="Sequence_4">
        <assign name="asnCatchAll">
          <copy>
            <from expression="concat(&quot;An error occured in ProcessODAmexL3CreditSettlement while processing the file &quot;, bpws:getVariableData('File_Name_In','inboundHeader','/ns7:InboundFTPHeaderType/ns7:fileName'), &quot;.&quot;)"/>
            <to variable="ErrorMessageText"/>
          </copy>
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getInstanceId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessId"/>
          </copy>
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessDomain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessURL()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:SystemName"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorDateTime"/>
          </copy>
          <copy>
            <from expression="'Catch All on the Main Sequence'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ProcessInfo/ns10:BusinessProcessStep"/>
          </copy>
          <copy>
            <from expression="ora:getFaultName()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorCode"/>
          </copy>
          <copy>
            <from variable="ErrorMessageText"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorDescription"/>
          </copy>
          <copy>
            <from expression="ora:getFaultAsString()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorText"/>
          </copy>
          <copy>
            <from expression="'system'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorType"/>
          </copy>
          <copy>
            <from expression="'fatal'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:ErrorDetails/ns10:ErrorSeverity"/>
          </copy>
          <copy>
            <from expression="ora:doc('${server_url}/xmllib/fin/GlobalParameters.xml','/GlobalParameters/FaultNotification/SendNotification')"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns10:ErrorData/ns10:Notification/ns10:sendNotification"/>
          </copy>
        </assign>
        <invoke name="invkCatchAll" partnerLink="ODErrorLogger"
                portType="ns9:ODErrorLogger" operation="initiate"
                inputVariable="invkCatchAll_initiate"/>
        <assign name="AssignReprocessData">
          <copy>
            <from expression="xp20:format-dateTime(xp20:current-dateTime(),'[D01][M01][Y][H01][m01][s01][f001]')"/>
            <to variable="TimeStamp"/>
          </copy>
          <copy>
            <from expression="concat(substring-before(bpws:getVariableData('File_Name_In','inboundHeader','/ns7:InboundFTPHeaderType/ns7:fileName'),'.'),'_REJECTED_',bpws:getVariableData('TimeStamp'),'.set')"/>
            <to variable="ReprocessHeader"
                query="/ns14:OutboundFileHeaderType/ns14:fileName"/>
          </copy>
          <copy>
            <from variable="ReceiveODAmexL3Cpc_Get_InputVariable"
                  part="container" query="/ns5:container"/>
            <to variable="InvokeFilePutBack_In" part="container"
                query="/ns5:container"/>
          </copy>
        </assign>
        <invoke name="InvokeFilePutBack" partnerLink="PutReprocessFile"
                portType="ns13:Write_ptt" operation="Write"
                inputVariable="InvokeFilePutBack_In"
                bpelx:inputHeaderVariable="ReprocessHeader"/>
        <terminate name="Terminate"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <sequence name="main">
    <receive name="ReceiveODAmexL3Cpc" partnerLink="GetODAmexL3CpcSrvc"
             portType="ns3:Get_ptt" operation="Get"
             variable="ReceiveODAmexL3Cpc_Get_InputVariable"
             createInstance="yes" bpelx:headerVariable="File_Name_In"/>
    <scope name="setProcessHeader">
      <sequence name="setProcessTitle">
        <assign name="initializeInstanceTitle">
          <copy>
            <from expression="substring( concat( &quot;Processing &quot;, bpws:getVariableData('File_Name_In','inboundHeader','/ns7:InboundFTPHeaderType/ns7:fileName')),1,50)"/>
            <to variable="ProcessHeader"/>
          </copy>
          <copy>
            <from expression="'AMEXCCSETFileName'"/>
            <to variable="XSLT_PARAM"
                query="/ns18:parameters/ns18:item/ns18:name"/>
          </copy>
          <copy>
            <from variable="File_Name_In" part="inboundHeader"
                  query="/ns7:InboundFTPHeaderType/ns7:fileName"/>
            <to variable="XSLT_PARAM"
                query="/ns18:parameters/ns18:item/ns18:value"/>
          </copy>
        </assign>
        <bpelx:exec name="setTitle" language="java" version="1.5">
          <![CDATA[try {             
                         String title = (String)getVariableData("ProcessHeader");             
                         setTitle(title);             
                         addAuditTrailEntry(title) ;             
                       } catch (Exception ex) {             
                         addAuditTrailEntry(ex.toString());             
                         ex.printStackTrace();             
          }]]>
        </bpelx:exec>
      </sequence>
    </scope>
    <assign name="TransformAmexL3CpcToProcessInv">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:processXSLT('AmexL3CpcToProcessInv.xsl',bpws:getVariableData('ReceiveODAmexL3Cpc_Get_InputVariable','container'))"/>
        <to variable="ProcessInvVar" part="payload"/>
      </copy>
    </assign>
    <assign name="AssignProcessInvToB2BSrvc">
      <copy>
        <from variable="ProcessInvVar" part="payload"/>
        <to variable="InvokeB2BSrvc_initiate_InputVariable" part="payload"/>
      </copy>
    </assign>
    <invoke name="InvokeB2BSrvc" partnerLink="B2BAmexCreditSettlementOutbound"
            portType="ns4:B2BAmexCreditSettlementOutbound" operation="initiate"
            inputVariable="InvokeB2BSrvc_initiate_InputVariable"/>
    <receive name="ReceiveB2BSrvc" partnerLink="B2BAmexCreditSettlementOutbound"
             portType="ns4:B2BAmexCreditSettlementOutboundCallback"
             operation="onResult" variable="ReceiveB2BSrvc_Response_In"
             createInstance="no"/>
  </sequence>
</process>
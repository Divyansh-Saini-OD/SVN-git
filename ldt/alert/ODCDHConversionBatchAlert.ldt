    , &batch_creation_Date\n\
    , &batch_original_system\n\
    , &instance_name\n\
    , &system_date\n\
    , &age\n\
    , &email_subject_prefix\n\
    , &email_send_to\n\
from hz_imp_batch_summary hibs, v$instance i\n\
where nvl(hibs.batch_status, 'ACTIVE') = 'ACTIVE'\n\
and round((sysdate-hibs.creation_date)*24, 0) > nvl(fnd_profile.value('XX_CDH_CONV_ALERT_BATCH_AGE'), 18)\n\
and nvl(fnd_profile.value('XX_CDH_CONV_ALERT_BATCH_ENABLE_EMAILS'), 'Y') = 'Y'\n\

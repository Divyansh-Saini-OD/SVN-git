UPLOAD XX_FIN_TRANSLATERESPONSIBILITY
"BEGIN
     DECLARE
      ln_translate_id         NUMBER;
      ln_resp_id              NUMBER;
      ln_last_updated_by      NUMBER;     
      ld_last_update_date     DATE;
      ln_db_last_updated_by   NUMBER;      
      ld_db_last_update_date  DATE;
      ln_resp_appl_id         NUMBER;
      
     BEGIN
        ln_last_updated_by := fnd_load_util.owner_id(:OWNER);
        ld_last_update_date := nvl(to_date(:LAST_UPDATE_DATE, 'RRRR/MM/DD'), sysdate);

        SELECT translate_id
	  INTO ln_translate_id
          FROM XX_FIN_TRANSLATEDEFINITION
         WHERE translation_name = :TRANSLATION_NAME;
         
        SELECT application_id
          INTO ln_resp_appl_id
          FROM FND_APPLICATION
         WHERE application_short_name=:RESP_APP_SHORT_NAME;
        
        SELECT responsibility_id 
          INTO ln_resp_id
          FROM FND_RESPONSIBILITY
         WHERE responsibility_key = :RESP_KEY
           AND application_id = ln_resp_appl_id;
             
        BEGIN
	 SELECT LAST_UPDATED_BY, LAST_UPDATE_DATE
	  INTO ln_db_last_updated_by, ld_db_last_update_date
	  FROM XX_FIN_TRANSLATERESPONSIBILITY
	 WHERE translate_id       =ln_translate_id
	   AND responsibility_id  =ln_resp_id;

	  IF (fnd_load_util.upload_test(ln_last_updated_by, 
				      ld_last_update_date, 
				      ln_db_last_updated_by,
				      ld_db_last_update_date, 
				      :CUSTOM_MODE)
	    ) THEN
	    
                    UPDATE XX_FIN_TRANSLATERESPONSIBILITY
                    SET
                    read_only_flag           =  :read_only_flag, 
		    last_update_date         =  ld_last_update_date, 
		    last_updated_by          =  ln_last_updated_by, 
		    last_update_login        =  0
		    WHERE translate_id       =ln_translate_id
		      AND responsibility_id  =ln_resp_id;      
		    
          END IF;
        EXCEPTION
        WHEN NO_DATA_FOUND THEN
		    INSERT INTO XX_FIN_TRANSLATERESPONSIBILITY
		    (
		     security_value_id,
		     translate_id, 
		     responsibility_id, 
		     read_only_flag, 
		     creation_date, 
		     created_by,
		     last_update_date, 
		     last_updated_by, 
		     last_update_login
		    ) 
		    VALUES(
		    XX_FIN_TRANSLATERESPONSIBIL_S.NEXTVAL, 
		    ln_translate_id, 
		    ln_resp_id,
		    :read_only_flag, 
		    sysdate, 
		    ln_last_updated_by,
		    sysdate, 
		    ln_last_updated_by, 
		    0); 	    
	END;
     END;
 END;"
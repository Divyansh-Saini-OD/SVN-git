DOWNLOAD XX_FIN_TRANSLATEDEFINITION
       "SELECT translation_name, related_module AS app_short_name, source_system,
       target_system, purpose, translate_description, source_field1,
       source_field2, source_field3, source_field4, source_field5,
       source_field6, source_field7, target_field1, target_field2,
       target_field3, target_field4, target_field5, target_field6,
       target_field7, target_field8, target_field9, target_field10,
       target_field11, target_field12, target_field13, target_field14,
       target_field15, target_field16, target_field17, target_field18,
       target_field19, target_field20,
       target_field21, target_field22, target_field23, target_field24,
       target_field25, target_field26, target_field27, target_field28,
       target_field29, target_field30,
       fnd_load_util.owner_name (last_updated_by) owner,
       TO_CHAR (last_update_date, 'RRRR/MM/DD') last_update_date,
       TO_CHAR (start_date_active, 'RRRR/MM/DD') start_date_active,
       TO_CHAR (end_date_active, 'RRRR/MM/DD') end_date_active, enabled_flag,
       source_field8, source_field9, source_field10, do_not_refresh
  FROM xx_fin_translatedefinition
 WHERE (   :app_short_name IS NULL
        OR (:app_short_name IS NOT NULL 
		    AND related_module LIKE :app_short_name
           )
       )
   AND (   :translation_name IS NULL
        OR (    :translation_name IS NOT NULL
            AND translation_name LIKE :translation_name
           )
       )
   AND enabled_flag = 'Y'
   AND SYSDATE BETWEEN start_date_active AND NVL (end_date_active, SYSDATE)"

DOWNLOAD XX_FIN_TRANSLATEVALUES
       "SELECT xftv.source_value1, xftv.source_value2, xftv.source_value3,
	       xftv.source_value4, xftv.source_value5, xftv.source_value6,
	       xftv.source_value7, xftv.target_value1, xftv.target_value2,
	       xftv.target_value3, xftv.target_value4, xftv.target_value5,
	       xftv.target_value6, xftv.target_value7, xftv.target_value8,
	       xftv.target_value9, xftv.target_value10, xftv.target_value11,
	       xftv.target_value12, xftv.target_value13, xftv.target_value14,
	       xftv.target_value15, xftv.target_value16, xftv.target_value17,
	       xftv.target_value18, xftv.target_value19, xftv.target_value20,
           xftv.target_value21,
	       xftv.target_value22, xftv.target_value23, xftv.target_value24,
	       xftv.target_value25, xftv.target_value26, xftv.target_value27,
	       xftv.target_value28, xftv.target_value29, xftv.target_value30,           
	       fnd_load_util.owner_name (xftv.last_updated_by) owner,
	       TO_CHAR (xftv.last_update_date, 'RRRR/MM/DD') last_update_date,
	       TO_CHAR (xftv.start_date_active, 'RRRR/MM/DD') start_date_active,
	       TO_CHAR (xftv.end_date_active, 'RRRR/MM/DD') end_date_active,
	       xftv.read_only_flag, xftv.enabled_flag, xftv.source_value8,
	       xftv.source_value9, xftv.source_value10
	  FROM xx_fin_translatevalues xftv, xx_fin_translatedefinition xftd
	 WHERE xftd.translate_id = xftv.translate_id
	   AND xftd.related_module = :app_short_name
	   AND xftd.translation_name = :translation_name
	   AND xftd.enabled_flag = 'Y'
	   AND SYSDATE BETWEEN xftd.start_date_active
			   AND NVL (xftd.end_date_active, SYSDATE)
	   AND xftv.enabled_flag = 'Y'
	   AND SYSDATE BETWEEN xftv.start_date_active
			   AND NVL (xftv.end_date_active, SYSDATE)"
	   
DOWNLOAD XX_FIN_TRANSLATERESPONSIBILITY
       "SELECT fr.responsibility_key,
	       fa.application_short_name AS resp_app_short_name, xft.read_only_flag,
	       fnd_load_util.owner_name (xft.last_updated_by) owner,
	       TO_CHAR (xft.last_update_date, 'RRRR/MM/DD') last_update_date
	  FROM xx_fin_translateresponsibility xft,
	       fnd_responsibility fr,
	       fnd_application fa,
	       xx_fin_translatedefinition xftd
	 WHERE xftd.translate_id = xft.translate_id
	   AND xftd.related_module = :app_short_name
	   AND xftd.enabled_flag = 'Y'
	   AND SYSDATE BETWEEN xftd.start_date_active
			   AND NVL (xftd.end_date_active, SYSDATE)
	   AND fr.responsibility_id = xft.responsibility_id
	   AND xftd.translation_name = :translation_name
	   AND fa.application_id = fr.application_id"	   
	   

UPLOAD XX_FIN_TRANSLATEVALUES
"BEGIN
     DECLARE
      ln_translate_id         NUMBER;
      ld_start_date           DATE;
      ld_end_date             DATE;
      ln_last_updated_by      NUMBER;
      ld_last_update_date     DATE;
      ln_db_last_updated_by   NUMBER;      
      ld_db_last_update_date  DATE;

      
     BEGIN
        ln_last_updated_by  := fnd_load_util.owner_id(:OWNER);
        ld_last_update_date := nvl(to_date(:LAST_UPDATE_DATE, 'RRRR/MM/DD'), sysdate);
        ----------------------------------------------------------
        -- Translate all char Date-Values to date
        ----------------------------------------------------------
        ld_start_date        := NVL(TO_DATE(:start_date_active, 'RRRR/MM/DD'), SYSDATE);
        ld_end_date          := NVL(TO_DATE(:end_date_active,   'RRRR/MM/DD'), NULL);

        SELECT translate_id
	  INTO ln_translate_id
          FROM XX_FIN_TRANSLATEDEFINITION
         WHERE translation_name = :translation_name;
        BEGIN
	  SELECT LAST_UPDATED_BY, LAST_UPDATE_DATE
	    INTO ln_db_last_updated_by, ld_db_last_update_date
	    FROM XX_FIN_TRANSLATEVALUES
	   WHERE translate_id    =ln_translate_id
	     AND source_value1||'$#'||source_value2||'$#'||source_value3||'$#'||
	         source_value4||'$#'||source_value5||'$#'||source_value6||'$#'||
	         source_value7||'$#'||source_value8||'$#'||source_value9||'$#'||
	         source_value10
	         = 
	         :source_value1||'$#'||:source_value2||'$#'||:source_value3||'$#'||
	         :source_value4||'$#'||:source_value5||'$#'||:source_value6||'$#'||
	         :source_value7||'$#'||:source_value8||'$#'||:source_value9||'$#'||
	         :source_value10
	     AND target_value1||'$#'||target_value2||'$#'||target_value3||'$#'||
	         target_value4||'$#'||target_value5||'$#'||target_value6||'$#'||
	         target_value7||'$#'||target_value8||'$#'||target_value9||'$#'||
	         target_value10
	         = 
	         :target_value1||'$#'||:target_value2||'$#'||:target_value3||'$#'||
	         :target_value4||'$#'||:target_value5||'$#'||:target_value6||'$#'||
	         :target_value7||'$#'||:target_value8||'$#'||:target_value9||'$#'||
	         :target_value10;
	  IF (fnd_load_util.upload_test(ln_last_updated_by, 
				        ld_last_update_date, 
				        ln_db_last_updated_by,
				        ld_db_last_update_date, 
				        :CUSTOM_MODE)
	                               ) 
	  THEN
	               UPDATE  XX_FIN_TRANSLATEVALUES
	               SET			
		       target_value1 	   =:target_value1, 
		       target_value2 	   =:target_value2, 
		       target_value3	   =:target_value3,
		       target_value4 	   =:target_value4, 
		       target_value5 	   =:target_value5, 
		       target_value6	   =:target_value6,
		       target_value7 	   =:target_value7, 
		       target_value8 	   =:target_value8, 
		       target_value9	   =:target_value9,
		       target_value10      =:target_value10, 
		       target_value11      =:target_value11, 
		       target_value12	   =:target_value12,
		       target_value13      =:target_value13, 
		       target_value14      =:target_value14, 
		       target_value15	   =:target_value15,
		       target_value16      =:target_value16, 
		       target_value17      =:target_value17, 
		       target_value18	   =:target_value18,
		       target_value19      =:target_value19, 
		       target_value20      =:target_value20, 
		       target_value21      =:target_value21, 
		       target_value22	   =:target_value22,
		       target_value23      =:target_value23, 
		       target_value24      =:target_value24, 
		       target_value25	   =:target_value25,
		       target_value26      =:target_value26, 
		       target_value27      =:target_value27, 
		       target_value28	   =:target_value28,
		       target_value29      =:target_value29, 
		       target_value30      =:target_value30, 
		       last_update_date    = ld_last_update_date, 
		       last_updated_by     = ln_last_updated_by,
		       last_update_login   = 0, 
		       start_date_active   =ld_start_date, 
		       end_date_active     =ld_end_date,
		       read_only_flag      =:read_only_flag, 
		       enabled_flag 	   =:enabled_flag 
		 WHERE translate_id        =ln_translate_id
		   AND source_value1||'$#'||source_value2||'$#'||source_value3||'$#'||
		       source_value4||'$#'||source_value5||'$#'||source_value6||'$#'||
		       source_value7||'$#'||source_value8||'$#'||source_value9||'$#'||
		       source_value10
		       = 
		       :source_value1||'$#'||:source_value2||'$#'||:source_value3||'$#'||

		      target_field17	     = :target_field17, 
		      target_field18	     = :target_field18, 
		      target_field19	     = :target_field19,
		      target_field20	     = :target_field20, 
		      target_field21	     = :target_field21,
		      target_field22	     = :target_field22, 
		      target_field23	     = :target_field23, 
		      target_field24	     = :target_field24, 
		      target_field25	     = :target_field25,
		      target_field26	     = :target_field26, 
		      target_field27	     = :target_field27, 
		      target_field28	     = :target_field28, 
		      target_field29	     = :target_field29,
		      target_field30	     = :target_field30, 
		      last_update_date	     = ld_last_update_date,
		      last_updated_by	     = ln_last_updated_by, 
		      last_update_login	     = 0, 
		      start_date_active	     = ld_start_date, 
		      end_date_active	     = ld_end_date,
		      enabled_flag	     = :enabled_flag, 
		      do_not_refresh	     = :do_not_refresh 
		      WHERE translation_name = :TRANSLATION_NAME
			AND related_module   = :APP_SHORT_NAME;
          END IF;
	EXCEPTION
          WHEN NO_DATA_FOUND THEN 
		      INSERT INTO XX_FIN_TRANSLATEDEFINITION
		      (translate_id, 
		       translation_name, 
		       source_system, 
		       target_system, 

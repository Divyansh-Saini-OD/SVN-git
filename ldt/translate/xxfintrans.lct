        -- Translate all char Date-Values to date
        ----------------------------------------------------------
        ld_start_date        := NVL(TO_DATE(:start_date_active, 'RRRR/MM/DD'), SYSDATE);
        ld_end_date          := NVL(TO_DATE(:end_date_active,   'RRRR/MM/DD'), NULL);
        
        BEGIN
                 
                
		SELECT LAST_UPDATED_BY, LAST_UPDATE_DATE
		  INTO ln_db_last_updated_by, ld_db_last_update_date
		  FROM xx_fin_translatedefinition
		 WHERE translation_name = :TRANSLATION_NAME;
          
	  IF (fnd_load_util.upload_test(ln_last_updated_by, 
				      ld_last_update_date, 
				      ln_db_last_updated_by,
				      ld_db_last_update_date, 
				      :CUSTOM_MODE)
	    ) THEN
		      UPDATE XX_FIN_TRANSLATEDEFINITION
		      SET
		      source_system          = :source_system, 
		      target_system          = :target_system, 
		      purpose                = :purpose,
		      translate_description  = :translate_description, 
		      related_module         = :app_short_name, 
		      source_field1          = :source_field1, 
		      source_field2 	     = :source_field2,

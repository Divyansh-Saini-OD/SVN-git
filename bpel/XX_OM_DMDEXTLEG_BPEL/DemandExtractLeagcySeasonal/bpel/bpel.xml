<?xml version = '1.0' encoding = 'UTF-8'?>
<BPELSuitcase>
   <BPELProcess id="DemandExtractLeagcySeasonal" src="DemandExtractLeagcySeasonal.bpel">
      <partnerLinkBindings>
         <partnerLinkBinding name="FTPGetFile">
            <property name="wsdlLocation">FTPGetFile.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="FTPPutFile">
            <property name="wsdlLocation">FTPPutFile.wsdl</property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
      </partnerLinkBindings>
      <activationAgents>
         <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="FTPGetFile">
            <property name="portType">Get_ptt</property>
         </activationAgent>
      </activationAgents>
   </BPELProcess>
</BPELSuitcase>

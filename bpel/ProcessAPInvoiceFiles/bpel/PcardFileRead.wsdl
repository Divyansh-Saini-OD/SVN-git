<definitions
     name="PcardFileRead"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/file/PcardFileRead/"
     xmlns="http://schemas.xmlsoap.org/wsdl/"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/file/PcardFileRead/"
     xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
     xmlns:imp1="http://TargetNamespace.com/ServiceName"
     xmlns:hdr="http://xmlns.oracle.com/pcbpel/adapter/file/"
    >
    <import namespace="http://xmlns.oracle.com/pcbpel/adapter/file/" location="fileAdapterOutboundHeader.wsdl"/>
    <types>
      <schema targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/file/PcardFileRead/"
              xmlns="http://www.w3.org/2001/XMLSchema" >
        <import namespace="http://TargetNamespace.com/ServiceName" schemaLocation="Pcard.xsd" />
        <element name="empty"><complexType/></element>
      </schema>
    </types>
    <message name="Empty_msg">
        <part name="Empty" element="tns:empty"/>
    </message>
    <message name="Root-Element_msg">
        <part name="Root-Element" element="imp1:Root-Element"/>
    </message>
    <portType name="SynchChunkRead_ptt">
        <operation name="SynchChunkRead">
            <input message="tns:Empty_msg"/>
            <output message="tns:Root-Element_msg"/>
        </operation>
    </portType>
    <binding name="SynchChunkRead_binding" type="tns:SynchChunkRead_ptt">
    <jca:binding  />
        <operation name="SynchChunkRead">
      <jca:operation
          PhysicalDirectory="Inbound_Path"
          InteractionSpec="oracle.tip.adapter.file.outbound.ChunkedInteractionSpec"
          ChunkSize="300"
          DeleteFile="true"
          FileName="File_Name_In"
          OpaqueSchema="false" >
      </jca:operation>
      <input>
        <jca:header message="hdr:OutboundHeader_msg" part="outboundHeader"/>
      </input>
        </operation>
    </binding>
    <service name="PcardFileRead">
        <port name="SynchChunkRead_pt" binding="tns:SynchChunkRead_binding">
      <jca:address location="eis/FileAdapter" />
        </port>
    </service>
  <plt:partnerLinkType name="SynchChunkRead_plt" >
    <plt:role name="SynchChunkRead_role" >
      <plt:portType name="tns:SynchChunkRead_ptt" />
    </plt:role>
  </plt:partnerLinkType>
</definitions>

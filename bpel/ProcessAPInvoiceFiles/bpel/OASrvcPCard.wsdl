<definitions
     name="OASrvcPCard"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/apps/OASrvcPCard/"
     xmlns="http://schemas.xmlsoap.org/wsdl/"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/apps/OASrvcPCard/"
     xmlns:db="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_AP_INV_IN_PCARD_BPEL_PKG/PROCESS_PCARD_INVOICE/"
     xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
     xmlns:hdr="http://xmlns.oracle.com/pcbpel/adapter/appscontext/"
    >
    <import namespace="http://xmlns.oracle.com/pcbpel/adapter/appscontext/" location="AppsContextHeader.wsdl"/>
    <import namespace="http://xmlns.oracle.com/pcbpel/adapter/db/" location="DBAdapterOutboundHeader.wsdl"/>
    <types>
     <schema xmlns="http://www.w3.org/2001/XMLSchema">
       <import namespace="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_AP_INV_IN_PCARD_BPEL_PKG/PROCESS_PCARD_INVOICE/"
               schemaLocation="APPS_XX_AP_INV_IN_PCARD_BPEL_PKG_PROCESS_PCARD_INVOICE.xsd" />
     </schema>
    </types>
    <message name="args_in_msg">
        <part name="InputParameters" element="db:InputParameters"/>
    </message>
    <message name="args_out_msg">
        <part name="OutputParameters" element="db:OutputParameters"/>
    </message>
    <portType name="OASrvcPCard_ptt">
        <operation name="OASrvcPCard">
            <input message="tns:args_in_msg"/>
            <output message="tns:args_out_msg"/>
        </operation>
    </portType>
    <binding name="OASrvcPCard_binding" type="tns:OASrvcPCard_ptt">
    <jca:binding  />
        <operation name="OASrvcPCard">
      <jca:operation
          SchemaName="APPS"
          PackageName="XX_AP_INV_IN_PCARD_BPEL_PKG"
          ProcedureName="PROCESS_PCARD_INVOICE"
          InteractionSpec="oracle.tip.adapter.apps.AppsStoredProcedureInteractionSpec"
          Username="sysadmin"
          Responsibility="System Administrator" >
      </jca:operation>
      <input>
        <jca:header message="hdr:Header_msg" part="Header"/>
      </input>
            <output/>
        </operation>
    </binding>
    <service name="OASrvcPCard">
        <port name="OASrvcPCard_pt" binding="tns:OASrvcPCard_binding">
<!--Your runtime connection is declared in 
J2EE_HOME/application-deployments/default/AppsAdapter/oc4j-ra.xml 
These mcf properties here are from your design time connection and 
save you from having to edit that file and restart the application server 
if eis/Apps/GSIDEV01 is missing. 
These mcf properties are safe to remove.-->
      <jca:address location="eis/apps/EBSAppsFin" UIConnectionName="GSIDEV01" UIOracleAppType="DBOBJECT"
          ManagedConnectionFactory="oracle.tip.adapter.apps.AppsManagedConnectionFactory"
           />
        </port>
    </service>
  <plt:partnerLinkType name="OASrvcPCard_plt" >
    <plt:role name="OASrvcPCard_role" >
      <plt:portType name="tns:OASrvcPCard_ptt" />
    </plt:role>
  </plt:partnerLinkType>
</definitions>

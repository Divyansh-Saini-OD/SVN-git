    <sequence name="main">
        <!-- Receive input from requestor. (Note: This maps to operation defined in I1153_DPSNowPrintEBS_JobConf_Child.wsdl) -->
        <receive name="receiveJobConfMaster" partnerLink="client"
                 portType="client:I1153_DPSNowPrintEBS_JobConf_Child"
                 operation="initiate" variable="receiveInputIn"
                 createInstance="yes">
            <bpelx:annotation>
                <bpelx:documentation>This activity is used to receive the
                                     Acknowledgement message from the Master
                                     Process as a part of the Process</bpelx:documentation>
            </bpelx:annotation>
        </receive>
        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <assign name="assignInVar">
            <bpelx:annotation>
                <bpelx:documentation>This activity is used to assign the input
                                     message from the Nowdocs to invoke the DB
                                     Package for updation of the acknowedgement</bpelx:documentation>
            </bpelx:annotation>
            <copy>
                <from variable="receiveInputIn" part="payload"
                      query="/ns1:ConfirmOrderRequest/ns1:ConfirmOrderBody/ns1:OrderNumber"/>
                <to variable="invkplUpdConfInVar" part="InputParameters"
                    query="/ns3:InputParameters/P_ORDER_NUMBER"/>
            </copy>
            <copy>
                <from variable="receiveInputIn" part="payload"
                      query="/ns1:ConfirmOrderRequest/ns1:ConfirmOrderBody/ns1:OrderSubNumber"/>
                <to variable="invkplUpdConfInVar" part="InputParameters"
                    query="/ns3:InputParameters/P_LINE_ID"/>
            </copy>
            <copy>
                <from expression="'invkplUpdConf - This activity invoke the Database Procedure for Updating Job Confirmation DPS status in the Order Lines table'"/>
                <to variable="asnActivityNameVar"/>
            </copy>
            <copy>
                <from expression="ora:getContentAsString(bpws:getVariableData('receiveInputIn','payload','/ns1:ConfirmOrderRequest'))"/>
                <to variable="messageDataVar"/>
            </copy>
            <copy>
                <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/UserName")'/>
                <to variable="invkplUpdConfInVar" part="InputParameters"
                    query="/ns3:InputParameters/P_USER_NAME"/>
            </copy>
            <copy>
                <from expression='ora:doc("${server_url}/xmllib/otc/GlobalParameters.xml","/GlobalParameters/OTC/DPS/RespName")'/>
                <to variable="invkplUpdConfInVar" part="InputParameters"
                    query="/ns3:InputParameters/P_RESP_NAME"/>
            </copy>
        </assign>
        <invoke name="invokeplUpdConf" partnerLink="plDBUpdConf"
                portType="ns2:plDBUpdConf_ptt" operation="plDBUpdConf"
                inputVariable="invkplUpdConfInVar"
                outputVariable="invkplUpdConfOutVar">
            <bpelx:annotation>
                <bpelx:documentation>This activity is used to invoke the DB
                                     Package for updating the acknowledgement
                                     from Nowdocs</bpelx:documentation>
            </bpelx:annotation>
        </invoke>
        <assign name="assignOutvartoMaster">
            <bpelx:annotation>
                <bpelx:documentation>This activity is used to assign the output
                                     message from DB Package to the Master

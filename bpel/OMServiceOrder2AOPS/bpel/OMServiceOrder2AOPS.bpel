    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="SrStatusOm2Aops">
        <!-- Receive input from requestor. (Note: This maps to operation defined in OMServiceOrder2AOPS.wsdl) -->
        <scope name="ScopeOm2Aops">
            <faultHandlers>
                <catchAll>
                    <sequence name="SequenceCatchAll">
                        <assign name="assignOMSRFaultCatch">
                            <copy>
                                <from expression="concat(ora:getProcessId(),ora:getProcessVersion())"/>
                                <to variable="invokeOMSRFaultCatch_InputVariable"
                                    part="payload"
                                    query="/ns9:ErrorData/ns9:ProcessInfo/ns9:BusinessProcessName"/>
                            </copy>
                            <copy>
                                <from expression="ora:getInstanceId()"/>
                                <to variable="invokeOMSRFaultCatch_InputVariable"
                                    part="payload"
                                    query="/ns9:ErrorData/ns9:ProcessInfo/ns9:BusinessProcessId"/>
                            </copy>
                            <copy>
                                <from expression="ora:getDomainId()"/>
                                <to variable="invokeOMSRFaultCatch_InputVariable"
                                    part="payload"
                                    query="/ns9:ErrorData/ns9:ProcessInfo/ns9:BusinessProcessDomain"/>
                            </copy>
                            <copy>
                                <from expression="'SR UPDATE TO AOPS'"/>
                                <to variable="invokeOMSRFaultCatch_InputVariable"
                                    part="payload"
                                    query="/ns9:ErrorData/ns9:ProcessInfo/ns9:SystemName"/>
                            </copy>
                            <copy>
                                <from expression="ora:getCurrentDateTime()"/>
                                <to variable="invokeOMSRFaultCatch_InputVariable"
                                    part="payload"
                                    query="/ns9:ErrorData/ns9:ErrorDetails/ns9:ErrorDateTime"/>
                            </copy>
                            <copy>
                                <from expression="ora:doc('${server_url}/xmllib/otc/GlobalParameters.xml','/GlobalParameters/OMServiceOrder2AOPS/FaultNotification/SendNotification')"/>
                                <to variable="invokeOMSRFaultCatch_InputVariable"
                                    part="payload"
                                    query="/ns9:ErrorData/ns9:Notification/ns9:sendNotification"/>
                            </copy>
                            <copy>
                                <from expression="'System'"/>
                                <to variable="invokeOMSRFaultCatch_InputVariable"
                                    part="payload"
                                    query="/ns9:ErrorData/ns9:ErrorDetails/ns9:ErrorType"/>
                            </copy>
                            <copy>
                                <from expression="'Fatal'"/>
                                <to variable="invokeOMSRFaultCatch_InputVariable"
                                    part="payload"
                                    query="/ns9:ErrorData/ns9:ErrorDetails/ns9:ErrorSeverity"/>
                            </copy>
                            <copy>
                                <from expression="ora:doc('${server_url}/xmllib/otc/GlobalParameters.xml','/GlobalParameters/OMServiceOrder2AOPS/FaultNotification/EmailTo')"/>
                                <to variable="invokeOMSRFaultCatch_InputVariable"
                                    part="payload"
                                    query="/ns9:ErrorData/ns9:Notification/ns9:RecipientList"/>
                            </copy>
                        </assign>
                        <invoke name="invokeOMSRFaultCatch"
                                partnerLink="ODErrorLogger"
                                portType="ns4:ODErrorLogger"
                                operation="initiate"
                                inputVariable="invokeOMSRFaultCatch_InputVariable"/>
                        <terminate name="terminateProcess"/>
                    </sequence>
                </catchAll>
            </faultHandlers>
            <sequence name="SequenceOm2AopsMain">
                <receive name="dequeueFromEbs"
                         partnerLink="dequeueSrupdate2Aops"
                         portType="ns11:Dequeue_ptt" operation="Dequeue"
                         variable="dequeueFromEbs_InputVariable"
                         createInstance="yes"/>
                <!--              <assign name="Assign_1">
                    <copy>
                    </copy>
                </assign>  -->
                <assign name="TransformSrStatus">
                    <bpelx:annotation>
                        <bpelx:pattern>transformation</bpelx:pattern>
                    </bpelx:annotation>
                    <copy>
                        <from expression="ora:processXSLT('TransformationSRStatus.xsl',bpws:getVariableData('dequeueFromEbs_InputVariable','Root-Element'))"/>
                        <to variable="EnqueueAops_InputVariable"
                            part="Root-Element"/>
                    </copy>
                </assign>
                <sequence name="SequenceInstanceTitle">
                    <assign name="assignInitTitle">
                        <copy>
                            <from variable="dequeueFromEbs_InputVariable"
                                  part="Root-Element"
                                  query="/ns18:Root-Element/ns18:ODTechService/ns18:request_number"/>
                            <to variable="globalSrNumber"/>
                        </copy>
                        <copy>
                            <from variable="dequeueFromEbs_InputVariable"
                                  part="Root-Element"
                                  query="/ns18:Root-Element/ns18:ODTechService/ns18:request_status"/>
                            <to variable="globalSrUpdate"/>
                        </copy>
                    </assign>
                    <bpelx:exec name="setInstanceTitle" language="java"
                                version="1.3">
                        <![CDATA[try {                                
  String title1 = "UpdateToAOPS_SR#";            
  String title2 = (String)getVariableData("globalSrNumber");        
  String title3 = (String)getVariableData("globalSrUpdate");   
  String title4 = "_" ; 
  String title = title1 + title2 + title4 + title3;            
  setTitle(title);                                
  addAuditTrailEntry(title) ;                                
} catch (Exception ex) {                                
  addAuditTrailEntry(ex.toString());                                
  ex.printStackTrace();                                
}]]>
                    </bpelx:exec>
                </sequence>
                <invoke name="EnqueueAops" partnerLink="enqueueSrupdate2Aops"
                        portType="ns12:Enqueue_ptt" operation="Enqueue"
                        inputVariable="EnqueueAops_InputVariable"/>
                <!--<scope name="ScopeEnqueue">
                   <faultHandlers>
                        <catchAll>
                            <sequence name="SequenceEnqueueExceptions">
                                <assign name="assignExceptions">
                                    <copy>
                                        <from expression="concat(ora:getProcessId(),ora:getProcessVersion())"/>
                                        <to variable="Invokeexceptions_InputVariable"
                                            part="payload"
                                            query="/ns15:ExceptionData/ns15:ProcessInfo/ns15:BusinessProcessName"/>
                                    </copy>
                                    <copy>
                                        <from expression="ora:getInstanceId()"/>
                                        <to variable="Invokeexceptions_InputVariable"
                                            part="payload"
                                            query="/ns15:ExceptionData/ns15:ProcessInfo/ns15:BusinessProcessId"/>
                                    </copy>
                                    <copy>
                                        <from expression="ora:getDomainId()"/>
                                        <to variable="Invokeexceptions_InputVariable"
                                            part="payload"
                                            query="/ns15:ExceptionData/ns15:ProcessInfo/ns15:BusinessProcessDomain"/>
                                    </copy>
                                    <copy>
                                        <from expression="'SR UPDATE TO AOPS'"/>
                                        <to variable="Invokeexceptions_InputVariable"
                                            part="payload"
                                            query="/ns15:ExceptionData/ns15:ProcessInfo/ns15:SystemName"/>
                                    </copy>
                                    <copy>
                                        <from expression="ora:getCurrentDateTime()"/>
                                        <to variable="Invokeexceptions_InputVariable"
                                            part="payload"
                                            query="/ns15:ExceptionData/ns15:ExceptionDetails/ns15:ExceptionDateTime"/>
                                    </copy>
                                    <copy>
                                        <from expression="ora:doc('${server_url}/xmllib/otc/GlobalParameters.xml','/GlobalParameters/OMServiceOrder2AOPS/FaultNotification/SendNotification')"/>
                                        <to variable="Invokeexceptions_InputVariable"
                                            part="payload"
                                            query="/ns15:ExceptionData/ns15:Notification/ns15:sendNotification"/>
                                    </copy>
                                    <copy>
                                        <from expression="ora:doc('${server_url}/xmllib/otc/GlobalParameters.xml','/GlobalParameters/OMServiceOrder2AOPS/FaultNotification/EmailTo')"/>
                                        <to variable="Invokeexceptions_InputVariable"
                                            part="payload"
                                            query="/ns15:ExceptionData/ns15:Notification/ns15:RecipientList"/>
                                    </copy>
                                </assign>
                                <invoke name="InvokeExceptions"
                                        partnerLink="SRexceptions"
                                        portType="ns13:SRexceptions"
                                        operation="initiate"
                                        inputVariable="Invokeexceptions_InputVariable"/>
                                <terminate name="terminateProcess"/>
                            </sequence>
                        </catchAll>
                    </faultHandlers> 
                    <sequence name="SequenceEnqueue"/>
                </scope> -->
            </sequence>
        </scope>
        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
    </sequence>
</process>
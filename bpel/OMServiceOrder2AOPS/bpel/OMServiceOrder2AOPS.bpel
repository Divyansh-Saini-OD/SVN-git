        <!-- <variable name="InvokeEnqueueAops_Enqueue_InputVariable"
                  messageType="ns12:Root-Element_msg"/>
        <variable name="receiveAqFromEbs_InputVariable"
                  messageType="ns11:Root-Element_msg"/> -->

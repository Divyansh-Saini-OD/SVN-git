        <variable name="dequeueFromEbs_InputVariable"
                  messageType="ns11:Root-Element_msg"/>
        <variable name="EnqueueAops_InputVariable"
                  messageType="ns12:Root-Element_msg"/>
        <variable name="globalSrNumber" type="xsd:string"/>
        <variable name="globalSrUpdate" type="xsd:string"/>
    </variables>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

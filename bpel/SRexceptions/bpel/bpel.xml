<?xml version = '1.0' encoding = 'UTF-8'?>
<BPELSuitcase>
   <BPELProcess id="SRexceptions" src="SRexceptions.bpel">
      <partnerLinkBindings>
         <partnerLinkBinding name="client">
            <property name="wsdlLocation">SRexceptions.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="EbsSRexceptionSrvc">
            <property name="wsdlLocation">EbsSRexceptionSrvc.wsdl</property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODErrorLogger">
            <property name="wsdlLocation">http://esbdev01.na.odcorp.net/orabpel/default/ODErrorLogger/ODErrorLogger?wsdl</property>
         </partnerLinkBinding>
      </partnerLinkBindings>
   </BPELProcess>
</BPELSuitcase>

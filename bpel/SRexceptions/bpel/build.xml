<?xml version="1.0" encoding="windows-1252" ?>
<project default="compile" basedir=".">
    <target name="compile">
       <echo>
--------------------------------------------------------------

| Compiling bpel processes ${process.name}, revision ${rev}

--------------------------------------------------------------

</echo>
<bpelc rev="${rev}" input="${process.dir}/bpel/bpel.xml" out="${process.dir}/output" home="${bpel.home}">
           <customize> 
           
        <partnerLinkBindings>
	      <partnerLinkBinding name="client">
	            <property name="wsdlLocation">SRexceptions.wsdl</property>
	      </partnerLinkBinding>
	      <partnerLinkBinding name="EbsSRexceptionSrvc">
	            <property name="wsdlLocation">EbsSRexceptionSrvc.wsdl</property>
	            <property name="retryInterval">60</property>
	       </partnerLinkBinding>
	       <partnerLinkBinding name="ODErrorLogger">
	            <property name="wsdlLocation">http://${hostname}:${http.port}/orabpel/${ODErrorLogger.domain}/ODErrorLogger/ODErrorLogger?wsdl</property>
	        </partnerLinkBinding>
         </partnerLinkBindings>
      	<activationAgents>
         <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="DequeueAopsScmPod">
            <!--             <property name="clusterGroupId">${process.name}_${cluster.group.id}</property>
             <property name="useJgroupConfigFile">${useJgroupConfigFile}</property -->
         </activationAgent>
      </activationAgents>
  </customize>
          </bpelc>
     </target>
</project>

  Purpose: Empty BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="ProcessCreditSettlement"
         targetNamespace="http://xmlns.oracle.com/ProcessCreditSettlement"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns4="http://www.openapplications.org/oagis/9"
         xmlns:ns7="http://xmlns.oracle.com/pcbpel/adapter/ftp/"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns5="http://www.openapplications.org/oagis/9/officedepot/1"
         xmlns:ns18="http://schemas.oracle.com/service/bpel/common"
         xmlns:client="http://xmlns.oracle.com/ProcessCreditSettlement"
         xmlns:ns6="ODError"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:ns11="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ns1="http://xmlns.officedepot.com/B2BAmexCreditSettlementOutbound"
         xmlns:ehdr="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/ftp/TDSettlementFTPRead/"
         xmlns:ns2="http://xmlns.oracle.com/ODErrorLogger"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns8="http://xmlns.oracle.com/ODTransactionLogger"
         xmlns:ns10="http://xmlns.oracle.com/TransactionData">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <partnerLink myRole="B2BAmexCreditSettlementOutboundRequester"
                 name="B2BAmexCreditSettlementOutbound"
                 partnerRole="B2BAmexCreditSettlementOutboundProvider"
                 partnerLinkType="ns1:B2BAmexCreditSettlementOutbound"/>
    <partnerLink myRole="ODErrorLoggerRequester" name="ODErrorLogger"
                 partnerRole="ODErrorLoggerProvider"
                 partnerLinkType="ns2:ODErrorLogger"/>
    <partnerLink myRole="Get_role" name="TDSettlementFTPRead"
                 partnerLinkType="ns3:Get_plt"/>
    <partnerLink myRole="ODTransactionLoggerRequester"
                 name="ODTransactionLogger"
                 partnerRole="ODTransactionLoggerProvider"
                 partnerLinkType="ns8:ODTransactionLogger"/>
  </partnerLinks>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <variable name="InvokeB2BAmexCreditSettlementOutbound_initiate_In"
              messageType="ns1:B2BAmexCreditSettlementOutboundRequestMessage"/>
    <variable name="ReceiveTDSettlement_Get_In"
              messageType="ns3:ProcessInvoice_msg"/>
    <variable name="invkCatchAll_initiate"
              messageType="ns2:ODErrorLoggerRequestMessage"/>
    <variable name="ErrorMessageText" type="xsd:string"/>
    <variable name="FileName_In" messageType="ns7:InboundHeader_msg"/>
    <variable name="ProcessHeader" type="xsd:string"/>
    <variable name="invkTransactionLogger_initiate_InputVariable"
              messageType="ns8:ODTransactionLoggerRequestMessage"/>
    <variable name="ReceiveB2BResponseIn"
              messageType="ns1:B2BAmexCreditSettlementOutboundResponseMessage"/>
    <variable name="xmlFragment" type="xsd:string"/>
    <variable name="RecordCount" type="xsd:string"/>
    <variable name="NumberOfInvoices" type="xsd:int"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="Sequence_4">
        <assign name="asnCatchAll">
          <copy>
            <from expression="concat(&quot;An error occured in ProcessCreditSettlement while processing the file &quot;,bpws:getVariableData('FileName_In','inboundHeader','/ns7:InboundFTPHeaderType/ns7:fileName'), &quot;.&quot;)"/>
            <to variable="ErrorMessageText"/>
          </copy>
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:ProcessInfo/ns6:BusinessProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getInstanceId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:ProcessInfo/ns6:BusinessProcessId"/>
          </copy>
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:ProcessInfo/ns6:BusinessProcessDomain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessURL()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:ProcessInfo/ns6:SystemName"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:ErrorDetails/ns6:ErrorDateTime"/>
          </copy>
          <copy>
            <from expression="'Catch All on the Main Sequence'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:ProcessInfo/ns6:BusinessProcessStep"/>
          </copy>
          <copy>
            <from expression="ora:getFaultName()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:ErrorDetails/ns6:ErrorCode"/>
          </copy>
          <copy>
            <from variable="ErrorMessageText"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:ErrorDetails/ns6:ErrorDescription"/>
          </copy>
          <copy>
            <from expression="ora:getFaultAsString()"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:ErrorDetails/ns6:ErrorText"/>
          </copy>
          <copy>
            <from expression="'system'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:ErrorDetails/ns6:ErrorType"/>
          </copy>
          <copy>
            <from expression="'fatal'"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:ErrorDetails/ns6:ErrorSeverity"/>
          </copy>
          <copy>
            <from expression="ora:doc('${server_url}/xmllib/fin/GlobalParameters.xml','/GlobalParameters/FaultNotification/SendNotification')"/>
            <to variable="invkCatchAll_initiate" part="payload"
                query="/ns6:ErrorData/ns6:Notification/ns6:sendNotification"/>
          </copy>
        </assign>
        <scope name="logTransactionErrorLogger">
          <sequence name="Sequence_6">
            <assign name="logTrnLoggerData">
              <copy>
                <from expression="'Error'"/>
                <to variable="invkTransactionLogger_initiate_InputVariable"
                    part="payload"
                    query="/ns10:TransactionData/ns10:ProcessInfo/ns10:ProcessStatus"/>
              </copy>
            </assign>
            <invoke name="invkTrnErrorLogger" partnerLink="ODTransactionLogger"
                    portType="ns8:ODTransactionLogger" operation="initiate"
                    inputVariable="invkTransactionLogger_initiate_InputVariable"/>
          </sequence>
        </scope>
        <invoke name="invkCatchAll" partnerLink="ODErrorLogger"
                portType="ns2:ODErrorLogger" operation="initiate"
                inputVariable="invkCatchAll_initiate"/>
        <terminate name="Terminate"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <receive name="ReceiveTDSettlement" createInstance="yes"
             partnerLink="TDSettlementFTPRead" portType="ns3:Get_ptt"
             operation="Get" variable="ReceiveTDSettlement_Get_In"
             bpelx:headerVariable="FileName_In"/>
    <scope name="LogTransactionData">
      <sequence name="Sequence_5">
        <assign name="copyTransactionData">
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:ProcessInfo/ns10:Domain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:ProcessInfo/ns10:ProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getInstanceId()"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:ProcessInfo/ns10:InstanceId"/>
          </copy>
          <copy>
            <from expression="'Start'"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:ProcessInfo/ns10:ProcessStatus"/>
          </copy>
          <copy>
            <from expression="'FTP'"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:MessageDetails/ns10:MessageOperation"/>
          </copy>
          <copy>
            <from expression="'Tech Depot Credit Settlement Extract'"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:MessageDetails/ns10:MessageSourceSystemComponent"/>
          </copy>
          <copy>
            <from expression="'Payload'"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:MessageDetails/ns10:MessageType"/>
          </copy>
          <copy>
            <from>
              <EntiltyList xmlns="http://xmlns.oracle.com/TransactionData">
                <EntityID EntityType="SourceFileName"/>
              </EntiltyList>
            </from>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload" query="/ns10:TransactionData/ns10:EntiltyList"/>
          </copy>
          <copy>
            <from expression="concat('&lt;EntityID EntityType=&quot;SourceFileName&quot;>', bpws:getVariableData('FileName_In','inboundHeader','/ns7:InboundFTPHeaderType/ns7:fileName'), '&lt;/EntityID>')"/>
            <to variable="xmlFragment"/>
          </copy>
          <copy>
            <from expression="ora:parseEscapedXML(bpws:getVariableData('xmlFragment'))"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:EntiltyList/ns10:EntityID[1]"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:ProcessInfo/ns10:transactionStartTime"/>
          </copy>
          <copy>
            <from expression="'I1287_Techdepot_Settlement'"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:ProcessInfo/ns10:SystemName"/>
          </copy>
          <copy>
            <from expression="'Tech Depot'"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:ProcessInfo/ns10:TradingPartnerDetails/ns10:TPFrom"/>
          </copy>
          <copy>
            <from expression="'Tech Depot Web Site'"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:MessageDetails/ns10:MessageSourceSystem"/>
          </copy>
          <copy>
            <from expression="'B2B- Amex'"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:ProcessInfo/ns10:TradingPartnerDetails/ns10:TPTo"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:MessageDetails/ns10:MessageDateTime"/>
          </copy>
        </assign>
        <invoke name="invkTransactionLogger" partnerLink="ODTransactionLogger"
                portType="ns8:ODTransactionLogger" operation="initiate"
                inputVariable="invkTransactionLogger_initiate_InputVariable"/>
      </sequence>
    </scope>
    <scope name="setProcessHeader">
      <sequence name="setProcessTitle">
        <assign name="initializeInstanceTitle">
          <copy>
            <from expression="substring( concat( &quot;Processing &quot;, bpws:getVariableData('FileName_In','inboundHeader','/ns7:InboundFTPHeaderType/ns7:fileName')),1,50)"/>
            <to variable="ProcessHeader"/>
          </copy>
        </assign>
        <bpelx:exec name="setTitle" language="java" version="1.5">
          <![CDATA[try {             
                         String title = (String)getVariableData("ProcessHeader");             
                         setTitle(title);             
                         addAuditTrailEntry(title) ;             
                       } catch (Exception ex) {             
                         addAuditTrailEntry(ex.toString());             
                         ex.printStackTrace();             
          }]]>
        </bpelx:exec>
      </sequence>
    </scope>
    <assign name="NumberOfInvoices">
      <copy>
        <from expression="ora:countNodes('ReceiveTDSettlement_Get_In','ProcessInvoice','/ns4:ProcessInvoice/ns4:DataArea/ns4:Invoice/ns4:InvoiceHeader')"/>
        <to variable="NumberOfInvoices"/>
      </copy>
    </assign>
    <assign name="AssignB2BAmexSettlement_In">
      <copy>
        <from variable="ReceiveTDSettlement_Get_In" part="ProcessInvoice"/>
        <to variable="InvokeB2BAmexCreditSettlementOutbound_initiate_In"
            part="payload"/>
      </copy>
    </assign>
    <invoke name="InvokeB2BAmexCreditSettlementOutbound"
            partnerLink="B2BAmexCreditSettlementOutbound"
            portType="ns1:B2BAmexCreditSettlementOutbound" operation="initiate"
            inputVariable="InvokeB2BAmexCreditSettlementOutbound_initiate_In"/>
    <receive name="ReceiveB2BResponse"
             partnerLink="B2BAmexCreditSettlementOutbound"
             portType="ns1:B2BAmexCreditSettlementOutboundCallback"
             operation="onResult" variable="ReceiveB2BResponseIn"
             createInstance="no"/>
    <scope name="logTransactionEnd">
      <sequence name="Sequence_7">
        <assign name="logTransactionEndData">
          <copy>
            <from expression="'Success'"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload"
                query="/ns10:TransactionData/ns10:ProcessInfo/ns10:ProcessStatus"/>
          </copy>
          <copy>
            <from expression="concat('&lt;EntityList xmlns=&quot;http://xmlns.oracle.com/TransactionData&quot;>&lt;EntityID EntityType=&quot;SourceFileName&quot;>',bpws:getVariableData('FileName_In','inboundHeader','/ns7:InboundFTPHeaderType/ns7:fileName'),'&lt;/EntityID>&lt;EntityID EntityType=&quot;B2BResponse&quot;>',bpws:getVariableData('ReceiveB2BResponseIn','payload','/ns1:B2BAmexCreditSettlementOutboundProcessResponse/ns1:result'),'&lt;/EntityID>&lt;EntityID EntityType=&quot;NumberOfInvoices&quot;>',bpws:getVariableData('NumberOfInvoices'),'&lt;/EntityID>&lt;EntityID EntityType=&quot;ReleaseESPJob&quot;>NO&lt;/EntityID>&lt;/EntityList>')"/>
            <to variable="xmlFragment"/>
          </copy>
          <copy>
            <from expression="ora:parseEscapedXML(bpws:getVariableData('xmlFragment'))"/>
            <to variable="invkTransactionLogger_initiate_InputVariable"
                part="payload" query="/ns10:TransactionData/ns10:EntiltyList"/>
          </copy>
        </assign>
        <invoke name="invkLogTransactionEndData"
                partnerLink="ODTransactionLogger"
                portType="ns8:ODTransactionLogger" operation="initiate"
                inputVariable="invkTransactionLogger_initiate_InputVariable"/>
      </sequence>
    </scope>
  </sequence>
</process>
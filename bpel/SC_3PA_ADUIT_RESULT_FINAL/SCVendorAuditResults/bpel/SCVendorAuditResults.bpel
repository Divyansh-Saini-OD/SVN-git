-->
<process name="SCVendorAuditResults"
         targetNamespace="http://xmlns.oracle.com/SCVendorAuditResults"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns4="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ns7="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns5="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:client="http://xmlns.oracle.com/SCVendorAuditResults"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:ns6="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ns9="http://www.example.org"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/apps/OASrvcAuditResult/"
         xmlns:ns3="ODError" xmlns:ns2="http://xmlns.oracle.com/ODErrorLogger"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns10="http://xmlns.oracle.com/ScAudirResultsResponse"
         xmlns:ns8="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_QA_SC_VEN_3PA_ADT_BPEL_PKG/VENDOR_AUDIT_RESULT/">
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <partnerLinks>
        <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
        <partnerLink name="client" partnerLinkType="client:SCVendorAuditResults"
                     myRole="SCVendorAuditResultsProvider"
                     partnerRole="SCVendorAuditResultsRequester"/>
        <partnerLink name="OASrvcAuditResult"
                     partnerRole="OASrvcAuditResult_role"
                     partnerLinkType="ns1:OASrvcAuditResult_plt"/>
        <partnerLink myRole="ODErrorLoggerRequester" name="ODErrorLogger"
                     partnerRole="ODErrorLoggerProvider"
                     partnerLinkType="ns2:ODErrorLogger"/>
    </partnerLinks>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <variables>
        <!-- Reference to the message passed as input during initiation -->
        <variable name="inputVariable"
                  messageType="client:SCVendorAuditResultsRequestMessage"/>
        <!-- Reference to the message that will be sent back to the requester during callback -->
        <variable name="outputVariable"
                  messageType="client:SCVendorAuditResultsResponseMessage"/>
        <variable name="InvokeOASrvc_InputVariable"
                  messageType="ns1:args_in_msg"/>
        <variable name="InvokeOASrvc_OutputVariable"
                  messageType="ns1:args_out_msg"/>
        <variable name="InvokeCatchAll_InputVariable"
                  messageType="ns2:ODErrorLoggerRequestMessage"/>
        <variable name="GlobalTStatus" type="xsd:string"/>
        <variable name="GlobalInspNo" type="xsd:string"/>
        <variable name="GlobalVendorNo" type="xsd:string"/>
    </variables>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="SCAuditResultsMain">
        <!-- Receive input from requestor. (Note: This maps to operation defined in SCVendorAuditResults.wsdl) -->
        <scope name="ScopeScAuditResults">
            <faultHandlers>
                <catchAll>
                    <sequence name="SCAuditResultsCatchAll">
                        <assign name="AssignCatchAll">
                            <copy>
                                <from expression="concat(ora:getProcessId(),ora:getProcessVersion())"/>
                                <to variable="InvokeCatchAll_InputVariable"
                                    part="payload"
                                    query="/ns3:ErrorData/ns3:ProcessInfo/ns3:BusinessProcessName"/>
                            </copy>
                            <copy>
                                <from expression="ora:getInstanceId()"/>
                                <to variable="InvokeCatchAll_InputVariable"
                                    part="payload"
                                    query="/ns3:ErrorData/ns3:ProcessInfo/ns3:BusinessProcessId"/>
                            </copy>
                            <copy>
                                <from expression="ora:getDomainId()"/>
                                <to variable="InvokeCatchAll_InputVariable"
                                    part="payload"
                                    query="/ns3:ErrorData/ns3:ProcessInfo/ns3:BusinessProcessDomain"/>
                            </copy>
                            <copy>
                                <from expression="'SC VENDOR AUDIT RESULTS'"/>
                                <to variable="InvokeCatchAll_InputVariable"
                                    part="payload"
                                    query="/ns3:ErrorData/ns3:ProcessInfo/ns3:SystemName"/>
                            </copy>
                            <copy>
                                <from expression="ora:getCurrentDateTime()"/>
                                <to variable="InvokeCatchAll_InputVariable"
                                    part="payload"
                                    query="/ns3:ErrorData/ns3:ErrorDetails/ns3:ErrorDateTime"/>
                            </copy>
                            <copy>
                                <from expression='"Catch All at Dequeue vendor Ack message"'/>
                                <to variable="InvokeCatchAll_InputVariable"
                                    part="payload"
                                    query="/ns3:ErrorData/ns3:ProcessInfo/ns3:BusinessProcessStep"/>
                            </copy>
                            <copy>
                                <from expression='"System"'/>
                                <to variable="InvokeCatchAll_InputVariable"
                                    part="payload"
                                    query="/ns3:ErrorData/ns3:ErrorDetails/ns3:ErrorType"/>
                            </copy>
                        </assign>
                        <invoke name="InvokeCatchAll"
                                partnerLink="ODErrorLogger"
                                portType="ns2:ODErrorLogger"
                                operation="initiate"
                                inputVariable="InvokeCatchAll_InputVariable"/>
                        <assign name="AssignErrResponse">
                            <copy>
                                <from expression="'E'"/>
                                <to variable="outputVariable" part="payload"
                                    query="/ns10:ScAudirResultsResponse/ns10:ReturnCode"/>
                            </copy>
                            <copy>
                                <from expression="'Catch All Exception'"/>
                                <to variable="outputVariable" part="payload"
                                    query="/ns10:ScAudirResultsResponse/ns10:ReturnMsg"/>
                            </copy>
                        </assign>
                        <reply name="ReplyErrResponse" partnerLink="client"
                               portType="client:SCVendorAuditResults"
                               operation="initiate" variable="outputVariable"/>
                        <terminate name="TerminateProcess"/>
                    </sequence>
                </catchAll>
            </faultHandlers>
            <sequence name="SCAuditResultsSrvc">
                <receive name="receiveInput" partnerLink="client"
                         portType="client:SCVendorAuditResults"
                         operation="initiate" variable="inputVariable"
                         createInstance="yes"/>
                <assign name="TransformAuditResult">
                    <bpelx:annotation>
                        <bpelx:pattern>transformation</bpelx:pattern>
                    </bpelx:annotation>
                    <copy>
                        <from expression="ora:processXSLT('TransformMsg.xsl',bpws:getVariableData('inputVariable','payload'))"/>
                        <to variable="InvokeOASrvc_InputVariable"
                            part="InputParameters"/>
                    </copy>
                </assign>
                <assign name="AssignTitleValues">
                    <copy>
                        <from variable="inputVariable" part="payload"
                              query="/ns9:ComplainceReport/ns9:AuditRecord/ns9:TransmissionStatus"/>
                        <to variable="GlobalTStatus"/>
                    </copy>
                    <copy>
                        <from variable="inputVariable" part="payload"
                              query="/ns9:ComplainceReport/ns9:AuditRecord/ns9:InspectionNo"/>
                        <to variable="GlobalInspNo"/>
                    </copy>
                    <copy>
                        <from variable="inputVariable" part="payload"
                              query="/ns9:ComplainceReport/ns9:AuditRecord/ns9:Vendor/ns9:ODVendorNo"/>
                        <to variable="GlobalVendorNo"/>
                    </copy>
                </assign>
                <bpelx:exec name="setTitle" language="java" version="1.3">
                    <![CDATA[try {                            
  String title1 = (String)getVariableData("GlobalTStatus");        
  String title2 = (String)getVariableData("GlobalVendorNo");      
  String title3 = (String)getVariableData("GlobalInspNo");      
  String title4 = "_" ;    
  String title5 = "Insp#";   
  String title6 = "Vendor#";   
  String title = title1 + title4 + title6 +title2 + title4 + title5 + title3;        
  setTitle(title);                            
  addAuditTrailEntry(title) ;                            
} catch (Exception ex) {                            
  addAuditTrailEntry(ex.toString());                            
  ex.printStackTrace();                            
}]]>
                </bpelx:exec>
                <invoke name="InvokeOASrvc" partnerLink="OASrvcAuditResult"
                        portType="ns1:OASrvcAuditResult_ptt"
                        operation="OASrvcAuditResult"
                        inputVariable="InvokeOASrvc_InputVariable"
                        outputVariable="InvokeOASrvc_OutputVariable"/>
            </sequence>
        </scope>
        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <assign name="TransformResponse">
            <bpelx:annotation>
                <bpelx:pattern>transformation</bpelx:pattern>
            </bpelx:annotation>
            <copy>
                <from expression="ora:processXSLT('TransformResponse.xsl',bpws:getVariableData('InvokeOASrvc_OutputVariable','OutputParameters'))"/>
                <to variable="outputVariable" part="payload"/>
            </copy>
        </assign>
        <invoke name="callbackClient" partnerLink="client"
                portType="client:SCVendorAuditResultsCallback"
                operation="onResult" inputVariable="outputVariable"/>
    </sequence>
</process>
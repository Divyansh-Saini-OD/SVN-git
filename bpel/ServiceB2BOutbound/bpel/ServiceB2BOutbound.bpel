<process name="ServiceB2BOutbound"
         targetNamespace="http://xmlns.oracle.com/ServiceB2BOutbound"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns4="http://TargetNamespace.com/ServiceB2BOutbound"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:client="http://xmlns.oracle.com/ServiceB2BOutbound"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ns1="http://schemas.officedepot.com/ODTechServices/routeTDS"
         xmlns:ehdr="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ns3="ODError" xmlns:ns2="http://xmlns.oracle.com/ODErrorLogger"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="client" partnerLinkType="client:ServiceB2BOutbound"
                 myRole="ServiceB2BOutboundProvider"/>
    <partnerLink myRole="routeTDS_PortType_Role" name="routeTDS"
                 partnerRole="routeTDS_PortType_Role"
                 partnerLinkType="ns1:routeTDS_PortType_PL"/>
    <partnerLink myRole="ODErrorLoggerRequester" name="ODErrorLogger"
                 partnerRole="ODErrorLoggerProvider"
                 partnerLinkType="ns2:ODErrorLogger"/>
  </partnerLinks>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable"
              messageType="client:ServiceB2BOutboundRequestMessage"/>
    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable"
              messageType="client:ServiceB2BOutboundResponseMessage"/>
    <variable name="InvokeWebMethodsWS_InputVariable"
              messageType="ns1:routeTDS_PortType_routeTDS"/>
    <variable name="InvokeWebMethodsWS_OutputVariable"
              messageType="ns1:routeTDS_PortType_routeTDSResponse"/>
    <variable name="InvokeCatchAll_InputVariable"
              messageType="ns2:ODErrorLoggerRequestMessage"/>
  </variables>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <!-- Receive input from requestor. (Note: This maps to operation defined in ServiceB2BOutbound.wsdl) -->
    <scope name="ScopeB2BOutbound">
      <faultHandlers>
        <catchAll>
          <sequence name="SequenceCatchAll">
            <assign name="AssignCatchAll">
              <copy>
                <from expression="concat(ora:getProcessId(),ora:getProcessVersion())"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns3:ErrorData/ns3:ProcessInfo/ns3:BusinessProcessName"/>
              </copy>
              <copy>
                <from expression="ora:getInstanceId()"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns3:ErrorData/ns3:ProcessInfo/ns3:BusinessProcessId"/>
              </copy>
              <copy>
                <from expression="ora:getDomainId()"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns3:ErrorData/ns3:ProcessInfo/ns3:BusinessProcessDomain"/>
              </copy>
              <copy>
                <from expression="'VENDOR OUTBOUND'"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns3:ErrorData/ns3:ProcessInfo/ns3:SystemName"/>
              </copy>
              <copy>
                <from expression="ora:getCurrentDateTime()"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns3:ErrorData/ns3:ErrorDetails/ns3:ErrorDateTime"/>
              </copy>
              <copy>
                <from expression="'Catch All at Dequeue vendor outbound message'"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns3:ErrorData/ns3:ProcessInfo/ns3:BusinessProcessStep"/>
              </copy>
              <copy>
                <from expression="'System'"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns3:ErrorData/ns3:ErrorDetails/ns3:ErrorType"/>
              </copy>
            </assign>
            <invoke name="InvokeCatchAll" partnerLink="ODErrorLogger"
                    portType="ns2:ODErrorLogger" operation="initiate"
                    inputVariable="InvokeCatchAll_InputVariable"/>
            <terminate name="terminateProcess"/>
          </sequence>
        </catchAll>
      </faultHandlers>
      <sequence name="SequenceB2BOutbound">
        <receive name="receiveInput" partnerLink="client"
                 portType="client:ServiceB2BOutbound" operation="process"
                 variable="inputVariable" createInstance="yes"/>
        <assign name="AssignMsgToWS">
          <copy>
            <from expression="ora:getContentAsString(bpws:getVariableData('inputVariable','payload'))"/>
            <to variable="InvokeWebMethodsWS_InputVariable" part="parameters"
                query="/ns1:routeTDS/TDSRequest/ns1:ODTechServiceRequest"/>
          </copy>
          <copy>
            <from expression="ora:parseEscapedXML(ora:getContentAsString(bpws:getVariableData('inputVariable','payload')))"/>
            <to variable="InvokeWebMethodsWS_InputVariable" part="parameters"
                query="/ns1:routeTDS/TDSRequest/ns1:ODTechServiceRequest"/>
          </copy>
        </assign>
        <invoke name="InvokeWebMethodsWS" partnerLink="routeTDS"
                portType="ns1:routeTDS_PortType" operation="routeTDS"
                inputVariable="InvokeWebMethodsWS_InputVariable"
                outputVariable="InvokeWebMethodsWS_OutputVariable"/>
        <assign name="TransformResponse">
          <bpelx:annotation>
            <bpelx:pattern>transformation</bpelx:pattern>
          </bpelx:annotation>
          <copy>
            <from expression="ora:processXSLT('TransformResponse_1.xsl',bpws:getVariableData('InvokeWebMethodsWS_OutputVariable','parameters'))"/>
            <to variable="outputVariable" part="payload"/>
          </copy>
        </assign>
        <reply name="replyOutput" partnerLink="client"
               portType="client:ServiceB2BOutbound" operation="process"
               variable="outputVariable"/>
      </sequence>
    </scope>
    <!-- Generate reply to synchronous request -->
  </sequence>
</process>
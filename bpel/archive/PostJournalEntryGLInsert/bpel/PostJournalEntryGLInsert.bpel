                    <copy>
                        <from expression="'1'"/>
                        <to variable="Invoke_ODErrorLogger_In"
                            query="/ns15:ErrorData/ns15:ErrorDetails/ns15:ErrorSeverity"
                            part="payload"/>
                    </copy>
                    <copy>

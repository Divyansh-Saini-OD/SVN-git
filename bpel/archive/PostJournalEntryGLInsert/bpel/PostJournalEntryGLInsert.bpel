                <invoke name="Invoke_ODErrorLogger" partnerLink="ODErrorLogger"
                        portType="ns6:ODErrorLogger" operation="initiate"
                        inputVariable="Invoke_ODErrorLogger_In"/>
            </sequence>
        </catch>
        <catch faultName="bpws:selectionFailure" faultVariable="RemoteFaultVar">
            <sequence name="Sequence_13">
                <assign name="Assign_Error_Data">
                    <copy>
                        <from expression="concat(ora:getProcessId(), ora:getProcessVersion())"/>
                        <to variable="Invoke_ODErrorLogger_In"
                            query="/ns15:ErrorData/ns15:ProcessInfo/ns15:BusinessProcessName"
                            part="payload"/>
                    </copy>
                    <copy>
                        <from expression="ora:getInstanceId()"/>
                        <to variable="Invoke_ODErrorLogger_In"

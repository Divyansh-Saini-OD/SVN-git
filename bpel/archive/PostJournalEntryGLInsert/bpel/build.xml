<?xml version="1.0" encoding="windows-1252" ?>
<project default="compile" basedir=".">
    <target name="compile">
       <echo>
--------------------------------------------------------------

| Compiling bpel process ${process.name}, revision ${rev}

--------------------------------------------------------------
        </echo>
        <bpelc rev="${rev}" input="${process.dir}/bpel/bpel.xml" out="${process.dir}/output" home="${bpel.home}">
           <customize> 
	         <partnerLinkBinding name="client">
	            <property name="wsdlLocation">PostJournalEntryGLInsert.wsdl</property>
	         </partnerLinkBinding>
	         <partnerLinkBinding name="GLInterfaceNASTGInsert">
	            <property name="wsdlLocation">GLInterfaceNASTGInsert.wsdl</property>
	            <property name="retryInterval">60</property>
	         </partnerLinkBinding>
	         <partnerLinkBinding name="ODErrorLogger">
	            <property name="wsdlLocation">http://${hostname}:${http.port}/orabpel/${ODErrorLogger.domain}/ODErrorLogger/ODErrorLogger?wsdl</property>
	         </partnerLinkBinding>
       </customize>
        </bpelc>
   </target>
</project>
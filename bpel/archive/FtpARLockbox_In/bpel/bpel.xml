<?xml version = '1.0' encoding = 'UTF-8'?>
<BPELSuitcase>
   <BPELProcess id="FtpARLockbox_In" src="FtpARLockbox_In.bpel">
      <partnerLinkBindings>
         <partnerLinkBinding name="ServiceFileRead">
            <property name="wsdlLocation">ServiceFileRead.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ServiceLockboxPutFile">
            <property name="wsdlLocation">ServiceLockboxPutFile.wsdl</property>
            <property name="retryInterval">60</property>
            <property name="Outbound_Path" type="LogicalDirectory">${ebs.integration.directory}xxfin/inbound</property>
            <property name="portType">Put_ptt</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODErrorLogger">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/default/ODErrorLogger/ODErrorLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="Service_XX_BPEL_INTEGRATION_PKG_Call">
            <property name="wsdlLocation">Service_XX_BPEL_INTEGRATION_PKG_Call.wsdl</property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODTransactionLogger">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/default/ODTransactionLogger/ODTransactionLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="BPELInvokeESP">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/fin/BPELInvokeESP/BPELInvokeESP?wsdl</property>
         </partnerLinkBinding>
      </partnerLinkBindings>
      <activationAgents>
         <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="ServiceFileRead">
            <property name="Inbound_Path" type="LogicalDirectory"></property>
            <property name="portType">Get_ptt</property>
            <property name="Archive_Path" type="LogicalArchiveDirectory"></property>
         </activationAgent>
      </activationAgents>
   </BPELProcess>
</BPELSuitcase>

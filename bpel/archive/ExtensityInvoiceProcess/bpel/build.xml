<?xml version="1.0" encoding="windows-1252" ?>
<project default="compile" basedir=".">
    <target name="compile">
       <echo>
--------------------------------------------------------------

| Compiling bpel process ${process.name}, revision ${rev}

--------------------------------------------------------------
        </echo>
        <bpelc rev="${rev}" input="${process.dir}/bpel/bpel.xml" out="${process.dir}/output" home="${bpel.home}">
           <customize> 
	         <partnerLinkBinding name="ExtensityInvoiceRead">
	            <property name="wsdlLocation">ExtensityInvoiceRead.wsdl</property>
	         </partnerLinkBinding>
	         <partnerLinkBinding name="ProcessInvoiceInbound_OAGIS">
	            <property name="wsdlLocation">http://${hostname}:${http.port}/orabpel/${ProcessInvoiceInbound_OAGIS.domain}/ProcessInvoiceInbound_OAGIS/ProcessInvoiceInbound_OAGIS?wsdl</property>
	         </partnerLinkBinding>
	         <partnerLinkBinding name="ODErrorLogger">
	            <property name="wsdlLocation">http://${hostname}:${http.port}/orabpel/${ODErrorLogger.domain}/ODErrorLogger/ODErrorLogger?wsdl</property>
	         </partnerLinkBinding>
	         <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="ExtensityInvoiceRead">
	            <property name="portType">Get_ptt</property>
	            <property name="FTPArchiveDIR">${ebs.archive.directory}xxfin/inbound</property>
	            <property name="FTPInboundDir">xxfin/ftp/in/invoice/nonstandard</property>
	         </activationAgent>
       </customize>
        </bpelc>
   </target>
</project>
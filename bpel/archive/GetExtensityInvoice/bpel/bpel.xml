<?xml version = '1.0' encoding = 'UTF-8'?>
<BPELSuitcase>
   <BPELProcess id="GetExtensityInvoice" src="GetExtensityInvoice.bpel">
      <partnerLinkBindings>
         <partnerLinkBinding name="readExtensityInvoice">
            <property name="wsdlLocation">readExtensityInvoice.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="writeExtensityInvoice">
            <property name="wsdlLocation">writeExtensityInvoice.wsdl</property>
            <property name="ExtensityInvoiceOut" type="LogicalDirectory"></property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODErrorLogger">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/default/ODErrorLogger/ODErrorLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODTransactionLogger">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/default/ODTransactionLogger/ODTransactionLogger?wsdl</property>
         </partnerLinkBinding>
      </partnerLinkBindings>
      <activationAgents>
         <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="readExtensityInvoice">
            <property name="ExtensityIn" type="LogicalDirectory"></property>
            <property name="portType">Get_ptt</property>
         </activationAgent>
      </activationAgents>
   </BPELProcess>
</BPELSuitcase>

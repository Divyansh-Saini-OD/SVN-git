<definitions
     name="ServiceFileRead"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/ftp/ServiceFileRead/"
     xmlns="http://schemas.xmlsoap.org/wsdl/"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/ftp/ServiceFileRead/"
     xmlns:ns1="http://schemas.oracle.com/service/bpel/common"
     xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
     xmlns:pc="http://xmlns.oracle.com/pcbpel/"
     xmlns:imp1="http://xmlns.oracle.com/pcbpel/adapter/file/attachment/"
     xmlns:hdr="http://xmlns.oracle.com/pcbpel/adapter/ftp/"
    >
    <import namespace="http://schemas.oracle.com/bpel/extension" location="RuntimeFault.wsdl"/>
    <import namespace="http://xmlns.oracle.com/pcbpel/adapter/ftp/" location="ftpAdapterInboundHeader.wsdl"/>
    <types>
        <schema xmlns="http://www.w3.org/2001/XMLSchema">
            <import namespace="http://xmlns.oracle.com/pcbpel/adapter/file/attachment/" schemaLocation="attachment.xsd"/>
        </schema>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://schemas.oracle.com/service/bpel/common" schemaLocation="params.xsd"/>
        </xsd:schema>
    </types>
    <message name="attachmentElement_msg">
        <part name="attachmentElement" element="imp1:attachmentElement"/>
    </message>
    <portType name="Get_ptt">
        <operation name="Get">
            <input message="tns:attachmentElement_msg"/>
        </operation>
    </portType>
    <binding name="Get_binding" type="tns:Get_ptt">
        <pc:inbound_binding/>
        <operation name="Get">
            <jca:operation FileType="binary" LogicalDirectory="Inbound_Path" ActivationSpec="oracle.tip.adapter.ftp.inbound.FTPActivationSpec"
                 LogicalArchiveDirectory="Archive_Path" DeleteFile="true" IncludeFiles="BNKSTMT.*"
                 ExcludeFiles=".*\.trg" PollingFrequency="1800" MinimumAge="60" FileModificationTime="FileSystem"
                 ModificationTimeFormat="4,18,yyyyMMddHHmmSS" OpaqueSchema="false" AsAttachment="true"/>
            <input>
                <jca:header message="hdr:InboundHeader_msg" part="inboundHeader"/>
            </input>
        </operation>
    </binding>
    <service name="ServiceFileRead">
        <port name="Get_pt" binding="tns:Get_binding">
            <jca:address location="eis/ftp/EBSCentralExternal" UIincludeWildcard="BNKSTMT*" UIexcludeWildcard="*.trg"/>
        </port>
    </service>
    <plt:partnerLinkType name="Get_plt">
        <plt:role name="Get_role">
            <plt:portType name="tns:Get_ptt"/>
        </plt:role>
    </plt:partnerLinkType>
</definitions>

            <to variable="FileNameOut" part="outboundHeader"
                query="/ns7:OutboundFTPHeaderType/ns7:fileName"/>
          </copy>
        </assign>
        <invoke name="Invoke_1" partnerLink="FilePutRemoteFault"
                portType="ns6:Put_ptt" operation="Put"
                inputVariable="InvokeFilePutRemoteFault"
                bpelx:inputHeaderVariable="FileNameOut"/>
      </sequence>
    </catch>
    <catchAll>
      <sequence name="Sequence_4">
        <assign name="asnCatchAll">
          <copy>

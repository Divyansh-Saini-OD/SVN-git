  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <receive name="ReceiveFileGet" createInstance="yes" portType="ns5:Get_ptt"
             variable="Receive_Get" partnerLink="SupplierReceivablesFileRead"
             operation="Get" bpelx:headerVariable="FileNameIn"/>
    <assign name="Transform">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:processXSLT('TransformationData.xsl',bpws:getVariableData('Receive_Get','Root-Element'))"/>
        <to variable="InvokeXX_AP_SCR_RECEIVER_STGInsert"
            part="XxApScrReceiverStgCollection"/>
      </copy>
    </assign>
    <invoke name="InvokeXX_AP_SCR_RECEIVER_STGInsert"
            partnerLink="XX_AP_SCR_RECEIVER_STGInsert"
            portType="ns3:XX_AP_SCR_RECEIVER_STGInsert_ptt" operation="insert"
            inputVariable="InvokeXX_AP_SCR_RECEIVER_STGInsert"/>
  </sequence>
</process>
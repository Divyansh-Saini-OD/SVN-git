                                          </xsd:complexType>
                                       </xsd:element>
                                       <xsd:element name="Auditors"
                                                    minOccurs="1" maxOccurs="1">
                                          <xsd:complexType>
                                             <xsd:sequence>
                                                <xsd:element name="Auditor"
                                                             minOccurs="1"
                                                             maxOccurs="unbounded">
                                                   <xsd:complexType>
                                                      <xsd:sequence>
                                                         <xsd:element name="OD_SC_AuditorName"
                                                                      type="xsd:string"/>
                                                         <xsd:element name="OD_SC_AuditorLevel"
                                                                      type="xsd:string"/>
                                                      </xsd:sequence>
                                                   </xsd:complexType>
                                                </xsd:element>
                                             </xsd:sequence>
                                          </xsd:complexType>
                                       </xsd:element>
                                       <xsd:element name="Findings"
                                                    minOccurs="0" maxOccurs="1">
                                          <xsd:complexType>
                                             <xsd:sequence>
                                                <xsd:element name="Finding"
                                                             minOccurs="0"
                                                             maxOccurs="unbounded">
                                                   <xsd:complexType>
                                                      <xsd:sequence>

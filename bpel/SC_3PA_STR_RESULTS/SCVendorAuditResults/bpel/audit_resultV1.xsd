                                                         <xsd:element name="SubSection"
                                                                      type="xsd:string"/>
                                                         <xsd:element name="Question"
                                                                      type="xsd:string"/>
                                                         <xsd:element name="AuditorComments"
                                                                      type="xsd:string"/>
                                                      </xsd:sequence>
                                                   </xsd:complexType>
                                                </xsd:element>
                                             </xsd:sequence>
                                          </xsd:complexType>
                                       </xsd:element>
                                    </xsd:sequence>
                                 </xsd:complexType>
                              </xsd:element>
                           </xsd:sequence>
                        </xsd:complexType>
                     </xsd:element>
                  </xsd:sequence>                                         
                         </xsd:complexType> 
                        </xsd:element>
                      </xsd:sequence>  
                    </xsd:complexType>  
                  </xsd:element>  

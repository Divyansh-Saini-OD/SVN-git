                                                                      type="xsd:string"/>
                                                         <xsd:element name="Country"
                                                                      type="xsd:string"/>
                                                      </xsd:sequence>
                                                   </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="VendorContacts"
                                                             minOccurs="1"
                                                             maxOccurs="1">
                                                   <xsd:complexType>
                                                      <xsd:sequence>
                                                         <xsd:element name="VendorContact"
                                                                      minOccurs="1"
                                                                      maxOccurs="unbounded">
                                                            <xsd:complexType>
                                                               <xsd:sequence>
                                                                  <xsd:element name="ContactName"
                                                                               type="xsd:string"/>

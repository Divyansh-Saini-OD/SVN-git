<?xml version = '1.0' encoding = 'UTF-8'?>
<BPELSuitcase>
   <BPELProcess id="SynchEBStoMVSData" src="SynchEBStoMVSData.bpel">
      <partnerLinkBindings>
         <partnerLinkBinding name="FTPFileGet">
            <property name="wsdlLocation">FTPFileGet.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="FTPFilePut">
            <property name="wsdlLocation">FTPFilePut.wsdl</property>
            <property name="retryInterval">60</property>
            <property name="Outbound_Path" type="LogicalDirectory"></property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODErrorLogger">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/default/ODErrorLogger/ODErrorLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODTransactionLogger">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/default/ODTransactionLogger/ODTransactionLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="BPELInvokeESP">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/fin/BPELInvokeESP/BPELInvokeESP?wsdl</property>
         </partnerLinkBinding>
      </partnerLinkBindings>
      <activationAgents>
         <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="FTPFileGet">
            <property name="MVSFileArchiveDir" type="LogicalArchiveDirectory"></property>
            <property name="portType">Get_ptt</property>
            <property name="MVSFileInDir" type="LogicalDirectory"></property>
         </activationAgent>
      </activationAgents>
   </BPELProcess>
</BPELSuitcase>

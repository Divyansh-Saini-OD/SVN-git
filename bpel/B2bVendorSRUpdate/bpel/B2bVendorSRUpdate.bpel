<process name="B2bVendorSRUpdate"
         targetNamespace="http://xmlns.oracle.com/B2bVendorSRUpdate"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns4="ODError"
         xmlns:ns7="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_CS_TDS_VEN_PKG/UPDATE_REQUEST/"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns5="ODSRExceptions"
         xmlns:client="http://xmlns.oracle.com/B2bVendorSRUpdate"
         xmlns:ns6="http://xmlns.oracle.com/pcbpel/adapter/apps/EbsvendorSrUpdate/"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ns1="http://TargetNamespace.com/serviceRequestUpdate"
         xmlns:ehdr="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ns3="http://xmlns.oracle.com/SRexceptions"
         xmlns:ns2="http://xmlns.oracle.com/ODErrorLogger"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="client" partnerLinkType="client:B2bVendorSRUpdate"
                 myRole="B2bVendorSRUpdateProvider"/>
    <partnerLink myRole="ODErrorLoggerRequester" name="ODErrorLogger"
                 partnerRole="ODErrorLoggerProvider"
                 partnerLinkType="ns2:ODErrorLogger"/>
    <partnerLink myRole="SRexceptionsRequester" name="SRexceptions"
                 partnerRole="SRexceptionsProvider"
                 partnerLinkType="ns3:SRexceptions"/>
    <partnerLink name="EbsvendorSrUpdate" partnerRole="EbsvendorSrUpdate_role"
                 partnerLinkType="ns6:EbsvendorSrUpdate_plt"/>
  </partnerLinks>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable"
              messageType="client:B2bVendorSRUpdateRequestMessage"/>
    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable"
              messageType="client:B2bVendorSRUpdateResponseMessage"/>
    <variable name="InvokeCatchAll_InputVariable"
              messageType="ns2:ODErrorLoggerRequestMessage"/>
    <variable name="InvokeSrexcepitons_InputVariable"
              messageType="ns3:SRexceptionsRequestMessage"/>
    <variable name="EbsvendorSrUpdate_InputVariable"
              messageType="ns6:args_in_msg"/>
    <variable name="EbsvendorSrUpdate_OutputVariable"
              messageType="ns6:args_out_msg"/>
  </variables>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="mainVendorSrUpdate">
    <!-- Receive input from requestor. (Note: This maps to operation defined in B2bVendorSRUpdate.wsdl) -->
    <scope name="ScopeMainVendorUpdate">
      <faultHandlers>
        <catchAll>
          <sequence name="SequenceCatchAll">
            <assign name="assignCatchAll">
              <copy>
                <from expression="ora:doc('${server_url}/xmllib/otc/GlobalParameters.xml','/GlobalParameters/OMServiceOrder2AOPS/FaultNotification/EmailTo')"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns4:ErrorData/ns4:Notification/ns4:RecipientList"/>
              </copy>
              <copy>
                <from expression="ora:doc('${server_url}/xmllib/otc/GlobalParameters.xml','/GlobalParameters/OMServiceOrder2AOPS/FaultNotification/SendNotification')"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns4:ErrorData/ns4:Notification/ns4:sendNotification"/>
              </copy>
              <copy>
                <from expression="concat(ora:getProcessId(),ora:getProcessVersion())"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns4:ErrorData/ns4:ProcessInfo/ns4:BusinessProcessName"/>
              </copy>
              <copy>
                <from expression="ora:getInstanceId()"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns4:ErrorData/ns4:ProcessInfo/ns4:BusinessProcessId"/>
              </copy>
              <copy>
                <from expression="ora:getDomainId()"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns4:ErrorData/ns4:ProcessInfo/ns4:BusinessProcessDomain"/>
              </copy>
              <copy>
                <from expression='"VENDOR UPDATE"'/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns4:ErrorData/ns4:ProcessInfo/ns4:SystemName"/>
              </copy>
              <copy>
                <from expression="ora:getCurrentDateTime()"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns4:ErrorData/ns4:ErrorDetails/ns4:ErrorDateTime"/>
              </copy>
              <copy>
                <from expression="bpws:getVariableData('inputVariable','payload','/ns1:serviceRequestUpdate')"/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns4:ErrorData/ns4:MessageDetails/ns4:MessageData"/>
              </copy>
              <copy>
                <from expression='"Catch All at Dequeue vendor update message"'/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns4:ErrorData/ns4:ProcessInfo/ns4:BusinessProcessStep"/>
              </copy>
              <copy>
                <from expression='"System"'/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns4:ErrorData/ns4:ErrorDetails/ns4:ErrorType"/>
              </copy>
              <copy>
                <from expression='"Fatal"'/>
                <to variable="InvokeCatchAll_InputVariable" part="payload"
                    query="/ns4:ErrorData/ns4:ErrorDetails/ns4:ErrorSeverity"/>
              </copy>
            </assign>
            <invoke name="invokeCatchAll" partnerLink="ODErrorLogger"
                    portType="ns2:ODErrorLogger" operation="initiate"
                    inputVariable="InvokeCatchAll_InputVariable"/>
          </sequence>
        </catchAll>
      </faultHandlers>
      <sequence name="SequenceVendorSrUpdate">
        <receive name="receiveVendorUpdate" partnerLink="client"
                 portType="client:B2bVendorSRUpdate" operation="process"
                 variable="inputVariable" createInstance="yes"/>
        <assign name="TransformSrStatus">
          <bpelx:annotation>
            <bpelx:pattern>transformation</bpelx:pattern>
          </bpelx:annotation>
          <copy>
            <from expression="ora:processXSLT('TransformVendorSrupdate.xsl',bpws:getVariableData('inputVariable','payload'))"/>
            <to variable="EbsvendorSrUpdate_InputVariable"
                part="InputParameters"/>
          </copy>
        </assign>
        <scope name="ScopeInvokeEbs">
          <faultHandlers>
            <catchAll>
              <sequence name="SequenceEbsExceptions">
                <assign name="assignEbsExceptions">
                  <copy>
                    <from expression="concat(ora:getProcessId(),ora:getProcessVersion())"/>
                    <to variable="InvokeSrexcepitons_InputVariable"
                        part="payload"
                        query="/ns5:ExceptionData/ns5:ProcessInfo/ns5:BusinessProcessName"/>
                  </copy>
                  <copy>
                    <from expression="ora:getInstanceId()"/>
                    <to variable="InvokeSrexcepitons_InputVariable"
                        part="payload"
                        query="/ns5:ExceptionData/ns5:ProcessInfo/ns5:BusinessProcessId"/>
                  </copy>
                  <copy>
                    <from expression="ora:getDomainId()"/>
                    <to variable="InvokeSrexcepitons_InputVariable"
                        part="payload"
                        query="/ns5:ExceptionData/ns5:ProcessInfo/ns5:BusinessProcessDomain"/>
                  </copy>
                  <copy>
                    <from expression="'VENDOR UPDATE'"/>
                    <to variable="InvokeSrexcepitons_InputVariable"
                        part="payload"
                        query="/ns5:ExceptionData/ns5:ProcessInfo/ns5:SystemName"/>
                  </copy>
                  <copy>
                    <from expression="ora:getCurrentDateTime()"/>
                    <to variable="InvokeSrexcepitons_InputVariable"
                        part="payload"
                        query="/ns5:ExceptionData/ns5:ExceptionDetails/ns5:ExceptionDateTime"/>
                  </copy>
                  <copy>
                    <from expression="ora:doc('${server_url}/xmllib/otc/GlobalParameters.xml','/GlobalParameters/OMServiceOrder2AOPS/FaultNotification/SendNotification')"/>
                    <to variable="InvokeSrexcepitons_InputVariable"
                        part="payload"
                        query="/ns5:ExceptionData/ns5:Notification/ns5:sendNotification"/>
                  </copy>
                </assign>
                <invoke name="InvokeEbsexcepitons" partnerLink="SRexceptions"
                        portType="ns3:SRexceptions" operation="initiate"
                        inputVariable="InvokeSrexcepitons_InputVariable"/>
              </sequence>
            </catchAll>
          </faultHandlers>
          <sequence name="SequenceInvokeEbs">
            <invoke name="nvokeEbsSrUpdate" partnerLink="EbsvendorSrUpdate"
                    portType="ns6:EbsvendorSrUpdate_ptt"
                    operation="EbsvendorSrUpdate"
                    inputVariable="EbsvendorSrUpdate_InputVariable"
                    outputVariable="EbsvendorSrUpdate_OutputVariable"/>
            <assign name="assignReturnStatus">
              <copy>
                <from variable="EbsvendorSrUpdate_OutputVariable"
                      part="OutputParameters"
                      query="/ns7:OutputParameters/ns7:X_RETURN_STATUS"/>
                <to variable="outputVariable" part="payload"
                    query="/client:B2bVendorSRUpdateProcessResponse/client:ReturnCode"/>
              </copy>
              <copy>
                <from variable="EbsvendorSrUpdate_OutputVariable"
                      part="OutputParameters"
                      query="/ns7:OutputParameters/ns7:X_MSG_DATA"/>
                <to variable="outputVariable" part="payload"
                    query="/client:B2bVendorSRUpdateProcessResponse/client:ReturnMsg"/>
              </copy>
            </assign>
          </sequence>
        </scope>
        <reply name="replyUpdateStatus" partnerLink="client"
               portType="client:B2bVendorSRUpdate" operation="process"
               variable="outputVariable"/>
      </sequence>
    </scope>
    <!-- Generate reply to synchronous request -->
  </sequence>
</process>
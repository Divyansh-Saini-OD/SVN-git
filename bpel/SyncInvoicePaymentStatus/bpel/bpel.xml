<?xml version = '1.0' encoding = 'UTF-8'?>
<BPELSuitcase>
   <BPELProcess id="SyncInvoicePaymentStatus" src="SyncInvoicePaymentStatus.bpel">
      <partnerLinkBindings>
         <partnerLinkBinding name="ODErrorLogger">
            <property name="wsdlLocation">http://esbdev01.na.odcorp.net/orabpel/default/ODErrorLogger/ODErrorLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="GetInvoiceStatusFromEBS">
            <property name="wsdlLocation">GetInvoiceStatusFromEBS.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ServiceEBSEscheats">
            <property name="wsdlLocation">ServiceEBSEscheats.wsdl</property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ServiceCentral">
            <property name="wsdlLocation">ServiceCentral.wsdl</property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ServiceMVS">
            <property name="wsdlLocation">ServiceMVS.wsdl</property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ServiceRetail">
            <property name="wsdlLocation">ServiceRetail.wsdl</property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="BPELInvokeESP">
            <property name="wsdlLocation">http://esbdev01.na.odcorp.net/orabpel/financebatch/BPELInvokeESP/BPELInvokeESP?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODTransactionLogger">
            <property name="wsdlLocation">http://esbdev01.na.odcorp.net/orabpel/default/ODTransactionLogger/ODTransactionLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ServiceDisputeResolution">
            <property name="wsdlLocation">ServiceDisputeResolution.wsdl</property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
      </partnerLinkBindings>
      <activationAgents>
         <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="GetInvoiceStatusFromEBS">
            <property name="InvoicePaymentStausInputDirectory" type="LogicalDirectory"></property>
            <property name="portType">Get_ptt</property>
            <property name="InvoicePaymentStausArchiveDirectory" type="LogicalArchiveDirectory"></property>
         </activationAgent>
      </activationAgents>
   </BPELProcess>
</BPELSuitcase>

<?xml version="1.0" encoding="windows-1252" ?>
<project default="compile" basedir=".">
    <target name="compile">

        <echo>
--------------------------------------------------------------
| Compiling bpel process ${process.name}, revision ${rev}
--------------------------------------------------------------
        </echo>

        <bpelc rev="${rev}" input="${process.dir}/bpel/bpel.xml" out="${process.dir}/output" home="${bpel.home}">
               <customize>
                 
                  <partnerLinkBinding name="DPSReleaseOrder_Child">
                    <property name="wsdlLocation">http://${http.hostname}:${http.port}/orabpel/${domain}/DPSReleaseOrder_Child/DPSReleaseOrder_Child?wsdl</property>
                    <property name="retryInterval">${db.retryInterval}</property>
                    <property name="retryMaxCount">${db.retryMaxCount}</property>
                    <property name="timeout">${db.timeout}</property>
				  </partnerLinkBinding>
                  <partnerLinkBinding name="DPSJobConfirmation_Child">
                   <property name="wsdlLocation">http://${http.hostname}:${http.port}/orabpel/${domain}/DPSJobConfirmation_Child/DPSJobConfirmation_Child?wsdl</property>
                    <property name="retryInterval">${db.retryInterval}</property>
                    <property name="retryMaxCount">${db.retryMaxCount}</property>
                    <property name="timeout">${db.timeout}</property>
                  </partnerLinkBinding>
                  <partnerLinkBinding name="ProcessNowDocsReceive">
                    <property name="wsdlLocation">http://${http.hostname}:${http.port}/orabpel/${domain}/ProcessNowDocsReceive/ProcessNowDocsReceive?wsdl</property>
                    <property name="retryInterval">${db.retryInterval}</property>
                    <property name="retryMaxCount">${db.retryMaxCount}</property>
                    <property name="timeout">${db.timeout}</property>
                  </partnerLinkBinding>
                  <partnerLinkBinding name="SendNowDocsMsgProcess">
                   <property name="wsdlLocation">http://${http.hostname}:${http.port}/orabpel/${domain}/SendNowDocsMsgProcess/SendNowDocsMsgProcess?wsdl</property>
                    <property name="retryInterval">${db.retryInterval}</property>
                    <property name="retryMaxCount">${db.retryMaxCount}</property>
                    <property name="timeout">${db.timeout}</property>
                  </partnerLinkBinding>
                  <partnerLinkBinding name="SendSalesOrderInfo">
                   <property name="wsdlLocation">http://${http.hostname}:${http.port}/orabpel/${domain}/SendSalesOrderInfo/SendSalesOrderInfo?wsdl</property>
                    <property name="retryInterval">${db.retryInterval}</property>
                    <property name="retryMaxCount">${db.retryMaxCount}</property>
                    <property name="timeout">${db.timeout}</property>
                  </partnerLinkBinding>
                  <partnerLinkBinding name="DPSCancel_SendSalesOrderInfo">
					<property name="wsdlLocation">http://${http.hostname}:${http.port}/orabpel/${domain}/DPSCancel_SendSalesOrderInfo/DPSCancel_SendSalesOrderInfo?wsdl</property>
                    <property name="retryInterval">${db.retryInterval}</property>
                    <property name="retryMaxCount">${db.retryMaxCount}</property>
                    <property name="timeout">${db.timeout}</property>
				  </partnerLinkBinding>
				  <partnerLinkBinding name="DPSCancel_SendNowDocsMsgProcess">
                    <property name="wsdlLocation">http://${http.hostname}:${http.port}/orabpel/${domain}//DPSCancel_SendNowDocsMsgProcess/DPSCancel_SendNowDocsMsgProcess?wsdl</property>
                    <property name="retryInterval">${db.retryInterval}</property>
                    <property name="retryMaxCount">${db.retryMaxCount}</property>
                    <property name="timeout">${db.timeout}</property>
                  </partnerLinkBinding>
              </customize>
            </bpelc>

    </target>
</project>
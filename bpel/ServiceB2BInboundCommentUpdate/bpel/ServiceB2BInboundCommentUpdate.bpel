<process name="ServiceB2BInboundCommentUpdate"
         targetNamespace="http://xmlns.oracle.com/ServiceB2BInboundCommentUpdate"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns4="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ns7="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns5="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:client="http://xmlns.oracle.com/ServiceB2BInboundCommentUpdate"
         xmlns:ns6="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:ns11="http://xmlns.oracle.com/B2bVendorSRUpdate"
         xmlns:ns9="ODError" xmlns:ns1="http://xmlns.oracle.com/ODErrorLogger"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/apps/VendorCommentUpdate/"
         xmlns:ns2="http://xmlns.oracle.com/SRexceptions"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns8="ODSRExceptions"
         xmlns:ns10="http://TargetNamespace.com/serviceB2BInboundCommentUpdate">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="client"
                 partnerLinkType="client:ServiceB2BInboundCommentUpdate"
                 myRole="ServiceB2BInboundCommentUpdateProvider"/>
    <partnerLink myRole="SRexceptionsProvider" name="SRexceptions"
                 partnerRole="SRexceptionsRequester"
                 partnerLinkType="ns2:SRexceptions"/>
    <partnerLink name="VendorCommentUpdate"
                 partnerRole="VendorCommentUpdate_role"
                 partnerLinkType="ns3:VendorCommentUpdate_plt"/>
    <partnerLink myRole="ODErrorLoggerRequester" name="ODErrorLogger"
                 partnerRole="ODErrorLoggerProvider"
                 partnerLinkType="ns1:ODErrorLogger"/>
  </partnerLinks>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable"
              messageType="client:ServiceB2BInboundCommentUpdateRequestMessage"/>
    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable"
              messageType="client:ServiceB2BInboundCommentUpdateResponseMessage"/>
    <variable name="InvokeSRexceptions_InputVariable"
              messageType="ns2:SRexceptionsResponseMessage"/>
    <variable name="InvokeCommentUpdate_InputVariable"
              messageType="ns3:args_in_msg"/>
    <variable name="InvokeCommentUpdate_OutputVariable"
              messageType="ns3:args_out_msg"/>
    <variable name="InvokeCatch_InputVariable"
              messageType="ns1:ODErrorLoggerRequestMessage"/>
    <variable name="globalSrNumber" type="xsd:string"/>
  </variables>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <!-- Receive input from requestor. (Note: This maps to operation defined in ServiceB2BInboundCommentUpdate.wsdl) -->
    <scope name="Scope_1">
      <faultHandlers>
        <catchAll>
          <sequence name="SequenceCatchAll">
            <assign name="assignErrResponse">
              <copy>
                <from expression="'F'"/>
                <to variable="outputVariable" part="payload"
                    query="/ns11:B2bVendorSRUpdateProcessResponse/ns11:ReturnCode"/>
              </copy>
              <copy>
                <from expression="'Error in Dequeue of Paylaod'"/>
                <to variable="outputVariable" part="payload"
                    query="/ns11:B2bVendorSRUpdateProcessResponse/ns11:ReturnMsg"/>
              </copy>
            </assign>
            <reply name="reeplyErrResponse" partnerLink="client"
                   portType="client:ServiceB2BInboundCommentUpdate"
                   operation="process" variable="outputVariable"/>
            <assign name="AssignCatch">
              <copy>
                <from expression="concat(ora:getProcessId(),ora:getProcessVersion())"/>
                <to variable="InvokeCatch_InputVariable" part="payload"
                    query="/ns9:ErrorData/ns9:ProcessInfo/ns9:BusinessProcessName"/>
              </copy>
              <copy>
                <from expression="ora:getInstanceId()"/>
                <to variable="InvokeCatch_InputVariable" part="payload"
                    query="/ns9:ErrorData/ns9:ProcessInfo/ns9:BusinessProcessId"/>
              </copy>
              <copy>
                <from expression="ora:getDomainId()"/>
                <to variable="InvokeCatch_InputVariable" part="payload"
                    query="/ns9:ErrorData/ns9:ProcessInfo/ns9:BusinessProcessDomain"/>
              </copy>
              <copy>
                <from expression="'VENDOR COMMENT UPDATE'"/>
                <to variable="InvokeCatch_InputVariable" part="payload"
                    query="/ns9:ErrorData/ns9:ProcessInfo/ns9:SystemName"/>
              </copy>
              <copy>
                <from expression="ora:getCurrentDateTime()"/>
                <to variable="InvokeCatch_InputVariable" part="payload"
                    query="/ns9:ErrorData/ns9:ErrorDetails/ns9:ErrorDateTime"/>
              </copy>
              <copy>
                <from expression='"System"'/>
                <to variable="InvokeCatch_InputVariable" part="payload"
                    query="/ns9:ErrorData/ns9:ErrorDetails/ns9:ErrorType"/>
              </copy>
            </assign>
            <invoke name="InvokeCatch" partnerLink="ODErrorLogger"
                    portType="ns1:ODErrorLogger" operation="initiate"
                    inputVariable="InvokeCatch_InputVariable"/>
            <terminate name="terminateProcess"/>
          </sequence>
        </catchAll>
      </faultHandlers>
      <sequence name="SequencecommentUpdate">
        <receive name="receiveCommentUpdate" partnerLink="client"
                 portType="client:ServiceB2BInboundCommentUpdate"
                 operation="process" variable="inputVariable"
                 createInstance="yes"/>
        <sequence name="SequenceInstanceTitle">
          <sequence name="SequenceTitleUpdate">
            <assign name="initializeTitle">
              <copy>
                <from variable="inputVariable" part="payload"
                      query="/ns10:serviceRequestCommentUpdate/ns10:serviceRequestNo"/>
                <to variable="globalSrNumber"/>
              </copy>
            </assign>
            <bpelx:exec name="setInstanceTitle" language="java" version="1.3">
              <![CDATA[try {                              
  String title1 = "ServcB2BCommUpdate_SR#";          
  String title2 = (String)getVariableData("globalSrNumber");          
  String title = title1 + title2;          
  setTitle(title);                              
  addAuditTrailEntry(title) ;                              
} catch (Exception ex) {                              
  addAuditTrailEntry(ex.toString());                              
  ex.printStackTrace();                              
}]]>
            </bpelx:exec>
          </sequence>
        </sequence>
        <scope name="InvokeEbs">
          <faultHandlers>
            <catchAll>
              <sequence name="SequenceSRexceptions">
                <assign name="assignErrResponse">
                  <copy>
                    <from expression="'F'"/>
                    <to variable="outputVariable" part="payload"
                        query="/ns11:B2bVendorSRUpdateProcessResponse/ns11:ReturnCode"/>
                  </copy>
                  <copy>
                    <from expression="'Error in Execution of EBZ API'"/>
                    <to variable="outputVariable" part="payload"
                        query="/ns11:B2bVendorSRUpdateProcessResponse/ns11:ReturnMsg"/>
                  </copy>
                </assign>
                <reply name="replyErrResponse" partnerLink="client"
                       portType="client:ServiceB2BInboundCommentUpdate"
                       operation="process" variable="outputVariable"/>
                <assign name="AssignSRexceptions">
                  <copy>
                    <from expression="concat(ora:getProcessId(),ora:getProcessVersion())"/>
                    <to variable="InvokeSRexceptions_InputVariable"
                        part="payload"
                        query="/ns8:ExceptionData/ns8:ProcessInfo/ns8:BusinessProcessName"/>
                  </copy>
                  <copy>
                    <from expression="ora:getInstanceId()"/>
                    <to variable="InvokeSRexceptions_InputVariable"
                        part="payload"
                        query="/ns8:ExceptionData/ns8:ProcessInfo/ns8:BusinessProcessId"/>
                  </copy>
                  <copy>
                    <from expression="ora:getDomainId()"/>
                    <to variable="InvokeSRexceptions_InputVariable"
                        part="payload"
                        query="/ns8:ExceptionData/ns8:ProcessInfo/ns8:BusinessProcessDomain"/>
                  </copy>
                  <copy>
                    <from expression="'VENDOR COMMENT UPDATE'"/>
                    <to variable="InvokeSRexceptions_InputVariable"
                        part="payload"
                        query="/ns8:ExceptionData/ns8:ProcessInfo/ns8:SystemName"/>
                  </copy>
                  <copy>
                    <from expression="ora:getCurrentDateTime()"/>
                    <to variable="InvokeSRexceptions_InputVariable"
                        part="payload"
                        query="/ns8:ExceptionData/ns8:ExceptionDetails/ns8:ExceptionDateTime"/>
                  </copy>
                  <copy>
                    <from variable="inputVariable" part="payload"/>
                    <to variable="InvokeSRexceptions_InputVariable"
                        part="payload"
                        query="/ns8:ExceptionData/ns8:MessageDetails/ns8:MessageType"/>
                  </copy>
                </assign>
                <invoke name="InvokeSRexceptions" partnerLink="SRexceptions"
                        portType="ns2:SRexceptionsCallback" operation="onResult"
                        inputVariable="InvokeSRexceptions_InputVariable"/>
                <terminate name="terminateProcess"/>
              </sequence>
            </catchAll>
          </faultHandlers>
          <sequence name="SequenceInvokeEBZ">
            <assign name="TransCommentUpdate">
              <bpelx:annotation>
                <bpelx:pattern>transformation</bpelx:pattern>
              </bpelx:annotation>
              <copy>
                <from expression="ora:processXSLT('TransformCommentUpdate.xsl',bpws:getVariableData('inputVariable','payload'))"/>
                <to variable="InvokeCommentUpdate_InputVariable"
                    part="InputParameters"/>
              </copy>
            </assign>
            <invoke name="InvokeCommentUpdate" partnerLink="VendorCommentUpdate"
                    portType="ns3:VendorCommentUpdate_ptt"
                    operation="VendorCommentUpdate"
                    inputVariable="InvokeCommentUpdate_InputVariable"
                    outputVariable="InvokeCommentUpdate_OutputVariable"/>
            <assign name="TransCommentUpdateResponse">
              <bpelx:annotation>
                <bpelx:pattern>transformation</bpelx:pattern>
              </bpelx:annotation>
              <copy>
                <from expression="ora:processXSLT('TransCommentUpdResponse.xsl',bpws:getVariableData('InvokeCommentUpdate_OutputVariable','OutputParameters'))"/>
                <to variable="outputVariable" part="payload"/>
              </copy>
            </assign>
          </sequence>
        </scope>
        <reply name="replyCommentUpdateResponse" partnerLink="client"
               portType="client:ServiceB2BInboundCommentUpdate"
               operation="process" variable="outputVariable"/>
      </sequence>
    </scope>
    <!-- Generate reply to synchronous request -->
  </sequence>
</process>
<definitions
     name="ServiceFileRead"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/ftp/ServiceFileRead/"
     xmlns="http://schemas.xmlsoap.org/wsdl/"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/ftp/ServiceFileRead/"
     xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
     xmlns:pc="http://xmlns.oracle.com/pcbpel/"
     xmlns:imp1="http://www.openapplications.org/oagis/9/officedepot/1"
     xmlns:hdr="http://xmlns.oracle.com/pcbpel/adapter/ftp/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    >
    <import namespace="http://xmlns.oracle.com/pcbpel/adapter/ftp/" location="ftpAdapterInboundHeader.wsdl"/>
    <import namespace="http://schemas.oracle.com/bpel/extension" location="RuntimeFault.wsdl"/>
    <types>
      <schema xmlns="http://www.w3.org/2001/XMLSchema" >
        <import namespace="http://www.openapplications.org/oagis/9/officedepot/1" schemaLocation="XXARINVPRNTSLCTEDI.xsd" />
        <xsd:import namespace="http://schemas.oracle.com/service/bpel/common" schemaLocation="params.xsd"/>        
      </schema>
    </types>
    <message name="XXARINVPRNEDI_msg">
        <part name="XXARINVPRNEDI" element="imp1:XXARINVPRNEDI"/>
    </message>
    <portType name="Get_ptt">
        <operation name="Get">
            <input message="tns:XXARINVPRNEDI_msg"/>
        </operation>
    </portType>
    <binding name="Get_binding" type="tns:Get_ptt">
    <pc:inbound_binding  />
        <operation name="Get">
      <jca:operation
          FileType="binary"
          LogicalDirectory="Logical_Path"
          ActivationSpec="oracle.tip.adapter.ftp.inbound.FTPActivationSpec"
          LogicalArchiveDirectory="Archive_Path"
          LogicalErrorArchiveDirectory="ErrorArchive_Path"
          DeleteFile="true"
          IncludeFiles="XX_AR_EDI.*\.xml"
          PublishSize="30"
          PollingFrequency="1800"
          MinimumAge="300"
          FileModificationTime="FileSystem"
          ModificationTimeFormat="4,18,yyyyMMddHHmmSS"
          OpaqueSchema="false" >
      </jca:operation>
      <input>
        <jca:header message="hdr:InboundHeader_msg" part="inboundHeader"/>
      </input>
        </operation>
    </binding>
    <service name="ServiceFileRead">
        <port name="Get_pt" binding="tns:Get_binding">
      <jca:address location="eis/ftp/EBSCentralExternal" UIincludeWildcard="XX_AR_EDI*.xml" />
        </port>
    </service>
  <plt:partnerLinkType name="Get_plt" >
    <plt:role name="Get_role" >
      <plt:portType name="tns:Get_ptt" />
    </plt:role>
  </plt:partnerLinkType>
</definitions>


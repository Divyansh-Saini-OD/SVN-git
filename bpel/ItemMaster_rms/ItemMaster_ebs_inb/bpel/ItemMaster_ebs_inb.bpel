                        <switch name="Switch_1">
                            <case condition="bpws:getVariableData('inputVariable','payload','/ns1:SYNC_ITEM_MASTER/ns1:VERB/ns1:ACTION_TYPE')='ITEM_MASTER' or bpws:getVariableData('inputVariable','payload','/ns1:SYNC_ITEM_MASTER/ns1:VERB/ns1:ACTION_TYPE')='ITEM_LOC'">
                                <bpelx:annotation>
                                    <bpelx:pattern>ITEM MASTER AND LOC</bpelx:pattern>
                                </bpelx:annotation>
                                <sequence name="Sequence_1">
                                    <assign name="TransformRMS_EBS">
                                        <bpelx:annotation>
                                            <bpelx:pattern>transformation</bpelx:pattern>
                                        </bpelx:annotation>
                                        <copy>
                                            <from expression="ora:processXSLT('TransformRMStoEBS.xsl',bpws:getVariableData('inputVariable','payload'))"/>
                                            <to variable="Invoke_I0142_ItemEBSIn_Input"
                                                part="InputParameters"/>
                                        </copy>
                                    </assign>
                                    <invoke name="Invoke_I0142_ItemEBSIn"
                                            portType="ns3:I0142_ItemEBSIn_ptt"
                                            inputVariable="Invoke_I0142_ItemEBSIn_Input"
                                            outputVariable="Invoke_I0142_ItemEBSIn_Output"
                                            partnerLink="I0142_ItemEBSIn"
                                            operation="I0142_ItemEBSIn"/>
                                    <assign name="Asn_ItemMasterError">
                                        <copy>
                                            <from expression="ora:integer(bpws:getVariableData('Invoke_I0142_ItemEBSIn_Output','OutputParameters','/ns5:OutputParameters/X_MESSAGE_CODE'))"/>
                                            <to variable="var_ebs_errorcd"/>
                                        </copy>
                                        <copy>
                                            <from expression="substring(concat(bpws:getVariableData('var_processname'),bpws:getVariableData('Invoke_I0142_ItemEBSIn_Output','OutputParameters','/ns5:OutputParameters/X_MESSAGE_DATA')),1,2000)"/>
                                            <to variable="var_ebs_error_msg"/>
                                        </copy>
                                    </assign>
                                </sequence>
                            </case>
                            <case condition="bpws:getVariableData('inputVariable','payload','/ns1:SYNC_ITEM_MASTER/ns1:VERB/ns1:ACTION_TYPE')='ITEM_SUPP' 
                                          or bpws:getVariableData('inputVariable','payload','/ns1:SYNC_ITEM_MASTER/ns1:VERB/ns1:ACTION_TYPE')='ITEM_SUPP_COO'
                                          or bpws:getVariableData('inputVariable','payload','/ns1:SYNC_ITEM_MASTER/ns1:VERB/ns1:ACTION_TYPE')='ITEM_SUPP_UPC'
                                          or bpws:getVariableData('inputVariable','payload','/ns1:SYNC_ITEM_MASTER/ns1:VERB/ns1:ACTION_TYPE')='ITEM_SUPP_COUNTRY'
                                          or bpws:getVariableData('inputVariable','payload','/ns1:SYNC_ITEM_MASTER/ns1:VERB/ns1:ACTION_TYPE')='ITEM_SUPP_COUNTRY_DIM'
                                          or bpws:getVariableData('inputVariable','payload','/ns1:SYNC_ITEM_MASTER/ns1:VERB/ns1:ACTION_TYPE')='ITEM_SUPP_COUNTRY_LOC'">

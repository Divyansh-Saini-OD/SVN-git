                                            <to variable="Invoke_XX_PO_ASL_API_Input"
                                                part="InputParameters"/>
                                        </copy>
                                    </assign>
                                    <invoke name="Invoke_XX_PO_ASL_API"
                                            partnerLink="XX_PO_ASL_API"
                                            portType="ns9:XX_PO_ASL_API_ptt"
                                            operation="XX_PO_ASL_API"
                                            inputVariable="Invoke_XX_PO_ASL_API_Input"
                                            outputVariable="Invoke_XX_PO_ASL_API_Output"/>
                                    <assign name="Asn_ItemSuppError">
                                        <copy>
                                            <from expression="ora:integer(bpws:getVariableData('Invoke_XX_PO_ASL_API_Output','OutputParameters','/ns10:OutputParameters/ns10:X_MESSAGE_CODE'))"/>
                                            <to variable="var_ebs_errorcd"/>
                                        </copy>
                                        <copy>
                                            <from expression="ora:integer(bpws:getVariableData('Invoke_XX_PO_ASL_API_Output','OutputParameters','/ns10:OutputParameters/ns10:X_MESSAGE_DATA'))"/>
                                            <to variable="var_ebs_error_msg"/>
                                        </copy>
                                    </assign>
                                </sequence>
                            </case>
                            <otherwise>
                                <terminate name="Terminate"/>
                            </otherwise>
                        </switch>
                        <assign name="Asn_retry_invoke">
                            <copy>
                                <from expression="'N'"/>
                                <to variable="retry_invoke"/>
                            </copy>
                        </assign>
                        <switch name="Switch_I0142_itemEBSIn">
                            <case condition="bpws:getVariableData('Invoke_I0142_ItemEBSIn_Output','OutputParameters','/ns5:OutputParameters/X_MESSAGE_CODE')=-1">
                                <bpelx:annotation>
                                    <bpelx:pattern>EBS Error</bpelx:pattern>
                                </bpelx:annotation>
                                <sequence name="Seq_EBSError">
                                    <assign name="AsnInvI0142toItemMas_confirm">
                                        <copy>
                                            <from variable="inputVariable"
                                                  part="payload"
                                                  query="/ns1:SYNC_ITEM_MASTER/ns1:VERB/ns1:PROCESS_NAME"/>
                                            <to variable="InvI0142EBSIn_ItemMas_confirm_inb_Input"
                                                part="payload"
                                                query="/ns1:SYNC_ITEM_MASTER_CONFIRM/ns1:VERB/ns1:PROCESS_NAME"/>
                                        </copy>
                                        <copy>
                                            <from variable="inputVariable"
                                                  part="payload"
                                                  query="/ns1:SYNC_ITEM_MASTER/ns1:VERB/ns1:PROCESS_ID"/>
                                            <to variable="InvI0142EBSIn_ItemMas_confirm_inb_Input"
                                                part="payload"
                                                query="/ns1:SYNC_ITEM_MASTER_CONFIRM/ns1:VERB/ns1:PROCESS_ID"/>
                                        </copy>
                                        <copy>
                                            <from variable="var_ebs_errorcd"/>
                                            <to variable="InvI0142EBSIn_ItemMas_confirm_inb_Input"
                                                part="payload"
                                                query="/ns1:SYNC_ITEM_MASTER_CONFIRM/ns1:NOUN/ns1:ERROR/ns1:CODE"/>
                                        </copy>
                                        <copy>

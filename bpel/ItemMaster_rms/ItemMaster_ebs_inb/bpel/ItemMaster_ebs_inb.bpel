                                </bpelx:annotation>
                                <sequence name="Sequence_2">
                                    <assign name="Transfom_ItemSupp">
                                        <bpelx:annotation>
                                            <bpelx:pattern>transformation</bpelx:pattern>
                                        </bpelx:annotation>
                                        <copy>

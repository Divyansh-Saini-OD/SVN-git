<?xml version = '1.0' encoding = 'UTF-8'?>
<BPELSuitcase>
   <BPELProcess id="FTPAPNonStandardInvoice" src="FTPAPNonStandardInvoice.bpel">
      <partnerLinkBindings>
         <partnerLinkBinding name="FTPNonStdFileGet">
            <property name="wsdlLocation">FTPNonStdFileGet.wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODErrorLogger">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/default/ODErrorLogger/ODErrorLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ProcessAPInvoiceFiles">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/fin/ProcessAPInvoiceFiles/ProcessAPInvoiceFiles?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="FileNonStdFilePut">
            <property name="wsdlLocation">FileNonStdFilePut.wsdl</property>
            <property name="Outbound_Path" type="LogicalDirectory"></property>
            <property name="retryInterval">60</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ODTransactionLogger">
            <property name="wsdlLocation">http://chilsoa01d.na.odcorp.net:7778/orabpel/default/ODTransactionLogger/ODTransactionLogger?wsdl</property>
         </partnerLinkBinding>
         <partnerLinkBinding name="ThirdPartyVendorFilePut">
            <property name="wsdlLocation">ThirdPartyVendorFilePut.wsdl</property>
            <property name="retryInterval">60</property>
            <property name="APInvoice_Directory" type="LogicalDirectory"></property>
         </partnerLinkBinding>
      </partnerLinkBindings>
      <activationAgents>
         <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="FTPNonStdFileGet">
            <property name="Inbound_Path" type="LogicalDirectory"></property>
            <property name="portType">Get_ptt</property>
            <property name="Archive_Path" type="LogicalArchiveDirectory"></property>
         </activationAgent>
      </activationAgents>
   </BPELProcess>
</BPELSuitcase>

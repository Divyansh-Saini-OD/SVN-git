<?xml version="1.0" encoding="windows-1252" ?>
<project default="compile" basedir=".">
    <target name="compile">

        <echo>
--------------------------------------------------------------
| Compiling bpel process ${process.name}, revision ${rev}
--------------------------------------------------------------
        </echo>
    <bpelc rev="${rev}" input="${process.dir}/bpel/bpel.xml" out="${process.dir}/output" home="${bpel.home}">
       <customize> 
  	  <partnerLinkBinding name="FTPNonStdFileGet">
	       <property name="wsdlLocation">FTPNonStdFileGet.wsdl</property>
	  </partnerLinkBinding>
          <partnerLinkBinding name="ODErrorLogger">
     	       <property name="wsdlLocation">http://${hostname}:${http.port}/orabpel/${ODErrorLogger.domain}/ODErrorLogger/ODErrorLogger?wsdl</property>
          </partnerLinkBinding>	  
         <partnerLinkBinding name="ProcessAPInvoiceFiles">
            <property name="wsdlLocation">http://${hostname}:${http.port}/orabpel/${ProcessAPInvoiceFiles.domain}/ProcessAPInvoiceFiles/ProcessAPInvoiceFiles?wsdl</property>
         </partnerLinkBinding>
          <partnerLinkBinding name="ODTransactionLogger">
                   <property name="wsdlLocation">http://${hostname}:${http.port}/orabpel/${ODTransactionLogger.domain}/ODTransactionLogger/ODTransactionLogger?wsdl</property>
          </partnerLinkBinding>
		  <partnerLinkBinding name="FileNonStdFilePut">
            <property name="wsdlLocation">FileNonStdFilePut.wsdl</property>
           <property name="Outbound_Path" >${ebs.temp.directory}invoice/nonstandard</property>                
          </partnerLinkBinding>
          <partnerLinkBinding name="ThirdPartyVendorFilePut">
            <property name="wsdlLocation">ThirdPartyVendorFilePut.wsdl</property>
            <property name="APInvoice_Directory">${ebs.apinvoice.directory}</property>
          </partnerLinkBinding>
		  
          <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="FTPNonStdFileGet">
            <property name="rejectedMessageHandlers" >file://${rejectedMessage.basedirectory}/${process.name}                bpel://${default.domain.locator}|${rejectedMessage.processname}|${rejectedMessage.operation}|message</property>
            <property name="fatalErrorFailoverProcess">bpel://${default.domain.locator}|${fatalErrorMessage.processname}|${fatalErrorMessage.operation}|message</property>
            <property name="Inbound_Path">xxfin/ftp/in/invoice/nonstandard</property>
            <property name="portType">Get_ptt</property>
            <property name="Archive_Path" >${ebs.archive.directory}xxfin/inbound</property>
          </activationAgent>
        </customize>
      </bpelc>
    </target>
</project>

  Oracle JDeveloper BPEL Designer 
  
  Created: Fri Jul 25 15:51:17 EDT 2008
  Author:  Srinidhi-Krishna
  Purpose: Empty BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="FTPTDMSupplier"
         targetNamespace="http://xmlns.oracle.com/FTPTDMOut"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns13="ODError"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/file/attachment/"
         xmlns:ns7="http://xmlns.oracle.com/TransactionData"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns12="http://xmlns.oracle.com/ODErrorLogger"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/ftp/"
         xmlns:client="http://xmlns.oracle.com/FTPTDMOut"
         xmlns:ns6="http://xmlns.oracle.com/ODTransactionLogger"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:ns11="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ns9="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/ftp/ReadTDMFile/"
         xmlns:ns14="http://xmlns.oracle.com/BPELInvokeESP"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/ftp/WriteToFTPServer/"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/ftp/WriteTDMOutbound/"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns10="http://www.oracle.com/XSL/Transform/java/oracle.tip.esb.server.headers.ESBHeaderFunctions"
         xmlns:ns8="http://xmlns.oracle.com/bpel/workflow/xpath">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <partnerLink myRole="Get_role" name="ReadTDMFile"
                 partnerLinkType="ns1:Get_plt"/>
    <partnerLink name="WriteTDMOutbound" partnerRole="Put_role"
                 partnerLinkType="ns2:Put_plt"/>
    <partnerLink name="WriteToFTPServer" partnerRole="Put_role"
                 partnerLinkType="ns3:Put_plt"/>
    <partnerLink myRole="ODTransactionLoggerRequester"
                 name="ODTransactionLogger"
                 partnerRole="ODTransactionLoggerProvider"
                 partnerLinkType="ns6:ODTransactionLogger"/>
    <partnerLink myRole="ODErrorLoggerRequester" name="ODErrorLogger"
                 partnerRole="ODErrorLoggerProvider"
                 partnerLinkType="ns12:ODErrorLogger"/>
    <partnerLink myRole="BPELInvokeESPRequester" name="BPELInvokeESP"
                 partnerRole="BPELInvokeESPProvider"
                 partnerLinkType="ns14:BPELInvokeESP"/>
  </partnerLinks>
  <variables>
    <variable name="rcvTDMFile_Get_InputVariable"
              messageType="ns1:attachmentElement_msg"/>
    <variable name="invkTDMOutbound_Put_InputVariable"
              messageType="ns2:attachmentElement_msg"/>
    <variable name="invkTDMFTPServer_Put_InputVariable"
              messageType="ns3:attachmentElement_msg"/>
    <variable name="File_Name_In" element="ns5:InboundFTPHeaderType"/>
    <variable name="File_Name_Out" element="ns5:OutboundFTPHeaderType"/>
    <variable name="invkStartTransactionData_initiate_InputVariable"
              messageType="ns6:ODTransactionLoggerRequestMessage"/>
    <variable name="ProcessHeader" type="xsd:string"/>
    <variable name="invkODErrorLogger_initiate_InputVariable"
              messageType="ns12:ODErrorLoggerRequestMessage"/>
    <variable name="xmlFragment" type="xsd:string"/>
    <variable name="invkESP_initiate_InputVariable"
              messageType="ns14:BPELInvokeESPRequestMessage"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="Sequence_3">
        <scope name="logTransactionError">
          <sequence name="Sequence_4">
            <assign name="asnLogTransactionError">
              <copy>
                <from expression="'Error'"/>
                <to variable="invkStartTransactionData_initiate_InputVariable"
                    part="payload"
                    query="/ns7:TransactionData/ns7:ProcessInfo/ns7:ProcessStatus"/>
              </copy>
            </assign>
            <invoke name="invkLogTransactionError"
                    partnerLink="ODTransactionLogger"
                    portType="ns6:ODTransactionLogger" operation="initiate"
                    inputVariable="invkStartTransactionData_initiate_InputVariable"/>
          </sequence>
        </scope>
        <assign name="asnCatchAll">
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="invkODErrorLogger_initiate_InputVariable"
                part="payload"
                query="/ns13:ErrorData/ns13:ProcessInfo/ns13:BusinessProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getInstanceId()"/>
            <to variable="invkODErrorLogger_initiate_InputVariable"
                part="payload"
                query="/ns13:ErrorData/ns13:ProcessInfo/ns13:BusinessProcessId"/>
          </copy>
          <copy>
            <from expression="ora:getDomainId()  "/>
            <to variable="invkODErrorLogger_initiate_InputVariable"
                part="payload"
                query="/ns13:ErrorData/ns13:ProcessInfo/ns13:BusinessProcessDomain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessURL()"/>
            <to variable="invkODErrorLogger_initiate_InputVariable"
                part="payload"
                query="/ns13:ErrorData/ns13:ProcessInfo/ns13:SystemName"/>
          </copy>
          <copy>
            <from expression="'Catch All on the Main Sequence'"/>
            <to variable="invkODErrorLogger_initiate_InputVariable"
                part="payload"
                query="/ns13:ErrorData/ns13:ProcessInfo/ns13:BusinessProcessStep"/>
          </copy>
          <copy>
            <from expression="ora:getFaultName()"/>
            <to variable="invkODErrorLogger_initiate_InputVariable"
                part="payload"
                query="/ns13:ErrorData/ns13:ErrorDetails/ns13:ErrorCode"/>
          </copy>
          <copy>
            <from expression="ora:getFaultAsString()"/>
            <to variable="invkODErrorLogger_initiate_InputVariable"
                part="payload"
                query="/ns13:ErrorData/ns13:ErrorDetails/ns13:ErrorText"/>
          </copy>
          <copy>
            <from expression="'system'"/>
            <to variable="invkODErrorLogger_initiate_InputVariable"
                part="payload"
                query="/ns13:ErrorData/ns13:ErrorDetails/ns13:ErrorType"/>
          </copy>
          <copy>
            <from expression="'fatal'"/>
            <to variable="invkODErrorLogger_initiate_InputVariable"
                part="payload"
                query="/ns13:ErrorData/ns13:ErrorDetails/ns13:ErrorSeverity"/>
          </copy>
          <copy>
            <from expression="ora:doc('${server_url}/xmllib/fin/GlobalParameters.xml','/GlobalParameters/FaultNotification/SendNotification')"/>
            <to variable="invkODErrorLogger_initiate_InputVariable"
                part="payload"
                query="/ns13:ErrorData/ns13:Notification/ns13:sendNotification"/>
          </copy>
          <copy>
            <from expression="concat(&quot;Error encountered in FTPTDMSupplier, while copying file &quot;,bpws:getVariableData('File_Name_In','/ns5:InboundFTPHeaderType/ns5:fileName'), &quot; to Target directories&quot;)"/>
            <to variable="invkODErrorLogger_initiate_InputVariable"
                part="payload"
                query="/ns13:ErrorData/ns13:ErrorDetails/ns13:ErrorDescription"/>
          </copy>
        </assign>
        <invoke name="invkODErrorLogger" partnerLink="ODErrorLogger"
                portType="ns12:ODErrorLogger" operation="initiate"
                inputVariable="invkODErrorLogger_initiate_InputVariable"/>
        <terminate name="Terminate"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <sequence name="main">
    <receive name="rcvTDMFile" createInstance="yes" partnerLink="ReadTDMFile"
             portType="ns1:Get_ptt" operation="Get"
             variable="rcvTDMFile_Get_InputVariable"
             bpelx:headerVariable="File_Name_In"/>
    <scope name="logTransactionData">
      <sequence name="Sequence_1">
        <assign name="asnStartTransactionDatat">
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:ProcessInfo/ns7:Domain"/>
          </copy>
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:ProcessInfo/ns7:ProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getInstanceId()"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:ProcessInfo/ns7:InstanceId"/>
          </copy>
          <copy>
            <from expression="'Start'"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:ProcessInfo/ns7:ProcessStatus"/>
          </copy>
          <copy>
            <from expression="'FTP' "/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:MessageDetails/ns7:MessageOperation"/>
          </copy>
          <copy>
            <from expression="'Opaque'   "/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:MessageDetails/ns7:MessageType"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()  "/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:ProcessInfo/ns7:transactionStartTime"/>
          </copy>
          <copy>
            <from expression="'EBS'"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:MessageDetails/ns7:MessageSourceSystem"/>
          </copy>
          <copy>
            <from>
              <EntiltyList xmlns="http://xmlns.oracle.com/TransactionData">
                <EntityID EntityType="SourceFileName"/>
              </EntiltyList>
            </from>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload" query="/ns7:TransactionData/ns7:EntiltyList"/>
          </copy>
          <copy>
            <from expression="concat('&lt;EntityID EntityType=&quot;SourceFileName&quot;>', bpws:getVariableData('File_Name_In','/ns5:InboundFTPHeaderType/ns5:fileName'), '&lt;/EntityID>')"/>
            <to variable="xmlFragment"/>
          </copy>
          <copy>
            <from expression="ora:parseEscapedXML(bpws:getVariableData('xmlFragment'))"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:EntiltyList/ns7:EntityID[1]"/>
          </copy>
          <copy>
            <from expression="'I0380_Supp_TDM'"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:ProcessInfo/ns7:SystemName"/>
          </copy>
          <copy>
            <from expression="'EBS'"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:ProcessInfo/ns7:TradingPartnerDetails/ns7:TPFrom"/>
          </copy>
          <copy>
            <from expression="'TDM'"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:ProcessInfo/ns7:TradingPartnerDetails/ns7:TPTo"/>
          </copy>
        </assign>
        <invoke name="invkStartTransactionData"
                partnerLink="ODTransactionLogger"
                portType="ns6:ODTransactionLogger" operation="initiate"
                inputVariable="invkStartTransactionData_initiate_InputVariable"/>
      </sequence>
    </scope>
    <scope name="setProcessHeader">
      <sequence name="Sequence_5">
        <assign name="asnProcessHeader">
          <copy>
            <from expression="substring(concat(&quot;Moving &quot;, bpws:getVariableData('File_Name_In','/ns5:InboundFTPHeaderType/ns5:fileName')),1,50)"/>
            <to variable="ProcessHeader"/>
          </copy>
        </assign>
        <bpelx:exec name="setTitle" language="java" version="1.5">
          <![CDATA[/*Write your java code below e.g.  
	System.out.println("Hello, World"); 
*/ 
 
try {                   
  String title = (String)getVariableData("ProcessHeader");                   
  setTitle(title);                   
  addAuditTrailEntry(title) ;                   
} catch (Exception ex) {                   
  addAuditTrailEntry(ex.toString());                   
  ex.printStackTrace();                   
}]]>
        </bpelx:exec>
      </sequence>
    </scope>
    <assign name="asnTDMFiles">
      <copy>
        <from variable="rcvTDMFile_Get_InputVariable" part="attachmentElement"
              query="/ns4:attachmentElement"/>
        <to variable="invkTDMOutbound_Put_InputVariable"
            part="attachmentElement" query="/ns4:attachmentElement"/>
      </copy>
      <copy>
        <from variable="rcvTDMFile_Get_InputVariable" part="attachmentElement"
              query="/ns4:attachmentElement"/>
        <to variable="invkTDMFTPServer_Put_InputVariable"
            part="attachmentElement" query="/ns4:attachmentElement"/>
      </copy>
      <copy>
        <from expression="bpws:getVariableData('File_Name_In','/ns5:InboundFTPHeaderType/ns5:fileName')"/>
        <to variable="File_Name_Out"
            query="/ns5:OutboundFTPHeaderType/ns5:fileName"/>
      </copy>
    </assign>
    <invoke name="invkTDMOutbound" partnerLink="WriteTDMOutbound"
            portType="ns2:Put_ptt" operation="Put"
            inputVariable="invkTDMOutbound_Put_InputVariable"
            bpelx:inputHeaderVariable="File_Name_Out"/>
    <invoke name="invkTDMFTPServer" partnerLink="WriteToFTPServer"
            portType="ns3:Put_ptt" operation="Put"
            inputVariable="invkTDMFTPServer_Put_InputVariable"
            bpelx:inputHeaderVariable="File_Name_Out"/>
    <scope name="callESP">
      <sequence name="Sequence_6">
        <assign name="asnESP">
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="invkESP_initiate_InputVariable" part="payload"
                query="/ns14:BPELInvokeESPProcessRequest/ns14:processName"/>
          </copy>
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="invkESP_initiate_InputVariable" part="payload"
                query="/ns14:BPELInvokeESPProcessRequest/ns14:processDomain"/>
          </copy>
          <copy>
            <from expression="'TDMsupplout'"/>
            <to variable="invkESP_initiate_InputVariable" part="payload"
                query="/ns14:BPELInvokeESPProcessRequest/ns14:filePattern"/>
          </copy>
        </assign>
        <invoke name="invkESP" partnerLink="BPELInvokeESP"
                portType="ns14:BPELInvokeESP" operation="initiate"
                inputVariable="invkESP_initiate_InputVariable"/>
      </sequence>
    </scope>
    <scope name="logTransactionEnd">
      <sequence name="Sequence_2">
        <assign name="asnLogTransactionEnd">
          <copy>
            <from expression="'Success'"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:ProcessInfo/ns7:ProcessStatus"/>
          </copy>
          <copy>
            <from>
              <EntiltyList xmlns="http://xmlns.oracle.com/TransactionData">
                <EntityID EntityType="SourceFileName"/>
                <EntityID EntityType="TargetFileName"/>
                <EntityID EntityType="ReleaseESPJob"/>
              </EntiltyList>
            </from>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload" query="/ns7:TransactionData/ns7:EntiltyList"/>
          </copy>
          <copy>
            <from expression="concat('&lt;EntityID EntityType=&quot;SourceFileName&quot;>', bpws:getVariableData('File_Name_In','/ns5:InboundFTPHeaderType/ns5:fileName'), '&lt;/EntityID>')"/>
            <to variable="xmlFragment"/>
          </copy>
          <copy>
            <from expression="ora:parseEscapedXML(bpws:getVariableData('xmlFragment'))"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:EntiltyList/ns7:EntityID[1]"/>
          </copy>
          <copy>
            <from expression="concat('&lt;EntityID EntityType=&quot;TargetFileName&quot;>', bpws:getVariableData('File_Name_Out','/ns5:OutboundFTPHeaderType/ns5:fileName'), '&lt;/EntityID>')"/>
            <to variable="xmlFragment"/>
          </copy>
          <copy>
            <from expression="ora:parseEscapedXML(bpws:getVariableData('xmlFragment'))"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:EntiltyList/ns7:EntityID[2]"/>
          </copy>
          <copy>
            <from expression="concat('&lt;EntityID EntityType=&quot;ReleaseESPJob&quot;>', 'YES', '&lt;/EntityID>')"/>
            <to variable="xmlFragment"/>
          </copy>
          <copy>
            <from expression="ora:parseEscapedXML(bpws:getVariableData('xmlFragment'))"/>
            <to variable="invkStartTransactionData_initiate_InputVariable"
                part="payload"
                query="/ns7:TransactionData/ns7:EntiltyList/ns7:EntityID[3]"/>
          </copy>
        </assign>
        <invoke name="invkLogTransactionEnd" partnerLink="ODTransactionLogger"
                portType="ns6:ODTransactionLogger" operation="initiate"
                inputVariable="invkStartTransactionData_initiate_InputVariable"/>
      </sequence>
    </scope>
  </sequence>
</process>
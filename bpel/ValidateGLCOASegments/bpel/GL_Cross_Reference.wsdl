<definitions
     name="GL_Cross_Reference"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/db/GL_Cross_Reference/"
     xmlns="http://schemas.xmlsoap.org/wsdl/"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/db/GL_Cross_Reference/"
     xmlns:db="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_GL_CROSS_VALIDATION/"
     xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
     xmlns:hdr="http://xmlns.oracle.com/pcbpel/adapter/db/"
    >
    <import namespace="http://xmlns.oracle.com/pcbpel/adapter/db/" location="DBAdapterOutboundHeader.wsdl"/>
    <types>
     <schema xmlns="http://www.w3.org/2001/XMLSchema">
       <import namespace="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_GL_CROSS_VALIDATION/"
               schemaLocation="APPS_XX_GL_CROSS_VALIDATION.xsd" />
     </schema>
    </types>
    <message name="args_in_msg">
        <part name="InputParameters" element="db:InputParameters"/>
    </message>
    <message name="args_out_msg">
        <part name="OutputParameters" element="db:OutputParameters"/>
    </message>
    <portType name="GL_Cross_Reference_ptt">
        <operation name="GL_Cross_Reference">
            <input message="tns:args_in_msg"/>
            <output message="tns:args_out_msg"/>
        </operation>
    </portType>
    <binding name="GL_Cross_Reference_binding" type="tns:GL_Cross_Reference_ptt">
    <jca:binding  />
        <operation name="GL_Cross_Reference">
      <jca:operation
          SchemaName="APPS"
          ProcedureName="XX_GL_CROSS_VALIDATION"
          InteractionSpec="oracle.tip.adapter.db.DBStoredProcedureInteractionSpec" >
      </jca:operation>
      <input>
        <jca:header message="hdr:OutboundHeader_msg" part="outboundHeader"/>
      </input>
            <output/>
        </operation>
    </binding>
    <service name="GL_Cross_Reference">
        <port name="GL_Cross_Reference_pt" binding="tns:GL_Cross_Reference_binding">
<!--Your runtime connection is declared in 
J2EE_HOME/application-deployments/default/DbAdapter/oc4j-ra.xml 
These mcf properties here are from your design time connection and 
save you from having to edit that file and restart the application server 
if eis/db/EBSApps is missing. 
These mcf properties are safe to remove.-->
      <jca:address location="eis/db/EBSAppsFin" UIConnectionName="EIS_APPS_EBS_APPS_DBConn"
          ManagedConnectionFactory="oracle.tip.adapter.db.DBManagedConnectionFactory"
      />
        </port>
    </service>
  <plt:partnerLinkType name="GL_Cross_Reference_plt" >
    <plt:role name="GL_Cross_Reference_role" >
      <plt:portType name="tns:GL_Cross_Reference_ptt" />
    </plt:role>
  </plt:partnerLinkType>
</definitions>

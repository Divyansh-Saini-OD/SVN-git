<process name="ShipStatusInterface"
         targetNamespace="http://xmlns.oracle.com/ShipStatusInterface"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_PO_SHIPSTATUS_PKG/XX_PO_SHIPSTATUS_UPDATE/"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/db/ReadPO_Lib_Schema/"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns3="http://www.openapplications.org/oagis/9/officedepot/1"
         xmlns:ns2="http://www.openapplications.org/oagis/9"
         xmlns:client="http://xmlns.oracle.com/ShipStatusInterface"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc">
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <partnerLinks>
        <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
        <partnerLink name="client" partnerLinkType="client:ShipStatusInterface"
                     myRole="ShipStatusInterfaceProvider"
                     partnerRole="ShipStatusInterfaceRequester"/>
        <partnerLink name="ReadPO_Lib_Schema"
                     partnerRole="ReadPO_Lib_Schema_role"
                     partnerLinkType="ns1:ReadPO_Lib_Schema_plt"/>
    </partnerLinks>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <variables>
        <!-- Reference to the message passed as input during initiation -->
        <variable name="inputVariable"
                  messageType="client:ShipStatusInterfaceRequestMessage"/>
        <!-- Reference to the message that will be sent back to the requester during callback -->
        <variable name="outputVariable"
                  messageType="client:ShipStatusInterfaceResponseMessage"/>
        <variable name="Invoke_SS_Int_ReadPO_Lib_Schema_InputVariable"
                  messageType="ns1:args_in_msg"/>
        <variable name="Invoke_SS_Int_ReadPO_Lib_Schema_OutputVariable"
                  messageType="ns1:args_out_msg"/>
    </variables>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="main">
        <!-- Receive input from requestor. (Note: This maps to operation defined in ShipStatusInterface.wsdl) -->
        <receive name="Receive_Canonical" partnerLink="client"
                 portType="client:ShipStatusInterface" operation="initiate"
                 variable="inputVariable" createInstance="yes"/>
        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <assign name="Canonical_to_SP_Parms">
            <copy>
                <from variable="inputVariable" part="payload"
                      query="/ns2:ShowShipment/ns2:DataArea/ns2:Shipment/ns2:ShipmentHeader/ns2:Status/ns2:Code"/>
                <to variable="Invoke_SS_Int_ReadPO_Lib_Schema_InputVariable"
                    part="InputParameters"
                    query="/ns4:InputParameters/I_STATUS_CODE"/>
            </copy>
            <copy>
                <from variable="inputVariable" part="payload"
                      query="/ns2:ShowShipment/ns2:DataArea/ns2:Shipment/ns2:ShipmentHeader/ns2:Status/ns2:EffectiveDateTime"/>
                <to variable="Invoke_SS_Int_ReadPO_Lib_Schema_InputVariable"
                    part="InputParameters"
                    query="/ns4:InputParameters/I_SHIPDATETIME"/>
            </copy>
            <copy>
                <from variable="inputVariable" part="payload"
                      query="/ns2:ShowShipment/ns2:DataArea/ns2:Shipment/ns2:ShipmentHeader/ns2:Status/ns2:ReasonCode"/>
                <to variable="Invoke_SS_Int_ReadPO_Lib_Schema_InputVariable"
                    part="InputParameters"
                    query="/ns4:InputParameters/I_REASON_CD"/>
            </copy>
            <copy>
                <from variable="inputVariable" part="payload"
                      query="/ns2:ShowShipment/ns2:DataArea/ns2:Shipment/ns2:ShipmentHeader/ns2:CarrierParty/ns2:PartyIDs/ns2:ID"/>
                <to variable="Invoke_SS_Int_ReadPO_Lib_Schema_InputVariable"
                    part="InputParameters" query="/ns4:InputParameters/I_SCAC"/>
            </copy>
            <copy>
                <from variable="inputVariable" part="payload"
                      query="/ns2:ShowShipment/ns2:DataArea/ns2:Shipment/ns2:ShipmentItem/ns2:PurchaseOrderReference/ns2:DocumentID/ns2:ID"/>
                <to variable="Invoke_SS_Int_ReadPO_Lib_Schema_InputVariable"
                    part="InputParameters"
                    query="/ns4:InputParameters/I_DOCUMENT_NBR"/>
            </copy>
            <copy>
                <from variable="inputVariable" part="payload"
                      query="/ns2:ShowShipment/ns2:DataArea/ns2:Shipment/ns2:ShipmentHeader/ns2:DocumentID/ns2:ID"/>
                <to variable="Invoke_SS_Int_ReadPO_Lib_Schema_InputVariable"
                    part="InputParameters"
                    query="/ns4:InputParameters/I_BILL_OF_LADING"/>
            </copy>
            <copy>
                <from variable="inputVariable" part="payload"
                      query="/ns2:ShowShipment/ns2:DataArea/ns2:Shipment/ns2:ShipmentHeader/ns2:DocumentReference/ns2:DocumentID/ns2:ID"/>
                <to variable="Invoke_SS_Int_ReadPO_Lib_Schema_InputVariable"
                    part="InputParameters"
                    query="/ns4:InputParameters/I_PRO_BILL_NBR"/>
            </copy>
        </assign>
        <invoke name="Invoke_SS_Int" partnerLink="ReadPO_Lib_Schema"
                portType="ns1:ReadPO_Lib_Schema_ptt"
                operation="ReadPO_Lib_Schema"
                inputVariable="Invoke_SS_Int_ReadPO_Lib_Schema_InputVariable"
                outputVariable="Invoke_SS_Int_ReadPO_Lib_Schema_OutputVariable"/>
        <assign name="Store_SP_RetCode">
            <copy>
                <from variable="Invoke_SS_Int_ReadPO_Lib_Schema_OutputVariable"
                      part="OutputParameters"
                      query="/ns4:OutputParameters/XX_PO_SHIPSTATUS_UPDATE"/>
                <to variable="outputVariable" part="payload"
                    query="/client:ShipStatusInterfaceProcessResponse/client:result"/>
            </copy>
        </assign>
        <invoke name="callbackClient" partnerLink="client"
                portType="client:ShipStatusInterfaceCallback"
                operation="onResult" inputVariable="outputVariable"/>
    </sequence>
</process>
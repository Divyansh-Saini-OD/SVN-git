      <partnerLinkBinding name="DequeueMessage">
        <property name="wsdlLocation">DequeueMessage.wsdl</property>
      </partnerLinkBinding>
      <partnerLinkBinding name="POAcknowledgementProcess">
        <property name="wsdlLocation">http://chilsoa02d.na.odcorp.net:7777/orabpel/otc/POAcknowledgementProcess/POAcknowledgementProcess?wsdl</property>
        <property name="retryInterval">${db.retryInterval}</property>
        <property name="retryMaxCount">${db.retryMaxCount}</property>
        <property name="timeout">${db.timeout}</property>
      </partnerLinkBinding>
    </partnerLinkBindings>
    <activationAgents>
      <activationAgent className="oracle.tip.adapter.fw.agent.jca.JCAActivationAgent" partnerLink="DequeueMessage">
        <property name="portType">Dequeue_ptt</property>
      </activationAgent>
    </activationAgents>
  </BPELProcess>
</BPELSuitcase>

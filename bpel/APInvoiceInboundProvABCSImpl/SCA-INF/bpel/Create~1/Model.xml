<?xml version="1.0" encoding="UTF-8"?>
<process name="CreateAPInoiceAsync" targetNamespace="http://xmlns.officedepot.com/TDS/APInvoiceInboundProvABCSImpl/CreateAPInoiceAsync" xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/" xmlns:client="http://xmlns.officedepot.com/TDS/APInvoiceInboundProvABCSImpl/CreateAPInoiceAsync" xmlns:ora="http://schemas.oracle.com/xpath/extension" xmlns:bpelx="http://schemas.oracle.com/bpel/extension" xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/" xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/jms/TDS/APInvoiceInboundProvABCSImpl/CommonErrorLoggingService" xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/apps/TDS/APInvoiceInboundProvABCSImpl/APInvoiceCreationService" xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/db/APPS/XX_CS_TDS_AP_INVOICE_PKG/INSERT_PROC/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns4="http://xmlns.officedepot.com/TDS/APInvoiceInboundProvABCSImpl/APInvoiceResponseEBM" xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20" xmlns:ns6="http://www.officedepot.com/officedepot/ODComnErrorStructure/1.0" xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc" xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue" xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath" xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath" xmlns:ns5="http://www.officedepot.com/TDS/APInvoiceInboundProvABCSImpl/APInvoiceEBM" xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions" xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk" xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions" xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap" id="BpPrc0">

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
    <partnerLinks>
        <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
        <partnerLink name="createapinoiceasync_client" partnerLinkType="client:CreateAPInoiceAsync" myRole="CreateAPInoiceAsyncProvider" partnerRole="CreateAPInoiceAsyncRequester"/>
        <partnerLink name="CommonErrorLoggingService" partnerLinkType="ns1:Produce_Message_plt" partnerRole="Produce_Message_role"/>
        <partnerLink name="APInvoiceCreationService" partnerLinkType="ns2:APInvoiceCreationService_plt" partnerRole="APInvoiceCreationService_role"/>
    </partnerLinks>

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <variables>
        <!-- Reference to the message passed as input during initiation -->
        <variable name="inputVariable" messageType="client:CreateAPInoiceAsyncRequestMessage"/>

        <!-- Reference to the message that will be sent back to the requester during callback -->
        <variable name="outputVariable" messageType="client:CreateAPInoiceAsyncResponseMessage"/>
        <variable name="invokeAPInvokeCreationServiceOuput" messageType="ns2:args_out_msg"/>
        <variable name="invokeAPInvokeCreationServiceInput" messageType="ns2:args_in_msg"/>
    <variable name="InvokeCommonErrorLoggerToSendErrorDataInput" messageType="ns1:Produce_Message_msg"/>
  </variables>
  <faultHandlers id="BpFhs0">
    <catchAll id="BpCAl0">
      <sequence name="SequenceCatchAll" id="BpSeq0">
        <assign name="AssignErrorToReplyMessage" id="BpAss0">
          <copy>
            <from expression="ora:getPreference('EBIZ_ERROR_STATUS')"/>
            <to variable="outputVariable" part="payload" query="/ns4:APInvoiceResponseEBM/ns4:Status"/>
          </copy>
          <copy>
            <from expression="concat(ora:getPreference('ERROR_DESCRIPTION_TEXT'),bpws:getVariableData('inputVariable','payload','/ns5:ApInvoiceEBM/ns5:ApInvoiceEBO/ns5:ApInvoiceHeader/ns5:InvoiceNum'))"/>
            <to variable="outputVariable" part="payload" query="/ns4:APInvoiceResponseEBM/ns4:Message"/>
          </copy>
        </assign>
        <invoke name="callbackClientWithErrorMessage" bpelx:invokeAsDetail="no" inputVariable="outputVariable" partnerLink="createapinoiceasync_client" portType="client:CreateAPInoiceAsyncCallback" operation="APInvoiceCreationServiceResponse" id="BpInv0"/>
        <assign name="AssignPopulateCommonErrorLoggingData" id="BpAss1">
          <copy>
            <from expression="ora:getProcessId()"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ProcessInfo/ns6:ProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getPreference('ERROR_TYPE')"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ErrorDetails/ns6:ErrorType"/>
          </copy>
          <copy>
            <from expression="ora:getPreference('ERROR_SEVERITY')"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ErrorDetails/ns6:ErrorSeverity"/>
          </copy>
          <copy>
            <from expression="ora:getFaultAsString()"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ErrorDetails/ns6:ErrorText"/>
          </copy>
          <copy>
            <from expression="ora:getContentAsString(bpws:getVariableData('inputVariable','payload','/ns5:ApInvoiceEBM'))"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:MessageDetails/ns6:MessagePayload"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ErrorDetails/ns6:ErrorDateTime"/>
          </copy>
          <copy>
            <from expression="ora:getPreference('CATCH_ALL_PROCESS_STEP')"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ErrorDetails/ns6:ProcessStep"/>
          </copy>
          <copy>
            <from expression="ora:getFaultName()"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ErrorDetails/ns6:ErrorCode"/>
          </copy>
          <copy>
            <from expression="ora:getDomainId()"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ProcessInfo/ns6:Domain"/>
          </copy>
          <copy>
            <from expression="ora:getCompositeInstanceId()"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:MessageDetails/ns6:MessageID"/>
          </copy>
          <copy>
            <from expression="ora:getCurrentDateTime()"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:MessageDetails/ns6:MessageDateTime"/>
          </copy>
          <copy>
            <from expression="ora:getPreference('MESSAGE_TYPE')"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:MessageDetails/ns6:MessageType"/>
          </copy>
          <copy>
            <from expression="ora:getPreference('MESSAGE_VERSION')"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:MessageDetails/ns6:MessageVersion"/>
          </copy>
          <copy>
            <from expression="ora:getPreference('MESSAGE_OPERATION')"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:MessageDetails/ns6:MessageOperation"/>
          </copy>
          <copy>
            <from expression="ora:getPreference('SOURCE_SYSTEM_NAME')"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:MessageDetails/ns6:MessageSourceSystem"/>
          </copy>
          <copy>
            <from expression="concat(ora:getPreference('ERROR_DESCRIPTION_TEXT'),bpws:getVariableData('inputVariable','payload','/ns5:ApInvoiceEBM/ns5:ApInvoiceEBO/ns5:ApInvoiceHeader/ns5:InvoiceNum'))"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ErrorDetails/ns6:ErrorDescription"/>
          </copy>
          <copy>
            <from expression="ora:getProcessURL()"/>
            <to variable="InvokeCommonErrorLoggerToSendErrorDataInput" part="body" query="/ns6:ODComnErrorStructure/ns6:ODComnErrorStructureList/ns6:ProcessInfo/ns6:SystemName"/>
          </copy>
        </assign>
        <invoke name="InvokeCommonErrorLoggerToSendErrorData" inputVariable="InvokeCommonErrorLoggerToSendErrorDataInput" partnerLink="CommonErrorLoggingService" portType="ns1:Produce_Message_ptt" operation="Produce_Message" bpelx:invokeAsDetail="no" id="BpInv1"/>
      </sequence>
    </catchAll>
  </faultHandlers>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="main" id="BpSeq1">
        <!-- Receive input from requestor. (Note: This maps to operation defined in CreateAPInoiceAsync.wsdl) -->
        <receive name="receiveInput" partnerLink="createapinoiceasync_client" portType="client:CreateAPInoiceAsync" operation="APInvoiceCreationService" variable="inputVariable" createInstance="yes" id="BpRcv0"/>

        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
    <assign name="TransformAPInvoiceEBMToABMFormat" id="BpAss2">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:doXSLTransformForDoc('xsl/TransformAPInvoiceEBMToABMFormat.xsl', $inputVariable.payload)"/>
        <to variable="invokeAPInvokeCreationServiceInput" part="InputParameters"/>
      </copy>
    </assign>
    <invoke name="InvokeAPInvokeCreationService" inputVariable="invokeAPInvokeCreationServiceInput" outputVariable="invokeAPInvokeCreationServiceOuput" partnerLink="APInvoiceCreationService" portType="ns2:APInvoiceCreationService_ptt" operation="APInvoiceCreationService" bpelx:invokeAsDetail="no" id="BpInv2"/>
        <assign name="AssignResponseFromServiceToReply" id="BpAss3">
            <copy>
                <from variable="invokeAPInvokeCreationServiceOuput" part="OutputParameters" query="/ns3:OutputParameters/ns3:X_STATUS"/>
                <to variable="outputVariable" part="payload" query="/ns4:APInvoiceResponseEBM/ns4:Status"/>
            </copy>
            <copy>
                <from variable="invokeAPInvokeCreationServiceOuput" part="OutputParameters" query="/ns3:OutputParameters/ns3:X_MSG_DATA"/>
                <to variable="outputVariable" part="payload" query="/ns4:APInvoiceResponseEBM/ns4:Message"/>
            </copy>
        </assign>
        <invoke name="callbackClientWithResponseFromAPInvoiceCreationService" partnerLink="createapinoiceasync_client" portType="client:CreateAPInoiceAsyncCallback" operation="APInvoiceCreationServiceResponse" inputVariable="outputVariable" id="BpInv3"/>
    </sequence>
</process><!--

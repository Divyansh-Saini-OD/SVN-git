<process name="DPSCancelOrderProcess_Main"
         targetNamespace="http://xmlns.oracle.com/DPSCancelOrderProcess_Main"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/apps/BusinessEventRead/"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns3="http://xmlns.oracle.com/xdb/APPS"
         xmlns:ns2="http://xmlns.oracle.com/DPSCancel_SendSalesOrderInfo"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:client="http://xmlns.oracle.com/DPSCancelOrderProcess_Main"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:orcl="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <partnerLink myRole="DEQUEUE_role" name="BusinessEventRead"
                 partnerLinkType="ns1:DEQUEUE_plt"/>
    <partnerLink name="SendSalesOrderInfo"
                 partnerRole="DPSCancel_SendSalesOrderInfoProvider"
                 partnerLinkType="ns2:DPSCancel_SendSalesOrderInfo"/>
  </partnerLinks>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <variable name="ReceiveBusinessEventIn" messageType="ns1:WF_EVENT_T_msg"/>
    <variable name="InvokeOrderInfoProcessIn"
              messageType="ns2:DPSCancel_SendSalesOrderInfoRequestMessage"/>
    <variable name="InvokeOrderInfoProcessOut"
              messageType="ns2:DPSCancel_SendSalesOrderInfoResponseMessage"/>
  </variables>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <receive name="ReceiveBusinessEvent" partnerLink="BusinessEventRead"
             portType="ns1:DEQUEUE_ptt" operation="DEQUEUE"
             variable="ReceiveBusinessEventIn" createInstance="yes"/>
    <assign name="AssignSessionID">
      <copy>
        <from variable="ReceiveBusinessEventIn" part="WF_EVENT_T"
              query="/ns3:WF_EVENT_T/EVENT_KEY"/>
        <to variable="InvokeOrderInfoProcessIn" part="payload"
            query="/ns2:DPSCancel_SendSalesOrderInfoProcessRequest/ns2:input"/>
      </copy>
    </assign>
    <invoke name="InvokeOrderInfoProcess" partnerLink="SendSalesOrderInfo"
            portType="ns2:DPSCancel_SendSalesOrderInfo" operation="process"
            inputVariable="InvokeOrderInfoProcessIn"
            outputVariable="InvokeOrderInfoProcessOut"/>
  </sequence>
</process>
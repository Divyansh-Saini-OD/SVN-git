<?xml version="1.0" encoding="windows-1252" ?>
<project default="compile" basedir=".">
    <target name="compile">

        <echo>
--------------------------------------------------------------
| Compiling bpel process ${process.name}, revision ${rev}
--------------------------------------------------------------
        </echo>

        <bpelc rev="${rev}" input="${process.dir}/bpel/bpel.xml" out="${process.dir}/output">
         <customize> 
            <partnerLinkBinding name="SendSalesOrderInfo">
              <property name="wsdlLocation">http://${http.hostname}:${http.port}/orabpel/${domain}/DPSCancel_SendSalesOrderInfo/DPSCancel_SendSalesOrderInfo?wsdl</property>
			   <property name="retryInterval">${db.retryInterval}</property>
			   <property name="retryMaxCount">${db.retryMaxCount}</property>
			   <property name="timeout">${db.timeout}</property>
            </partnerLinkBinding>
          </customize>
        </bpelc>
    </target>
</project>